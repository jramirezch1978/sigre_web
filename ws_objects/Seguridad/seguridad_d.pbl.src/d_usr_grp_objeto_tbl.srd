 retrieve="select lower(os.sistema) sistema, go.objeto, substr(os.obs_seguridad,2) obs_seguridad, 
       decode( go.flag_insertar,  '1', 'I','-') flag_insertar,  decode( go.flag_eliminar, '1', 'E','-') flag_eliminar, 
       decode( go.flag_modificar, '1', 'M','-') flag_modificar, decode( go.flag_consultar, '1', 'C','-') flag_consultar,
       decode( go.flag_anular,    '1', 'A','-') flag_anular,    decode( go.flag_cancelar,  '1', 'K','-') flag_cancelar,
       lower(go.grupo) grupo
  from grp_obj go, usr_grp ug, objeto_sis os
 where go.grupo = ug.grupo and go.objeto = os.objeto and ug.cod_usr = :as_usuario 
   and go.grupo <> 'UsuRes' and go.grupo <> 'ADMIN'
 union all
 select lower(os.sistema) sistema, uo.objeto, substr(os.obs_seguridad,2) obs_seguridad,  
       decode( uo.flag_insertar,  '1', 'I','-') flag_insertar,  decode( uo.flag_eliminar, '1', 'E','-') flag_eliminar, 
       decode( uo.flag_modificar, '1', 'M','-') flag_modificar, decode( uo.flag_consultar, '1', 'C','-') flag_consultar,
       decode( uo.flag_anular,    '1', 'A','-') flag_anular,    decode( uo.flag_cancelar,  '1', 'K','-') flag_cancelar,
       null grupo
 from usuario_obj uo, objeto_sis os
where uo.objeto = os.objeto and uo.cod_usr = :as_usuario
order by sistema, obs_seguridad, objeto, grupo" arguments=(("as_usuario", string))  sort="objeto_sis_sistema A grp_obj_objeto A " )
