 retrieve="SELECT ~"USR_GRP~".~"COD_USR~",
       ~"USUARIO~".~"NOMBRE~",   
       ~"GRP_OBJ~".~"FLAG_INSERTAR~",   
       ~"GRP_OBJ~".~"FLAG_ELIMINAR~",   
       ~"GRP_OBJ~".~"FLAG_MODIFICAR~",   
       ~"GRP_OBJ~".~"FLAG_CONSULTAR~",
       ~"GRP_OBJ~".~"FLAG_ANULAR~", 
       ~"GRP_OBJ~".~"FLAG_CANCELAR~",
       ~"GRP_OBJ~".~"GRUPO~", 
       ~"USUARIO~".~"FLAG_ESTADO~" 
  FROM ~"GRP_OBJ~",   
       ~"USR_GRP~",
       ~"USUARIO~"
 WHERE ( ~"GRP_OBJ~".~"GRUPO~" = ~"USR_GRP~".~"GRUPO~" ) and
       ( ~"USUARIO~".~"COD_USR~" = ~"USR_GRP~".~"COD_USR~") and
       ( ~"GRP_OBJ~".~"OBJETO~" = :as_objeto )  and
       ( ~"GRP_OBJ~".~"GRUPO~" <> 'UsuRes' )
 UNION        
SELECT ~"USUARIO_OBJ~".~"COD_USR~",
       ~"USUARIO~".~"NOMBRE~",   
