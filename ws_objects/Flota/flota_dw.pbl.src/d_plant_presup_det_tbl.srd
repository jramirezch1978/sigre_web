         FPPD.CNTA_PRSP,   
			FPPD.CENCOS,
			FPPD.ITEM,
			FPPD.DESCRIPCION,
			CC.DESC_CENCOS,
         FPPD.RATIO,   
         PC.DESCRIPCION,
			FPPD.FLAG_REPLICACION  
    FROM FL_PLANT_PRESUP_DET FPPD,   
         PRESUPUESTO_CUENTA PC,
			CENTROS_COSTO CC  
