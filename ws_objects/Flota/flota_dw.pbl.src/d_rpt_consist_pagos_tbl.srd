table(column=(type=char(8) updatewhereclause=yes name=cod_trabajador dbname="maestro.cod_trabajador" )
 column=(type=char(93) updatewhereclause=yes name=nom_tripulante dbname="nom_tripulante" )
 column=(type=number updatewhereclause=yes name=dias dbname="dias" )
 column=(type=number updatewhereclause=yes name=remuneracion dbname="remuneracion" )
 column=(type=number updatewhereclause=yes name=bonificacion dbname="bonificacion" )
 column=(type=number updatewhereclause=yes name=gratificacion dbname="gratificacion" )
 column=(type=number updatewhereclause=yes name=vacaciones dbname="vacaciones" )
 column=(type=number updatewhereclause=yes name=cts dbname="cts" )
 column=(type=number updatewhereclause=yes name=quinta_categ_extra dbname="quinta_categ_extra" )
 column=(type=number updatewhereclause=yes name=bonif_extra dbname="bonif_extra" )
 retrieve="SELECT m.cod_trabajador,
       m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1 AS nom_tripulante,
       COUNT(*) AS dias,
       (SELECT NVL(SUM(t.participacion_pesca),0) 
          FROM fl_participacion_pesca t
         WHERE t.tripulante = m.cod_trabajador
           and to_number(to_char(t.fecha, 'yyyy')) = :an_year
			  and to_number(to_char(t.fecha, 'mm')) between :an_mes1 and :an_mes2 ) AS remuneracion,
       (SELECT NVL(SUM(t.bonificacion_pesca),0) 
          FROM fl_participacion_pesca t
         WHERE t.tripulante = m.cod_trabajador
		     and to_number(to_char(t.fecha, 'yyyy')) = :an_year
			  and to_number(to_char(t.fecha, 'mm')) between :an_mes1 and :an_mes2) AS bonificacion,
       (SELECT NVL(SUM(t.gratificacion),0) 
          FROM fl_participacion_pesca t
         WHERE t.tripulante = m.cod_trabajador
			  and to_number(to_char(t.fecha, 'yyyy')) = :an_year
			  and to_number(to_char(t.fecha, 'mm')) between :an_mes1 and :an_mes2) AS gratificacion,
       (SELECT NVL(SUM(t.vacaciones),0) 
          FROM fl_participacion_pesca t
         WHERE t.tripulante = m.cod_trabajador
			  and to_number(to_char(t.fecha, 'yyyy')) = :an_year
			  and to_number(to_char(t.fecha, 'mm')) between :an_mes1 and :an_mes2) AS vacaciones,
       (SELECT NVL(SUM(t.vacaciones),0) 
          FROM fl_participacion_pesca t
         WHERE t.tripulante = m.cod_trabajador
			  and to_number(to_char(t.fecha, 'yyyy')) = :an_year
			  and to_number(to_char(t.fecha, 'mm')) between :an_mes1 and :an_mes2) AS CTS,
       (SELECT NVL(SUM(usf_fl_conv_mon(t2.dscto_quinta_categ, cp.cod_moneda, (SELECT cod_soles FROM logparam WHERE reckey = '1'), cp.fecha_emision) ),0) 
          FROM fl_consist_incent_trab t2,
               cntas_pagar            cp
         WHERE cp.tipo_doc = t2.tipo_doc
           AND cp.nro_doc  = t2.nro_doc
           AND cp.cod_relacion = t2.cod_relacion
           AND t2.cod_relacion = m.cod_trabajador
			  and t2.ano = :an_year
			  and t2.mes between :an_mes1 and :an_mes2) AS quinta_categ_extra,
       (SELECT NVL(SUM(usf_fl_conv_mon(cpd.importe, cp.cod_moneda, (SELECT cod_soles FROM logparam WHERE reckey = '1'), cp.fecha_emision) ),0) 
          FROM fl_consist_incent_trab t2,
               cntas_pagar_det        cpd,
               cntas_pagar            cp
         WHERE cpd.tipo_doc = t2.tipo_doc
           AND cpd.nro_doc  = t2.nro_doc
           AND cpd.cod_relacion = t2.cod_relacion
           AND cp.tipo_doc      = t2.tipo_doc
           AND cp.nro_doc       = t2.nro_doc
           AND cp.cod_relacion  = t2.cod_relacion
           AND t2.cod_relacion = m.cod_trabajador
			  and t2.ano = :an_year
			  and t2.mes between :an_mes1 and :an_mes2) AS bonif_extra
FROM fl_asistencia a,
     maestro       m
WHERE a.tripulante = m.cod_trabajador 
  and to_number(to_char(a.fecha, 'yyyy')) = :an_year
  and to_number(to_char(a.fecha, 'mm')) between :an_mes1 and :an_mes2   
GROUP BY m.cod_trabajador,
       m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1" arguments=(("an_year", number),("an_mes1", number),("an_mes2", number)) )
