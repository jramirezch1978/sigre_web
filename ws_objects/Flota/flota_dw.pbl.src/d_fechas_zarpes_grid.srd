 column=(type=char(10) update=yes updatewhereclause=yes name=registro_zarpe dbname="fl_parte_de_pesca.registro_zarpe" )
 column=(type=datetime update=yes updatewhereclause=yes name=fecha_hora_zarpe dbname="fl_parte_de_pesca.fecha_hora_zarpe" )
 column=(type=number update=yes updatewhereclause=yes name=tripulantes dbname="tripulantes" )
 retrieve="select flpp.parte_pesca, flpp.registro_zarpe, flpp.fecha_hora_zarpe, 
       (select count(*) from fl_tripulacion_zarpe fltz 
               where fltz.parte_pesca = flpp.parte_pesca ) as tripulantes
from fl_parte_de_pesca flpp
where flpp.nave_real = :ls_cod_nave
  and Trunc(flpp.fecha_hora_zarpe) >= :ld_fecha1
  and Trunc(flpp.fecha_hora_zarpe) <= :ld_fecha2
  and flpp.parte_pesca <> :ls_parte_pesca
order by flpp.parte_pesca" update="fl_parte_de_pesca" updatewhere=1 updatekeyinplace=no arguments=(("ls_cod_nave", string),("ls_parte_pesca", string),("ld_fecha1", date),("ld_fecha2", date)) )
