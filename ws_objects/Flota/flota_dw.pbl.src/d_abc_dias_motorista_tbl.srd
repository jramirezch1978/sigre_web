 retrieve="SELECT a.mes, a.anio, a.cod_motorista,
       t.NOM_TRABAJADOR,
       a.nro_dias,
     	a.cargo_tripulante,
     	fc.descr_cargo,
     	a.nave,
     	tg.nomb_nave
FROM   fl_dias_motorista   	a,
       vw_pr_trabajador    	t,
    	 TG_NAVES          		TG,
    	 fl_cargo_tripulantes   fc
where a.cod_motorista 	 = t.COD_TRABAJADOR     
  and a.cargo_tripulante = fc.cargo_tripulante
  and a.nave				 = tg.nave
  and a.anio				 = :an_year
  and a.mes					 = :an_mes
order by a.anio, a.mes, t.nom_trabajador" update="fl_dias_motorista" updatewhere=0 updatekeyinplace=yes arguments=(("an_year", number),("an_mes", number)) )
