 retrieve="  SELECT FLC.CLIENTE,   
         FLC.DIRECCION_OFICINA,   
         FLC.DIRECCION_PLANTA,   
         FLC.REPRESENTANTE_LEGAL,   
         FLC.TELEFONO01,   
         FLC.TELEFONO02,   
         FLC.FAX,   
         FLC.UBIGEO_OFICINA,   
         FLC.UBIGEO_PLANTA,   
         FLC.PROVEEDOR,   
         FLC.FLAG_ESTADO,   
         P.NOM_PROVEEDOR,   
         TG_UBIGEO_A.UBIGE_DESCRIPCION,   
         TG_UBIGEO_B.UBIGE_DESCRIPCION,
			FLC.FLAG_REPLICACION
    FROM FL_CLIENTES FLC,   
         PROVEEDOR P,   
         TG_UBIGEO TG_UBIGEO_A,   
         TG_UBIGEO TG_UBIGEO_B  
   WHERE FLC.ubigeo_planta = TG_UBIGEO_B.ubigeo_codigo (+)
     and FLC.ubigeo_oficina = TG_UBIGEO_A.ubigeo_codigo (+)
     and FLC.CLIENTE = P.PROVEEDOR 
     and FLC.CLIENTE = :as_proveedor
" update="FL_CLIENTES" updatewhere=1 updatekeyinplace=no arguments=(("as_proveedor", string)) )
