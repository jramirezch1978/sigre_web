 column=(type=number updatewhereclause=yes name=horas dbname="fl_asistencia.horas" )
 retrieve="SELECT m.cod_trabajador, 
       m.NOM_TRABAJADOR,
       a.nave, 
       tn.nomb_nave,
       a.hora_ing,
       a.hora_sal,
		   a.horas
FROM fl_asistencia     a,
     vw_pr_trabajador  m,
     tg_naves          tn
WHERE a.tripulante = m.cod_trabajador
  AND tn.nave      = a.nave
  AND trunc(a.fecha) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
  and tn.nave		 	= :as_nave
  and a.tripulante 	like :as_tripulante
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_nave", string),("as_tripulante", string))  sort="nave A " )
