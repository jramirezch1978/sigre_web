 retrieve="select 	flon.nro_orden, 
			flon.cod_origen, 
			flon.nave,
			ot.fec_solicitud, 
			ot.fec_inicio, 
			ot.descripcion,
			ot.cencos_slc,
			ot.cod_maquina,
			flon.flag_replicacion
from 		orden_trabajo ot,
		 	fl_ot_nave flon
where ot.nro_orden  = flon.nro_orden
  and ot.cod_origen = flon.cod_origen
  and flon.nave = :as_nave
  and ( to_char(ot.fec_inicio, 'yyyymmdd' ) >= to_char(:ad_fecha1, 'yyyymmdd')
    and to_char(ot.fec_inicio, 'yyyymmdd' ) <= to_char(:ad_fecha2, 'yyyymmdd') )
order by ot.cod_origen, ot.fec_inicio, ot.nro_orden   
" update="fl_ot_nave" updatewhere=0 updatekeyinplace=yes arguments=(("as_nave", string),("ad_fecha1", date),("ad_fecha2", date)) )
