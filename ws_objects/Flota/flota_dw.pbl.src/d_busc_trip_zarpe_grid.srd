 column=(type=char(10) updatewhereclause=yes name=fecha dbname="fecha" )
 column=(type=char(8) updatewhereclause=yes name=nave dbname="fl_parte_de_pesca.nave" )
 column=(type=number updatewhereclause=yes name=tripulantes dbname="tripulantes" )
 retrieve="select distinct flpp.parte_pesca, to_char(flpp.fecha_hora_zarpe, 'dd/mm/yyyy') as fecha, flpp.nave_real as nave, 
	count(fltz.tripulante) as tripulantes
from fl_tripulacion_zarpe FLTZ,
		 Fl_Parte_De_Pesca FLPP
where FLPP.Parte_Pesca = fltz.parte_pesca
  and ( FLPP.NAVE_REAL = :as_nave 
    and TO_CHAR( FLPP.FECHA_HORA_ZARPE, 'YYYYMMDD') >= TO_CHAR( :ad_fecha1, 'YYYYMMDD') 
    and TO_CHAR( FLPP.FECHA_HORA_ZARPE, 'YYYYMMDD') <= TO_CHAR( :ad_fecha2, 'YYYYMMDD') )
group by to_char(flpp.fecha_hora_zarpe, 'dd/mm/yyyy'), flpp.nave_real, flpp.parte_pesca


" arguments=(("as_nave", string),("ad_fecha1", date),("ad_fecha2", date)) )
