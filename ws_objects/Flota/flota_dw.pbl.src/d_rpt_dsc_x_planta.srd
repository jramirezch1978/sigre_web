 retrieve="select rtrim(p.nom_proveedor) || ' ' || rtrim(flp.descr_puerto) as destino, 
       tn.nomb_nave as nave,
       sum(b.cantidad_real - b.cantidad_castigada) as cantidad,
       sum(b.cantidad_real - b.cantidad_castigada) /
          (select sum(b1.cantidad_real - b1.cantidad_castigada)
          from fl_parte_de_pesca a1,
               fl_venta          b1
         where a1.parte_pesca = b1.parte_pesca
           and a1.nave_real   = a.nave_real
			  and trunc(b1.hora_inicio_descarga) between trunc(:ad_fecha1) and trunc(:ad_fecha2) ) * 100 as porc,
           (select sum(b1.cantidad_real - b1.cantidad_castigada)
          from fl_parte_de_pesca a1,
               fl_venta          b1
         where a1.parte_pesca = b1.parte_pesca
           and a1.nave_real   = a.nave_real
			  and trunc(b1.hora_inicio_descarga) between trunc(:ad_fecha1) and trunc(:ad_fecha2) ) as total
from fl_parte_de_pesca a,
     fl_venta          b,
     proveedor         p,
     fl_puertos        flp,
     tg_naves          tn
where a.parte_pesca = b.parte_pesca
  and p.proveedor   = b.cliente     
  and flp.puerto    = a.puerto_arribo
  and tn.nave       = a.nave_real
  and trunc(b.hora_inicio_descarga) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
group by rtrim(p.nom_proveedor) || ' ' || rtrim(flp.descr_puerto), 
       tn.nomb_nave,
       a.nave_real  
  
 " arguments=(("ad_fecha1", date),("ad_fecha2", date))  sort="destino A " )
