 retrieve="  SELECT FT.TRIPULANTE,   
         FT.CARGO_TRIPULANTE,   
 			FC.DESCR_CARGO, 
         FT.NAVE,  
			TN.NOMB_NAVE AS NOMB_NAVE,
         FT.ORIGEN,   
         FT.COD_TRABAJADOR,
		   MA.NOM_TRABAJADOR AS NOMBRE_TRIPULANTE, 
         FT.TITULO,   
         FT.LIBRETA_EMBARQUE,   
         FT.NRO_CENSO,   
         FT.DNI,   
         FT.FECHA_NACIMIENTO,   
         FT.FECHA_INGRESO,   
         FT.FLAG_ESTADO,
			FT.FLAG_REPLICACION,
			FT.FEC_VIGENCIA
    FROM FL_TRIPULANTES FT, 
			FL_CARGO_TRIPULANTES FC, 
			VW_PR_TRABAJADOR MA, 
			TG_NAVES TN
WHERE FC.CARGO_TRIPULANTE = FT.CARGO_TRIPULANTE AND
		MA.COD_TRABAJADOR   = FT.COD_TRABAJADOR AND
		TN.NAVE = FT.NAVE AND
		FT.TRIPULANTE = :TRIPULANTE


" update="FL_TRIPULANTES" updatewhere=0 updatekeyinplace=yes arguments=(("tripulante", string)) )
