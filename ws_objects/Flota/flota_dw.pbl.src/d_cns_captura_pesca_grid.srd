 column=(type=char(50) updatewhereclause=yes name=nomb_nave dbname="tg_naves.nomb_nave" )
 column=(type=number updatewhereclause=yes name=pesca dbname="pesca" )
 column=(type=number updatewhereclause=yes name=importe_total dbname="importe_total" )
 column=(type=number updatewhereclause=yes name=precio_promedio dbname="precio_promedio" )
 retrieve="select 	tn.nave, tn.nomb_nave, 
				sum(flv.cantidad_real - flv.cantidad_castigada) as pesca,
        sum ( (flv.cantidad_real - flv.cantidad_castigada) * 
					 			usf_fl_conv_mon(flv.precio_unitario, flv.cod_moneda, 'US$', 
                trunc( flpp.fecha_hora_arribo ) ) ) as Importe_total,
        sum ( (flv.cantidad_real - flv.cantidad_castigada) * 
 								usf_fl_conv_mon(flv.precio_unitario, flv.cod_moneda, 'US$', 
                trunc( flpp.fecha_hora_arribo ) ) ) / 
			sum(flv.cantidad_real - flv.cantidad_castigada) as Precio_promedio
from 	tg_naves tn,
		fl_venta flv,
      fl_parte_de_pesca flpp
where flv.parte_pesca = flpp.parte_pesca
	and flpp.nave_real = tn.nave
	and tn.flag_tipo_flota = 'P'
	and tn.flag_estado = '1'
	and to_char(flpp.fecha_hora_arribo, 'yyyymmdd' ) >= to_char(:ad_fecha1, 'yyyymmdd')
	and to_char(flpp.fecha_hora_arribo, 'yyyymmdd' ) <= to_char(:ad_fecha2, 'yyyymmdd')
group by tn.nave, tn.nomb_nave" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
