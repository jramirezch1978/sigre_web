 column=(type=decimal(2) updatewhereclause=yes name=cantidad_real dbname="fl_venta.cantidad_real" )
 column=(type=decimal(2) updatewhereclause=yes name=cantidad_castigada dbname="fl_venta.cantidad_castigada" )
 column=(type=number updatewhereclause=yes name=cantidad_vendida dbname="cantidad_vendida" )
 column=(type=datetime updatewhereclause=yes name=fecha_hora_arribo dbname="fl_parte_de_pesca.fecha_hora_arribo" )
 column=(type=char(50) updatewhereclause=yes name=puerto dbname="fl_puertos.puerto" )
 column=(type=number updatewhereclause=yes name=precio dbname="precio" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="select  flv.cliente,
		  p.nom_proveedor as nomb_cliente,
		  flv.cantidad_real, 
		  flv.cantidad_castigada, 
        flv.cantidad_real - flv.cantidad_castigada as cantidad_vendida,
        flpp.fecha_hora_arribo, 
	     flp.descr_puerto as puerto,
        usf_fl_conv_mon(flv.precio_unitario, flv.cod_moneda, 'US$', 
				trunc(flpp.fecha_hora_arribo) ) as precio,
        (flv.cantidad_real - flv.cantidad_castigada) *  
        usf_fl_conv_mon(flv.precio_unitario, flv.cod_moneda, 'US$', 
				trunc(flpp.fecha_hora_arribo) ) as importe
from 	fl_venta flv,
		fl_parte_de_pesca flpp,
      fl_puertos flp,
      proveedor p
where flv.parte_pesca = flpp.parte_pesca
  and flp.puerto = flpp.puerto_arribo      
  and p.proveedor = flv.cliente
  and flpp.nave_real = :ls_nave
  and to_char(flpp.fecha_hora_arribo, 'yyyymmdd') >= :ls_fecha1
  and to_char(flpp.fecha_hora_arribo, 'yyyymmdd') <= :ls_fecha2
order by flpp.fecha_hora_arribo " arguments=(("ls_nave", string),("ls_fecha1", string),("ls_fecha2", string)) )
