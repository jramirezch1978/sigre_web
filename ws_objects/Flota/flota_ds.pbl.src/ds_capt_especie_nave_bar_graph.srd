table(column=(type=number updatewhereclause=yes name=pesca dbname="pesca" )
 column=(type=char(50) updatewhereclause=yes name=especie dbname="tg_especies.descr_especie" )
 column=(type=char(50) updatewhereclause=yes name=nave dbname="tg_naves.nomb_nave" )
 retrieve="select nvl(sum(nvl(flv.cantidad_real,0) - nvl(flv.cantidad_castigada,0)),0) as pesca, 
		 tge.descr_especie, tn.nomb_nave
from 	fl_venta flv,
			tg_especies tge,
			fl_parte_de_pesca flpp,
			tg_naves tn
where tge.especie = flv.especie
  and flpp.parte_pesca = flv.parte_pesca
  and tn.nave = flpp.nave_real
  and tn.flag_tipo_flota = 'P'
  and ( to_char(flpp.fecha_hora_arribo, 'yyyymmdd') >=  to_char(:ad_fecha1, 'yyyymmdd')
	 and to_char(flpp.fecha_hora_arribo, 'yyyymmdd') <=  to_char(:ad_fecha2, 'yyyymmdd') )  
group by tge.descr_especie, tn.nomb_nave" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
