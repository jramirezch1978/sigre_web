 retrieve="  SELECT ~"GAN_DESCT_VARIABLE~".~"COD_TRABAJADOR~",   
         ~"CODIGO_RELACION~".~"NOMBRE~",   
         ~"GAN_DESCT_VARIABLE~".~"CONCEP~",   
         ~"CONCEPTO~".~"DESC_CONCEP~",   
         ~"GAN_DESCT_VARIABLE~".~"IMP_VAR~",   
         ~"MAESTRO~".~"TIPO_TRABAJADOR~",   
         ~"TIPO_TRABAJADOR~".~"DESC_TIPO_TRA~",   
         ~"GAN_DESCT_VARIABLE~".~"FEC_MOVIM~",
         ~"MAESTRO~".~"COD_ORIGEN~"
    FROM ~"GAN_DESCT_VARIABLE~",   
         ~"CODIGO_RELACION~",   
         ~"CONCEPTO~",   
         ~"MAESTRO~",   
         ~"TIPO_TRABAJADOR~"  
   WHERE gan_desct_variable.cod_trabajador = codigo_relacion.cod_relacion (+)
     and concepto.concep (+) = gan_desct_variable.concep
     and gan_desct_variable.cod_trabajador = maestro.cod_trabajador (+)
     and tipo_trabajador.tipo_trabajador (+) = maestro.tipo_trabajador
	  AND trunc(~"GAN_DESCT_VARIABLE~".~"FEC_MOVIM~") = trunc(:ld_proceso) 
	  AND ~"GAN_DESCT_VARIABLE~".~"CONCEP~" in ( :ls_concep ) 
	  AND ~"GAN_DESCT_VARIABLE~".~"TIPO_DOC~" = :ls_tipo_doc 
	  AND ~"MAESTRO~".~"TIPO_TRABAJADOR~" = :ls_tipo_trabajador 
	  AND ~"MAESTRO~".~"COD_ORIGEN~" = :ls_origen
" update="GAN_DESCT_VARIABLE" updatewhere=0 updatekeyinplace=yes arguments=(("ld_proceso", date),("ls_concep", stringlist),("ls_tipo_doc", string),("ls_tipo_trabajador", string),("ls_origen", string)) )
