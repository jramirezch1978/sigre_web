 column=(type=char(30) updatewhereclause=yes name=desc_tipo_tra dbname="tipo_trabajador.desc_tipo_tra" )
 column=(type=char(2) updatewhereclause=yes name=tipo_doc_ident_rtps dbname="vw_pr_trabajador.tipo_doc_ident_rtps" )
