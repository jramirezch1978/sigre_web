 column=(type=datetime updatewhereclause=yes name=fec_cese dbname="vw_pr_trabajador.fec_cese" )
 column=(type=number updatewhereclause=yes name=dias_periodo dbname="dias_periodo" )
 column=(type=number updatewhereclause=yes name=dias_laborados dbname="dias_laborados" )
 column=(type=number updatewhereclause=yes name=dias_subsidios dbname="dias_subsidios" )
 column=(type=number updatewhereclause=yes name=dias_permisos dbname="dias_permisos" )
 column=(type=number updatewhereclause=yes name=vacaciones_proyectadas dbname="vacaciones_proyectadas" )
 column=(type=number updatewhereclause=yes name=vacaciones_gozadas dbname="vacaciones_gozadas" )
 retrieve="select m.COD_TRABAJADOR,
       m.nom_trabajador,
		 tt.tipo_trabajador, tt.desc_tipo_tra,
       m.TIPO_DOC_IDENT_RTPS,
       m.NRO_DOC_IDENT_RTPS,
       m.fec_ingreso, m.fec_cese,
       decode(m.FEC_CESE, null, sysdate, m.fec_cese) - m.FEC_INGRESO + 1 as dias_periodo,
       PKG_RRHH.of_dias_laborados(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) as dias_laborados,
		 PKG_RRHH.of_dias_subsidios(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) as dias_subsidios,
		 PKG_RRHH.of_dias_permisos(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) as dias_permisos,

       round( case
				     when PKG_RRHH.of_dias_laborados(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
		 		  			 PKG_RRHH.of_dias_subsidios(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
					 		 PKG_RRHH.of_dias_permisos(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) > decode(m.FEC_CESE, null, sysdate, m.fec_cese) - m.FEC_INGRESO + 1 then
					     decode(m.FEC_CESE, null, sysdate, m.fec_cese) - m.FEC_INGRESO + 1
					  else
					     PKG_RRHH.of_dias_laborados(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
		 		  		  PKG_RRHH.of_dias_subsidios(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
					 	  PKG_RRHH.of_dias_permisos(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese))  
              end  / 365.25 * PKG_CONFIG.USF_GET_PARAMETER('DIAS_VACACIONES', 15), 0) as vacaciones_proyectadas,
       
		 PKG_RRHH.of_dias_vacaciones(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) as vacaciones_gozadas
           
        
from 	vw_pr_trabajador 	m,
		tipo_trabajador	tt
where m.tipo_trabajador = tt.tipo_trabajador
  and m.cod_origen 		= :as_origen
  and m.tipo_trabajador like :as_tipo_trabajador
  and m.cod_trabajador  like :as_codtra
  and m.flag_estado		= '1'
  and (PKG_RRHH.of_dias_laborados(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
		 PKG_RRHH.of_dias_subsidios(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese)) +
		 PKG_RRHH.of_dias_permisos(m.COD_TRABAJADOR, m.fec_ingreso, decode(m.FEC_CESE, null, sysdate, m.fec_cese))) > 0" arguments=(("as_origen", string),("as_tipo_trabajador", string),("as_codtra", string))  sort="desc_tipo_tra A nom_trabajador A " )
