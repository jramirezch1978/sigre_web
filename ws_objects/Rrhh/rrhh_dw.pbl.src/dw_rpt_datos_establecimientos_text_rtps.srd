 retrieve="SELECT RPAD(NVL(E.TIPO_ESTABLECIMIENTO, ' '),2) || '|' ||
       RPAD(NVL(E.COD_ESTABLECIMIENTO, ' '),4)  || '|' ||
       RPAD(NVL(E.DENOMINACION, ' '),60)        || '|' ||
       RPAD(NVL(E.DOMICILIO_FISCAL, ' '), 1)    || '|' ||
       RPAD(NVL(E.COD_CONDICION_RPTS, ' '),2 )  || '|' ||
       RPAD(NVL(EM.RUC, ' '), 11)               || '|' ||
       RPAD(NVL(EM.NOMBRE, ' '), 60)            || '|' ||
       RPAD(NVL(E.TIPO_VIA, ' '),2)             || '|' ||
       RPAD(NVL(E.NOMBRE_VIA, ' '), 20)         || '|' ||
       RPAD(NVL(E.NUMERO_VIA, ' '), 4)          || '|' ||
       RPAD(NVL(E.INTERIOR, ' '), 4 )           || '|' ||
       RPAD(NVL(E.TIPO_ZONA, ' '), 2 )          || '|' ||
       RPAD(NVL(E.NOMBRE_ZONA, ' '), 20 )       || '|' ||
       RPAD(NVL(E.REFERENCIA, ' '), 40)         || '|' ||
       SUBSTR(E.COD_DPTO, -2 , 2) || SUBSTR(E.COD_PROV, -2, 2) || SUBSTR(E.COD_DISTR,-2, 2)      
       
FROM   rrhh_establecimientos_rtps E,
       EMPRESA                    EM

WHERE  E.COD_EMPRESA  = EM.COD_EMPRESA
  AND  INSTR(:as_cod_origen, E.COD_ORIGEN) > 0 
" arguments=(("AS_COD_ORIGEN", string)) )
