 retrieve="select rrhh.descripcion as vinculo,
       nvl(a.nombre1 ,' ')||' '||nvl(a.nombre2,' ')||' '||nvl(a.apel_paterno,' ')||' '||nvl(a.apel_materno,' ') as nombre_apellido,
       b.desc_ocupacion as ocupacion, case when fec_nacimiento is not null then
