                              else 0 end as edad
  from carga_familiar a, rrhh_ocupacion_rtps b, rrhh_vinculo_familiar_rtps rrhh
 where a.cod_trabajador = :as_codtra and a.cod_vinc_familiar = :as_codcarfam
