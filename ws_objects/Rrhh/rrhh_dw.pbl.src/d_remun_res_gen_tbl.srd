 column=(type=char(25) updatewhereclause=yes name=desc_breve dbname="concepto.desc_breve" )
 column=(type=char(50) updatewhereclause=yes name=desc_origen dbname="origen.nombre" )
 column=(type=char(30) updatewhereclause=yes name=desc_tipo_trab dbname="tipo_trabajador.desc_tipo_tra" )
 column=(type=char(2) updatewhereclause=yes name=cod_origen dbname="maestro.cod_origen" )
 column=(type=char(3) updatewhereclause=yes name=tipo_trabajador dbname="maestro.tipo_trabajador" )
 retrieve="select decode(substr(c.concep,1,1), '1', '1. GANANCIAS', '2', '2. DESCUENTOS', '3', '3. APORTACIONES', 'OTROS') as grupo, c.fec_proceso, to_char(c.fec_proceso, 'yyyy') as ano, to_char(c.fec_proceso, 'mm') as mes, c.concep, c.horas_trabaj, c.horas_pag, c.imp_soles, c.dias_trabaj, cpt.desc_breve, o.nombre, tt.desc_tipo_tra, m.cod_origen, m.tipo_trabajador
   from calculo c
      inner join maestro m on c.cod_trabajador = m.cod_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join concepto cpt on c.concep = cpt.concep   
   where c.concep <> (select rhp.cnc_total_ing  from rrhhparam rhp)
      and c.concep <> (select rhp.cnc_total_dsct from rrhhparam rhp)
      and c.concep <> (select rhp.cnc_total_aport from rrhhparam rhp)
      and c.concep <> (select rhp.cnc_total_pgd from rrhhparam rhp)
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
      
   union all

select decode(substr(hc.concep,1,1), '1', '1. GANANCIAS', '2', '2. DESCUENTOS', '3', '3. APORTACIONES', 'OTROS') as grupo, hc.fec_calc_plan, to_char(hc.fec_calc_plan, 'yyyy') as ano, to_char(hc.fec_calc_plan, 'mm') as mes, hc.concep, hc.horas_trabaj, hc.horas_pagad, hc.imp_soles, hc.dias_trabaj, cpt.desc_breve, o.nombre, tt.desc_tipo_tra, m.cod_origen, m.tipo_trabajador
   from historico_calculo hc
      left outer join calculo c on hc.concep = c.concep and hc.cod_trabajador = c.cod_trabajador and hc.fec_calc_plan = c.fec_proceso
      inner join maestro m on hc.cod_trabajador = m.cod_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join concepto cpt on hc.concep = cpt.concep
   where c.concep is null
      and hc.concep <> (select rhp.cnc_total_ing  from rrhhparam rhp)
      and hc.concep <> (select rhp.cnc_total_dsct from rrhhparam rhp)
      and hc.concep <> (select rhp.cnc_total_aport from rrhhparam rhp)
      and hc.concep <> (select rhp.cnc_total_pgd from rrhhparam rhp)
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
      
order by nombre, desc_tipo_tra, fec_proceso, grupo, concep" retrieve.asneeded=yes )
