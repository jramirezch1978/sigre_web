 column=(type=char(2) updatewhereclause=yes name=mes dbname="mes" values="ENERO	01/FEBRERO	02/MARZO	03/ABRIL	04/MAYO	05/JUNIO	06/JULIO	07/AGOSTO	08/SETIEMBRE	09/OCTUBRE	10/NOVIEMBRE	11/DICIEMBRE	12/" )
