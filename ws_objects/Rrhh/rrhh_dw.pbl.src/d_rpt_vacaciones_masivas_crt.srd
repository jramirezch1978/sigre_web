 retrieve="select '0' as checked,
	  	 t.cod_trabajador,
       m.NOM_TRABAJADOR,
       m.desc_area,
       m.desc_seccion,
		 m.fec_ingreso,
       t.concep || ' - ' || c.desc_concep as desc_concepto,
       t.importe,
       t.dias_trabajados,
       t.dias_vacaciones,
       t.vacaciones_gozadas,
       t.dias_vacaciones - t.vacaciones_gozadas as dias_pendientes,
       DECODE(t.flag_fijo_variable, 'F', '1. Fijo', '2. Variable') as flag_fijo_variable,
       t.importe
from TT_RRHH_VACACIONES_TRAB t,
     vw_pr_trabajador        m,
     concepto                c
where t.cod_trabajador = m.COD_TRABAJADOR     
  and t.concep         = c.concep"  sort="nom_trabajador A " )
