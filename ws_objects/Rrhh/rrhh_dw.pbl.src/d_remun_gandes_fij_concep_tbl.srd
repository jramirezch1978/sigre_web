 retrieve="select c.concep, c.cod_trabajador, c.imp_soles, c.fec_proceso, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, s.desc_seccion, m.cod_seccion  as seccion, cpt.desc_concep, m.cencos, cc.desc_cencos
      from calculo c
         left outer join concepto cpt on c.concep = cpt.concep
         left outer join maestro m on c.cod_trabajador = m.cod_trabajador
         left outer join origen o on m.cod_origen = o. cod_origen
         left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
         left outer join area a on m.cod_area = a.cod_area
         left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
         left outer join centros_costo cc on m.cencos = cc.cencos
      where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
union all

   select hc.concep, hc.cod_trabajador, hc.imp_soles, hc.fec_calc_plan, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, s.desc_seccion, m.cod_seccion as seccion, cpt.desc_concep, m.cencos, cc.desc_cencos
      from historico_calculo hc
         left outer join calculo c on hc.concep = c.concep and hc.cod_trabajador = c.cod_trabajador and hc.fec_calc_plan = c.fec_proceso
         left outer join concepto cpt on hc.concep = cpt.concep
         left outer join maestro m on hc.cod_trabajador = m.cod_trabajador
         left outer join origen o on m.cod_origen = o. cod_origen
         left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
         left outer join area a on m.cod_area = a.cod_area
         left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
         left outer join centros_costo cc on m.cencos = cc.cencos
      where c.concep is null
         and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

order by cod_origen, tipo_trabajador, fec_proceso, concep, seccion, apel_paterno, apel_materno, nombre1, nombre2" )
