 retrieve="select s.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, s.fec_movim, s.concep, cpt.desc_breve, s.nro_doc, s.horas_sobret, s.cod_usr, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, lpad(trim(to_char(c.ano_calc)),4,'0') || lpad(trim(to_char(c.semana_calc)),2,'0') as semana
   from sobretiempo_turno s
   left outer join maestro m on s.cod_trabajador = m.cod_trabajador
   left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   left outer join origen o on m.cod_origen = o.cod_origen
   left outer join calendario c on trunc(s.fec_movim) = trunc(c.fecha)
   left outer join concepto cpt on s.concep = cpt.concep
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

union all
   
select hs.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, hs.fec_movim, hs.concep, cpt.desc_breve, hs.nro_doc, hs.horas_sobret, hs.cod_usr, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, lpad(trim(to_char(c.ano_calc)),4,'0') || lpad(trim(to_char(c.semana_calc)),2,'0') as semana
   from historico_sobretiempo hs
   left outer join sobretiempo_turno s on s.cod_trabajador = hs.cod_trabajador and s.concep = hs.concep and trunc(s.fec_movim) = trunc(hs.fec_movim)
   left outer join maestro m on hs.cod_trabajador = m.cod_trabajador
   left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   left outer join origen o on m.cod_origen = o.cod_origen
   left outer join calendario c on trunc(hs.fec_movim) = trunc(c.fecha)
   left outer join concepto cpt on hs.concep = cpt.concep
   where s.concep is null
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
" )
