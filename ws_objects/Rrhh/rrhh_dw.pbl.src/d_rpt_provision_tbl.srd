 retrieve="select r.ano, r.mes, r.cod_origen, o.nombre as desc_origen, r.tipo_trabajador, t.desc_tipo_tra, r.cod_trabajador, 
		 trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,'')) as nombre, 
       sum(decode(r.flag_provis,'C',nvl(r.importe,0),0)) as prov_cts, 
       sum(decode(r.flag_provis,'G',nvl(r.importe,0),0)) as prov_gratif, 
       sum(decode(r.flag_provis,'V',nvl(r.importe,0),0)) as prov_vacac
 from rh_prov_vacac_gratif_cts r, maestro m, origen o, tipo_trabajador t 
where r.cod_trabajador=m.cod_trabajador 
  and r.cod_origen = o.cod_origen 
  and r.cod_origen like :as_cod_origen 
  and r.tipo_trabajador = t.tipo_trabajador 
  and r.tipo_trabajador like :as_tipo_trabaj 
  and r.ano=to_number(to_char(:ad_fec_proceso,'yyyy'))
  and r.mes=to_number(to_char(:ad_fec_proceso,'mm'))
group by r.ano, r.mes, r.cod_origen, o.nombre, r.tipo_trabajador, t.desc_tipo_tra, r.cod_trabajador, 
		   trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,''))
order by r.ano, r.mes, r.cod_origen, r.tipo_trabajador, r.cod_trabajador" arguments=(("as_cod_origen", string),("as_tipo_trabaj", string),("ad_fec_proceso", date)) )
