 column=(type=char(20) updatewhereclause=yes name=nom_trabajador dbname="vw_pr_trabajador_nom_trabajador" )
 column=(type=datetime updatewhereclause=yes name=fec_ingreso dbname="vw_pr_trabajador_fec_ingreso" )
 column=(type=number updatewhereclause=yes name=edad dbname="edad" )
 column=(type=char(20) updatewhereclause=yes name=desc_cargo dbname="cargo_desc_cargo" )
 column=(type=char(20) updatewhereclause=yes name=email dbname="vw_pr_trabajador_email" )
 column=(type=char(20) updatewhereclause=yes name=telefono1 dbname="vw_pr_trabajador_telefono1" )
 column=(type=char(20) updatewhereclause=yes name=procedencia dbname="vw_pr_trabajador_procedencia" )
 column=(type=number updatewhereclause=yes name=días_laborados dbname="días_laborados" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="select m.COD_TRABAJADOR,
       m.TIPO_DOC_IDENT_RTPS,
       m.NRO_DOC_IDENT_RTPS,
       m.NOM_TRABAJADOR,
       m.FEC_INGRESO,
       trunc((sysdate - m.FEC_NACIMIENTO) / 365) as edad,
       m.COD_CARGO,
       ca.desc_cargo,
       m.email,
       m.TELEFONO1,
       m.procedencia,
       pkg_rrhh.of_dias_laborados(m.cod_trabajador, m.TIPO_TRABAJADOR, trunc(:ad_desde), trunc(:ad_hasta)) as días_laborados,
       trim(to_char(vw.fec_parte, 'yyyy/mm/dd')) || ' ' || trim(to_char(vw.fec_parte, 'DAY', 'NLS_DATE_LANGUAGE=SPANISH'))  as fecha,
       vw.precio_unit * vw.cant_producida + vw.imp_hor_diu_nor + vw.imp_hor_diu_ext1 + vw.imp_hor_diu_ext2 + vw.imp_hor_noc_nor + vw.imp_hor_noc_ext1 + vw.imp_hor_noc_ext2 as importe
from vw_pr_trabajador m,
     vw_pd_rpt_destajo_jornal vw,
     cargo                    ca
where m.COD_TRABAJADOR 		= vw.cod_trabajador
  and m.COD_CARGO      		= ca.cod_cargo
  and m.cod_origen      	= :as_origen
  and m.tipo_trabajador 	like :as_tipo_trabajador
  and trunc(vw.fec_parte) 	between trunc(:ad_desde) and trunc(:ad_hasta)
ORDER BY nom_trabajador

" arguments=(("as_origen", string),("as_tipo_trabajador", string),("ad_desde", date),("ad_hasta", date))  sort="nom_trabajador A " )
