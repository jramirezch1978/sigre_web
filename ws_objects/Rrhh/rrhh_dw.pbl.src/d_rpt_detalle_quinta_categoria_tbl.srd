 retrieve="select 1 as item,'Sueldo o Jornal Basico' as descripcion ,Sum(r.valor) as valor
  from rrhh_formato_quinta_cat r
 where (r.factor        = 1                   ) and
       (r.tipo_concepto = '0'                 ) and 
		 (r.cod_trabajador = :as_cod_trabajador ) and
		 (r.ano				 = :an_year				 )
group by r.tipo_concepto      
union
select r.item ,Initcap(r.columnas),r.valor
  from rrhh_formato_quinta_cat r
 where (r.factor         = 1                  ) and
       (r.tipo_concepto  = '1'                ) and
		 (r.cod_trabajador = :as_cod_trabajador ) and
		 (r.ano				 = :an_year				 )
order by item" arguments=(("as_cod_trabajador", string),("an_year", number),("as_obs", string)) )
