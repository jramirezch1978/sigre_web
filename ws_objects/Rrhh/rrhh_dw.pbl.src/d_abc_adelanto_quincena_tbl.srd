 retrieve="select t.*, m.NOM_TRABAJADOR, c.desc_concep
from adelanto_quincena t,
     vw_pr_trabajador m,
     concepto         c
where t.cod_trabajador = m.cod_trabajador
  and t.concep         = c.concep
  and m.cod_origen	  like :as_origen
  and m.tipo_trabajador like :as_tipo_trabaj
  and to_number(to_char(t.fec_proceso, 'yyyy')) = :an_year
  and to_number(to_char(t.fec_proceso, 'mm')) 	= :an_mes
order by 2  " update="adelanto_quincena" updatewhere=0 updatekeyinplace=yes arguments=(("as_origen", string),("as_tipo_trabaj", string),("an_year", number),("an_mes", number)) )
