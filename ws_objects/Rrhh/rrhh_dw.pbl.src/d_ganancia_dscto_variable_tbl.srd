 retrieve="  SELECT GDV.COD_TRABAJADOR,   
         GDV.FEC_MOVIM,   
         GDV.CONCEP, co.desc_concep,
         GDV.IMP_VAR,   
         GDV.COD_USR,   
         GDV.FLAG_REPLICACION,   
         GDV.NRO_DIAS,   
         GDV.NRO_HORAS,   
         GDV.COD_LABOR,   
         L.DESC_LABOR,   
         GDV.NRO_CUOTAS,
			gdv.tipo_planilla
    FROM GAN_DESCT_VARIABLE	GDV,   
			concepto					co,
         LABOR  					L
   WHERE GDV.cod_labor 	= l.cod_labor (+)
     and gdv.concep		= co.concep
	  and GDV.COD_TRABAJADOR = :as_cod_trabajador
ORDER BY GDV.FEC_MOVIM ASC   
" update="GAN_DESCT_VARIABLE" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_trabajador", string)) )
