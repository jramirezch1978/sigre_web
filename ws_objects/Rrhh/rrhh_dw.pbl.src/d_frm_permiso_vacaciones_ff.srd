 retrieve="SELECt a.*,
       vw.NOM_TRABAJADOR,
       vw.FEC_INGRESO,
       vw.desc_seccion,
       vw.desc_area,
     vw.nro_doc_ident_rtps,
     vw.tipo_trabajador, tt.desc_tipo_tra,
     E.NOMBRE AS NOM_EMPRESA,
     c.desc_concep,
     u1.nombre as usuario_gen,
     u2.nombre as usuario_aprob
FROM RRHH_PERmiso_vacac a,
     vw_pr_trabajador   vw,
     concepto           c,
    EMPRESA         E,
    tipo_trabajador    tt,
    usuario            u1,
    usuario            u2
where a.concep          = c.concep   
  and a.cod_trabajador  = vw.cod_trabajador
  and vw.tipo_trabajador = tt.tipo_trabajador
  AND A.COD_EMPRESA    = E.COD_EMPRESA
  and a.cod_usr        = u1.cod_usr (+)
  and a.usr_aprob      = u2.cod_usr (+)
  and a.nro_permiso = :as_nro_permiso" arguments=(("as_nro_permiso", string)) )
