 column=(type=char(73) updatewhereclause=yes name=seccion dbname="seccion" )
 retrieve="select c.cod_trabajador, c.concep, c.fec_proceso, c.imp_soles, m.apel_paterno, m.apel_materno, m.nombre1, 
		 m.nombre2, m.tipo_trabajador, m.porc_judicial, 
		 upper(decode(trim(nvl(j.nro_cnta_ahorro,' ')) || 's', 's', 'sin tarjeta', 'con tarjeta')) as tarjeta, 
		 initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_origen, initcap(o.nombre) as desc_origen, 
		 m.cod_seccion, s.desc_seccion, j.cod_alimentista, j.nom_pension, 
		 nvl(j.porcentaje,0) as porcntaje, cpt.desc_concep, j.importe, j.nro_cnta_ahorro, 
		 initcap(trim(a.desc_area) || ' (' || m.cod_area || ') - ' || trim(s.desc_seccion) || ' (' || m.cod_seccion || ')' ) as seccion
   from calculo c
      inner join judicial j on c.cod_trabajador = j.cod_trabajador and c.concep = j.concep
      left outer join maestro m on c.cod_trabajador = m.cod_trabajador
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
      left outer join area a on m.cod_area = a.cod_area
      left outer join concepto cpt on c.concep = cpt.concep
   where c.concep in (select j.concep from judicial j)
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

union all

select hc.cod_trabajador, hc.concep, hc.fec_calc_plan, hc.imp_soles, m.apel_paterno, m.apel_materno, m.nombre1, 
		 m.nombre2, m.tipo_trabajador, m.porc_judicial, 
		 upper(decode(trim(nvl(j.nro_cnta_ahorro,' ')) || 's', 's', 'sin tarjeta', 'con tarjeta')) as tarjeta, 
		 initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_origen, initcap(o.nombre) as desc_origen, 
		 m.cod_seccion, s.desc_seccion, j.cod_alimentista, j.nom_pension, 
		 nvl(j.porcentaje,0) as porcntaje, cpt.desc_concep, j.importe, j.nro_cnta_ahorro, 
		 initcap(trim(a.desc_area) || ' (' || m.cod_area || ') - ' || trim(s.desc_seccion) || ' (' || m.cod_seccion || ')' ) as seccion
   from historico_calculo hc
      left outer join calculo c on hc.concep = c.concep and hc.cod_trabajador = c.cod_trabajador and trunc(hc.fec_calc_plan) = trunc(c.fec_proceso)
      inner join judicial j on hc.cod_trabajador = j.cod_trabajador and hc.concep = j.concep
      left outer join maestro m on hc.cod_trabajador = m.cod_trabajador
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
      left outer join area a on m.cod_area = a.cod_area
      left outer join concepto cpt on hc.concep = cpt.concep
   where c.concep is null
         and hc.concep in (select j.concep from judicial j)
         and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

order by cod_origen, tipo_trabajador, tarjeta, fec_proceso, apel_paterno, apel_materno, nombre1, nombre2, nom_pension
" )
