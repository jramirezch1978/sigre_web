 retrieve="select vbd.cod_trabajador, vbd.periodo, vbd.flag_estado, vbd.sldo_dias_vacacio, vbd.sldo_dias_bonif, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_seccion, s.desc_seccion, m.cod_area, a.desc_area, m.tipo_trabajador, tt.desc_tipo_tra, m.cod_origen, o.nombre
   from vacac_bonif_deveng vbd
      inner join maestro m on vbd.cod_trabajador = m.cod_trabajador
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join area a on m.cod_area = a.cod_area
      inner join seccion s  on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
   where m.flag_estado = '1'
      and vbd.flag_estado = '1'
      and vbd.sldo_dias_vacacio + vbd.sldo_dias_bonif <> 0
   order by m.cod_origen, m.tipo_trabajador, m.cod_seccion, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, vbd.periodo" )
