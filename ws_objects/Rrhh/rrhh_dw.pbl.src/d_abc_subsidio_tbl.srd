 retrieve="SELECT rs.nro_subsidio, rs.fecha_ini, rs.flag_estado, 
       rs.fec_registro, rs.cod_suspension_lab, rs.cod_naturaleza_lesion, 
       rs.cod_parte_lesionada, rs.citt, rs.fecha_fin, 
		 rs.dias_teoricos, rs.dias_reales, 
       rs.dias_x_cobrar, rs.dias_cobrados, rs.observacion, 
       rs.cod_usr, rs.cod_trabajador, rs.flag_replicacion, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,' ')) as nombre, 
       susl.descripcion, natl.descripcion, parcl.descripcion
  FROM rrhh_subsidio rs, maestro m, rrhh_tipo_susp_laboral_rtps susl, 
       rrhh_naturaleza_lesion_rtps natl, rrhh_parte_cuerpo_les_rtps parcl
 WHERE rs.cod_trabajador = m.cod_trabajador and  
       rs.cod_suspension_lab = susl.cod_suspension_lab and  
       rs.cod_naturaleza_lesion = natl.cod_naturaleza_lesion (+) and  
       rs.cod_parte_lesionada = parcl.cod_parte_lesionada (+) and 
       rs.nro_subsidio = :as_nro_subsidio
 " update="rrhh_subsidio" updatewhere=0 updatekeyinplace=no arguments=(("as_nro_subsidio", string)) )
