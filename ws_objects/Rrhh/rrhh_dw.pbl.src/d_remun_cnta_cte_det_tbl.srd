 column=(type=char(1) updatewhereclause=yes name=cnta_crrte_cod_sit_prest dbname="cnta_crrte.cod_sit_prest" )
 retrieve="select m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, cc.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, cc.nro_cuotas, cc.mont_original, det.imp_dscto, cc.sldo_prestamo, cc.fec_prestamo, to_char(cc.fec_prestamo, 'mm') as mes, to_char(cc.fec_prestamo, 'yyyy') as ano, cc.concep, cpt.desc_concep, cc.tipo_doc, cc.nro_doc, cc.fec_prestamo, cc.cod_sit_prest
   from cnta_crrte cc
      left outer join maestro m on cc.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join concepto cpt on cc.concep = cpt.concep
      left outer join calculo c on ( cc.cod_trabajador = c.cod_trabajador and cc.concep = c.concep )
      left outer join cnta_crrte_detalle det on ( cc.cod_trabajador = det.cod_trabajador and cc.tipo_doc = det.tipo_doc and cc.nro_doc = det.nro_doc and c.cod_trabajador = det.cod_trabajador and c.fec_proceso = det.fec_dscto )
   where cc.cod_sit_prest in ('A', 'S')
      and m.flag_estado = '1'
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by cod_origen, tipo_trabajador, concep, apel_paterno, apel_materno, nombre1, nombre2
 " )
