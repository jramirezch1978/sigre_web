table(column=(type=char(20) updatewhereclause=yes name=cod_origen dbname="vw_pr_trabajador_cod_origen" )
 column=(type=char(20) updatewhereclause=yes name=tipo_trabajador dbname="vw_pr_trabajador_tipo_trabajador" )
