 column=(type=char(20) updatewhereclause=yes name=nom_trabajador dbname="vw_pr_trabajador_nom_trabajador" )
 column=(type=char(20) updatewhereclause=yes name=dni dbname="vw_pr_trabajador_dni" )
 column=(type=datetime updatewhereclause=yes name=fec_ingreso dbname="vw_pr_trabajador_fec_ingreso" )
 column=(type=number updatewhereclause=yes name=imp_pagado dbname="imp_pagado" )
 retrieve="select m.COD_ORIGEN,
       m.TIPO_TRABAJADOR,   
       m.COD_TRABAJADOR,   
       m.NOM_TRABAJADOR,   
       ca.CONCEP as concepto,   
       trim(co.DESC_breve) as desc_concepto,   
       m.NRO_DOC_IDENT_RTPS as dni,
       m.fec_ingreso,
       sum(USP_SIGRE_RRHH.of_dias_asist(m.cod_trabajador, ca.fec_proceso, m.cod_origen, ca.tipo_planilla )) as dias_trabaj,
       sum(ca.imp_soles) as imp_pagado,
       to_number(to_char(ca.fec_proceso, 'yyyy')) as ano,
       to_char(ca.fec_proceso, 'mm') || ' - ' ||
       case to_char(ca.fec_proceso, 'mm') 
            when '01' then 'Enero'
            when '02' then 'Febrero'
            when '03' then 'Marzo'
            when '04' then 'Abril'
            when '05' then 'Mayo'
            when '06' then 'Junio'
            when '07' then 'Julio'
            when '08' then 'Agosto'
            when '09' then 'Setiembre'
            when '10' then 'Octubre'
            when '11' then 'Noviembre'
            when '12' then 'Diciembre'
       end as mes

  from vw_pr_trabajador       m, 
       calculo                ca, 
       concepto               co
  where ca.cod_trabajador = m.cod_trabajador
    and ca.concep         = co.concep
    and m.cod_origen      like :as_origen
    and m.tipo_trabajador like :as_tipo
    and m.cod_trabajador  like :as_codigo
    and substr(ca.concep,1,1) in ('1', '3')
    and to_char(ca.fec_proceso, 'yyyymm') between trim(to_char(:an_year1, '0000')) || trim(to_char(:an_mes1, '00')) and trim(to_char(:an_year2, '0000')) || trim(to_char(:an_mes2, '00'))
 group by m.COD_ORIGEN,
          m.TIPO_TRABAJADOR,   
          m.COD_TRABAJADOR,   
          m.NOM_TRABAJADOR,   
          ca.CONCEP,
          trim(co.DESC_breve),
          m.NRO_DOC_IDENT_RTPS,
          m.fec_ingreso,
          to_number(to_char(ca.fec_proceso, 'yyyy')),
          to_char(ca.fec_proceso, 'mm') || ' - ' ||
          case to_char(ca.fec_proceso, 'mm') 
              when '01' then 'Enero'
              when '02' then 'Febrero'
              when '03' then 'Marzo'
              when '04' then 'Abril'
              when '05' then 'Mayo'
              when '06' then 'Junio'
              when '07' then 'Julio'
              when '08' then 'Agosto'
              when '09' then 'Setiembre'
              when '10' then 'Octubre'
              when '11' then 'Noviembre'
              when '12' then 'Diciembre'
          end
union all
select m.COD_ORIGEN,
       hc.TIPO_TRABAJADOR,   
       m.COD_TRABAJADOR,   
       m.NOM_TRABAJADOR,   
       hc.CONCEP as concepto,   
       trim(co.DESC_breve) as desc_concepto,   
       m.NRO_DOC_IDENT_RTPS as dni,
       m.fec_ingreso,
       sum(USP_SIGRE_RRHH.of_dias_asist(m.cod_trabajador, hc.fec_calc_plan, m.cod_origen, hc.tipo_planilla )) as dias_trabaj,
       sum(hc.imp_soles) as imp_pagado,
       to_number(to_char(hc.fec_calc_plan, 'yyyy')) as ano,
       to_char(hc.fec_calc_plan, 'mm') || ' - ' ||
       case to_char(hc.fec_calc_plan, 'mm') 
          when '01' then 'Enero'
          when '02' then 'Febrero'
          when '03' then 'Marzo'
          when '04' then 'Abril'
          when '05' then 'Mayo'
          when '06' then 'Junio'
          when '07' then 'Julio'
          when '08' then 'Agosto'
          when '09' then 'Setiembre'
          when '10' then 'Octubre'
          when '11' then 'Noviembre'
          when '12' then 'Diciembre'
       end as mes
  from vw_pr_trabajador       m, 
       historico_calculo      hc, 
       concepto               co
  where hc.cod_trabajador = m.cod_trabajador
    and hc.concep         = co.concep
    and m.cod_origen      like :as_origen
    and hc.tipo_trabajador like :as_tipo
    and m.cod_trabajador  like :as_codigo
    and substr(hc.concep,1,1) in ('1', '3')
    and to_char(hc.fec_calc_plan, 'yyyymm') between trim(to_char(:an_year1, '0000')) || trim(to_char(:an_mes1, '00')) and trim(to_char(:an_year2, '0000')) || trim(to_char(:an_mes2, '00'))
 group by m.COD_ORIGEN,
          hc.TIPO_TRABAJADOR,   
          m.COD_TRABAJADOR,   
          m.NOM_TRABAJADOR,   
          hc.CONCEP,
          trim(co.DESC_breve),
          m.NRO_DOC_IDENT_RTPS,
          m.fec_ingreso,
          to_number(to_char(hc.fec_calc_plan, 'yyyy')),
          to_char(hc.fec_calc_plan, 'mm') || ' - ' ||
          case to_char(hc.fec_calc_plan, 'mm') 
              when '01' then 'Enero'
              when '02' then 'Febrero'
              when '03' then 'Marzo'
              when '04' then 'Abril'
              when '05' then 'Mayo'
              when '06' then 'Junio'
              when '07' then 'Julio'
              when '08' then 'Agosto'
              when '09' then 'Setiembre'
              when '10' then 'Octubre'
              when '11' then 'Noviembre'
              when '12' then 'Diciembre'
          end
ORDER BY nom_trabajador, concepto

