 column=(type=char(30) updatewhereclause=yes name=desc_tipo_tra_1 dbname="tipo_trabajador.desc_tipo_tra" )
 column=(type=char(10) updatewhereclause=yes name=fec_proceso dbname="fec_proceso" )
