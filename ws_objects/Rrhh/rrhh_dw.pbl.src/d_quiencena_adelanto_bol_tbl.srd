 column=(type=datetime updatewhereclause=yes name=fecha dbname="fecha" )
 retrieve="select m.cod_seccion, aq.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, o.nombre, tt.desc_tipo_tra, to_char(aq.fec_proceso, 'dd/mm/yyyy') as fec_proceso, aq.imp_adelanto, trunc(aq.fec_proceso) as fecha
   from adelanto_quincena aq
      left outer join maestro m on aq.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
		left outer join concepto c on aq.concep = c.concep
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   
   union all

select m.cod_seccion, hc.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, o.nombre, tt.desc_tipo_tra, to_char(hc.fec_calc_plan, 'dd/mm/yyyy') as fec_proceso, hc.imp_soles, trunc(hc.fec_calc_plan) as fecha
   from historico_calculo hc 
      inner join grupo_calculo gc on hc.concep = gc.concepto_gen
      left outer join adelanto_quincena aq on hc.cod_trabajador = aq.cod_trabajador 
         and hc.concep = aq.concep 
         and trunc(hc.fec_calc_plan) = trunc(aq.fec_proceso)
      left outer join maestro m on hc.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
		left outer join concepto c on hc.concep = c.concep
   where gc.grupo_calculo = (select rc.adelanto_quincena from rrhhparam_cconcep rc where rc.reckey = '1') 
      and aq.concep is null
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

order by fecha, nombre, desc_tipo_tra, apel_paterno, apel_materno, nombre1, nombre2" )
