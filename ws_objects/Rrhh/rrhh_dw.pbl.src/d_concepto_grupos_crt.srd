 retrieve="SELECT DISTINCT 
		 case
		    when gcd.grupo_calculo is not null then
       	    GCD.GRUPO_CALCULO || '-' || GC.DESC_GRUPO 
			 else
			    ''
		 end AS grupo_calculo,
       hc.concep AS concepto,
       C.Desc_Concep as desc_concepto,
       1 AS CANTIDAD
FROM GRUPO_CALCULO GC,
     GRUPO_CALCULO_DET GCD,
     HISTORICO_CALCULO HC,
     CONCEPTO C
WHERE HC.CONCEP			= GCD.CONCEPTO_CALC  (+)
  and GCD.GRUPO_CALCULO = GC.GRUPO_CALCULO	(+)
  AND HC.CONCEP			= C.CONCEP
  AND SUBSTR(HC.CONCEP,1,1) IN ('1','2')
  AND to_number(TO_CHAR(HC.FEC_CALC_PLAN,'YYYY'))	= :ani_year
  AND to_number(TO_CHAR(HC.FEC_CALC_PLAN,'MM'))		= :ani_mes
ORDER BY concepto, grupo_Calculo
" arguments=(("ani_year", number),("ani_mes", number))  sort="concepto A " )
