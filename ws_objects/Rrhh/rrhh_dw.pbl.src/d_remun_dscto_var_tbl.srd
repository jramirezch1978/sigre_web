 column=(type=char(3) updatewhereclause=yes name=tipo_trabajador dbname="maestro.tipo_trabajador" )
 column=(type=char(30) updatewhereclause=yes name=desc_tipo_trab dbname="desc_tipo_trab" )
 retrieve="select cl.cod_trabajador, cl.concep, cl.fec_proceso, cl.imp_soles, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, cp.desc_concep, m.cod_seccion, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador , initcap(tt.desc_tipo_tra) as desc_tipo_trab
   from calculo cl
      left outer join concepto cp  on cl.concep = cp.concep
      left outer join maestro m on cl.cod_trabajador = m.cod_trabajador
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
   where cp.grupo_calc in (select rhp.grc_dsc_jdc from rrhhparam rhp where rhp.reckey = '1' union all select rhp.grc_dsc_fijo from rrhhparam rhp where rhp.reckey = '1' union all select rhp.grc_dsc_var from rrhhparam rhp where rhp.reckey = '1')
      and cl.concep not in (select rp.cnc_total_dsct from rrhhparam rp where rp.reckey = '1' union all select rp.cnc_total_pgd from rrhhparam rp where rp.reckey = '1')
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

union all

select hc.cod_trabajador, hc.concep, hc.fec_calc_plan, hc.imp_soles, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, cp.desc_concep, m.cod_seccion, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador , initcap(tt.desc_tipo_tra) as desc_tipo_trab
   from historico_calculo hc
      left outer join calculo cl on hc.cod_trabajador = cl.cod_trabajador and hc.concep = cl.concep and trunc(hc.fec_calc_plan) = trunc(cl.fec_proceso)
      left outer join concepto cp  on hc.concep = cp.concep
      left outer join maestro m on cl.cod_trabajador = m.cod_trabajador
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
   where cp.grupo_calc in (select rhp.grc_dsc_jdc from rrhhparam rhp where rhp.reckey = '1' union all select rhp.grc_dsc_fijo from rrhhparam rhp where rhp.reckey = '1' union all select rhp.grc_dsc_var from rrhhparam rhp where rhp.reckey = '1')
      and hc.concep not in (select rp.cnc_total_dsct from rrhhparam rp where rp.reckey = '1' union all select rp.cnc_total_pgd from rrhhparam rp where rp.reckey = '1')
      and cl.concep is null
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

