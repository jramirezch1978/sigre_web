 column=(type=char(30) updatewhereclause=yes name=tipo_trabajador_desc_tipo_tra dbname="tipo_trabajador.desc_tipo_tra" )
 retrieve="select g.cod_trabajador, g.fec_proceso, g.imp_adelanto, m.cod_seccion, s.desc_seccion, m.cod_area, a.desc_area, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra
   from gratificacion g
      inner join maestro m on g.cod_trabajador = m.cod_trabajador
      inner join seccion s on m.cod_seccion = s.cod_seccion and m.cod_area = s.cod_area
      inner join area a on m.cod_area = a.cod_area
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by m.cod_origen, m.tipo_trabajador, m.cod_seccion, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" )
