table(column=(type=char(63) updatewhereclause=yes name=compute_0001 dbname="compute_0001" )
 retrieve="SELECT c1||'|'||c2||'|'||c3||'|'||c5||'|'
FROM (
	SELECT c1,c2,c3, sum(c4) as c5
	 FROM (
		SELECT RPAD(NVL(M.TIPO_DOC_IDENT_RTPS, ' '), 2) c1, 
				 trim(RPAD(NVL(M.NRO_DOC_IDENT_RTPS, ' '), 15)) c2,
				 RPAD(NVL(case when I.COD_SUSPENSION_LAB is null then '08' else i.cod_suspension_lab end, ' '), 2) c3,
				 SUM(I.FEC_HASTA - I.FEC_DESDE +1 ) c4
		FROM   INASISTENCIA   I,
				 MAESTRO        M,
				 CONCEPTO       C
		WHERE  I.COD_TRABAJADOR = M.COD_TRABAJADOR
			AND I.CONCEP         = C.CONCEP
			AND C.FLAG_SUBSIDIO  = '0' 
			AND to_number(to_char(i.fec_movim, 'yyyy')) 	= :AN_YEAR
			AND to_number(to_char(i.fec_movim, 'mm'))    = :AN_MES
			and (I.COD_SUSPENSION_LAB not in ('21', '22', '23') or i.cod_suspension_lab is null)
			AND INSTR(:as_cod_origen, M.COD_ORIGEN) > 0
		GROUP BY RPAD(NVL(M.TIPO_DOC_IDENT_RTPS, ' '), 2), 
				 	trim(RPAD(NVL(M.NRO_DOC_IDENT_RTPS, ' '), 15)),
				 	RPAD(NVL(case when I.COD_SUSPENSION_LAB is null then '08' else i.cod_suspension_lab end, ' '), 2)
		union
		SELECT RPAD(NVL(M.TIPO_DOC_IDENT_RTPS, ' '), 2) c1,
				 trim(RPAD(NVL(M.NRO_DOC_IDENT_RTPS, ' '), 15)) c2,
				 '08' c3,
				 SUM(TT.FECHA - TT.FECHA +1) c4
		FROM   tt_rh_dias_no_laborados   tt,
				 MAESTRO        M
		WHERE  tt.COD_TRABAJADOR = M.COD_TRABAJADOR
			AND M.FLAG_ESTADO    = '1'
			AND INSTR(:as_cod_origen, M.COD_ORIGEN) > 0
		GROUP BY RPAD(NVL(M.TIPO_DOC_IDENT_RTPS, ' '), 2) ,
				 trim(RPAD(NVL(M.NRO_DOC_IDENT_RTPS, ' '), 15)) ,
				 '08' )
	GROUP BY c1,c2,c3)" arguments=(("AN_YEAR", number),("AN_MES", number),("AS_COD_ORIGEN", string)) )
