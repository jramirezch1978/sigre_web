 retrieve="SELECt a.*,
       vw.NOM_TRABAJADOR,
       vw.FEC_INGRESO,
       vw.desc_seccion,
       vw.desc_area,
     vw.nro_doc_ident_rtps,
     vw.tipo_trabajador, tt.desc_tipo_tra,
     E.NOMBRE AS NOM_EMPRESA,
     c.desc_concep
FROM RRHH_PERmiso_vacac a,
     vw_pr_trabajador   vw,
     concepto           c,
    EMPRESA         E,
    tipo_trabajador    tt
where a.concep          = c.concep   
  and a.cod_trabajador  = vw.cod_trabajador
  and vw.tipo_trabajador = tt.tipo_trabajador
  AND A.COD_EMPRESA    = E.COD_EMPRESA
  and a.nro_permiso = :as_nro_permiso" update="RRHH_PERmiso_vacac" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_permiso", string)) )
