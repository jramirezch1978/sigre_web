 column=(type=char(8) updatewhereclause=yes name=judicial_cod_alimentista dbname="judicial.cod_alimentista" )
 retrieve="select c.cod_trabajador, c.concep, c.fec_proceso, j.importe, j.porcentaje, decode(nvl(j.porcentaje, 0),0,'S/.', '%') as tipo, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_origen, initcap(o.nombre) as desc_origen, a.desc_area, m.cod_area, s.desc_seccion, m.cod_seccion, j.nom_pension, j.cod_alimentista
   from calculo c
      inner join judicial j on c.concep = j.concep and c.cod_trabajador = j.cod_trabajador
      left outer join maestro m on j.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

union all

select hc.cod_trabajador, hc.concep, hc.fec_calc_plan, j.importe, j.porcentaje, decode(nvl(j.porcentaje, 0),0,'S/.', '%') as tipo, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_origen, initcap(o.nombre) as desc_origen, a.desc_area, m.cod_area, s.desc_seccion, m.cod_seccion, j.nom_pension, j.cod_alimentista
   from historico_calculo hc
      left outer join calculo c on hc.concep = c.concep and hc.cod_trabajador = c.cod_trabajador and trunc(hc.fec_calc_plan) = trunc(c.fec_proceso)
      inner join judicial j on hc.concep = j.concep and hc.cod_trabajador = j.cod_trabajador
      left outer join maestro m on j.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
   where c.concep is null
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

order by cod_origen, tipo_trabajador, apel_paterno, apel_materno, nombre1, nombre2      " )
