 retrieve="select h.cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,'')) nombre, 
       m.fec_ingreso, m.dni, h.banco_cts, b.nom_banco, h.nro_cuenta_cts, h.moneda_cts, h.fecha_calculo, c.vta_dol_prom, 
       (h.cts_mes01+h.cts_mes02+h.cts_mes03+h.cts_mes04+h.cts_mes05+h.cts_mes06) importe_cts, 
       (h.dias_asist) as dias, 
       h.ingresos_fijos, 
       h.ingresos_variables, 
       h.ingresos_h_extras, 
       h.ingresos_gratif, 
       h.ingresos_otros 
  from hist_prov_cts_gratif h, maestro m, banco b, calendario c  
where h.cod_trabajador=m.cod_trabajador and 
      m.cod_banco_cts = b.cod_banco (+) and 
      trunc(h.fecha_calculo) = trunc(c.fecha) and 
      trunc(h.fecha_proceso) = :ad_fecha_proceso and 
      h.cod_origen like :as_origen and 
      h.tipo_trabajador like :as_tipo_trabaj " arguments=(("ad_fecha_proceso", date),("as_origen", string),("as_tipo_trabaj", string)) )
