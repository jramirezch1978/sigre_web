 retrieve="SELECT t.cod_trabajador,
		 t.item, 
       m.apel_paterno||' '||m.apel_materno||', '||m.nombre1||' '||m.nombre2 AS nombre,
       t.cod_tipo_extincion,
       e.descripcion,
       t.fec_inicio_periodo,
       t.fec_final_periodo,
		 t.fec_final_periodo - t.fec_inicio_periodo + 1 as dias_periodo ,
		 usp_sigre_rrhh.of_dias_asist(t.cod_trabajador, t.fec_inicio_periodo, t.fec_final_periodo) as dias_laborados,
       t.fec_liquidacion, t.nro_contrato, t.flag_liquidacion, t.tipo_doc_liquidacion, t.nro_doc_liquidacion,
		 t.vacaciones_gozadas,
		 t.NRO_DIAS_VACAC,
		 t.flag_is_renovable,
		 t.obs
  FROM maestro                      m,
       rrhh_periodos_laborales_rtps t,
       RRHH_TIPO_EXT_CONT_RTPS      e
 WHERE m.cod_trabajador     = t.cod_trabajador
   AND t.cod_tipo_extincion = e.cod_tipo_extincion (+)
   AND M.COD_TRABAJADOR     = :as_cod_trabajador" update="rrhh_periodos_laborales_rtps" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_trabajador", string)) )
