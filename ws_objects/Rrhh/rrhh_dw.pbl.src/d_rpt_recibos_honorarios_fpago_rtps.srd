table(column=(type=char(4000) updatewhereclause=yes name=dato dbname="dato" )
 retrieve="select distinct trim(to_char(p.tipo_doc_ident, '00')) || '|' ||
       trim(p.ruc) || '|R|' || 
       PKG_FACT_ELECTRONICA.of_get_serie(cp.nro_doc) ||'|' || 
       case when instr(cp.nro_doc, '-') > 0 then
            trim(to_char(substr(trim(cp.nro_doc), instr(cp.nro_doc, '-') + 1), '00000000')) 
       else
            trim(cp.nro_doc) 
       end || '|' ||
       trim(to_char(
			case
				when cp.cod_moneda = PKG_LOGISTICA.of_soles(null) then
					cp.importe_doc 
				else
					cp.importe_doc * cp.tasa_cambio
			end + 
				(select nvl(sum(
								case 
									when cp.cod_moneda = PKG_LOGISTICA.of_soles(null) then
										cpi.importe
									else
										cpi.importe * cp.tasa_cambio
								end ),0) 
               from cp_doc_det_imp cpi 
              where cpi.cod_relacion = cpd.cod_relacion 
                and cpi.tipo_doc     = cp.tipo_doc
                and cpi.nro_doc      = cp.nro_doc) 
       , '999990.00')) ||'|' ||
       trim(to_char(cp.fecha_emision, 'dd/mm/yyyy')) ||'|' || 
       trim(to_char(s.fecha_pago, 'dd/mm/yyyy')) ||'|' ||
       case when (select nvl(sum(
										case
											when cp.cod_moneda = PKG_LOGISTICA.of_soles(null) then
												cpi.importe
											else
												cpi.importe * cp.tasa_cambio
										end),0) 
                    from cp_doc_det_imp cpi 
                   where cpi.cod_relacion = cpd.cod_relacion 
                     and cpi.tipo_doc     = cp.tipo_doc
                     and cpi.nro_doc      = cp.nro_doc) > 0 then '1'
       else '0' end || '|' ||
		 ''  || '|' ||
		 ''  || '|' as dato
from cntas_pagar cp,
     cntas_pagar_det cpd,
     proveedor       p,
	  (select cbd.cod_relacion, cbd.tipo_doc, cbd.nro_doc,
				 max(cb.fecha_emision) as fecha_pago
		  from caja_bancos cb,
				 caja_bancos_det cbd,
				 cntas_pagar     cp2
		 where cb.origen      = cbd.origen
			and cb.nro_registro = cbd.nro_registro
			and cb.flag_estado  <> '0'
			and cbd.cod_relacion = cp2.cod_relacion
			and cbd.tipo_doc     = cp2.tipo_doc
			and cbd.nro_doc      = cp2.nro_doc 
			and cbd.tipo_doc		= 'RE'
			and trim(to_char(cb.ano, '0000')) || trim(to_char(cb.mes, '00')) = trim(to_char(:an_year, '0000')) || trim(to_char(:an_mes, '00'))
		   and instr(:as_cod_origen, cp2.origen) > 0
		group by cbd.cod_relacion, cbd.tipo_doc, cbd.nro_doc
    union all
    select lq.proveedor, lq.tipo_doc, lq.nro_doc,
           	DECODE(max(sg.fecha_aprobacion), null, max(cp2.fecha_emision), max(sg.fecha_aprobacion)) as fecha_pago
      from solicitud_giro sg,
           solicitud_giro_liq_det lq,
           cntas_pagar            cp2
     where sg.nro_solicitud = lq.nro_solicitud
       and lq.proveedor = cp2.cod_relacion
       and lq.tipo_doc     = cp2.tipo_doc
       and lq.nro_doc      = cp2.nro_doc 
       and to_char(decode(sg.fecha_aprobacion, null, cp2.fecha_emision, sg.fecha_aprobacion), 'yyyymm') = trim(to_char(:an_year, '0000')) || trim(to_char(:an_mes, '00'))
       and instr(:as_cod_origen, sg.origen) > 0
    group by lq.proveedor, lq.tipo_doc, lq.nro_doc
	  ) S
where cp.cod_relacion 	= p.proveedor
  and cp.cod_relacion 	= cpd.cod_relacion
  and cp.tipo_doc     	= cpd.tipo_doc
  and cp.nro_doc      	= cpd.nro_doc
  and cp.cod_relacion 	= s.cod_relacion
  and cp.tipo_doc			= s.tipo_doc
  and cp.nro_doc			= s.nro_doc
  and cp.tipo_doc     	= 'RE'
  and cp.flag_estado  	<> '0'
order by 1
  
" arguments=(("AN_YEAR", number),("AN_MES", number),("AS_COD_ORIGEN", string)) )
