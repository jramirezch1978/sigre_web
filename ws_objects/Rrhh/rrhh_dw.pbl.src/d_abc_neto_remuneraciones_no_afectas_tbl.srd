 retrieve="select hc.cod_trabajador,m.apel_paterno||' '||m.apel_materno||' '||m.nombre1 as nombres,
       Decode(to_char(hc.fec_calc_plan,'mm'),'01','Enero','02','Febrero','03','Marzo','04','Abril','05','Mayo','06','Junio','07','Julio','08','Agosto','09','Setiembre','10','Octubre','11','Noviembre','12','Diciembre' ) as mes,
		 to_char(hc.fec_calc_plan,'mm') as nro_mes,  Sum(hc.imp_soles) as monto_mes
  from historico_calculo hc ,maestro m
 where (hc.cod_trabajador                = m.cod_trabajador   ) and
       (to_char(hc.fec_calc_plan,'yyyy') = :as_ano            ) and
       (hc.concep                        = :as_concep_remunera) and
       (m.cod_origen							LIKE :as_origen	  	  ) and
       (m.tipo_trabajador					LIKE :as_ttrab		  )	 and
       (m.cod_trabajador                NOT in ( select hc.cod_trabajador
                                                from historico_calculo hc ,maestro m
                                               where (hc.cod_trabajador                = m.cod_trabajador  ) and
                                                     (to_char(hc.fec_calc_plan,'yyyy') = :as_ano           ) and
                                                     (hc.concep                        = :as_concep_quinta ) and
                                                     (m.cod_origen							LIKE :as_origen	  ) and
                                                     (m.tipo_trabajador					   LIKE :as_ttrab		  )
                                              group by hc.cod_trabajador ))

group by hc.cod_trabajador,m.apel_paterno||' '||m.apel_materno||' '||m.nombre1,to_char(hc.fec_calc_plan,'mm')


order by hc.cod_trabajador,to_char(hc.fec_calc_plan,'mm')" filter="grand_sum_monto_mes >  adc_monto "arguments=(("as_ano", string),("as_concep_remunera", string),("as_concep_quinta", string),("as_origen", string),("as_ttrab", string),("adc_monto", number))  sort="nombres A " )
