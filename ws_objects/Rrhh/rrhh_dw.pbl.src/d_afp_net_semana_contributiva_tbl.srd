table(column=(type=char(25) updatewhereclause=yes name=datos dbname="datos" )
 retrieve="select tipo_doc_afpnet 										||
		 lpad(trim(nro_doc_ident_rtps), 20, ' ') 		|| 
		 lpad(trim(to_char(semana, '00')), 2, '0') 	|| 
		 '1' 														||
		 cargo  as datos
from (	
		SELECT distinct
				 r.tipo_doc_afpnet,
				 m.nro_doc_ident_rtps,
				 fl.semana,
				 max(case
					when fl.cargo_tripulante = '01' then
					  '1' -- patron
					when fl.cargo_tripulante = '02' then
					  '2' -- segundo patron  
					when fl.cargo_tripulante = '03' then
					  '3' -- motorista
					when fl.cargo_tripulante = '04' then
					  '4' -- segundo motorista
					when fl.cargo_tripulante = '05' then
					  '5' -- segundo motorista  
					else '6'
				 end) as cargo
		  from (select fl1.tripulante, trunc(fl1.fecha) as fecha, max(fl1.cargo_tripulante) as cargo_tripulante,
							(select trim(to_char(s.semana, '00'))
								from semanas s
							  where trunc(fl1.fecha) between trunc(s.fecha_inicio) and trunc(s.fecha_fin)) as semana
					 from fl_asistencia fl1
					group by fl1.tripulante, trunc(fl1.fecha), fl1.fecha) fl,
				 maestro       m,
				 rrhh_documento_identidad_rtps r,
				 (select cod_trabajador, max(cod_afp) as cod_afp
						  from (select distinct m1.cod_trabajador, m1.cod_afp
									 from calculo   c1,
											maestro   m1
									where c1.cod_trabajador  = m1.cod_trabajador
									  and to_char(c1.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
									  and m1.cod_afp is not null
								  union
								  select distinct hc.cod_trabajador, hc.cod_afp
									 from historico_calculo hc
									where to_char(hc.FEC_CALC_PLAN, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
									  and hc.cod_afp is not null
								  union
								  select distinct a.cod_trabajador, m1.cod_afp
									 from liquidacion_benef a,
											liquidacion_benef_det b,
											maestro               m1
									where a.nro_liquidacion = b.nro_liquidacion
									  and a.cod_trabajador = m1.cod_trabajador
									  and to_Char(a.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
									  and a.nro_liquidacion in (select nro_liquidacion from liquidacion_benef_det where flag_titulo_lbs = '5')
									  and b.flag_titulo_lbs = '4'
									  and a.flag_estado   <> '0'
									  and m1.cod_afp  is not null) s
						 group by cod_trabajador) tt
		 where fl.tripulante 									= m.cod_trabajador
    		and fl.tripulante										= tt.cod_trabajador
			and m.cod_trabajador									in (select distinct c1.cod_trabajador 
																			   from calculo c1 
																			  where to_char(c1.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
																			 union
																			 select distinct c2.cod_trabajador 
																			   from historico_calculo c2 
																			  where to_char(c2.FEC_CALC_PLAN, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
																			 union
																			 select cod_trabajador
																			   from liquidacion_benef
																			  where to_Char(fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00')))
			and m.tipo_doc_ident_rtps 							= r.cod_doc_identidad
			and trunc(to_char(fl.fecha, 'yyyy')) 			= :an_year
		   and trunc(to_char(fl.fecha, 'mm')) 				= :an_mes
			and nvl(m.flag_pensionista, '0') = '0'
		   AND INSTR(:as_cod_origen, M.COD_ORIGEN) 		> 0 
		   and INSTR(:as_tipo_trabaj, M.tipo_trabajador) > 0 
			and m.nro_afp_trabaj is not null 
			and ((select NVL(sum(c.imp_soles),0)
									 from calculo c,
											grupo_calculo_det  gcd
									where c.concep          = gcd.concepto_calc
									  and c.cod_trabajador  = m.cod_trabajador
									  and gcd.GRUPO_CALCULO = (select concep_calculo_afp from rrhhparam_cconcep where reckey = '1')
									  and to_char(c.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))) +
							(select NVL(sum(hc.imp_soles),0)
									 from historico_calculo hc,
											grupo_calculo_det  gcd
									where hc.concep         = gcd.concepto_calc
									  and hc.cod_trabajador = m.cod_trabajador
									  and gcd.GRUPO_CALCULO = (select concep_calculo_afp from rrhhparam_cconcep where reckey = '1')
									  and to_char(hc.FEC_CALC_PLAN, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))) +
						  (select nvl(sum(b.importe),0)
							from liquidacion_benef a,
								 liquidacion_benef_det b
							where a.nro_liquidacion = b.nro_liquidacion
							  and to_Char(a.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
							  and a.nro_liquidacion in (select nro_liquidacion from liquidacion_benef_det where flag_titulo_lbs = '5')
							  and b.flag_titulo_lbs = '4'
							  and a.flag_estado   <> '0'
							  and a.cod_trabajador = m.cod_trabajador)) > 0
		group by tipo_doc_afpnet,m.nro_doc_ident_rtps, fl.semana
		order by tipo_doc_afpnet,m.nro_doc_ident_rtps, semana
	)s
" arguments=(("as_cod_origen", string),("as_tipo_trabaj", string),("an_year", number),("an_mes", number)) )
