 retrieve="select TIPO_TRABAJADOR,   
       seccion,   
       COD_TRABAJADOR,   
       nom_trabajador,   
       concepto,   
       desc_concepto,   
       cod_afp,   
       COD_CARGO,   
       DESC_CARGO,   
       CENCOS,   
       DESC_CENCOS,   
       FEC_INGRESO,   
       FEC_NACIMIENTO,   
       FLAG_ESTADO,   
       COD_ORIGEN,   
       DNI,
		 usf_rh_calc_dias_grupo(cod_trabajador, cod_origen, tipo_trabajador, trunc(:ad_hasta) - trunc(:ad_desde) + 1, trunc(:ad_desde), trunc(:ad_hasta)) as dias_trabaj, 
		 sum(imp_pagado) as imp_pagado
from (
		select m.TIPO_TRABAJADOR,   
				 m.COD_SECCION || ' ' || s.DESC_SECCION as seccion,   
				 m.COD_TRABAJADOR,   
				 m.nom_trabajador,   
				 ca.CONCEP as concepto,   
				 co.desc_breve as desc_concepto,   
				 DECODE(m.COD_AFP, null, 'ONP', m.COD_AFP) as cod_afp,   
				 m.COD_CARGO,   
				 c.DESC_CARGO,   
				 m.CENCOS,   
				 cc.DESC_CENCOS,   
				 m.FEC_INGRESO,   
				 m.FEC_NACIMIENTO,   
				 m.FLAG_ESTADO,   
				 m.COD_ORIGEN,   
				 m.DNI,
				 sum(ca.imp_soles) as IMP_PAGADO
		  from vw_pr_trabajador m, 
				 seccion       	s, 
				 cargo         	c, 
				 calculo       	ca, 
				 concepto      	co,
				 banco				b1,
				 banco				b2,
				 centros_costo	cc
		  where m.cod_area    = s.cod_area 
			 and m.cod_seccion = s.cod_seccion 
			 and m.cod_cargo   = c.cod_cargo 
			 and ca.cod_trabajador = m.cod_trabajador
			 and ca.concep         = co.concep
			 and m.cod_banco		  = b1.cod_banco (+)
			 and m.cod_banco_cts	  = b2.cod_banco (+)
			 and m.cencos			  = cc.cencos
			 and m.cod_origen      = :as_origen
			 and m.tipo_trabajador like :as_tipo_trabajador
			 and m.cod_trabajador  like :as_trabajador
			 and trunc(ca.fec_proceso) between trunc(:ad_desde) and trunc(:ad_hasta)
		group by m.TIPO_TRABAJADOR,   
				 m.COD_SECCION || ' ' || s.DESC_SECCION,
				 m.COD_TRABAJADOR,   
				 m.nom_trabajador,   
				 ca.CONCEP,
				 co.desc_breve,
				 DECODE(m.COD_AFP, null, 'ONP', m.COD_AFP),
				 m.COD_CARGO,   
				 c.DESC_CARGO,   
				 m.CENCOS,   
				 cc.DESC_CENCOS,   
				 m.FEC_INGRESO,   
				 m.FEC_NACIMIENTO,   
				 m.FLAG_ESTADO,   
				 m.COD_ORIGEN,   
				 m.DNI
		union all
		select ca.TIPO_TRABAJADOR,   
				 m.COD_SECCION || ' ' || s.DESC_SECCION as seccion,   
				 m.COD_TRABAJADOR,   
				 m.nom_trabajador,   
				 ca.CONCEP as concepto,   
				 co.desc_breve as desc_concep,   
				 DECODE(m.COD_AFP, null, 'ONP', m.COD_AFP) as cod_afp,   
				 m.COD_CARGO,   
				 c.DESC_CARGO,   
				 m.CENCOS,   
				 cc.DESC_CENCOS,   
				 m.FEC_INGRESO,   
				 m.FEC_NACIMIENTO,   
				 m.FLAG_ESTADO,   
				 m.COD_ORIGEN,   
				 m.DNI,
				 sum(ca.imp_soles) as IMP_PAGADO
		  from vw_pr_trabajador		m, 
				 seccion       		s, 
				 cargo        		 	c, 
				 historico_calculo 	ca, 
				 concepto      		co,
				 banco					b1,
				 banco					b2,
				 centros_costo			cc
		  where m.cod_area    		= s.cod_area 
			 and m.cod_seccion 		= s.cod_seccion 
			 and m.cod_cargo   		= c.cod_cargo 
			 and ca.cencos				= cc.cencos
			 and ca.cod_trabajador = m.cod_trabajador
			 and ca.concep         = co.concep
			 and m.cod_banco		  = b1.cod_banco (+)
			 and m.cod_banco_cts	  = b2.cod_banco (+)
			 and m.cod_origen      = :as_origen
			 and ca.tipo_trabajador like :as_tipo_trabajador
			 and m.cod_trabajador  like :as_trabajador
			 and trunc(ca.fec_calc_plan) between trunc(:ad_desde) and trunc(:ad_hasta)
		group by ca.TIPO_TRABAJADOR,   
				 m.COD_SECCION || ' ' || s.DESC_SECCION,
				 m.COD_TRABAJADOR,   
				 m.nom_trabajador,   
				 ca.CONCEP,
				 co.desc_breve,
				 DECODE(m.COD_AFP, null, 'ONP', m.COD_AFP),
				 m.COD_CARGO,   
				 c.DESC_CARGO,   
				 m.CENCOS,   
				 cc.DESC_CENCOS,   
				 m.FEC_INGRESO,   
				 m.FEC_NACIMIENTO,   
				 m.FLAG_ESTADO,   
				 m.COD_ORIGEN,   
				 m.DNI
) s
group by  TIPO_TRABAJADOR,   
			 seccion,   
			 COD_TRABAJADOR,   
			 nom_trabajador,   
			 concepto,   
			 desc_concepto,   
			 cod_afp,   
			 COD_CARGO,   
			 DESC_CARGO,   
			 CENCOS,   
			 DESC_CENCOS,   
			 FEC_INGRESO,   
			 FEC_NACIMIENTO,   
			 FLAG_ESTADO,   
			 COD_ORIGEN,   
			 DNI

ORDER BY nom_trabajador, concepto

