 retrieve="  SELECT H.FECHA_PROCESO, 
         H.COD_TRABAJADOR,
         H.COD_ORIGEN,
         H.TIPO_TRABAJADOR,
         H.CTS_MES01,
         H.CTS_MES02,
         H.CTS_MES03,
         H.CTS_MES04,
         H.CTS_MES05,
         H.CTS_MES06,
         H.INGRESOS_FIJOS,
         H.INGRESOS_VARIABLES,
         H.INGRESOS_H_EXTRAS, 
         H.INGRESOS_GRATIF,
         H.INGRESOS_OTROS,
         H.FECHA_CALCULO,
         H.DIAS_ASIST,
         TRIM(M.APEL_PATERNO) ||' '||TRIM(M.APEL_MATERNO)||', '||TRIM(M.NOMBRE1)||' '||TRIM(NVL(M.NOMBRE2,' ')) AS Nombre
    FROM HIST_PROV_CTS_GRATIF H,   
         MAESTRO M
   WHERE ( M.COD_TRABAJADOR = H.COD_TRABAJADOR ) and  
         ( ( trunc( H.FECHA_PROCESO) = :ad_fec_proceso ) AND  
         ( H.COD_ORIGEN like :as_origen ) AND  
         ( H.TIPO_TRABAJADOR like :as_tipo_trabajador ) ) 
ORDER BY TRIM(M.APEL_PATERNO) ||' '||TRIM(M.APEL_MATERNO)||', '||TRIM(M.NOMBRE1)||' '||TRIM(NVL(M.NOMBRE2,' '))
" update="HIST_PROV_CTS_GRATIF" updatewhere=0 updatekeyinplace=no arguments=(("ad_fec_proceso", date),("as_origen", string),("as_tipo_trabajador", string)) )
