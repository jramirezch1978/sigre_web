 retrieve="select pvb.cod_trabajador, pvb.concep, pvb.importe, m.cod_seccion, m.cencos, cc.desc_cencos, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.tipo_trabajador, tt.desc_tipo_tra, m.cod_origen, o.nombre, pvb.concep, c.desc_breve, pvb.fec_proceso
   from prov_vac_bonif pvb
      inner join maestro m on pvb.cod_trabajador =  m.cod_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join centros_costo cc on m.cencos = cc.cencos
      inner join concepto c on pvb.concep = c.concep
   where pvb.importe <> 0 and pvb.concep in (select gc.concepto_gen from grupo_calculo gc where gc.grupo_calculo = '019')
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by m.cod_origen, m.tipo_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" )
