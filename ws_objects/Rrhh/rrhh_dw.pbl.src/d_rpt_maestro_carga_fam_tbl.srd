 retrieve="  SELECT  ~"CARGA_FAMILIAR~".~"COD_TRABAJADOR~" ,
           ~"CARGA_FAMILIAR~".~"SECUENCIA~" ,
           ~"CARGA_FAMILIAR~".~"COD_VINC_FAMILIAR~" ,
           ~"CARGA_FAMILIAR~".~"COD_GRADO_INST~" ,
           ~"CARGA_FAMILIAR~".~"APEL_PATERNO~" ,
           ~"CARGA_FAMILIAR~".~"APEL_MATERNO~" ,
           ~"CARGA_FAMILIAR~".~"NOMBRE1~" ,
           ~"CARGA_FAMILIAR~".~"NOMBRE2~" ,
           ~"CARGA_FAMILIAR~".~"FLAG_SEXO~" ,
           ~"CARGA_FAMILIAR~".~"FEC_NACIMIENTO~" ,
           ~"RRHH_ESTADO_CIVIL_RTPS~".~"DESCRIPCION~" ,
           ~"CARGA_FAMILIAR~".~"OCUPACION~" ,
           ~"CARGA_FAMILIAR~".~"FLAG_VIDA~" ,
           ~"CARGA_FAMILIAR~".~"FLAG_DEPENDENCIA~" ,
           ~"CARGA_FAMILIAR~".~"FLAG_TRABAJA_EMP~" ,
           ~"CARGA_FAMILIAR~".~"FLAG_REPLICACION~" ,
           ~"RRHH_VINCULO_FAMILIAR_RTPS~".~"DESCRIPCION~" ,
           Nvl(carga_familiar.apel_paterno,' ') ||' '|| Nvl(carga_familiar.apel_materno,' ') ||' '|| Nvl(carga_familiar.nombre1,' ') ||' '|| Nvl(carga_familiar.nombre2,' ') as nombres
    FROM ~"CARGA_FAMILIAR~" ,
           ~"RRHH_VINCULO_FAMILIAR_RTPS~",
           ~"RRHH_ESTADO_CIVIL_RTPS~"
     WHERE ( ~"RRHH_VINCULO_FAMILIAR_RTPS~".~"COD_VINC_FAMILIAR~" (+) = ~"CARGA_FAMILIAR~".~"COD_VINC_FAMILIAR~") and
          ( ~"RRHH_ESTADO_CIVIL_RTPS~".~"COD_ESTADO_CIVIL~" (+) = ~"CARGA_FAMILIAR~".~"COD_ESTADO_CIVIL~") and
          ( ( ~"CARGA_FAMILIAR~".~"COD_TRABAJADOR~" = :as_cod_trabajador ) )  " arguments=(("as_cod_trabajador", string))  sort="secuencia A " )
