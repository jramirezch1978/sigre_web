 retrieve=" 
SELECT  distinct
		  m.cod_trabajador,
        m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 AS ~"TRABAJADOR~",
        P.NACIONALIDAD,
        m.fec_nacimiento,
        m.fec_ingreso, m.fec_cese,
        USF_OBTIENE_EDAD(m.fec_nacimiento) as edad,
        rtrim(M.DIRECCION) AS DOMICILIO,
        d.desc_distrito,
        pc.desc_PROV,
        dp.desc_DPTO,
        m.dni,
        TP.DESC_TIPO_TRA,
        A.DESC_AREA,
        S.DESC_SECCION,
		  DECODE(tp.tipo_trabajador, 'EMP','mensuales.',
									  'por día.') as t_diaomes,
		  
		  DECODE(tp.tipo_trabajador, 'EMP',
		  USF_OBTIENE_GANANCIA_NETA_D(m.cod_trabajador),
		  USF_OBTIENE_GANANCIA_NETA(m.cod_trabajador)) as t_gdiaomes,
        USF_OBTIENE_MESES_DE_TRABAJO(:ad_cese, :ad_ingreso) as mes_trabajo,
		  USF_OBTIENE_DIAS_MES(:ad_cese, :ad_ingreso) as dias_trabajo,
		  :ad_ingreso as fecha_ini_contrato,
		  :ad_cese as fecha_fin_contrato,
		  (select dir_calle || ' ' || to_char(dir_numero) || ' ' || dir_urbanizacion from origen where cod_origen = :as_origen) as direccion_empresa,
		  (select e.nombre from empresa e, genparam g where e.cod_empresa = g.cod_empresa) as empresa,
		  (select e.ruc from empresa e, genparam g where e.cod_empresa = g.cod_empresa) as RUC_empresa,
		  (select e.representante from empresa e, genparam g where e.cod_empresa = g.cod_empresa) as representante_empresa,
		  (select e.identificacion from empresa e, genparam g where e.cod_empresa = g.cod_empresa) as dni_representante,
        (select dir_distrito from origen where cod_origen = :as_origen) as distrito_empresa,
		  (select dir_departamento from origen where cod_origen = :as_origen) as departamento_empresa,	
		  (select dir_provincia from origen where cod_origen = :as_origen) as provincia_empresa,
		  (select nro_censo from fl_tripulantes where tripulante = m.cod_trabajador) as nro_censo,
		  sysdate as fecha,
		  DECODE(to_char(sysdate, 'mm'), '01', 'ENERO', '02', 'FEBRERO', '03', 'MARZO', '04', 'ABRIL',
													'05', 'MAYO', '06', 'JUNIO', '07', 'JULIO', '08', 'AGOSTO',
													'09', 'SETIEMBRE', '10', 'OCTUBRE', '11', 'NOVIEMBRE', '12', 'DICIEMBRE') AS MES,
		  (select nomb_nave from tg_naves a, fl_tripulantes b where a.nave = b.nave and b.tripulante = m.cod_trabajador) as nave,
		  (select matricula from tg_naves a, fl_tripulantes b where a.nave = b.nave and b.tripulante = m.cod_trabajador) as matricula,
	     (select a.descr_cargo 
			  from fl_cargo_tripulantes a, fl_tripulantes b 
			 where a.cargo_tripulante = b.cargo_tripulante 
  			   and b.tripulante = m.cod_trabajador ) as cargo_tripulante
    FROM origen            o,
       maestro             m,
       pais                p,
       tipo_trabajador     tp,
       area                a,
       seccion             S,
       gan_desct_fijo      g,
       distrito            d,
       provincia_condado   pc,
       departamento_estado dp,
       ciudad              c
 WHERE m.cod_origen        = o.cod_origen
   and m.cod_area          = a.COD_AREA 
   and m.cod_seccion       = s.cod_seccion
   and m.tipo_trabajador   = tp.tipo_trabajador
   and m.cod_trabajador    = g.cod_trabajador (+)
   and d.cod_distr   (+)   = m.cod_distr
   and d.cod_dpto    (+)   = m.cod_dpto
   and d.cod_pais    (+)   = m.cod_pais
   and d.cod_prov    (+)   = m.cod_prov
   and pc.COD_PAIS   (+)   = d.COD_PAIS
   and pc.COD_DPTO   (+)  = d.COD_DPTO
   and pc.COD_PROV   (+)  = d.COD_PROV
   and dp.COD_PAIS   (+)  = pc.COD_PAIS
   and dp.COD_DPTO   (+)  = pc.COD_DPTO
   and p.COD_PAIS      	  = m.COD_PAIS
   and a.COD_AREA          = s.COD_AREA (+)
   and pc.COD_PAIS         = c.COD_PAIS (+)
   and pc.COD_DPTO         = c.COD_DPTO (+)
   and pc.COD_PROV         = c.COD_PROV (+)
   and m.cod_origen			= :as_origen
   and m.tipo_trabajador	= :as_tipo_trabajador
   and m.flag_estado			= :as_estado
   and m.cod_trabajador		like :as_cod_trab
   and m.cod_area				like :as_area
	and m.cod_seccion		   like :as_Seccion" arguments=(("as_cod_trab", string),("as_origen", string),("as_area", string),("as_seccion", string),("as_tipo_trabajador", string),("as_estado", string),("ad_ingreso", date),("ad_cese", date)) )
