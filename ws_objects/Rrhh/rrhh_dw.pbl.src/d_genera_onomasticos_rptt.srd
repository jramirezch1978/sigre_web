 retrieve="SELECT  distinct
        m.cod_trabajador,
        m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 AS ~"TRABAJADOR~",
        m.fec_nacimiento,
    	  TO_CHAR(m.fec_nacimiento,'dd')|| '  ' || decode(TO_CHAR(m.fec_nacimiento,'mm'),01,' de  Enero',02,'de  Febrero',
                                                       03,'de  Marzo',04, 'de  Abril', 05,'de  Mayo', 06, 'de  Junio', 07, 'de  Julio',
                                                       08,'de  Agosto', 09, 'de  Septiembre', 10,'de  Octubre', 11, 'de  Noviembre',
                                                       12,'de  Diciembre', null,'No definida') as onomastico
                
  FROM origen              o,
       maestro             m
      
   
    WHERE         
		 m.cod_origen        = o.cod_origen
 	and m.cod_origen        = :as_origen
	and m.cod_trabajador  in (:as_cod_trabajador)
   ORDER BY onomastico" arguments=(("as_origen", string),("as_cod_trabajador", stringlist)) )
