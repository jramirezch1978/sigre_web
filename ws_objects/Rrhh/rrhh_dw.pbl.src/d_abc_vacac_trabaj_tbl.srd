 retrieve="  SELECT V.COD_TRABAJADOR,   
         V.PERIODO_INICIO,   
         V.PERIODO_FIN,   
         V.CONCEP,   
         C.DESC_CONCEP,   
         V.DIAS_TOTALES,   
         V.DIAS_GOZADOS,   
         V.DIAS_PROGRAM,   
         V.FLAG_ESTADO,   
         V.COD_USR,   
         V.FLAG_REPLICACION  
    FROM RRHH_VACACIONES_TRABAJ V,   
         CONCEPTO C  
   WHERE ( V.CONCEP = C.CONCEP ) and  
         ( ( v.cod_trabajador = :as_cod_trabajador ) )   
ORDER BY V.COD_TRABAJADOR ASC,   
         V.PERIODO_INICIO ASC   
" update="RRHH_VACACIONES_TRABAJ" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_trabajador", string)) )
