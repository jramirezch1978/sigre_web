 retrieve="select acc.nro_convenio, e.nombre as nom_emp, e.ruc as ruc_emp, trim(e.dir_calle) || ' ' || trim(e.dir_numero) as dir_emp, c.descr_ciudad as ciu_emp, (select nvl(trim(m.nombre1)||' ' , '') || nvl(trim(m.nombre2)||' ' , '') || nvl(trim(m.apel_paterno)||' ' , '') || nvl(trim(m.apel_materno)||' ' , '') from seccion s inner join maestro m on s.cod_jefe_seccion = m.cod_trabajador where s.cod_seccion = '300') as nom_jefe, (select s.cod_jefe_seccion from seccion s inner join maestro m on s.cod_jefe_seccion = m.cod_trabajador where s.cod_seccion = '300') as cod_jefe, (select m.dni from seccion s inner join maestro m on s.cod_jefe_seccion = m.cod_trabajador where s.cod_seccion = '300') as dni_jefe, nvl(trim(m.apel_paterno)||' ' , '') || nvl(trim(m.apel_materno)||' ' , '') || nvl(trim(m.nombre1)||' ' , '') || nvl(trim(m.nombre2)||' ' , '') as nom_trab, m.dni as dni_trab, m.direccion as dir_trab, pc.desc_prov as prv_trab, d.desc_distrito as dist_trab, acc.imp_a_cuenta, acc.fec_proceso
   from adel_cnta_cts acc
      inner join maestro m on acc.cod_trabajador = m.cod_trabajador
      inner join empresa e on m.cod_empresa = e.cod_empresa
      inner join distrito d on m.cod_pais = d.cod_pais and m.cod_dpto = d.cod_dpto and m.cod_prov = d.cod_prov and m.cod_distr = d.cod_distr
      inner join provincia_condado pc on m.cod_pais = pc.cod_pais and m.cod_dpto = pc.cod_dpto and m.cod_prov = pc.cod_prov
      inner join ciudad c on e.ciu_cod_pais = c.cod_pais and e.ciu_cod_dpto = c.cod_dpto and e.ciu_cod_prov = c.cod_prov and e.cod_ciudad = m.cod_ciudad
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by acc.nro_convenio" )
