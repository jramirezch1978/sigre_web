 retrieve="select tgt.cod_trabajador, tgt.concep, tgt.importe, cr.nombre, m.cod_origen, o.nombre, m.cencos, cc.desc_cencos, m.cod_area, a.desc_area, m.cod_seccion, s.desc_seccion, m.tipo_trabajador, tt.desc_tipo_tra
   from tt_rh_ganfij_todo tgt
      inner join codigo_relacion cr on tgt.cod_trabajador = cr.cod_relacion
      inner join maestro m on tgt.cod_trabajador = m.cod_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join centros_costo cc on m.cencos = cc.cencos
      inner join area a on m.cod_area = a.cod_area
      inner join seccion s on m.cod_seccion = s.cod_seccion and m.cod_area = s.cod_area
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by m.cod_origen, m.tipo_trabajador, cr.nombre"  sort="cod_trabajador A nombre A cod_origen A nombre_0 A tipo_trabajador A desc_tipo_tra A cod_area A desc_area A cod_seccion A desc_seccion A cencos A desc_cencos A " )
