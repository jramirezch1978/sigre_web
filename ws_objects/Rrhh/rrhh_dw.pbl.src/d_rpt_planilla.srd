 retrieve="Select hc.fec_calc_plan, 
		 hc.cencos, 
		 cc.desc_cencos, 
		 cttc.cnta_prsp, 
		 pc.descripcion,
       decode(to_char(hc.fec_calc_plan,'mm'),'01',pp.proy_ene, '02',pp.proy_feb, '03',pp.proy_mar,
                                             '04',pp.proy_abr, '05',pp.proy_may, '06',pp.proy_jun,
                                             '07',pp.proy_jul, '08',pp.proy_ago, '09',pp.proy_set,
                                             '10',pp.proy_oct, '11',pp.proy_nov, '12',pp.proy_dic,0) As proyectado,
       decode(to_char(hc.fec_calc_plan,'mm'),'01',pp.ejec_ene, '02',pp.ejec_feb, '03',pp.ejec_mar,
                                             '04',pp.ejec_abr, '05',pp.ejec_may, '06',pp.ejec_jun,
                                             '07',pp.ejec_jul, '08',pp.ejec_ago, '09',pp.ejec_set,
                                             '10',pp.ejec_oct, '11',pp.ejec_nov, '12',pp.ejec_dic,0) As ejecutado,
       Sum(hc.imp_dolar) * -1 as ejecutado_RRHH
  From historico_calculo 		hc, 
		 maestro 					m, 
		 concepto_tip_trab_cnta cttc, 
       presupuesto_partida 	pp, 
		 centros_costo 			cc, 
		 presupuesto_cuenta pc
 Where hc.cod_trabajador 	= m.cod_trabajador
   And hc.concep 				= cttc.concep
   And m.tipo_trabajador 	= cttc.tipo_trabajador
   And hc.cencos 				= pp.cencos
   And cttc.cnta_prsp 		= pp.cnta_prsp
   And hc.cencos 				= cc.cencos (+)
   And cttc.cnta_prsp 		= pc.cnta_prsp (+)
   And pp.ano 					= :an_ano
   And hc.cod_origen 		= :as_origen
   And to_number(to_char(hc.fec_calc_plan,'mm')) 	= :an_mes
   And to_number(to_char(hc.fec_calc_plan,'yyyy')) = :an_ano
   And cttc.cnta_prsp Is Not Null
   And hc.cencos Like :as_cencos
 Group By hc.fec_calc_plan, hc.cencos, cc.desc_cencos, 
       cttc.cnta_prsp, pc.descripcion,
       pp.proy_ene, pp.proy_feb, pp.proy_mar, pp.proy_abr,
       pp.proy_may, pp.proy_jun, pp.proy_jul, pp.proy_ago,
       pp.proy_set, pp.proy_oct, pp.proy_nov, pp.proy_dic,
       pp.ejec_ene, pp.ejec_feb, pp.ejec_mar, pp.ejec_abr,
       pp.ejec_may, pp.ejec_jun, pp.ejec_jul, pp.ejec_ago,
       pp.ejec_set, pp.ejec_oct, pp.ejec_nov, pp.ejec_dic
 Order By hc.fec_calc_plan, hc.cencos, cttc.cnta_prsp" arguments=(("an_ano", number),("an_mes", number),("as_origen", string),("as_cencos", string)) )
