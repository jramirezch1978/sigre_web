 retrieve="select cc.cod_trabajador,
       m.NOM_TRABAJADOR,
       cc.tipo_doc,
       cc.nro_doc,
       cc.mont_original,
       cc.mont_cuota,
       cc.concep, c.desc_concep,
       sum(ccd.imp_dscto) as total_descuento,
       count(ccd.fec_dscto) as cuotas_descontadas,
       cc.sldo_prestamo,
		 cc.fec_prestamo,
		 cc.nro_cuotas,
       max(ccd.fec_dscto) as ult_fec_dscto
from cnta_crrte cc,
     cnta_crrte_detalle ccd,
     vw_pr_trabajador m,
     concepto         c
where cc.cod_trabajador = ccd.cod_trabajador
  and cc.tipo_doc       = ccd.tipo_doc
  and cc.nro_doc        = ccd.nro_doc
  and cc.cod_trabajador = m.cod_trabajador     
  and cc.concep         = c.concep  (+)
  and m.cod_origen		like :as_origen
  and m.tipo_trabajador	like :as_tipo_trabajador
group by  cc.cod_trabajador,
       m.NOM_TRABAJADOR,
       cc.tipo_doc,
       cc.nro_doc,
       cc.mont_original,
       cc.mont_cuota,
       cc.concep, c.desc_concep,
       cc.sldo_prestamo,
		 cc.fec_prestamo,
		 cc.nro_cuotas
having cc.mont_original - sum(ccd.imp_dscto) > 0
order by nom_trabajador, tipo_doc, nro_doc" arguments=(("as_tipo_trabajador", string),("as_origen", string))  sort="nom_trabajador A tipo_doc A nro_doc A " )
