compute(name=mes_trabajo visible="1" lineremove=no multiline=no width.autosize=yes width="0" alignment="0" format="[general]" expression=" mes_trabajo " )
compute(name=matricula visible="1" lineremove=no multiline=no width.autosize=yes width="0" alignment="0" format="[general]" expression=" matricula " )
