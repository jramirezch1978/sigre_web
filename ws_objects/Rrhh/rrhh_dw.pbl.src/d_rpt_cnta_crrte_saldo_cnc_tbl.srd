 column=(type=number updatewhereclause=yes name=pendiente_cerrar dbname="pendiente_cerrar" )
 retrieve="SELECT m.cod_origen, o.nombre as desc_origen, m.tipo_trabajador, t.desc_tipo_tra, c.concep, co.desc_concep, c.cod_trabajador, c.cod_sit_prest, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,' ')) as nombre, 
       c.tipo_doc, c.nro_doc, c.fec_prestamo, c.cod_moneda, c.mont_original, c.mont_cuota, c.sldo_prestamo, 
       decode(c.cod_moneda, :as_mon_sol, c.sldo_prestamo, c.sldo_prestamo/ca.cmp_dol_prom) as saldo_sol, 
       (select NVL(sum(cd.imp_dscto),0) 
          from cnta_crrte_detalle cd 
         where cd.cod_trabajador=c.cod_trabajador and c.tipo_doc=cd.tipo_doc and c.nro_doc=cd.nro_doc and cd.flag_estado='0') as pendiente_cerrar
FROM cnta_crrte c, maestro m, concepto co, origen o, tipo_trabajador t, calendario ca
WHERE c.cod_trabajador=m.cod_trabajador and
      (c.concep=co.concep) and  
      (trunc(c.fec_prestamo) = trunc(ca.fecha))  and 
      m.cod_origen=o.cod_origen and 
      m.tipo_trabajador = t.tipo_trabajador and 
      m.flag_estado<>'0' and 
      m.cod_origen like :as_origen and 
      m.tipo_trabajador like :as_tipo_trabaj and 
      c.sldo_prestamo > 0 and 
      c.flag_estado='1' and c.cod_sit_prest in ('A','S') 
ORDER BY m.cod_origen, m.tipo_trabajador, c.concep, c.cod_trabajador, c.tipo_doc, c.nro_doc " arguments=(("as_origen", string),("as_tipo_trabaj", string),("as_mon_sol", string)) )
