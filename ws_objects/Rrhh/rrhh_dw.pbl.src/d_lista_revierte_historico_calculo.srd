 column=(type=char(2) updatewhereclause=yes name=cod_origen dbname="maestro.cod_origen" )
 column=(type=char(1) updatewhereclause=yes name=tipo_planilla dbname="calculo.tipo_planilla" )
 column=(type=char(4000) updatewhereclause=yes name=desc_tipo_planilla dbname="desc_tipo_planilla" )
 column=(type=char(3) updatewhereclause=yes name=tipo_trabajador dbname="tipo_trabajador.tipo_trabajador" )
 column=(type=char(30) updatewhereclause=yes name=desc_tipo_tra dbname="tipo_trabajador.desc_tipo_tra" )
 column=(type=datetime updatewhereclause=yes name=fec_proceso dbname="calculo.fec_proceso" )
 column=(type=char(15) updatewhereclause=yes name=periodo dbname="periodo" )
 column=(type=number updatewhereclause=yes name=nro_registros dbname="nro_registros" )
 retrieve="select '0' as checked,
		 m.cod_origen,
       c.tipo_planilla,
       usp_sigre_rrhh.of_tipo_planilla(c.tipo_planilla) as desc_tipo_planilla,
       tt.tipo_trabajador, tt.desc_tipo_tra,
       c.fec_proceso,
       
       case 
         when trim(tt.tipo_trabajador) in (USP_SIGRE_RRHH.of_get_tipo_emp(null)) or c.tipo_planilla in ('G', 'V', 'C') then 
           trim(to_char(c.fec_proceso, 'Month', 'NLS_DATE_LANGUAGE = SPANISH')) || '/' || to_char(c.fec_proceso, 'yyyy')
         else
           trim(to_char(c.fec_proceso, 'yyyy')) || '-' || trim(to_char(r.nro_semana, '00'))
       end as periodo,
       count(distinct c.cod_trabajador) as nro_registros       
from calculo c,
     maestro m,
     tipo_trabajador tt,
     rrhh_param_org  r,
     (select distinct oto.cod_origen
       from ot_adm_origen  oto,
            ot_adm_usuario otu
      where oto.ot_adm     = otu.ot_adm
        and otu.cod_usr    = :as_user) o
where c.cod_trabajador = m.cod_trabajador
  and m.tipo_trabajador = tt.tipo_trabajador
  and m.cod_origen      = r.origen               
  and m.tipo_trabajador = r.tipo_trabajador      
  and c.fec_proceso     = r.fec_proceso          
  and c.tipo_planilla   = r.tipo_planilla        
  and m.cod_origen      = o.cod_origen
group by m.cod_origen, 
        c.tipo_planilla,
       usp_sigre_rrhh.of_tipo_planilla(c.tipo_planilla),
       tt.tipo_trabajador, tt.desc_tipo_tra,
       c.fec_proceso,
       to_number(to_char(c.fec_proceso, 'yyyy')),
       r.nro_semana  " arguments=(("as_user", string))  sort="fec_proceso A tipo_planilla A tipo_trabajador A " )
