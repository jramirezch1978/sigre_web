 column=(type=char(50) updatewhereclause=yes name=origen_nombre dbname="origen.nombre" )
 retrieve="select p.cod_trabajador, p.dias_trabaj, nvl(p.prov_cts_01,0) + nvl(p.prov_cts_02,0) + nvl(p.prov_cts_03,0) + nvl(p.prov_cts_04,0) + nvl(p.prov_cts_05,0) + nvl(p.prov_cts_06,0) as prov_cts, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cencos, cc.desc_cencos, m.tipo_trabajador, tt.desc_tipo_tra, m.cod_origen, o.nombre
   from prov_cts_gratif p
      inner join maestro m on p.cod_trabajador = m.cod_trabajador
      left outer join centros_costo cc on m.cencos = cc.cencos
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
   where m.flag_estado = '1'
      and m.flag_cal_plnlla = '1'
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
      and nvl(p.prov_cts_01,0) + nvl(p.prov_cts_02,0) + nvl(p.prov_cts_03,0) + nvl(p.prov_cts_04,0) + nvl(p.prov_cts_05,0) + nvl(p.prov_cts_06,0) <> 0
   order by cod_origen, tipo_trabajador, apel_paterno, apel_materno, nombre1, nombre2, desc_cencos
" )
