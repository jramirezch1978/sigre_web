 retrieve="select distinct
       case
         when m.email is not null and hc.FEC_ENVIO_EMAIL is null  then
           '1'
         else
           '0'
       end as checked,
       m.COD_TRABAJADOR,
       m.NOM_TRABAJADOR,
		 hc.tipo_trabajador,
       tt.desc_tipo_tra as desc_tipo_trabajador,
       m.TIPO_DOC_IDENT_RTPS,
       m.NRO_DOC_IDENT_RTPS,
       m.EMAIL,
		 hc.FEC_ENVIO_EMAIL,
		 hc.EMAIL_TO_SENDED
  from historico_calculo hc,
       vw_pr_trabajador  m,
       tipo_trabajador   tt
 where hc.cod_trabajador = m.COD_TRABAJADOR
   and hc.tipo_trabajador  = tt.tipo_trabajador
   and trim(hc.cod_origen)  || '|'  || to_char(hc.fec_calc_plan, 'dd/mm/yyyy') || '|' || hc.tipo_trabajador || '|' || 
       hc.tipo_planilla in (:as_opciones)
order by desc_tipo_trabajador, nom_trabajador" arguments=(("as_opciones", stringlist))  sort="desc_tipo_trabajador A nom_trabajador A " )
