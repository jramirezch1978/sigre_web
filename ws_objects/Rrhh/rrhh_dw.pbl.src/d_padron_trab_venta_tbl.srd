 column=(type=char(30) updatewhereclause=yes name=desc_seccion dbname="seccion.desc_seccion" )
 retrieve="select m.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.dni, m.cod_trab_antguo, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra, m.cod_seccion, s.desc_seccion
   from maestro m
      inner join origen o on m.cod_origen = o.cod_origen
      inner join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   where m.flag_estado = '1' 
      and m.flag_cal_plnlla = '1'
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
order by m.cod_origen, m.tipo_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" )
