table(column=(type=char(4) updatewhereclause=yes name=concep dbname="historico_calculo.concep" dbalias="hc.concep" )
 column=(type=char(60) updatewhereclause=yes name=concepto_desc_concepto dbname="concepto.desc_concepto" dbalias="co.desc_concepto" )
 column=(type=number updatewhereclause=yes name=imp_soles dbname="imp_soles" )
 retrieve="select hc.concep,
       co.desc_concep as desc_concepto,
       sum(hc.imp_soles) as imp_soles
from historico_calculo hc,
     concepto          co
where hc.concep        = co.concep
  and hc.tipo_trabajador like :as_tipo_trabaj
  and hc.fec_calc_plan = trunc(:ad_fec_proceso)
  and hc.concep like '2%'
  and hc.concep not in ('2399', '2398')
  and hc.cod_origen = :as_origen
  and hc.tipo_planilla = :as_tipo_planilla
group by hc.concep, co.desc_concep  
union
select hc.concep,
       co.desc_concep as desc_concepto,
       sum(hc.imp_soles) as imp_soles
from 	calculo 		hc,
		maestro		m,
     	concepto    co
where hc.concep        = co.concep
  and hc.cod_trabajador = m.cod_trabajador
  and m.tipo_trabajador like :as_tipo_trabaj
  and hc.fec_proceso = trunc(:ad_fec_proceso)
  and hc.concep like '2%'
  and hc.concep not in ('2399', '2398')
  and m.cod_origen = :as_origen
  and hc.tipo_planilla = :as_tipo_planilla
group by hc.concep, co.desc_concep  
" arguments=(("as_origen", string),("as_tipo_trabaj", string),("ad_Fec_proceso", date),("as_tipo_planilla", string))  sort="concep A " )
