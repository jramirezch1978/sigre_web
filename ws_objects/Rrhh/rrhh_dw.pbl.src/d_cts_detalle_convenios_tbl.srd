 column=(type=char(30) updatewhereclause=yes name=desc_tipo_tra dbname="tipo_trabajador.desc_tipo_tra" )
 retrieve="select '1' as flag, acc.cod_trabajador, acc.nro_convenio, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, acc.fec_proceso, null as fecha_det, acc.imp_a_cuenta, 0 as adelantos, (select nvl(sum((((nvl(ccc.imp_prdo_dpsto, 0) + nvl(ccc.int_legales, 0)) / 2) + nvl(ccc.cts_dispon_ant, 0)) - nvl(ccc.a_cnta_cts, 0)), 0) from cnta_crrte_cts ccc where ccc.cod_trabajador = acc.cod_trabajador) as saldo, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra
   from adel_cnta_cts acc
      inner join maestro m on acc.cod_trabajador = m.cod_trabajador
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

union all

select '2' as flag, acc.cod_trabajador, acc.nro_convenio, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, null as fec_proceso, acc.fec_proceso, 0, acc.imp_a_cuenta, 0, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra
  from adel_cnta_cts acc
     inner join maestro m on acc.cod_trabajador = m.cod_trabajador
     inner join origen o on m.cod_origen = o.cod_origen
     inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
  where nvl(acc.imp_a_cuenta,0) <> 0
     and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

  order by cod_origen, tipo_trabajador, nro_convenio, apel_paterno, apel_materno, nombre1, nombre2, flag" )
