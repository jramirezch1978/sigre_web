 column=(type=char(2) updatewhereclause=yes name=mes dbname="mes" values="ENERO (01)	01/FEBRERO (02)	02/MARZO (03)	03/ABRIL (04)	04/MAYO (05)	05/JUNIO (06)	06/JULIO (07)	07/AGOSTO (08)	08/SETIEMBRE (09)	09/OCTUBRE (10)	10/NOVIEMBRE (11)	11/DICIEMBRE(12)	12/" )
 column=(type=number updatewhereclause=yes name=numero_dias dbname="numero_dias" )
 retrieve="select pv.periodo, pv.fec_salida_vaca, pv.fec_retor_vaca, pv.flag_vacacio, m.cencos, m.cod_area, a.desc_area, m.cod_seccion, s.desc_seccion, pv.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra, to_char(pv.fec_salida_vaca,'MM') as mes, (pv.fec_retor_vaca - pv.fec_salida_vaca) + 1 as numero_dias
   from program_vacacion pv
      inner join maestro m on pv.cod_trabajador = m.cod_trabajador
      inner join seccion s on m.cod_seccion = s.cod_seccion and m.cod_area = s.cod_area
      inner join area a on m.cod_area = a.cod_area
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
    where m.flag_estado = '1'
       and m.flag_cal_plnlla = '1'
       and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by m.cod_origen, m.tipo_trabajador, m.cod_seccion,  pv.fec_salida_vaca, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2
" )
