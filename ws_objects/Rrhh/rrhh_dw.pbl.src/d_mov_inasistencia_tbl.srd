 retrieve="SELECT  I.COD_TRABAJADOR ,
        I.CONCEP , c.desc_concep, 
        I.FEC_DESDE ,
        I.FEC_HASTA ,
        I.FEC_MOVIM ,
        I.DIAS_INASIST ,
        I.COD_USR ,
        I.FLAG_REPLICACION   
   FROM INASISTENCIA I,
        concepto     c
  WHERE i.concep = c.concep
    and I.COD_TRABAJADOR = :as_cod_trabajador 
    and i.periodo_inicio is null
ORDER BY I.FEC_MOVIM          ASC  " update="INASISTENCIA" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_trabajador", string)) )
