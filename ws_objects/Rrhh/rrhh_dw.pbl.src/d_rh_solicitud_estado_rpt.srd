 retrieve="  SELECT ~"RRHH_CREDITO_SOLICITUD~".~"NRO_SOLICITUD~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"FECHA_SOLICITUD~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"COD_TRABAJADOR~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"CONCEP~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"IMPORTE~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"FLAG_ESTADO~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"FECHA_APROB_RRHH~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"USR_APROB_RRHH~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"COD_MONEDA~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"DEUDA_PEND_SOL~",   
         ~"RRHH_CREDITO_SOLICITUD~".~"NRO_CUOTAS~" ,
         ~"CNTAS_PAGAR~".~"TIPO_DOC~" ,
         ~"CNTAS_PAGAR~".~"NRO_DOC~"
    FROM ~"RRHH_CREDITO_SOLICITUD~", ~"CNTAS_PAGAR~"
   WHERE ~"RRHH_CREDITO_SOLICITUD~".~"NRO_SOLICITUD~" = ~"CNTAS_PAGAR~".~"NRO_SOL_CRED_RRHH~" (+) And
         ( ~"RRHH_CREDITO_SOLICITUD~".~"FLAG_ESTADO~" like :AS_FLAG_ESTADO ) AND  
         ( ~"RRHH_CREDITO_SOLICITUD~".~"FECHA_SOLICITUD~" >= :ad_inicio ) AND  
         ( ~"RRHH_CREDITO_SOLICITUD~".~"FECHA_SOLICITUD~" <= :ad_fin )   
ORDER BY ~"RRHH_CREDITO_SOLICITUD~".~"FLAG_ESTADO~" ASC,   
         ~"RRHH_CREDITO_SOLICITUD~".~"FECHA_SOLICITUD~" ASC,   
         ~"RRHH_CREDITO_SOLICITUD~".~"CONCEP~" ASC    
" arguments=(("as_flag_estado", string),("ad_inicio", date),("ad_fin", date)) )
