 retrieve="Select hc.cencos, 
		 hc.cod_trabajador, 
		 m.apel_paterno||' '||m.apel_materno||', '||m.nombre1 As nombre,
       cttc.cnta_prsp, 
		 c.desc_concep, 
		 hc.imp_dolar, 
		 hc.imp_soles
  From historico_calculo 		hc, 
		 concepto_tip_trab_cnta cttc,
       maestro 					m, 
		 presupuesto_cuenta 		pc, 
		 concepto 					c
 Where hc.cod_trabajador = m.cod_trabajador
   And hc.concep 			 = cttc.concep
   And cttc.concep 		 = c.concep
   And m.tipo_trabajador = cttc.tipo_trabajador
   And cttc.cnta_prsp 	 = pc.cnta_prsp
   And hc.cod_origen 	 = :as_origen
   And hc.cencos 			 = :as_cencos
   And to_number(to_char(hc.fec_calc_plan,'mm')) = :an_mes
   And to_number(to_char(hc.fec_calc_plan,'yyyy')) = :an_ano
" arguments=(("an_ano", number),("an_mes", number),("as_origen", string),("as_cencos", string))  sort="nombre A " )
