 retrieve="SELECT v.cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,' ')) as nombre_trabaj, 
       v.periodo_inicio, v.periodo_fin, v.dias_totales, v.dias_gozados, v.flag_estado, 
       m.cod_origen, o.nombre, m.flag_estado as estado_trab
  FROM rrhh_vacaciones_trabaj v, 
       maestro m, 
       origen o 
 WHERE v.cod_trabajador = m.cod_trabajador and 
       m.cod_origen = o.cod_origen and 
       m.cod_origen like :as_origen and 
       v.periodo_inicio > = :al_periodo_min and 
       v.periodo_fin <= :al_periodo_max and 
       m.cod_origen in (select oao.cod_origen 
								  from ot_adm_origen oao, ot_adm_usuario oau 
								 where oao.ot_adm = oau.ot_adm and oau.cod_usr = :as_cod_usr) 
ORDER BY m.cod_origen, v.periodo_inicio, v.cod_trabajador" arguments=(("as_origen", string),("al_periodo_min", number),("al_periodo_max", number),("as_cod_usr", string))  sort="origen_nombre A periodo_inicio A " )
