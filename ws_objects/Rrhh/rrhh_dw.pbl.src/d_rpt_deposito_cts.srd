 column=(type=number updatewhereclause=yes name=monto dbname="monto" )
 retrieve="SELECT m.cod_seccion, s.desc_seccion, h.cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||' , '||m.nombre1||trim(nvl(m.nombre2,' ')) nombre, 
       h.dias_asist, h.moneda_cts, m.cencos, c.desc_cencos, sum(pd.monto) monto 
  FROM hist_prov_cts_det pd, hist_prov_cts_gratif h, maestro m, centros_costo c, seccion s 
 WHERE (pd.fecha_proceso = h.fecha_proceso and 
        pd.cod_trabajador = h.cod_trabajador) and 
       (h.cod_trabajador = m.cod_trabajador) and 
       (m.cencos = c.cencos) and 
       (m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion) and 
       h.fecha_proceso = :ad_fecha_proc and 
       h.cod_origen like :as_origen and 
       h.tipo_trabajador like :as_tipo_trabaj 
GROUP BY m.cod_seccion, s.desc_seccion, h.cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||' , '||m.nombre1||trim(nvl(m.nombre2,' ')), 
       h.dias_asist, h.moneda_cts, m.cencos, c.desc_cencos 
ORDER BY m.cod_seccion, trim(m.apel_paterno)||' '||trim(m.apel_materno)||' , '||m.nombre1||trim(nvl(m.nombre2,' '))
" arguments=(("ad_fecha_proc", date),("as_origen", string),("as_tipo_trabaj", string)) )
