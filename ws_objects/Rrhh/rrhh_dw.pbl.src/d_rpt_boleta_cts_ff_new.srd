 column=(type=char(40) updatewhereclause=yes name=hist_prov_cts_det_glosa dbname="hist_prov_cts_det.glosa" )
 column=(type=decimal(2) updatewhereclause=yes name=hist_prov_cts_det_monto dbname="hist_prov_cts_det.monto" )
 retrieve="select h.cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,'')) nombre, 
       m.fec_ingreso, m.dni, h.banco_cts, b.nom_banco, h.nro_cuenta_cts, h.moneda_cts, h.fecha_calculo, c.vta_dol_prom, 
       (h.cts_mes01+h.cts_mes02+h.cts_mes03+h.cts_mes04+h.cts_mes05+h.cts_mes06) importe_cts, 
       (h.dias_asist) as dias, 
       h.ingresos_fijos, 
       h.ingresos_variables, 
       h.ingresos_h_extras, 
       h.ingresos_gratif, 
       h.ingresos_otros, 
       hd.item, 
       hd.glosa, 
       hd.monto 
  from hist_prov_cts_gratif h, maestro m, banco b, calendario c, hist_prov_cts_det hd   
where (h.fecha_proceso = hd.fecha_proceso and 
       h.cod_trabajador = hd.cod_trabajador ) and 
      (h.cod_trabajador=m.cod_trabajador and hd.cod_trabajador=m.cod_trabajador) and 
      (m.cod_banco_cts = b.cod_banco (+)) and 
      trunc(h.fecha_calculo) = trunc(c.fecha) and 
      trunc(h.fecha_proceso) = :ad_fecha_proceso and 
		h.cod_trabajador like :as_cod_trabajador and 
      h.cod_origen like :as_origen and 
      h.tipo_trabajador like :as_tipo_trabaj 
order by h.cod_trabajador, hd.item 
" arguments=(("ad_fecha_proceso", date),("as_origen", string),("as_tipo_trabaj", string),("as_cod_trabajador", string)) )
