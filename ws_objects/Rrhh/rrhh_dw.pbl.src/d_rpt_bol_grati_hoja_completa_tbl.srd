 retrieve="SELECT 	G.COD_TRABAJADOR,   
         	G.FEC_PROCESO,   
         	G.IMP_ADELANTO,   
         	M.APEL_PATERNO,   
         	M.APEL_MATERNO,   
         	M.NOMBRE1,   
         	M.NOMBRE2,   
         	M.COD_SECCION,   
         	S.DESC_SECCION,   
         	G.BONIF_EXT,   
			case
				when m.tipo_trabajador = 'DES' then
					null
				else
					G.DIAS_LABORADOS   
			end as dias_laborados,
         	G.REM_FIJA,   
         	G.REM_VARIABLE,   
			G.DESCUENTOS,
         	G.IMP_BRUTO
   FROM GRATIFICACION		G,   
             MAESTRO			M,   
             SECCION  			S
  WHERE M.COD_TRABAJADOR 	= G.COD_TRABAJADOR 
      and  M.COD_AREA 				= S.COD_AREA 				(+)
 	  and  M.COD_SECCION 			= S.COD_SECCION 			(+)
	  and  M.TIPO_TRABAJADOR 	= :as_tipo_trabajador 
	  AND  M.COD_ORIGEN 			= :as_origen 
	  AND  trunc(G.FEC_PROCESO) = :ad_fec_proceso
ORDER BY 	M.COD_SECCION ASC,   
         		M.APEL_PATERNO ASC,   
         		M.APEL_MATERNO ASC,   
         		M.NOMBRE1 ASC,   
         		M.NOMBRE2 ASC   
" arguments=(("as_tipo_trabajador", string),("as_origen", string),("ad_fec_proceso", date)) )
