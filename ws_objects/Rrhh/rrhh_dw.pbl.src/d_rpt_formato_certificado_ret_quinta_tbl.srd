 retrieve="  SELECT ~"RRHH_FORMATO_QUINTA_CAT~".~"COD_TRABAJADOR~",   
         (select e.nombre 
 				from empresa e 
				where e.cod_empresa in (select cod_empresa from genparam)) as rs_empresa,   
         (select e.ruc nombre 
				from empresa e 
				where e.cod_empresa in (select cod_empresa from genparam)) as ruc_empresa,   
         ~"RRHH_FORMATO_QUINTA_CAT~".~"ANO~",   
         Nvl(Initcap(nombre1),' ') ||' '|| Nvl(Initcap(nombre2),' ') ||' '|| 
			Nvl(Initcap(apel_paterno),' ') ||' '|| Nvl(Initcap(apel_materno),' ') as nombres
    FROM ~"RRHH_FORMATO_QUINTA_CAT~",   
         ~"MAESTRO~"  
   WHERE ( ~"MAESTRO~".~"COD_TRABAJADOR~" = ~"RRHH_FORMATO_QUINTA_CAT~".~"COD_TRABAJADOR~" )  and
			( ~"RRHH_FORMATO_QUINTA_CAT~".~"COD_TRABAJADOR~" = :as_cod_trabajador		  )  and  
			( ~"RRHH_FORMATO_QUINTA_CAT~".~"ANO~"				  = :an_year									  )  
group by ~"RRHH_FORMATO_QUINTA_CAT~".~"COD_TRABAJADOR~",
			~"RRHH_FORMATO_QUINTA_CAT~".~"ANO~" ,
			Nvl(Initcap(nombre1),' ') ||' '|| Nvl(Initcap(nombre2),' ') ||' '|| 
			Nvl(Initcap(apel_paterno),' ') ||' '|| Nvl(Initcap(apel_materno),' ') " arguments=(("as_cod_trabajador", string),("an_year", number),("adc_monto_uit", number),("as_obs", string)) )
