 retrieve="select m.tipo_doc_ident_rtps ||' |' || m.nro_doc_ident_rtps || '|' ||
       e.ruc ||'|' || r1.cod_establecimiento || '|'|| 
       ' ' || '|' as datos_rtps
from rrhh_trabaj_estab_rtps r1,
     maestro                m,
     rrhh_establecimientos_rtps es,
     empresa                e,
    (select distinct hc.cod_trabajador 
      from historico_calculo hc
      where to_char(hc.fec_calc_plan, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))
     union
     select distinct c.cod_trabajador
       from calculo c
     where to_char(c.fec_proceso, 'yyyymm') = to_char(:an_year) || trim(to_char(:an_mes, '00'))) s1
where r1.cod_trabajador = m.cod_trabajador
  and r1.cod_establecimiento = es.cod_establecimiento
  and es.cod_empresa         = e.cod_empresa
  and s1.cod_trabajador      = m.cod_trabajador   AND INSTR(:as_origen, M.COD_ORIGEN) > 0 

" arguments=(("an_year", number),("an_mes", number),("as_origen", string)) )
