 retrieve="select pvb.cod_trabajador, pvb.concep, pvb.importe, m.cencos, cc.desc_cencos, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra, pvb.concep, c.desc_breve
   from prov_vac_bonif pvb
      inner join maestro m on pvb.cod_trabajador = m.cod_trabajador
      inner join centros_costo cc on m.cencos = cc.cencos
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      inner join concepto c on pvb.concep = c.concep
   where pvb.importe <> 0 and pvb.concep in (select gc.concepto_gen from grupo_calculo gc where gc.grupo_calculo = '003')
   order by m.cod_origen, m.tipo_trabajador, m.cencos, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" )
