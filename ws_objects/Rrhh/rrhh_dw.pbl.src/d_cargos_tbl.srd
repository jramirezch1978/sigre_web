 column=(type=char(1) updatewhereclause=yes name=flag_manual_mof dbname="flag_manual_mof" )
 retrieve="  SELECT C.COD_CARGO,   
         C.DESC_CARGO,   
         C.CATEGORIA,   
         C.FLAG_REPLICACION,   
         C.PERFIL_RUTA,   
         C.FLAG_ESTADO,   
         C.NIVEL,   
         C.COD_OCUPACION_RTPS,   
         RO.DESC_OCUPACION,  
			case
				when c.manual_mof is null then
					'0'
				else		
					'1'
			end flag_manual_mof
    FROM CARGO						C,   
         RRHH_OCUPACION_RTPS	RO  
   WHERE RO.cod_ocupacion_rtps (+) = C.cod_ocupacion_rtps
ORDER BY C.COD_CARGO ASC   
" update="CARGO" updatewhere=0 updatekeyinplace=yes )
