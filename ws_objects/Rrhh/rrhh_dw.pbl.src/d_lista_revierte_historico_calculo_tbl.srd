 retrieve="select '0' as checked,
		 hc.cod_origen,
       hc.tipo_planilla,
       usp_sigre_rrhh.of_tipo_planilla(hc.tipo_planilla) as desc_tipo_planilla,
       hc.tipo_trabajador, 
		 tt.desc_tipo_tra,
       hc.fec_calc_plan as fec_proceso,
       case 
         when trim(hc.tipo_trabajador) in (USP_SIGRE_RRHH.of_get_tipo_emp(null)) or hc.tipo_planilla in ('G', 'V', 'C') then 
           trim(to_char(hc.fec_calc_plan, 'Month', 'NLS_DATE_LANGUAGE = SPANISH')) || '/' || to_char(hc.fec_calc_plan, 'yyyy')
         else
           trim(to_char(hc.fec_calc_plan, 'yyyy')) || '-' || trim(to_char(r.nro_semana, '00'))
       end as periodo,
       count(distinct hc.cod_trabajador) as nro_registros       
from historico_calculo hc,
     tipo_trabajador tt,
     rrhh_param_org  r,
     (select distinct oto.cod_origen
       from ot_adm_origen  oto,
            ot_adm_usuario otu
      where oto.ot_adm     = otu.ot_adm
        and otu.cod_usr    = :as_user) o
where hc.tipo_trabajador = tt.tipo_trabajador
  and hc.cod_origen      = r.origen               
  and hc.tipo_trabajador = r.tipo_trabajador      
  and hc.fec_calc_plan   = r.fec_proceso          
  and hc.tipo_planilla   = r.tipo_planilla        
  and hc.cod_origen      = o.cod_origen
group by hc.cod_origen, 
         hc.tipo_planilla,
       	usp_sigre_rrhh.of_tipo_planilla(hc.tipo_planilla),
       	hc.tipo_trabajador, tt.desc_tipo_tra,
       	hc.fec_calc_plan,
       	to_number(to_char(hc.fec_calc_plan, 'yyyy')),
       	r.nro_semana  " arguments=(("as_user", string))  sort="cod_origen A tipo_planilla A tipo_trabajador A fec_proceso D " )
