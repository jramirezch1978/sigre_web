 retrieve="  SELECT to_char(hc.fec_calc_plan,'yyyy/mm') as anomes     ,
         Nvl(m.nro_afp_trabaj,'XXXXXXXXXXXX')||' '||Nvl(m.apel_paterno,' ') ||' '|| Nvl(m.apel_materno,' ') ||' '|| Nvl(m.nombre1,' ') ||' '|| Nvl(m.nombre2,' ') as nombres,
			Sum(hc.imp_soles) as valor
    FROM historico_calculo hc,   
         maestro m
   WHERE ( hc.cod_trabajador = m.cod_trabajador ) and
				(hc.concep          in (select gc.concepto_gen from grupo_calculo gc
                               where gc.grupo_calculo  in (select v.afp_comision
                                                             from rrhhparam_cconcep v 
                                                            where v.reckey = '1' 
                                                           union
                                                           select v.afp_invalidez
                                                             from rrhhparam_cconcep v 
                                                            where v.reckey = '1'  ))        ) and
         (to_char(hc.fec_calc_plan,'yyyy') = :as_ano )  and
			(m.cod_afp			  = :as_cod_afp) AND
			(m.cod_origen = :as_cod_origen)
GROUP BY to_char(hc.fec_calc_plan,'yyyy/mm')   ,
         Nvl(m.nro_afp_trabaj,'XXXXXXXXXXXX')||' '||Nvl(m.apel_paterno,' ') ||' '|| Nvl(m.apel_materno,' ') ||' '|| Nvl(m.nombre1,' ') ||' '|| Nvl(m.nombre2,' ') 
	" arguments=(("as_ano", string),("as_cod_afp", string),("as_cod_origen", string))  sort="Mid(nombres,14) A " )
