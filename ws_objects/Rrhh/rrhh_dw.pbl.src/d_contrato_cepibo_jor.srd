 column=(type=char(30) updatewhereclause=yes name=area_desc_area dbname="area.desc_area" dbalias="a.desc_area" )
 column=(type=char(30) updatewhereclause=yes name=seccion_desc_seccion dbname="seccion.desc_seccion" dbalias="s.desc_seccion" )
