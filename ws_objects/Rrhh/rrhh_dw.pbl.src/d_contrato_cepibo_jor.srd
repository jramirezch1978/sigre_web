 retrieve="select m.apel_paterno||' '||m.apel_materno||', '||m.nombre1||' '||m.nombre2 as TRABAJADOR, 
       m.dni, a.desc_area, s.desc_seccion, 
(m.direccion||'-'||(select d.desc_distrito||'-'||p.desc_prov||'-'||de.desc_dpto from distrito d, provincia_condado p , departamento_estado de 
where d.cod_distr = m.cod_distr
  and d.cod_prov = m.cod_prov
  and d.cod_dpto = m.cod_dpto
  and d.cod_pais = m.cod_pais
  and d.cod_prov = p.cod_prov
  and p.cod_dpto = d.cod_dpto
  and d.cod_dpto = de.cod_dpto) )as direccion, 
(select a.dni from maestro a where a.cod_trabajador = :as_ger) as dni_ger, (select a.apel_paterno||' '||a.apel_materno||', '||a.nombre1||' '||a.nombre2 from maestro a where a.cod_trabajador = :as_ger) as nom_ger,
		trunc(:ad_ingreso) as ingreso, trunc(:ad_cese) as fin, to_char(:ad_ingreso,'dd') as dia, usf_fl_nomb_mes(to_Char(:ad_ingreso,'mm')) as mes, to_char(:ad_ingreso,'yyyy') as anio
  from maestro m, seccion s, area a
  where m.cod_seccion = s.cod_seccion (+) and
        m.cod_area = a.cod_area (+) AND
		m.cod_trabajador LIKE trim(:as_cod_trab)" arguments=(("as_cod_trab", string),("ad_ingreso", date),("ad_cese", date),("as_ger", string)) )
