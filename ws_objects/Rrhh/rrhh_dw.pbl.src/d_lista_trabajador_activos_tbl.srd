 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="maestro.flag_estado" values="Activo	1/Inactivo	0/" )
 retrieve="SELECT distinct(m.cod_trabajador) as cod_trabajador, 
       trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,' ')) as nombre, 
		 m.tipo_trabajador, 
		 m.cod_origen, 
		 m.fec_ingreso, 
		 m.fec_cese, 
       m.flag_estado 
  FROM maestro m
 WHERE m.cod_origen like :as_origen 
	AND m.tipo_trabajador like :as_tipo_trabajador 
   AND m.cod_trabajador like :as_cod_trabajador 
   and m.flag_estado = '1'
ORDER BY trim(m.apel_paterno)||' '||trim(m.apel_materno)||', '||trim(m.nombre1)||' '||trim(nvl(m.nombre2,' '))" arguments=(("as_origen", string),("as_tipo_trabajador", string),("as_cod_trabajador", string)) )
