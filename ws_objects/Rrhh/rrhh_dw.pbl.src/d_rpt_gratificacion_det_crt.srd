 column=(type=datetime updatewhereclause=yes name=fec_ingreso dbname="vw_pr_trabajador_fec_ingreso" )
 column=(type=decimal(0) updatewhereclause=yes name=gratificacion_dias_laborados dbname="gratificacion_dias_laborados" )
