 retrieve="select gd.cod_trabajador,
       m.NOM_TRABAJADOR,
       m.tipo_trabajador,
       m.FEC_INGRESO,
       g.dias_laborados,
       gd.fec_pago,
       case 
         when gd.flag_fijo_var = 'F' then '1.FIJO'
			when gd.flag_fijo_var = 'V' then '2.VARIABLE'
			when gd.flag_fijo_var = 'D' then '3.DESCUENTOS'
         else '4.BONIF.EXT.'
       end as fijo_variable,
       c.concep || '-' || c.desc_concep as desc_Concepto,
       gd.importe
from gratificacion g,
     gratificacion_det gd,
     vw_pr_trabajador m,
     concepto         c
where g.cod_trabajador = gd.cod_trabajador
  and g.fec_proceso    = gd.fec_proceso
  and g.periodo        = gd.periodo     
  and gd.cod_trabajador = m.cod_trabajador
  and gd.concepto       = c.concep
  and m.cod_origen		like :as_origen
  and m.tipo_trabajador	like :as_tipo_trabaj
  and g.fec_proceso		= :ad_fec_proceso
order by cod_trabajador, fijo_variable, fec_pago  
  " arguments=(("as_origen", string),("as_tipo_trabaj", string),("ad_fec_proceso", date))  sort="nom_trabajador A " )
