 retrieve="select cdu.cod_trabajador , m.apel_paterno    , m.apel_materno   , m.nombre1      ,
       m.nombre2          , m.cencos          , m.cod_area       , a.desc_area    ,
       m.cod_seccion      , s.desc_seccion    , m.dni            , m.cod_origen   ,
       o.nombre           , m.tipo_trabajador , tt.desc_tipo_tra , m.nro_cnta_cts ,
       cdu.liquidacion    , cdu.remuneracion  , cdu.fec_proceso  , m.cod_banco_cts  ,
		 Nvl(b.nom_banco,'Sin Cuenta de Banco') as nom_banco
  from cts_decreto_urgencia cdu,maestro m,
		 tipo_trabajador      tt ,origen  o,
       area                 a  ,seccion s,
		 banco					 b 
 where (cdu.cod_trabajador = m.cod_trabajador   )  and
       (m.tipo_trabajador  = tt.tipo_trabajador )  and
       (m.cod_origen       = o.cod_origen       )  and
       (m.cod_area         = a.cod_area         )  and
      ((a.cod_area         = s.cod_area         )  and
       (m.cod_seccion      = s.cod_seccion      )) and
       (cdu.remuneracion  <> 0                  )  and
		 (m.cod_banco_cts		= b.cod_banco		(+))  and
		 (m.cod_origen			= :as_cod_origen		)  and
		 (m.tipo_trabajador  = :as_tip_trab			)  and
		 (trunc(cdu.fec_proceso) = trunc(:ad_fec_proceso))
order by m.cod_origen, m.tipo_trabajador, cdu.fec_proceso, m.cod_seccion, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" arguments=(("as_cod_origen", string),("as_tip_trab", string),("ad_fec_proceso", date))  sort="cod_origen A tipo_trabajador A fec_proceso A cod_banco_cts A cod_seccion A " )
