 column=(type=char(2) updatewhereclause=yes name=mes dbname="mes" values="Enero	01/Febrero	02/Marzo	03/Abril	04/Mayo	05/Junio	06/Julio	07/Agosto	08/Setiembre	09/Octubre	10/Noviembre	11/Diciembre	12/" )
 column=(type=char(4) updatewhereclause=yes name=ano dbname="ano" )
 column=(type=char(60) updatewhereclause=yes name=concepto_desc_concep dbname="desc_concep" )
 retrieve="select aq.concep, aq.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, aq.imp_adelanto, aq.fec_proceso, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_area, m.cod_seccion, trim(nvl(initcap(s.desc_seccion), 'Sección no descrita')) as desc_seccion, to_char(aq.fec_proceso, 'mm') as mes, to_char(aq.fec_proceso, 'yyyy') as ano, initcap(c.desc_concep) as desc_concep
   from adelanto_quincena aq
      left outer join maestro m on aq.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
		left outer join concepto c on aq.concep = c.concep
   where m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   
   union all

select hc.concep, hc.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, hc.imp_soles, hc.fec_calc_plan, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.cod_area, m.cod_seccion, trim(nvl(initcap(s.desc_seccion), 'Sección no descrita')) as desc_seccion, to_char(hc.fec_calc_plan, 'mm') as mes, to_char(hc.fec_calc_plan, 'yyyy') as ano, initcap(c.desc_concep) as desc_concep
   from historico_calculo hc 
      inner join grupo_calculo gc on hc.concep = gc.concepto_gen
      left outer join adelanto_quincena aq on hc.cod_trabajador = aq.cod_trabajador 
         and hc.concep = aq.concep 
         and trunc(hc.fec_calc_plan) = trunc(aq.fec_proceso)
      left outer join maestro m on hc.cod_trabajador = m.cod_trabajador
      left outer join origen o on m.cod_origen = o.cod_origen
      left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
      left outer join area a on m.cod_area = a.cod_area
      left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
		left outer join concepto c on hc.concep = c.concep
   where gc.grupo_calculo = (select rc.adelanto_quincena from rrhhparam_cconcep rc where rc.reckey = '1') 
      and aq.concep is null
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

order by tipo_trabajador, cod_origen, fec_proceso, apel_paterno, apel_materno, nombre1, nombre2, cod_area, cod_seccion" )
