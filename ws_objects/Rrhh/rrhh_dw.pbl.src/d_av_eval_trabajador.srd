 retrieve="  SELECT ~"RRHH_TRABA_USR_EVALUADOR~".~"COD_USR~",   
         ~"RRHH_TRABA_USR_EVALUADOR~".~"COD_TRABAJADOR~",   
         ~"RRHH_TRABA_USR_EVALUADOR~".~"FLAG_ESTADO~",   
         ~"RRHH_TRABA_USR_EVALUADOR~".~"FLAG_REPLICACION~",   
         ~"MAESTRO~".~"APEL_PATERNO~",   
         ~"MAESTRO~".~"APEL_MATERNO~",   
         Nvl(MAESTRO.NOMBRE1,'')||' '||Nvl(MAESTRO.NOMBRE2,'') as nombres,
			usf_rh_status_eval_trabajador(:an_ano, :an_mes, rrhh_traba_usr_evaluador.cod_trabajador) as status,
			usf_rh_result_eval_trabajador(:an_ano, :an_mes, rrhh_traba_usr_evaluador.cod_trabajador) as eval
    FROM ~"RRHH_TRABA_USR_EVALUADOR~",   
         ~"MAESTRO~"  
   WHERE ( ~"MAESTRO~".~"COD_TRABAJADOR~" = ~"RRHH_TRABA_USR_EVALUADOR~".~"COD_TRABAJADOR~" ) and 
			(rrhh_traba_usr_evaluador.flag_estado = '1') and
         ( ( ~"RRHH_TRABA_USR_EVALUADOR~".~"COD_USR~" = :as_usuario ) )    
    order by 7" arguments=(("an_ano", number),("an_mes", number),("as_usuario", string))  sort="apel_paterno A " )
