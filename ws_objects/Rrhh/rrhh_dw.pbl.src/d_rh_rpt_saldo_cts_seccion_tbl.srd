 retrieve="select ct.cod_trabajador, 
       m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1 || ' ' || m.nombre2 as nom_trabajador,
       ct.fec_proceso,
       ct.remuneracion,
       ct.rem_variable,
       ct.gratificacion,
       ct.cencos,
       ct.dias_trabajados,
       ct.liquidacion,
       s.cod_seccion, s.desc_seccion,
       a.cod_area, a.desc_area
from cts_decreto_urgencia ct,
     maestro              m,
     area                 a,
     seccion              s
where ct.cod_trabajador = m.cod_trabajador
  and m.cod_area        = s.cod_area
  and m.cod_seccion     = s.cod_seccion
  and s.cod_area        = a.cod_area
  and m.tipo_trabajador = :as_tipo_trabajador
  and ct.fec_proceso	   = :ad_fec_proceso
      
" arguments=(("as_tipo_trabajador", string),("ad_fec_proceso", date)) )
