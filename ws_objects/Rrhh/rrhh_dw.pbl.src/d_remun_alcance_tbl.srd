 column=(type=char(4) updatewhereclause=yes name=ano dbname="ano" )
 column=(type=char(2) updatewhereclause=yes name=mes dbname="mes" values="ENERO	01/FEBRERO	02/MARZO	03/ABRIL	04/MAYO	05/JUNIO	06/JULIO	07/AGOSTO	08/SETIEMBRE	09/OCTUBRE	10/NOVIEMBRE	11/DICIEMBRE	12/" )
 retrieve="select c.cod_trabajador, c.imp_soles, c.fec_proceso as fecha, upper(decode(trim(nvl(m.nro_cnta_ahorro,' ')) || 's', 's', 'sin tarjeta', 'con tarjeta')) as tarjeta, m.cod_origen, initcap(o.nombre) as desc_origen, m.tipo_trabajador, initcap(tt.desc_tipo_tra) as desc_tipo_trab, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2, initcap(trim(a.desc_area) || ' (' || m.cod_area || ') - ' || trim(s.desc_seccion) || ' (' || m.cod_seccion || ')') as seccion, to_char(c.fec_proceso, 'yyyy') as ano, to_char(c.fec_proceso, 'mm') as mes
      from calculo c
         left outer join maestro m on c.cod_trabajador = m.cod_trabajador
         left outer join origen o on m.cod_origen = o.cod_origen
         left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
         left outer join area a on m.cod_area = a.cod_area
         left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
      where c.concep = (select rhp.cnc_total_pgd from rrhhparam rhp where rhp.reckey = '1')
         and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')

   union all

   select  hc.cod_trabajador,  hc.imp_soles,  hc.fec_calc_plan as fecha,  upper(decode(trim(nvl(m.nro_cnta_ahorro,' ')) || 's', 's', 'sin tarjeta', 'con tarjeta')) as tarjeta,  m.cod_origen,  initcap(o.nombre) as desc_origen,  m.tipo_trabajador,  initcap(tt.desc_tipo_tra) as desc_tipo_trab,  m.apel_paterno,  m.apel_materno,  m.nombre1,  m.nombre2,  initcap(trim(a.desc_area) || ' (' || m.cod_area || ') - ' || trim(s.desc_seccion) || ' (' || m.cod_seccion || ')') as seccion,  to_char(hc.fec_calc_plan, 'yyyy') as ano,  to_char(hc.fec_calc_plan, 'mm') as mes
      from historico_calculo hc
         left outer join calculo c on hc.concep = c.concep and hc.cod_trabajador = c.cod_trabajador and trunc(hc.fec_calc_plan) = trunc(c.fec_proceso)
         left outer join maestro m on hc.cod_trabajador = m.cod_trabajador
         left outer join origen o on m.cod_origen = o.cod_origen
         left outer join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
         left outer join area a on m.cod_area = a.cod_area
         left outer join seccion s on m.cod_area = s.cod_area and m.cod_seccion = s.cod_seccion
      where hc.concep = (select rhp.cnc_total_pgd from rrhhparam rhp where rhp.reckey = '1')
         and c.concep is null
         and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
         
order by cod_origen, tipo_trabajador, tarjeta, fecha, seccion, apel_paterno, apel_materno, nombre1, nombre2

" )
