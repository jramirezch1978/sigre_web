 column=(type=char(30) updatewhereclause=yes name=nombre2 dbname="maestro.nombre2" )
 retrieve="select cdu.cod_trabajador, m.cod_seccion, m.cod_area, m.cencos, cc.desc_cencos, cdu.liquidacion, cdu.fec_proceso, decode(to_char(cdu.fec_proceso, 'mm'), '01', (select p.prov_gratif_01 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '02', (select p.prov_gratif_02 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '03', (select p.prov_gratif_03 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '04', (select p.prov_gratif_04 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '05', (select p.prov_gratif_05 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '06', (select p.prov_gratif_06 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '07', (select p.prov_gratif_07 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '08', (select p.prov_gratif_08 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '09', (select p.prov_gratif_09 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '10', (select p.prov_gratif_10 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '11', (select p.prov_gratif_11 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador), '12', (select p.prov_gratif_12 from prov_cts_gratif p where p.cod_trabajador = cdu.cod_trabajador)) as imp_prov_gra, m.cod_origen, o.nombre, m.tipo_trabajador, tt.desc_tipo_tra, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2
   from cts_decreto_urgencia cdu 
      inner join maestro m on cdu.cod_trabajador = m.cod_trabajador
      inner join centros_costo cc on m.cencos = cc.cencos
      inner join origen o on m.cod_origen = o.cod_origen
      inner join tipo_trabajador tt on m.tipo_trabajador = tt.tipo_trabajador
   where m.flag_estado = '1' 
      and m.flag_cal_plnlla = '1' 
      and m.cod_origen in (select oao.cod_origen from ot_adm_origen oao inner join ot_adm_usuario oau on oao.ot_adm = oau.ot_adm where oau.cod_usr = 'mmcsn')
   order by m.cencos, m.cod_seccion, m.cod_trabajador" )
