 retrieve="select cc.cod_trabajador, cc.tipo_doc, cc.nro_doc, cc.sldo_prestamo,
       c.desc_concep, p.nom_proveedor
from cnta_crrte cc,
     concepto   c,
     proveedor  p
where cc.concep = c.concep
  and cc.cod_trabajador = p.proveedor
  and cc.flag_estado = '1'
  and cc.sldo_prestamo > 0
  and cc.cod_trabajador = :as_trabajador" arguments=(("as_trabajador", string)) )
