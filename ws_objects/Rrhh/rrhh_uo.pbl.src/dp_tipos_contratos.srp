 COLUMN(type=varchar,name="descrpcion",dbtype="Char(71)",nulls_allowed=yes)
 COLUMN(type=varchar,name="flag_estado",dbtype="Char(254)",nulls_allowed=yes))
RETRIEVE(statement="SELECT t_cont.codigo,              t_cont.descrpcion  ,  			'1' as flag_estado      FROM t_cont     ")
