 retrieve="select ca.nro_parte, ca.nro_item,
       ca.cod_art, a.desc_art, a.und, ca.cantidad,
       ca.cod_usr, ca.fec_registro,
       ca.org_am_sal, ca.nro_am_sal,
       op.nro_orden, op.oper_sec, am.nro_vale
from cam_parte_riego_art ca,
     articulo            a,
     articulo_mov        am,
     operaciones         op
where ca.cod_art = a.cod_art
  and ca.org_am_sal = am.cod_origen
  and ca.nro_am_sal = am.nro_mov     
  and am.oper_sec   = op.oper_sec
  and ca.nro_parte  = :as_nro_parte
  and ca.nro_item   = :an_nro_item
  " update="cam_parte_riego_art" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string),("an_nro_item", number)) )
