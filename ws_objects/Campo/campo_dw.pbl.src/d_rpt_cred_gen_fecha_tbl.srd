 retrieve="select cc.nro_credito, cc.fec_prestamo, 
       cc.proveedor cod_benef, b.nom_proveedor nom_benef, 
       ctp.desc_tipo_prestamo, cc.observaciones, 
       cc.cod_base, ab.desc_base, 
       ccd.proveedor cod_prod, p.nom_proveedor nom_prod, 
       cc.cod_moneda, ccd.monto_prestamo, 
       ccd.monto_interes, ccd.monto_comisiones, 
       ccd.nro_cuotas, ccd.monto_cuota,
       (select sum(cbd.importe) from caja_bancos_det cbd, caja_bancos cb where cbd.cod_relacion = ccd.proveedor and cbd.tipo_doc = ccd.tipo_doc_cxp and cbd.nro_doc = ccd.nro_doc_cxp and cb.origen = cbd.origen and cb.nro_registro = cbd.nro_registro and (cbd.factor = '-1' or cb.flag_tiptran = '3')) monto_cancelado,
       ccd.tipo_doc_cxp, ccd.nro_doc_cxp, ccd.monto_cancelado
  from cam_creditos cc,
       cam_creditos_det ccd,
       proveedor b,
       cam_tipo_prestamo ctp,
       proveedor p,
       ap_bases ab
  where cc.proveedor = b.proveedor AND
        cc.tipo_prestamo = ctp.tipo_prestamo AND
        cc.nro_credito = ccd.nro_credito AND
        cc.cod_base = ab.cod_base (+) AND
        ccd.proveedor = p.proveedor AND
        cc.flag_estado = '1' AND
        cc.proveedor like :as_benef AND
        cc.tipo_prestamo like :as_tipo_pre AND
        ccd.proveedor like :as_prod AND
        cc.observaciones like :as_obs AND
        cc.fec_prestamo BETWEEN :ad_fecha1 AND :ad_fecha2
 order by cc.fec_prestamo, b.nom_proveedor
        " arguments=(("as_benef", string),("as_tipo_pre", string),("as_prod", string),("as_obs", string),("ad_fecha1", date),("ad_fecha2", date)) )
