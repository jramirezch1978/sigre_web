 retrieve="select p.cod_labor, l.desc_labor, l.und, 
       sum(p.rendimiento) as rendimiento,
       sum(p.hrs_normales + p.hrs_extras_25 + p.hrs_extras_35 + p.hrs_noc_extras_35 + p.hrs_extras_100) as horas,
       sum(p.rendimiento) / sum(p.hrs_normales + p.hrs_extras_25 + p.hrs_extras_35 + p.hrs_noc_extras_35 + p.hrs_extras_100) as prom_x_hora
from pd_jornal_campo p,
     labor           l
where p.cod_labor = l.cod_labor
  and p.rendimiento > 0
  and trunc(p.fecha) between :ad_fecha1 and :ad_fecha2
group by p.cod_labor, l.desc_labor, l.und  " arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
