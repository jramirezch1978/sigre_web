 column=(type=char(8) updatewhereclause=yes name=cod_labor dbname="pd_jornal_campo.cod_labor" )
 column=(type=char(40) updatewhereclause=yes name=desc_labor dbname="labor.desc_labor" )
 column=(type=char(3) updatewhereclause=yes name=und dbname="labor.und" )
 column=(type=number updatewhereclause=yes name=rendimiento dbname="rendimiento" )
 column=(type=number updatewhereclause=yes name=horas dbname="horas" )
 column=(type=number updatewhereclause=yes name=nro_trabajadores dbname="nro_trabajadores" )
 column=(type=number updatewhereclause=yes name=promedio dbname="promedio" )
 column=(type=number updatewhereclause=yes name=avance dbname="avance" )
 column=(type=number updatewhereclause=yes name=avance_acum dbname="avance_acum" )
 retrieve="select p.fecha, lc.nro_lote, p.cod_labor, l.desc_labor, l.und,
       sum(p.rendimiento) as rendimiento,
       sum(p.hrs_normales + p.hrs_extras_25 + p.hrs_extras_35 + p.hrs_noc_extras_35 + p.hrs_extras_100) as horas,
		 count(distinct p.cod_trabajador) as nro_trabajadores,
       CASE  when sum(p.hrs_normales + p.hrs_extras_25 + p.hrs_extras_35 + p.hrs_noc_extras_35 + p.hrs_extras_100) <> 0 then
             sum(p.rendimiento) / sum(p.hrs_normales + p.hrs_extras_25 + p.hrs_extras_35 + p.hrs_noc_extras_35 + p.hrs_extras_100) 
       ELSE
             0
       END as promedio,
       usf_cam_avance_x_lote(:as_campana, lc.nro_lote, p.cod_labor, p.fecha) * 100 as avance,
       usf_cam_avance_x_lote_acum(:as_campana, lc.nro_lote, p.cod_labor, p.fecha) * 100 as avance_acum
from pd_jornal_campo p,
     pd_jornal_campo_lote pd,
     labor                l,
     lote_campo           lc
where p.fecha = pd.fecha
  and p.cod_trabajador = pd.cod_trabajador     
  and p.nro_item       = pd.nro_item
  and p.cod_labor      = l.cod_labor
  and pd.nro_lote      = lc.nro_lote
  and p.rendimiento    > 0
  and lc.nro_lote		  = :as_lote
  and p.cod_labor		  = :as_labor
  and trunc(p.fecha) between :ad_fecha1 and :ad_fecha2
group by p.fecha, lc.nro_lote, p.cod_labor, l.desc_labor, l.und" arguments=(("as_campana", string),("as_lote", string),("as_labor", string),("ad_fecha1", date),("ad_fecha2", date)) )
