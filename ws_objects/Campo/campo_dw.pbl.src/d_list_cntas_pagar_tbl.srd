 column=(type=char(180) updatewhereclause=yes name=descripcion dbname="cntas_pagar.descripcion" dbalias="cp.descripcion" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_pagar.cod_moneda" dbalias="cp.cod_moneda" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
