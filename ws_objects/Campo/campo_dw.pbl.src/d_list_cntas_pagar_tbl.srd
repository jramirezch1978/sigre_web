 column=(type=char(1) updatewhereclause=yes name=ck dbname="ck" values="	1/	0" )
 retrieve="  select cp.cod_relacion, cp.tipo_doc, cp.nro_doc,
			cp.fecha_emision,
         cp.descripcion,
         cp.cod_moneda, 
         decode(cp.cod_moneda, (select cod_soles from logparam where reckey = '1'), cp.saldo_sol, cp.saldo_dol) as importe,
         '' as confin,
			'0' as ck
  from cntas_pagar cp, 
       proveedor   p 
  where cp.cod_relacion = p.proveedor 
    and cp.tipo_doc     = (select a.doc_lc from ap_param a where a.origen = 'XX') 
    and cp.flag_estado  <> '0' 
    and cp.saldo_dol    > 0 
    and cp.saldo_sol    > 0 
    and trunc(cp.fecha_emision) <= trunc(:ad_Fecha)
	 and cp.cod_moneda	= :as_moneda
    and cp.cod_relacion = :as_proveedor
order by cp.fecha_emision" arguments=(("as_proveedor", string),("as_moneda", string),("ad_fecha", date))  sort="fecha_emision D tipo_doc A nro_doc A " )
