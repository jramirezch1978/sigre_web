 retrieve="select ccd.nro_credito, 
       ccd.nro_item,
       ccd.proveedor, p.nom_proveedor,
       ccd.fec_registro,
       ccd.fec_inicio_pago,
       ccd.cencos,
       ccd.cnta_prsp, ccd.centro_benef,
       ccd.nro_cuotas,
       ccd.monto_cuota,
       ccd.monto_prestamo,
       ccd.monto_interes,
       ccd.monto_comisiones,
       ccd.flag_estado,
       ccd.proveedor_cxp,
       ccd.tipo_doc_cxp,
       ccd.nro_doc_cxp,
		 ccd.cod_usr
from cam_creditos_det ccd,
     proveedor        p
where ccd.proveedor   = p.proveedor     
  and ccd.nro_credito = :as_nro_credito" update="cam_creditos_det" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_credito", string)) )
