 retrieve="select spl.semana, spld.cod_art, a.desc_art, ab.cod_base, ab.desc_base,
       sum(nvl(spld.pallet_01,0)+nvl(spld.pallet_02,0)+nvl(spld.pallet_03,0)+nvl(spld.pallet_04,0)+nvl(spld.pallet_05,0)+
       nvl(spld.pallet_06,0)+nvl(spld.pallet_07,0)+nvl(spld.pallet_08,0)+nvl(spld.pallet_09,0)+nvl(spld.pallet_10,0)+
       nvl(spld.pallet_11,0)+nvl(spld.pallet_12,0)+nvl(spld.pallet_13,0)+nvl(spld.pallet_14,0)+nvl(spld.pallet_15,0)+
       nvl(spld.pallet_16,0)+nvl(spld.pallet_17,0)+nvl(spld.pallet_18,0)+nvl(spld.pallet_19,0)+nvl(spld.pallet_20,0))as total,
       (select  sum(arc.cant_cajas)
          from  ap_control_cosecha acc,
                ap_control_cosecha_det accd,
                ap_proveedor_certif apc2,
                ap_bases ab2,
                ap_recibo_cajas arc,
                articulo a2
          where acc.nro_parte          = accd.nro_parte
            and accd.nro_certificacion = apc2.nro_certificacion
            and apc2.cod_base          = ab2.cod_base
            and acc.nro_parte          = arc.nro_parte
            and arc.cod_art_pptt       = spld.cod_art
            and apc2.cod_base          = ab.cod_base
            and acc.semana             = spl.semana
            and arc.cod_art_pptt = a2.cod_art)
  from sic_packing_list spl,
       sic_packing_list_det spld,
       articulo a,
       ap_proveedor_certif apc,
       ap_bases ab
 where spl.nro_registro      = spld.nro_registro
   and spld.cod_art          = a.cod_art
   and spld.nro_certificacion  = apc.nro_certificacion
   and apc.cod_base            = ab.cod_base
   and spl.semana between :an_semana1 and :an_semana2
   and spl.ano between :an_ano1 and :an_ano2
group by spl.semana, spld.cod_art, a.desc_art, ab.cod_base , ab.desc_base
" arguments=(("an_ano1", number),("an_semana1", number),("an_ano2", number),("an_semana2", number)) )
