 retrieve="select a.campana, c.descripcion,
       a.nro_lote, 
       a.cod_labor, l.desc_labor,
       a.fec_programada,
       a.fec_registro,
       a.hrs_programadas,
       a.hrs_ejec_normal,
       a.hrs_ejec_extras 
from cam_proy_horas_jornal a,
     campanas              c,
     lote_campo            lc,
     labor                 l
where a.campana = c.campana
  and a.nro_lote = lc.nro_lote
  and a.cod_labor = l.cod_labor    
  and a.nro_lote = :as_nro_lote
  and trunc(a.fec_programada) between :ad_fecha1 and :ad_fecha2 
order by a.cod_labor, a.fec_programada" update="cam_proy_horas_jornal" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_lote", string),("ad_fecha1", date),("ad_fecha2", date)) )
