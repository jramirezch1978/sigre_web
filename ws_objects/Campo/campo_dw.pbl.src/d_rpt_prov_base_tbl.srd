 column=(type=char(100) updatewhereclause=yes name=ap_bases_desc_base dbname="ap_bases.desc_base" )
 column=(type=char(8) updatewhereclause=yes name=cod_proveedor dbname="ap_proveedor_certif.cod_proveedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=char(50) updatewhereclause=yes name=nro_certificacion dbname="ap_proveedor_certif.nro_certificacion" )
 retrieve="SELECT apc.cod_base, ab.desc_base, apc.proveedor cod_proveedor, p.nom_proveedor, apc.nro_certificacion
       FROM ap_proveedor_certif apc,
            ap_bases ab,
            proveedor p
       WHERE apc.cod_base = ab.cod_base AND
             apc.proveedor = p.proveedor AND
             ab.cod_base LIKE :as_base AND
			p.proveedor LIKE :as_prod
	 ORDER BY P.NOM_PROVEEDOR" arguments=(("as_base", string),("as_prod", string)) )
