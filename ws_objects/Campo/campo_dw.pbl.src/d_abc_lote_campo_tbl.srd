 retrieve="select lc.nro_lote, 
       lc.cencos, cc.desc_cencos,
       lc.descripcion,
       lc.total_ha, 
       lc.flag_estado,
       lc.cod_sector, sc.desc_sector,
       lc.cod_usr,
       lc.cod_origen,
       lc.centro_benef, cb.desc_centro,
       lc.nro_lineas, lc.nro_plantas, lc.total_plantas
from lote_campo lc,
     sector_campo sc,
     centro_beneficio cb,
     centros_costo    cc
where lc.cod_sector = sc.cod_sector (+)
  and lc.centro_benef = cb.centro_benef (+)  
  and lc.cencos       = cc.cencos (+)" update="lote_campo" updatewhere=0 updatekeyinplace=yes )
