 retrieve="select p.fecha, p.cod_trabajador, p.nro_item, p.cod_labor, l.desc_labor, count(*) 
from pd_jornal_campo p,
     pd_jornal_campo_lote pd,
     labor                l,
	  tt_labores_selec	  tt
where p.fecha = pd.fecha
  and p.cod_trabajador = pd.cod_trabajador     
  and p.nro_item       = pd.nro_item
  and p.cod_labor      = l.cod_labor
  and p.cod_labor		  = tt.cod_labor
  and p.rendimiento    > 0
  and trunc(p.fecha) between :ad_fecha1 and :ad_fecha2
group by p.fecha, p.cod_trabajador, p.nro_item, p.cod_labor, l.desc_labor
having count(*) > 1
order by p.fecha" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
