 retrieve="select cc.nro_credito,
       cc.cod_origen,
       cc.fec_registro,
       cc.tipo_prestamo, ctp.desc_tipo_prestamo,
       cc.proveedor, p.nom_proveedor,
       cc.cod_base, ab.desc_base,
       cc.fec_prestamo,
       cc.flag_periodo,
       cc.tasa_interes,
       cc.total_prestamo,
       cc.total_interes,
       cc.total_comisiones,
       cc.flag_estado,
		 cc.cod_usr,
		 cc.observaciones,
		 cc.cod_moneda
from cam_creditos cc,
     ap_bases     ab,
     cam_tipo_prestamo ctp,
     proveedor         p
where cc.cod_base = ab.cod_base  (+)
  and cc.tipo_prestamo = ctp.tipo_prestamo
  and cc.proveedor     = p.proveedor
  and cc.nro_Credito	  = :as_nro_credito" update="cam_creditos" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_credito", string)) )
