 retrieve="select t.nro_parte, 
       t.fec_parte, t.operador, vw.NOM_TRABAJADOR,
       t.bomba, mq.desc_maq,
       t.tipo_riego, ct.desc_tipo_riego
from cam_parte_riego_cab t,
     vw_pr_trabajador    vw,
     maquina             mq,
     cam_tipo_riego      ct
where t.operador = vw.COD_TRABAJADOR     
  and t.bomba    = mq.cod_maquina
  and t.tipo_riego = ct.cod_tipo_riego (+)
order by t.fec_parte" )
