table(column=(type=char(8) updatewhereclause=yes name=proveedor dbname="proveedor.proveedor" dbalias="p.proveedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=nro_doc_ident dbname="proveedor.nro_doc_ident" dbalias="p.nro_doc_ident" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_pagar.cod_moneda" dbalias="cp.cod_moneda" )
 column=(type=number updatewhereclause=yes name=importe_cp dbname="importe_cp" )
 column=(type=char(10) updatewhereclause=yes name=nro_liquidacion dbname="nro_liquidacion" )
 column=(type=number updatewhereclause=yes name=importe_prestamo dbname="importe_prestamo" )
 retrieve="  select p.proveedor, p.nom_proveedor, p.nro_doc_ident,
         cp.cod_moneda, 
         sum(decode(cp.cod_moneda, (select cod_soles from logparam where reckey = '1'), cp.saldo_sol, cp.saldo_dol)) as importe_cp, 
			(select nro_liquid_campo
  			   from cam_liquidacion_pago clp
			  where clp.proveedor = p.proveedor
				 and clp.fec_pago	 = trunc(:ad_fec_pago)
				 and clp.flag_estado <> '0'
				 and rownum = 1) as nro_liquidacion,
         (select NVL(sum(usf_fl_conv_mon(ccd.monto_prestamo + ccd.monto_interes + ccd.monto_comisiones - NVL(ccd.monto_cancelado,0), cc.cod_moneda, cp.cod_moneda, cc.fec_prestamo)),0) 
            from cam_creditos cc, 
                 cam_creditos_det ccd 
           where cc.nro_credito = ccd.nro_credito 
             and ccd.proveedor  = p.proveedor 
				 and cc.FEC_PRESTAMO <= trunc(:ad_fec_pago)
             and ccd.monto_prestamo + ccd.monto_interes + ccd.monto_comisiones > nvl(ccd.monto_cancelado,0)) as importe_prestamo 
  from cntas_pagar cp, 
       proveedor   p 
  where cp.cod_relacion = p.proveedor 
    and cp.tipo_doc     = (select a.doc_lc from ap_param a where a.origen = 'XX') 
    and cp.flag_estado  <> '0' 
--    and cp.saldo_dol    > 0 
--    and cp.saldo_sol    > 0 
    and trunc(cp.fecha_emision) <= trunc(:ad_fec_pago)
group by p.proveedor, p.nom_proveedor, p.nro_doc_ident, cp.cod_moneda 
having (select NVL(sum(usf_fl_conv_mon(ccd.monto_prestamo + ccd.monto_interes + ccd.monto_comisiones, cc.cod_moneda, cp.cod_moneda, cc.fec_prestamo)),0) 
           from cam_creditos cc, 
                cam_creditos_det ccd 
          where cc.nro_credito = ccd.nro_credito 
            and ccd.proveedor  = p.proveedor 
				and cc.FEC_PRESTAMO <= trunc(:ad_fec_pago)
            /*and ccd.monto_prestamo + ccd.monto_interes + ccd.monto_comisiones > nvl(ccd.monto_cancelado,0)*/) > 0
order by p.proveedor

