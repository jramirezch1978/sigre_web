 retrieve="select cd.nro_parte, 
       cd.nro_item,
       cd.nro_lote, lc.descripcion as desc_lote,
       cd.total_plantas,
       cd.cod_usr,
       cd.nro_ciclo,
       cd.hora_inicio_riego,
       cd.hora_fin_riego,
		 (cd.hora_fin_riego - cd.hora_inicio_riego)*24 as hrs_riego,
       cd.fec_registro
from cam_parte_riego_det cd,
     lote_campo          lc
where cd.nro_lote = lc.nro_lote
  and cd.nro_parte = :as_nro_parte  
order by nro_item" update="cam_parte_riego_det" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string)) )
