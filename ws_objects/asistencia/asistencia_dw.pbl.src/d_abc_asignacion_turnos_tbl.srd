 retrieve="select m.cod_trabajador,
		 v.NOM_TRABAJADOR,
		 m.cod_origen,
       m.tipo_trabajador,
       tt.desc_tipo_tra as desc_tipo_trabajador,
       m.turno,
       t.descripcion as desc_turno
  from maestro m,
       vw_pr_Trabajador v,
       turno            t,
       tipo_trabajador  tt
 where m.cod_trabajador 	= v.COD_TRABAJADOR
   and m.tipo_trabajador  	= tt.tipo_trabajador
   and m.turno          	= t.turno           (+)
   and m.cod_origen			like :as_origen
   and m.tipo_trabajador 	like :as_tipo_trabaj
   and m.flag_estado			= '1'" update="maestro" updatewhere=0 updatekeyinplace=yes arguments=(("as_origen", string),("as_tipo_trabaj", string))  sort="tipo_trabajador A " )
