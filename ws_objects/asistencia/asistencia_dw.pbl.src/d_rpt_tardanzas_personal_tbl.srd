 column=(type=datetime updatewhereclause=yes name=fec_movimiento dbname="fec_movimiento" )
 column=(type=datetime updatewhereclause=yes name=fecha_marcacion dbname="fecha_marcacion" )
 column=(type=datetime updatewhereclause=yes name=fec_ingreso dbname="fec_ingreso" )
 column=(type=number updatewhereclause=yes name=min_tardanzas dbname="min_tardanzas" )
 retrieve="select a.codigo,
       m.NOM_TRABAJADOR,
       ar.cod_area,
       ar.desc_area,
       s.cod_seccion,
       s.desc_seccion,
       m.tipo_trabajador,
       tt.desc_tipo_tra,
       m.cod_origen,
       max(a.fec_movimiento) as fec_movimiento,
		 trunc(a.fec_movimiento) as fecha_marcacion,
       to_date(to_char(trunc(a.fec_movimiento), 'dd/mm/yyyy') || ' ' || to_char(tu.hora_inicio_norm + nvl(tu.tolerancia,0) / (24*60), 'hh:mi:ss'), 'dd/mm/yyyy hh:mi:ss') as fec_ingreso,
       (max(a.fec_movimiento) -  to_date(to_char(trunc(a.fec_movimiento), 'dd/mm/yyyy') || ' ' || to_char(tu.hora_inicio_norm + nvl(tu.tolerancia,0) / (24*60), 'hh:mi:ss'), 'dd/mm/yyyy hh:mi:ss')) * 24 * 60 as min_tardanzas                    
       
  from asistencia_ht580       a,
       turno                  tu,
       vw_pr_trabajador       m,
       origen                 o,
       tipo_trabajador        tt,
       area                   ar,
       seccion                s
 where a.turno            = tu.turno    
   and a.codigo           = m.cod_trabajador
   and m.cod_origen       = o.cod_origen
   and m.TIPO_TRABAJADOR  = tt.tipo_trabajador
   and m.COD_SECCION      = s.cod_seccion
   and s.cod_area         = ar.cod_area
   and (a.fec_movimiento -  to_date(to_char(a.fec_movimiento, 'dd/mm/yyyy') || ' ' || to_char(tu.hora_inicio_norm + nvl(tu.tolerancia,0) / (24*60), 'hh:mi:ss'), 'dd/mm/yyyy hh:mi:ss')) * 24 * 60 >= 0
   and trunc(a.fec_movimiento) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
   and a.flag_in_out      = '1'
   and m.cod_origen       Like :as_origen      
   and m.tipo_trabajador  Like :as_ttrabajador   
   and m.cod_trabajador   Like :as_cod_trabajador
group by  a.codigo,
			 m.NOM_TRABAJADOR,
			 ar.cod_area,
			 ar.desc_area,
			 s.cod_seccion,
			 s.desc_seccion,
			 m.tipo_trabajador,
			 tt.desc_tipo_tra,
			 m.cod_origen,
			 trunc(a.fec_movimiento),
       	 tu.hora_inicio_norm,
       	 tu.tolerancia
" arguments=(("as_origen", string),("as_ttrabajador", string),("as_cod_trabajador", string),("ad_fecha1", date),("ad_fecha2", date))  sort="desc_tipo_tra A fecha_marcacion A nom_trabajador A " )
