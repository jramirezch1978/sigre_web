 retrieve="SELECT M.COD_ORIGEN,
		 T.COD_TRABAJADOR,
       M.APEL_PATERNO||' '||M.APEL_MATERNO||', '||M.NOMBRE1||' '||M.NOMBRE2 AS TRABAJADOR,
       T.COD_TARJETA,
       T.FECHA_INICIO,
       T.FECHA_FIN,
       T.FLAG_ESTADO       
  FROM RRHH_ASIGNA_TRJT_RELOJ T,
       MAESTRO                M
 WHERE T.COD_TRABAJADOR = M.COD_TRABAJADOR
   AND M.COD_ORIGEN like :as_cod_origen
   AND M.COD_TRABAJADOR LIKE :as_cod_trabajador
   AND T.COD_TARJETA LIKE :as_cod_tarjeta
   AND M.FLAG_ESTADO <> '0'
   ORDER BY TRABAJADOR" arguments=(("as_cod_origen", string),("as_cod_trabajador", string),("as_cod_tarjeta", string)) )
