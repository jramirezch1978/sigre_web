table(column=(type=char(20) updatewhereclause=yes name=codigo dbname="asistencia_ht580.codigo" dbalias="a.codigo" )
 column=(type=char(100) updatewhereclause=yes name=desc_area dbname="vw_pr_trabajador.desc_area" dbalias="m.desc_area" )
 column=(type=char(30) updatewhereclause=yes name=desc_seccion dbname="vw_pr_trabajador.desc_seccion" dbalias="m.desc_seccion" )
 column=(type=char(2) updatewhereclause=yes name=flag_in_out dbname="flag_in_out" values="Ingreso a Planta	1/Salida de Planta	2/Salida a almorzar	3/Ingreso a Almorzar	4/Salida de comisión	5/Ingreso de comisión	6/Ingreso a producción	7/Salida de producción	8/Salida a Cenar	9/Ingreso de Cenar	10/" )
 column=(type=char(6) updatewhereclause=yes name=cod_usr dbname="asistencia_ht580.cod_usr" dbalias="a.cod_usr" )
 column=(type=char(4) updatewhereclause=yes name=turno dbname="asistencia_ht580.turno" dbalias="a.turno" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="fecha" )
 column=(type=char(200) updatewhereclause=yes name=nombre dbname="asistencia_ht580.nombre" dbalias=".nombre" )
 column=(type=char(3) updatewhereclause=yes name=tipo_trabajador dbname="asistencia_ht580.tipo_trabajador" dbalias=".tipo_trabajador" )
 column=(type=datetime updatewhereclause=yes name=hora_entrada dbname="hora_entrada" )
 column=(type=datetime updatewhereclause=yes name=hora_salida dbname="hora_salida" )
 column=(type=datetime updatewhereclause=yes name=hora_sal_almorzar dbname="hora_sal_almorzar" )
 column=(type=datetime updatewhereclause=yes name=hora_ing_almorzar dbname="hora_ing_almorzar" )
 retrieve="select 	a.codigo,
       	m.desc_area,
       	m.desc_seccion,
		trim(a.flag_in_out) as flag_in_out,
		a.cod_usr, 
		a.turno,
       	trunc(a.fec_movimiento) as fecha,
       	case 
			when m.cod_trabajador is not null then m.NOM_TRABAJADOR
         	when m2.cod_trabajador is not null then m2.NOM_TRABAJADOR
            when p.proveedor is not null then p.nom_proveedor
       	end as nombre,
       	case 
			when m.cod_trabajador is not null then m.tipo_trabajador
            when m2.cod_trabajador is not null then m2.tipo_trabajador
       	end as tipo_trabajador,
       	MAX(case when a.flag_in_out in ('1', '4', '6', '7', '10') then a.fec_marcacion else null end) as hora_entrada,
       	MAX(case when a.flag_in_out in ('2', '3', '5', '8', '9', '10') then a.fec_marcacion else null end) as hora_salida,
		MAX(case when a.flag_in_out in ('3') then a.fec_marcacion else null end) as hora_sal_almorzar,
       	MAX(case when a.flag_in_out in ('4') then a.fec_marcacion else null end) as hora_ing_almorzar
     
from asistencia_ht580 a,
     vw_pr_trabajador m,
     vw_pr_trabajador m2,
     proveedor        p
where a.codigo  = m.cod_trabajador (+)     
  and a.codigo  = m2.nro_doc_ident_rtps (+)
  and a.codigo  = p.nro_doc_ident (+)
  and trunc(a.fec_movimiento) between :ad_fecha1 and :ad_fecha2
  and a.cod_origen = :as_origen
group by  a.codigo,
			 m.desc_area,
			 m.desc_seccion,
			 trim(a.flag_in_out),
			 a.cod_usr, 
			 a.turno,
			 trunc(a.fec_movimiento),
			 case when m.cod_trabajador is not null then m.NOM_TRABAJADOR
					when m2.cod_trabajador is not null then m2.NOM_TRABAJADOR
					when p.proveedor is not null then p.nom_proveedor
			 end,
			 case when m.cod_trabajador is not null then m.tipo_trabajador
					when m2.cod_trabajador is not null then m2.tipo_trabajador
			 end
order by a.codigo, fecha" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_origen", string))  sort="tipo_trabajador A nombre A fecha A " )
