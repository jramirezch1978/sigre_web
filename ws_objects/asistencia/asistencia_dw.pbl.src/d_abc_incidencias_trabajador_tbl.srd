 retrieve="SELECT  asistencia.cod_trabajador,
           asistencia.fec_movim,
           asistencia.cod_tipo_mov,
           asistencia.fec_desde,
           asistencia.fec_hasta,
           asistencia.cod_usr              ,
           asistencia.flag_replicacion     ,
           asistencia.cod_origen           ,
           asistencia.ot_adm               ,
           asistencia.turno                ,
			  maestro.cod_origen              ,
			  maestro.tipo_trabajador         ,
			  origen.nombre			          ,
			  tipo_trabajador.desc_tipo_tra   ,
			  tipo_mov_asistencia.desc_movimi ,
			  Nvl(maestro.apel_paterno,' ')||' '||Nvl(maestro.apel_materno,' ')||' '||Nvl(maestro.nombre1,' ')||' '||Nvl(maestro.nombre2,' ') as Nombres
        FROM asistencia ,
             maestro    ,
				 origen		,
				 tipo_trabajador,
				 tipo_mov_asistencia
				
        WHERE ( maestro.cod_trabajador      = asistencia.cod_trabajador        )  and   
				  ( maestro.cod_origen		     = origen.cod_origen			         )  and
				  ( maestro.tipo_trabajador     = tipo_trabajador.tipo_trabajador  )  and
				  ( asistencia.cod_tipo_mov     = tipo_mov_asistencia.cod_tipo_mov )  and	
			    (( maestro.cod_origen          like :as_cod_origen              )  and    
      		  ( maestro.tipo_trabajador     like :as_tip_trabajador          )  and  
        		  ( asistencia.cod_trabajador   like :as_cod_trabajador          )) and
				  ( trunc(asistencia.fec_movim) between :ad_fecha_inicio and :ad_fecha_final )" update="asistencia" updatewhere=0 updatekeyinplace=no arguments=(("as_cod_origen", string),("as_tip_trabajador", string),("as_cod_trabajador", string),("ad_fecha_inicio", date),("ad_fecha_final", date))  sort="cod_origen A tipo_trabajador A cod_trabajador A fec_desde A " )
