table(column=(type=char(20) updatewhereclause=yes name=dni dbname="vw_pr_trabajador.dni" dbalias="m.dni" )
 column=(type=char(164) updatewhereclause=yes name=nombre dbname="vw_pr_trabajador.nombre" dbalias="m.nombre" )
 column=(type=char(100) updatewhereclause=yes name=desc_area dbname="vw_pr_trabajador.desc_area" dbalias="m.desc_area" )
 column=(type=char(100) updatewhereclause=yes name=desc_seccion dbname="vw_pr_trabajador.desc_seccion" dbalias="m.desc_seccion" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="fecha" )
 column=(type=datetime updatewhereclause=yes name=hora_entrada dbname="hora_entrada" )
 column=(type=datetime updatewhereclause=yes name=hora_salida dbname="hora_salida" )
 column=(type=number updatewhereclause=yes name=hrs_extras dbname="hrs_extras" )
 retrieve="select m.NRO_DOC_IDENT_RTPS as dni,
       m.NOM_TRABAJADOR as nombre,
       m.desc_area as desc_area,
       m.desc_Seccion as desc_Seccion,
       trunc(a.fec_parte) as fecha,
       min(b.hora_inicio) as hora_entrada,
       max(b.hora_fin) as hora_salida,
       case
          when (max(b.hora_fin) - min(b.hora_inicio)) * 24 > 8 then
            trunc((max(b.hora_fin) - min(b.hora_inicio)) * 24 - 8)
          else
             0
       end as hrs_extras
  from tg_pd_destajo a,
       tg_pd_destajo_det b,
       vw_pr_trabajador  m
where a.nro_parte = b.nro_parte
  and b.cod_trabajador = m.COD_TRABAJADOR
  and a.flag_estado <> '0'
  and trunc(a.fec_parte) 	between trunc(:ad_fecha1) and trunc(:ad_Fecha2)
  and m.cod_origen        	like :as_origen
  and m.tipo_trabajador 	like :as_ttrab
  and m.cod_trabajador		like :as_trabajador
group by m.NRO_DOC_IDENT_RTPS,
       m.NOM_TRABAJADOR,
       m.desc_area,
       m.desc_Seccion,
       trunc(a.fec_parte)
order by nom_trabajador, trunc(fec_parte)  

