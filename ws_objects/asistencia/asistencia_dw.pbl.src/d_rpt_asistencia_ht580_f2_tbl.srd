 retrieve="WITH marcaciones_base AS (
    -- Obtener todas las marcaciones con información del trabajador
    SELECT 
        a.codigo,
        COALESCE(
            m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1,
            m2.apel_paterno || ' ' || m2.apel_materno || ', ' || m2.nombre1,
            p.nom_proveedor
        ) AS nombre,
        COALESCE(m.tipo_trabajador, m2.tipo_trabajador) AS tipo_trabajador,
        COALESCE(m.cod_trabajador, m2.cod_trabajador) AS cod_trabajador,
        COALESCE(m.nro_doc_ident_rtps, m2.nro_doc_ident_rtps, a.codigo) AS nro_doc_ident_rtps,
        COALESCE(m.cod_cargo, m2.cod_cargo) AS cod_cargo,
        COALESCE(m.cod_area, m2.cod_area) AS cod_area,
        COALESCE(m.cod_seccion, m2.cod_seccion) AS cod_seccion,
        a.cod_usr,
        a.fec_movimiento,
		a.fec_marcacion,
        a.flag_in_out
    FROM asistencia_ht580 a
    	LEFT JOIN maestro m  ON a.codigo = m.cod_trabajador
    	LEFT JOIN maestro m2 ON a.codigo = m2.nro_doc_ident_rtps
    	LEFT JOIN proveedor p ON a.codigo = p.nro_doc_ident
    WHERE TRUNC(a.fec_movimiento) BETWEEN trunc(:ad_fecha1)
                                      AND trunc(:ad_fecha2)
      AND a.cod_origen = :as_origen
),
movimientos_agrupados AS (
    SELECT 
        codigo,
        nombre,
        tipo_trabajador,
        cod_trabajador,
        nro_doc_ident_rtps,
        cod_cargo,
        cod_area,
        cod_seccion,
        cod_usr,
        fec_movimiento,

        -- PLANTA (1→2): Tomar PRIMER ingreso y ÚLTIMA salida
        MIN(CASE WHEN flag_in_out = '1' THEN fec_marcacion END) AS ingreso_planta,
        MAX(CASE WHEN flag_in_out = '2' THEN fec_marcacion END) AS salida_planta,
        
        -- ALMUERZO (3→4): Tomar PRIMER salida y PRIMERA vuelta
        MIN(CASE WHEN flag_in_out = '3' THEN fec_marcacion END) AS salida_almuerzo,
        MIN(CASE WHEN flag_in_out = '4' THEN fec_marcacion END) AS regreso_almuerzo,
        
        -- COMISIÓN (5→6): Tomar PRIMER salida y PRIMERA vuelta
        MIN(CASE WHEN flag_in_out = '5' THEN fec_marcacion END) AS salida_comision,
        MIN(CASE WHEN flag_in_out = '6' THEN fec_marcacion END) AS regreso_comision,
        
        -- PRODUCCIÓN (7→8): Tomar PRIMER ingreso y ÚLTIMA salida
        MIN(CASE WHEN flag_in_out = '7' THEN fec_marcacion END) AS ingreso_produccion,
        MAX(CASE WHEN flag_in_out = '8' THEN fec_marcacion END) AS salida_produccion,
        
        -- CENA (9→10): Tomar PRIMER salida y PRIMERA vuelta
        MIN(CASE WHEN flag_in_out = '9' THEN fec_marcacion END) AS salida_cena,
        MIN(CASE WHEN flag_in_out = '10' THEN fec_marcacion END) AS regreso_cena
        
    FROM marcaciones_base
    GROUP BY 
        codigo, nombre, tipo_trabajador, cod_trabajador, nro_doc_ident_rtps,
        cod_cargo, cod_area, cod_seccion, cod_usr, fec_movimiento
)
SELECT 
    ma.nro_doc_ident_rtps,
    ma.nombre AS nom_trabajador,
    c.desc_cargo,
    s.desc_seccion,
    ma.tipo_trabajador,
    ma.fec_movimiento AS fec_movimiento,
    ma.cod_usr,
    ma.codigo,
    
    -- === PLANTA ===
    ma.ingreso_planta,
    ma.salida_planta,
    CASE 
        WHEN ma.salida_planta IS NOT NULL AND ma.ingreso_planta IS NOT NULL THEN
            ROUND((ma.salida_planta - ma.ingreso_planta) * 24, 2)
    END AS horas_planta,
    
    -- === ALMUERZO ===
    ma.salida_almuerzo,
    ma.regreso_almuerzo,
    CASE 
        WHEN ma.regreso_almuerzo IS NOT NULL AND ma.salida_almuerzo IS NOT NULL THEN
            ROUND((ma.regreso_almuerzo - ma.salida_almuerzo) * 60, 0)
    END AS minutos_almuerzo,
    
    -- === CENA ===
    ma.salida_cena,
    ma.regreso_cena,
    CASE 
        WHEN ma.regreso_cena IS NOT NULL AND ma.salida_cena IS NOT NULL THEN
            ROUND((ma.regreso_cena - ma.salida_cena) * 60, 0)
    END AS minutos_cena,
    
    -- === COMISIÓN ===
    ma.salida_comision,
    ma.regreso_comision,
    CASE 
        WHEN ma.regreso_comision IS NOT NULL AND ma.salida_comision IS NOT NULL THEN
            ROUND((ma.regreso_comision - ma.salida_comision) * 24, 2)
    END AS horas_comision,
    
    -- === PRODUCCIÓN ===
    ma.ingreso_produccion,
    ma.salida_produccion,
    CASE 
        WHEN ma.salida_produccion IS NOT NULL AND ma.ingreso_produccion IS NOT NULL THEN
            ROUND((ma.salida_produccion - ma.ingreso_produccion) * 24, 2)
    END AS horas_produccion,
    
    -- === HORAS NETAS TRABAJADAS ===
    CASE 
        WHEN ma.salida_planta IS NOT NULL AND ma.ingreso_planta IS NOT NULL THEN
            ROUND(
                (ma.salida_planta - ma.ingreso_planta) * 24
                - NVL((ma.regreso_almuerzo - ma.salida_almuerzo) * 24, 0)
                - NVL((ma.regreso_cena - ma.salida_cena) * 24, 0)
                - NVL((ma.regreso_comision - ma.salida_comision) * 24, 0)
            , 2)
    END AS horas_netas_trabajadas
    
FROM movimientos_agrupados ma
	LEFT JOIN cargo c    ON ma.cod_cargo = c.cod_cargo
	LEFT JOIN seccion s  ON ma.cod_area = s.cod_area AND ma.cod_seccion = s.cod_seccion
ORDER BY ma.fec_movimiento, ma.nombre" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_origen", string)) )
