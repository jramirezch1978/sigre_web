 retrieve="SELECT a.cod_trabajador , 
       m.apel_paterno||' '||m.apel_materno||', '||m.nombre1||' '||m.nombre2 AS trabajador,
       a.fec_desde		,
		 a.fec_hasta		,
       usf_rrhh_cadena_tiempo(Replace(rta.hora_entrada,'.',':')) ||' - '||
		 usf_rrhh_cadena_tiempo(Replace(rta.hora_salida,'.',':')) as turno, 
       a.cod_tipo_mov	, 
       tm.desc_movimi	, 
       a.cod_usr			, 
		 a.ot_adm			,
		 m.cod_origen		,
		 o.nombre			,
		 m.tipo_trabajador,
		 tt.desc_tipo_tra ,
		 m.cod_area			,
		 a.desc_area 		,	
		 m.cod_seccion		,
		 s.desc_seccion	,
		 decode(a.cod_tipo_mov,'11',((a.fec_desde -  to_date(to_char(a.fec_desde,'dd/mm/yyyy')||' '||usf_rrhh_cadena_tiempo(Replace(to_char(rta.hora_tolerancia_tardanza),'.',':')),'dd/mm/yyyy hh24:mi:ss') ) * 1440) ,0) min_tardanzas,
	     ((a.fec_hasta  -  to_date(to_char(a.fec_desde,'dd/mm/yyyy')||' '||usf_rrhh_cadena_tiempo(Replace(to_char(rta.hora_salida),'.',':')),'dd/mm/yyyy hh24:mi:ss') ) * 1440)  min_sobretiempo,
		 to_date(to_char(a.fec_desde,'dd/mm/yyyy')||' '||usf_rrhh_cadena_tiempo(Replace(to_char(rta.hora_salida),'.',':')),'dd/mm/yyyy hh24:mi:ss') as hora_salida

  FROM asistencia a,tipo_mov_asistencia   tm ,
       maestro    m,rrhh_turno_asistencia rta,
		 origen		o,tipo_trabajador			tt	,
		 area			a,seccion					s
 WHERE (a.cod_tipo_mov       = tm.cod_tipo_mov		 )
   AND (m.cod_trabajador     = a.cod_trabajador		 )
	AND (m.cod_origen			  = o.cod_origen			 )	
	AND (m.tipo_trabajador    = tt.tipo_trabajador   )	
	
	 AND	  (m.cod_area			= a.cod_area			)  
	AND	 ((m.cod_area			= s.cod_area		   )  
	AND	  (m.cod_seccion		= s.cod_seccion		)) 

   AND (rta.turno            = a.turno					 )
   AND (trunc(a.fec_desde) Between trunc(:ad_fecha1) AND trunc(:ad_fecha2))
	AND (m.cod_origen			 like :as_cod_origen     )
	AND (m.tipo_trabajador	 like :as_tiptrab			 )
   AND (m.cod_area          like :as_cod_area 		 )	
   AND (m.cod_seccion       like :as_cod_seccion 	 )	
   AND (a.cod_trabajador    like :as_cod_trabajador )	
   AND (a.cod_tipo_mov		 like :as_tipo_m			 )" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_cod_origen", string),("as_tiptrab", string),("as_cod_area", string),("as_cod_seccion", string),("as_cod_trabajador", string),("as_tipo_m", string))  sort="cod_origen A tipo_trabajador A cod_area A cod_seccion A asistencia_cod_trabajador A asistencia_fec_desde A " )
