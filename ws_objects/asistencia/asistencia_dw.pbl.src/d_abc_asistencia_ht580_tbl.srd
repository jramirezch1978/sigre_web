 column=(type=datetime updatewhereclause=yes name=hora_entrada dbname="hora_entrada" )
 column=(type=datetime updatewhereclause=yes name=hora_salida dbname="hora_salida" )
 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="vw_pr_trabajador.flag_estado" dbalias="m.flag_estado" )
 retrieve="select a.reckey,
       a.cod_origen,
       a.codigo,
       a.flag_in_out,
       a.fec_registro,
       a.fec_movimiento,
       a.cod_usr,
       a.direccion_ip,
       a.flag_verify_type,
       a.turno, 
		 a.lectura_pda,
		 to_char(a.fec_movimiento, 'HH24') as hora,
		 case when m.cod_trabajador is not null then m.nom_trabajador
            when m2.cod_trabajador is not null then m2.nom_trabajador
            when p.proveedor is not null then p.nom_proveedor
       end as nombre,
       case when m.cod_trabajador is not null then m.tipo_trabajador
            when m2.cod_trabajador is not null then m2.tipo_trabajador
       end as tipo_trabajador,
       case when m.cod_trabajador is not null then m.nro_doc_ident_rtps
            when m2.cod_trabajador is not null then m2.nro_doc_ident_rtps
       end as dni,
       decode(a.flag_in_out, '1', a.fec_movimiento) as hora_entrada,
       decode(a.flag_in_out, '2', a.fec_movimiento) as hora_salida,
		 m.flag_estado
		 
from asistencia_ht580 	a,
     vw_pr_trabajador   m,
     vw_pr_trabajador   m2,
     proveedor        	p
where a.codigo  = m.cod_trabajador 			(+)     
  and a.codigo  = m2.nro_doc_ident_rtps 	(+)
  and a.codigo  = p.nro_doc_ident 			(+)
  and trunc(a.fec_movimiento) between :ad_fecha1 and :ad_fecha2
  and (m.cod_area 	like :as_area 		or m.cod_area is null)
  and (m.cod_seccion like :as_seccion 	or m.cod_seccion is null)
  and a.codigo like :as_trabajador
order by a.codigo, a.fec_movimiento

" update="asistencia_ht580" updatewhere=0 updatekeyinplace=yes arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_area", string),("as_seccion", string),("as_trabajador", string))  sort="nombre A fec_movimiento A " )
