       m.tipo_doc_ident_rtps, 
       m.nro_doc_ident_rtps, 
       m.NOM_TRABAJADOR,
       c.desc_cargo,
       ts.desc_sangre,
       m.FEC_INGRESO,
       m.TIPO_TRABAJADOR, t.desc_tipo_tra,
       m.COD_AREA, m.desc_area, m.cod_seccion,m.desc_seccion
       
from vw_pr_trabajador        m,
     rrhh_documento_identidad_rtps t,
     tt_proveedor      tt,
     cargo             c,
     TIPO_SANGRE       TS,
     tipo_trabajador   t
where m.tipo_doc_ident_rtps = t.cod_doc_identidad (+)
  and m.cod_cargo           = c.cod_cargo         (+)
  AND m.COD_TIPO_SANGRE     = ts.cod_tipo_sangre  (+)
  and m.TIPO_TRABAJADOR     = t.tipo_trabajador   (+)
  and m.cod_trabajador = tt.proveedor
  and m.flag_estado = '1'
--  and m.foto_trabaj is not null
order by nom_trabajador" )
