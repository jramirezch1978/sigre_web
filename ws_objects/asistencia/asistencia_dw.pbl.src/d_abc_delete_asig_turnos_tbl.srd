 retrieve="SELECT rat.cod_trabajador ,
       rat.fecha          ,
       rat.turno          ,
       rta.hora_entrada   ,
       rta.hora_salida    ,
       rta.flag_dis_siguiente_normal ,       
       Nvl(m.apel_paterno,' ') ||' '|| Nvl(m.apel_materno,' ') ||' '|| Nvl(m.nombre1,' ') ||' '|| Nvl(m.nombre2,' ') as nombres,   
       m.cod_origen       ,
       o.nombre           ,  
       m.tipo_trabajador  ,
       tt.desc_tipo_tra   ,
       m.cod_area         ,
       a.desc_area    	  ,
		 '0' as flag_sel
 FROM rrhh_asignacion_turnos rat,   
      rrhh_turno_asistencia  rta,
      maestro                m  ,
      origen                 o  ,
      tipo_trabajador        tt ,
      area                   a
WHERE  (rat.turno         = rta.turno          ) and
       (m.cod_trabajador  = rat.cod_trabajador ) and  
       (m.cod_origen      = o.cod_origen       ) and
       (m.tipo_trabajador = tt.tipo_trabajador ) and
       (m.cod_area        = a.cod_area         ) and
		 (m.cod_origen      like :as_origen      ) and
		 (m.tipo_trabajador like :as_ttrab		  ) and
		 (m.cod_trabajador  like :as_codigo		  ) and
      ((rat.fecha         between :ad_fecha1 and :ad_fecha2 ) )    
" update="rrhh_asignacion_turnos" updatewhere=1 updatekeyinplace=yes arguments=(("as_origen", string),("as_ttrab", string),("as_codigo", string),("ad_fecha1", date),("ad_fecha2", date))  sort="cod_origen A tipo_trabajador A cod_area A fecha A " )
