FROM cotizacion_det td, cotizacion t, clientes c, contactos_clientes cc, direccion_clientes dc, articulos ar
where td.cotizacion_id=t.cotizacion_id and trim(t.cliente_id)=trim(c.cliente_id)  and t.contacto_id=cc.contacto_clien_id and t.direccion_id=dc.direccion_clien_id and trim(td.COD_ARTICULO)= trim(ar.COD_ARTICULO)
and td.cotizacion_id=:ai_cotizacion_id" arguments=(("ai_cotizacion_id", number)) )
