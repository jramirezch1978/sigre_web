 column=(type=char(260) updatewhereclause=yes name=contacto dbname="seguimientos.contacto" )
 column=(type=char(2000) updatewhereclause=yes name=seguimientos_conclusiones dbname="seguimientos.conclusiones" )
 column=(type=char(150) updatewhereclause=yes name=seguimientos_cargo dbname="seguimientos.cargo" )
 column=(type=char(50) updatewhereclause=yes name=cod_usr dbname="usuarios.cod_usr" )
 column=(type=char(250) updatewhereclause=yes name=clientes_nombre dbname="clientes.nombre" )
 column=(type=char(20) updatewhereclause=yes name=clientes_telef_1 dbname="clientes.telef_1" )
 column=(type=char(50) updatewhereclause=yes name=email dbname="clientes.email" )
 retrieve="select s.*,
		u.cod_usr,
		cl.nombre,
		cl.telef_1,
		cl.email
from  seguimientos s,
		usuarios u,
		clientes cl
where  s.usuario_id = u.usuario_id
	and s.cliente_id = cl.cliente_id
and s.cliente_id=:as_cliente_id" arguments=(("as_cliente_id", string)) )
