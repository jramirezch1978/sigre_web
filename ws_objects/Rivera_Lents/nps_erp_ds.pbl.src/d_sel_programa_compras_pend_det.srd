 retrieve="SELECT  PCD.NRO_ITEM ,    
        PCD.NRO_PROGRAMA ,
        PCD.COD_ART ,
        A.DESC_ART ,
        A.UND ,
        PCD.FEC_REQUERIDA ,
		  PCD.COMPRADOR ,
		  PCD.FLAG_ESTADO ,
		  PCD.FLAG_PRIORIDAD ,
		  PCD.ALMACEN,
	  	  NVL(PCD.cant_proyect,0) - NVL(cant_procesada,0) as cantidad,
		  PCD.ORG_AMP_OT_REF,
		  PCD.NRO_AMP_OT_REF,
		  PC.COD_ORIGEN,
		  A2.COD_SUB_CAT,
		  A2.DESC_SUB_CAT,
		  usf_cmp_oper_sec_prog(PCD.NRO_PROGRAMA, PCD.NRO_ITEM) as oper_sec,
		  usf_alm_saldo_total_alm(PCD.cod_art, PCD.almacen) as saldo_almacen,
		  usf_cmp_ot_adm(PCD.ORG_AMP_OT_REF, PCD.NRO_AMP_OT_REF) as OT_ADM,
		  USF_ALM_CANT_LIBRE_OT(PCD.ORG_AMP_OT_REF, PCD.NRO_AMP_OT_REF) as cant_requerida
   FROM PROG_COMPRAS_DET 	PCD,
		  PROG_COMPRAS 		PC,
        ARTICULO     		A,
		  articulo_sub_categ A2
  WHERE PCD.COD_ART 		 = A.COD_ART 
	 AND A2.COD_SUB_CAT   = A.SUB_CAT_ART
    AND PC.NRO_PROGRAMA  = PCD.NRO_PROGRAMA
    and PCD.NRO_PROGRAMA = :as_nro_programa 
	 and PCD.FLAG_ESTADO  = '1' 
	 and TRUNC(PCD.FEC_REQUERIDA) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
	 and PCD.COD_ART 		  LIKE :as_cod_art
	 and upper(A.DESC_ART) like :as_desc_art
    and PCD.ALMACEN       LIKE :as_almacen
    and A2.COD_SUB_CAT	  LIKE :as_cod_sub_cat
	 AND NVL(PCD.cant_proyect,0) - NVL(cant_procesada,0) > 0
 	 and ( PCD.NRO_AMP_OT_REF is null or 
			( PCD.NRO_AMP_OT_REF is not null and 
				USF_ALM_CANT_LIBRE_OT(PCD.ORG_AMP_OT_REF, PCD.NRO_AMP_OT_REF) > 0 ) )
  " arguments=(("as_nro_programa", string),("ad_fecha1", date),("ad_fecha2", date),("as_cod_art", string),("as_desc_art", string),("as_almacen", string),("as_cod_sub_cat", string)) )
