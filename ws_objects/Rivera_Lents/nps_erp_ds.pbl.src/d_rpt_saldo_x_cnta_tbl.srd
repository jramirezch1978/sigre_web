 retrieve="select cd.cnta_ctbl, c.desc_cnta, cd.cod_relacion, p.nom_proveedor, cd.tipo_docref1, cd.nro_docref1,
	    sum(decode(cd.flag_debhab,'D',cd.imp_movsol *-1,0) + decode(cd.flag_debhab,'H',cd.imp_movsol,0)) as Saldo_soles,
	    sum(decode(cd.flag_debhab,'D',cd.imp_movdol * -1,0) + decode(cd.flag_debhab,'H',cd.imp_movdol,0)) as Saldo_dolares
  from cntbl_asiento_det cd, 
		 proveedor p, 
		 cntbl_cnta c
 where cd.cnta_ctbl = c.cnta_ctbl
   and cd.cod_relacion = p.proveedor(+)
   and cd.ano= :an_year
   and mes between :an_mes1 and :an_mes2
	and substr(cd.cnta_ctbl,1,8)= :as_cntbl_cnta
group by cd.cnta_ctbl, c.desc_cnta, cd.cod_relacion, p.nom_proveedor, cd.tipo_docref1, cd.nro_docref1
having sum(decode(cd.flag_debhab,'D',cd.imp_movsol *-1,0) + decode(cd.flag_debhab,'H',cd.imp_movsol,0)) <> 0 or
	    sum(decode(cd.flag_debhab,'D',cd.imp_movdol * -1,0) + decode(cd.flag_debhab,'H',cd.imp_movdol,0)) <> 0" arguments=(("as_cntbl_cnta", string),("an_year", number),("an_mes1", number),("an_mes2", number)) )
