 retrieve="  SELECT ~"ORDEN_VENTA~".~"COD_ORIGEN~",   
         ~"ORDEN_VENTA~".~"NRO_OV~",   
         ~"ORDEN_VENTA~".~"FLAG_ESTADO~",   
         ~"ORDEN_VENTA~".~"FEC_REGISTRO~",   
         ~"ORDEN_VENTA~".~"MONTO_TOTAL~",   
         ~"ORDEN_VENTA~".~"COD_MONEDA~"  
    FROM ~"ORDEN_VENTA~"  
   WHERE ( ~"ORDEN_VENTA~".~"FLAG_ESTADO~" in ('1','2','3','4','5') ) AND  
         ( ~"ORDEN_VENTA~".~"CLIENTE~" = :as_cod_relacion )    
