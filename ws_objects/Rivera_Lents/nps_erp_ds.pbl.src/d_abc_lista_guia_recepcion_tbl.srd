 retrieve="SELECT DISTINCT A.ORIGEN,
       A.COD_GUIA_REC,
       A.FLAG_ESTADO,
       A.FECHA_REGISTRO,
       A.MONTO_TOT,
       A.COD_MONEDA,
		 A.COD_USR

FROM   AP_GUIA_RECEPCION 		A,
		 AP_GUIA_RECEPCION_DET  GD

WHERE  A.COD_GUIA_REC = GD.COD_GUIA_REC
 AND   A.flag_Estado <> '0'
 AND   A.PROVEEDOR = :AS_PROVEEDOR
 AND   NVL(GD.MONTO_FACTURADO,0) < ROUND( GD.PESO_VENTA * (NVL(GD.PRECIO_UNITARIO,0) +
			 NVL(GD.PRECIO_UNITARIO_CASTIGO,0) + NVL(GD.PRECIO_REPRESENTACION,0) + 
			 NVL(GD.PRECIO_AJUSTE_CNTBL,0)), 2)
" arguments=(("AS_PROVEEDOR", string)) )
