 retrieve="  SELECT ~"ARTICULO_MOV_PROY~".~"COD_ART~",   
         ~"ARTICULO~".~"NOM_ARTICULO~",   
         ~"ARTICULO_MOV_PROY~".~"CANT_PROCESADA~",   
         ~"ARTICULO_MOV_PROY~".~"CANT_FACTURADA~",   
         ~"ARTICULO_MOV_PROY~".~"DECUENTO~",  
         ~"ARTICULO_MOV_PROY~".~"IMPUESTO~",   
         ~"ARTICULO_MOV_PROY~".~"COD_MONEDA~",   
         ~"ARTICULO_MOV_PROY~".~"PRECIO_UNIT~"  ,
         ~"CONCEPTO_FINANCIERO~".~"CONFIN~",   
         ~"CONCEPTO_FINANCIERO~".~"MATRIZ_CNTBL~",   
         Nvl( ~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~",0) - Nvl(~"ARTICULO_MOV_PROY~".~"CANT_FACTURADA~",0) AS CANT_PENDIENTE   ,
			DECODE(~"ARTICULO_VENTA~".~"FLAG_DL27400~",'1',(SELECT tipo_cred_fiscal_dl27400 FROM comparam WHERE reckey = '1'),NULL) AS DL27400,
			~"ARTICULO~".~"COD_CLASE~",

			USF_COM_CENTRO_BENEFICIO_ART(almacen.cod_origen,articulo_mov_proy.cod_art) as centro_benef
    FROM ~"ARTICULO_MOV_PROY~",   
         ~"ARTICULO~",   
         ~"ARTICULO_VENTA~",   
         ~"CONCEPTO_FINANCIERO~",   
         ~"LOGPARAM~"  ,
			~"ALMACEN~"
   WHERE ( articulo.cod_art                   = articulo_venta.cod_art        (+)) and  
         ( articulo_venta.confin              = concepto_financiero.confin    (+)) and  
			( articulo_mov_proy.almacen          = almacen.almacen 					(+)) and
         ( ~"ARTICULO_MOV_PROY~".~"COD_ART~"      = ~"ARTICULO~".~"COD_ART~"             ) and  
         ( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~"     = ~"LOGPARAM~".~"DOC_OV~"              ) and  
         ( ( ~"ARTICULO_MOV_PROY~".~"COD_ORIGEN~" = :as_cod_origen 		            ) and
         ( ~"ARTICULO_MOV_PROY~".~"NRO_DOC~" 		 = :as_nro_doc 				         ) and
         ( Nvl( ~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~",0) - Nvl(~"ARTICULO_MOV_PROY~".~"CANT_FACTURADA~",0) > 0 ) )    


