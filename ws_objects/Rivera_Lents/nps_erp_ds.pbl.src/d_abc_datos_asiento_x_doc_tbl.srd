 retrieve="SELECT ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~"	 ,   
       DECODE(~"CNTBL_ASIENTO_DET~".~"FLAG_DEBHAB~",'D','H','D') AS FLAG_DEBHAB,
       ~"CNTBL_ASIENTO_DET~".~"CENCOS~"		 ,
		 ~"CNTBL_ASIENTO_DET~".~"COD_CTABCO~" ,
		 ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF2~",
		 ~"CNTBL_ASIENTO_DET~".~"DET_GLOSA~"  ,       
		 ~"CNTAS_COBRAR~".~"TIPO_DOC~"		    ,
		 ~"CNTAS_COBRAR~".~"NRO_DOC~" 		    , 
		 ~"CNTAS_COBRAR~".~"COD_RELACION~"    , 
		 ~"CNTAS_COBRAR~".~"TASA_CAMBIO~"	    ,
       '1'                 AS FLAB_TABOR  
  FROM ~"CNTBL_ASIENTO_DET~",   
       ~"CNTAS_COBRAR~"  
 WHERE  ( ~"CNTAS_COBRAR~".~"ORIGEN~"        = ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"        ) AND  
		  ( ~"CNTAS_COBRAR~".~"ANO~"           = ~"CNTBL_ASIENTO_DET~".~"ANO~"           ) AND  
		  ( ~"CNTAS_COBRAR~".~"MES~"	          = ~"CNTBL_ASIENTO_DET~".~"MES~"	       ) AND  
        ( ~"CNTAS_COBRAR~".~"NRO_LIBRO~" 	 = ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"      ) AND  
        ( ~"CNTAS_COBRAR~".~"NRO_ASIENTO~"   = ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~"   ) AND  
        ( ~"CNTAS_COBRAR~".~"TIPO_DOC~" 		 = ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~") AND  
        ( ~"CNTAS_COBRAR~".~"NRO_DOC~" 		 = ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"    ) AND  
        ( ~"CNTAS_COBRAR~".~"COD_RELACION~"  = ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~"  ) AND  
       (( ~"CNTAS_COBRAR~".~"COD_RELACION~" = :as_cod_relacion                     ) AND  
        ( ~"CNTAS_COBRAR~".~"TIPO_DOC~"     = :as_tipo_doc                         ) AND  
        ( ~"CNTAS_COBRAR~".~"NRO_DOC~"      = :as_nro_doc                          ))    
UNION        
SELECT ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~"	,   
       DECODE(~"CNTBL_ASIENTO_DET~".~"FLAG_DEBHAB~",'D','H','D') AS FLAG_DEBHAB,
       ~"CNTBL_ASIENTO_DET~".~"CENCOS~"		 ,
		 ~"CNTBL_ASIENTO_DET~".~"COD_CTABCO~" ,
		 ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF2~",
		 ~"CNTBL_ASIENTO_DET~".~"DET_GLOSA~"  ,       
		 ~"CNTAS_PAGAR~".~"TIPO_DOC~"		    ,
		 ~"CNTAS_PAGAR~".~"NRO_DOC~" 		    , 
		 ~"CNTAS_PAGAR~".~"COD_RELACION~"     , 
		 ~"CNTAS_PAGAR~".~"TASA_CAMBIO~"	    ,
       '3'                AS FLAB_TABOR  
  FROM ~"CNTBL_ASIENTO_DET~",   
       ~"CNTAS_PAGAR~"  
 WHERE  ( ~"CNTAS_PAGAR~".~"ORIGEN~"       = ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"       ) AND  
		  ( ~"CNTAS_PAGAR~".~"ANO~"          = ~"CNTBL_ASIENTO_DET~".~"ANO~"          ) AND  
		  ( ~"CNTAS_PAGAR~".~"MES~"	         = ~"CNTBL_ASIENTO_DET~".~"MES~"	       ) AND  
        ( ~"CNTAS_PAGAR~".~"NRO_LIBRO~" 	= ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"    ) AND  
        ( ~"CNTAS_PAGAR~".~"NRO_ASIENTO~"  = ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~"  ) AND  
        ( ~"CNTAS_PAGAR~".~"TIPO_DOC~" 		= ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~" ) AND  
        ( ~"CNTAS_PAGAR~".~"NRO_DOC~" 		= ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"  ) AND  
        ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~" ) AND  
       (( ~"CNTAS_PAGAR~".~"COD_RELACION~" = :as_cod_relacion                   ) AND  
        ( ~"CNTAS_PAGAR~".~"TIPO_DOC~"     = :as_tipo_doc                       ) AND  
        ( ~"CNTAS_PAGAR~".~"NRO_DOC~"      = :as_nro_doc                        ))    
        " arguments=(("as_cod_relacion", string),("as_tipo_doc", string),("as_nro_doc", string)) )
