 column=(type=char(60) updatewhereclause=yes name=desc_sub_cat dbname="articulo_sub_categ.desc_sub_cat" dbalias="A2.desc_sub_cat" )
 column=(type=char(1) updatewhereclause=yes name=flag_numero_serie dbname="articulo.flag_numero_serie" dbalias="A.flag_numero_serie" )
 column=(type=char(3) updatewhereclause=yes name=und2 dbname="articulo.und2" dbalias="A.und2" )
 column=(type=decimal(1) updatewhereclause=yes name=dias_rep_import dbname="articulo.dias_rep_import" dbalias="A.dias_rep_import" )
 column=(type=char(20) updatewhereclause=yes name=numero_serie dbname="numero_serie" )
 column=(type=char(20) updatewhereclause=yes name=codigo_serie dbname="articulo.codigo_serie" dbalias="A.codigo_serie" )
 retrieve="  SELECT A.COD_ART,   
         A.DESC_ART,   
         A.UND,   
         A.COSTO_ULT_COMPRA,   
         A.DIAS_REPOSICION,   
         A2.DESC_SUB_CAT, 
			A.Flag_numero_serie,  
         A.UND2,   
         A.DIAS_REP_IMPORT,
			'' as numero_serie,
			a.codigo_serie   
    FROM ARTICULO					A,   
         ARTICULO_SUB_CATEG	A2,   
         ARTICULO_CATEG  		A1
   WHERE A.SUB_CAT_ART = A2.COD_SUB_CAT 
     and A2.CAT_ART    = A1.CAT_ART 
  	  and A.FLAG_ESTADO = '1' 
	  AND A.FLAG_INVENTARIABLE = '1' 
     AND A1.COD_EMPRESA = :as_empresa  
	  and (A.COD_ART like :as_frase or
			 upper(A.DESC_ART) like :as_frase or
			 upper(A.CODIGO_SERIE) like:as_frase)
union
  SELECT DISTINCT A.COD_ART,   
         A.DESC_ART,   
         A.UND,   
         A.COSTO_ULT_COMPRA,   
         A.DIAS_REPOSICION,   
         A2.DESC_SUB_CAT, 
			A.Flag_numero_serie,   
         A.UND2,   
         A.DIAS_REP_IMPORT,
			am.numero_serie,
			a.codigo_serie
    FROM ARTICULO						A,   
         ARTICULO_SUB_CATEG		A2,   
         ARTICULO_CATEG  			A1,
			ARTICULO_MOV				AM,
			VALE_MOV						VM
   WHERE A.SUB_CAT_ART 	 = A2.COD_SUB_CAT 
     and A2.CAT_ART    	 = A1.CAT_ART 
  	  and A.COD_ART	 	 = AM.COD_ART
	  AND AM.NRO_VALE	  	 = VM.NRO_VALE
  	  and A.FLAG_ESTADO   = '1' 
	  AND A.FLAG_INVENTARIABLE = '1' 
	  and AM.FLAG_ESTADO <> '0'
	  AND VM.NRO_VALE	 	<> '0'
	  and VM.ALMACEN		like :as_almacen
     and am.numero_serie is not null
     AND A1.COD_EMPRESA = :as_empresa  
	  and ( A.COD_ART like :as_frase 
		  or upper(A.DESC_ART) like :as_frase 
		  or upper(A.CODIGO_SERIE) like:as_frase
		  or upper(AM.NUMERO_SERIE) like :as_frase)
" arguments=(("as_empresa", string),("as_frase", string),("as_almacen", string)) )
