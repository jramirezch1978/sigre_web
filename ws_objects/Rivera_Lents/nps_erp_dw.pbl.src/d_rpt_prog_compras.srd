 retrieve="select pcd.nro_item, pcd.fec_requerida, 
       pcd.flag_estado,
       pcd.cod_art,
       a.desc_art,
       a.und,
       pcd.cant_proyect,
       pcd.cant_procesada,
       pcd.flag_prioridad,
       pcd.comprador,
       pcd.almacen, 
       pc.nro_programa, 
       pc.fec_registro,
       pc.cod_usr, pc.flag_estado, 
       u.nombre,
       usf_cmp_oper_sec_prog(pcd.nro_programa, pcd.nro_item) as oper_sec,
		 usf_cmp_prec_ult_compra(pcd.cod_art, pcd.almacen) as prec_compra,
		 pc.observaciones
from prog_compras_det pcd,
     prog_compras     pc,
     articulo         a,
     usuario          u
where a.cod_Art       = pcd.cod_art     
  and pc.nro_programa = pcd.nro_programa
  and u.cod_usr       = pc.cod_usr
  and pc.nro_programa = :as_nro_programa
order by pcd.nro_item" arguments=(("as_nro_programa", string)) )
