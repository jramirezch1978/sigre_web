 retrieve="  SELECT ~"ARTICULO_VENTA~".~"COD_ART~",   
         ~"ARTICULO_VENTA~".~"TIPO_ENVASE~",   
         ~"ARTICULO_VENTA~".~"UND_CAJA~",   
         ~"ARTICULO_VENTA~".~"LOTE_MINIMO~",   
         ~"ARTICULO_VENTA~".~"DIAS_DESPACHO~",   
         ~"ARTICULO_VENTA~".~"PRECIO_MAYORISTA~",   
         ~"ARTICULO_VENTA~".~"PRECIO_MINORISTA~",   
         ~"ARTICULO_VENTA~".~"PRECIO_SUPERMERCADO~",   
         ~"ARTICULO_VENTA~".~"PRECIO_DISTRIBUIDOR~",   
         ~"ARTICULO_VENTA~".~"CNTA_CNTBL_EXPORT~",   
         ~"ARTICULO~".~"DESC_ART~",   
         ~"ARTICULO~".~"UND~",   
         ~"ARTICULO_VENTA~".~"CONFIN~",   
         ~"ARTICULO_VENTA~".~"COD_MONEDA~",   
         ~"ARTICULO_VENTA~".~"FLAG_DL27400~",   
         ~"ARTICULO_VENTA~".~"COSTO_PRODUCCION~",   
         ~"CONCEPTO_FINANCIERO_A~".~"DESCRIPCION~",   
         ~"ARTICULO_VENTA~".~"FLAG_SOLICITA_LOTE~",   
         ~"ARTICULO_VENTA~".~"CNTA_PRSP_VALE_SAL~",   
         ~"PRESUPUESTO_CUENTA~".~"DESCRIPCION~",   
         ~"ARTICULO_VENTA~".~"FLAG_REPLICACION~",   
         ~"ARTICULO_VENTA~".~"CONFIN_EXPORT~",   
         ~"CONCEPTO_FINANCIERO_B~".~"DESCRIPCION~"  
    FROM ~"ARTICULO_VENTA~",   
         ~"ARTICULO~",   
         ~"CONCEPTO_FINANCIERO~" ~"CONCEPTO_FINANCIERO_A~",   
         ~"PRESUPUESTO_CUENTA~",   
         ~"CONCEPTO_FINANCIERO~" ~"CONCEPTO_FINANCIERO_B~"  
   WHERE ( concepto_financiero_a.confin (+) = articulo_venta.confin) and  
         ( articulo_venta.cnta_prsp_vale_sal = presupuesto_cuenta.cnta_prsp (+)) and  
         ( articulo_venta.confin_export = concepto_financiero_b.confin (+)) and  
         ( ~"ARTICULO_VENTA~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" )   
ORDER BY ~"ARTICULO_VENTA~".~"COD_ART~" ASC   
" update="ARTICULO_VENTA" updatewhere=0 updatekeyinplace=yes )
