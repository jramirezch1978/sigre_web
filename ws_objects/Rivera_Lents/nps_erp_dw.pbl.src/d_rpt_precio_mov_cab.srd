 retrieve="select am.nro_vale, am.cod_art, ar.desc_art, am.precio_unit, tt.precio_unit,
       vm.fec_registro, vm.tipo_mov, 
		 tt.precio_unit - am.precio_unit as diferencia,
		 am.cencos,
		 am.cnta_prsp,
		 a.almacen,
		 a.desc_almacen
from TT_ALM_MV_PREC_UNIT tt,
     articulo_mov        am,
     vale_mov            vm,
     articulo            ar,
	  almacen			    a
where vm.nro_vale   = am.nro_vale
  and am.cod_origen = tt.cod_origen
  and am.nro_mov    = tt.nro_mov
  and ar.cod_art    = am.cod_art
  and a.almacen	  = vm.almacen
order by a.almacen, vm.fec_registro" )
