 retrieve="  SELECT ~"PROVEEDOR_A~".~"NOM_PROVEEDOR~" as cliente,   
         ~"EMBARQUE~".~"NAVE~"					,   
         ~"FL_PUERTOS_A~".~"DESCR_PUERTO~" as des_puerto_origen  ,   
         ~"FL_PUERTOS_B~".~"DESCR_PUERTO~" as desc_puerto_destino,   
         ~"PROVEEDOR_B~".~"NOM_PROVEEDOR~" as asegurador			 ,   
         ~"EMBARQUE~".~"INCOTERM~",   
         ~"BANCO~".~"NOM_BANCO~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~" ,
			~"CNTAS_COBRAR_DET~".~"COD_ART~" ,
			~"CNTAS_COBRAR_DET~".~"ITEM~" ,
			~"CNTAS_COBRAR_DET~".~"MARCAS~" ,
			~"CNTAS_COBRAR_DET~".~"DESCRIPCION~",
			~"CNTAS_COBRAR_DET~".~"CANTIDAD~" ,  
			~"ARTICULO~".~"UND~",
			~"CNTAS_COBRAR_DET~".~"PRECIO_UNITARIO~"   ,
			~"CNTAS_COBRAR_DET~".~"DESCUENTO~"   		,
			~"FACTURA_EXPORTACION~".~"GLOSA_DETALLE~"  ,
			~"CNTAS_COBRAR~".~"IMPORTE_DOC~",
			~"CNTAS_COBRAR~".~"COD_MONEDA~",
			~"MONEDA~".~"DESCRIPCION~",
			USF_CMP_DIRECCION_CODREL(~"CNTAS_COBRAR~".~"COD_RELACION~",'1') AS DIRECCION,
			~"FORMA_PAGO~".~"DESC_FORMA_PAGO~"
    FROM ~"CNTAS_COBRAR~",   
			~"CNTAS_COBRAR_DET~",   
			~"ARTICULO~",
         ~"PROVEEDOR~" ~"PROVEEDOR_A~",   
         ~"EMBARQUE~",   
         ~"FACTURA_EXPORTACION~",   
         ~"FL_PUERTOS~" ~"FL_PUERTOS_A~",   
         ~"FL_PUERTOS~" ~"FL_PUERTOS_B~",   
         ~"PROVEEDOR~" ~"PROVEEDOR_B~",   
         ~"BANCO~"  ,
			~"MONEDA~" ,
			~"FORMA_PAGO~"
	
   WHERE ( ~"CNTAS_COBRAR~".~"TIPO_DOC~"            = ~"CNTAS_COBRAR_DET~".~"TIPO_DOC~" 		) AND
			( ~"CNTAS_COBRAR~".~"NRO_DOC~" 				= ~"CNTAS_COBRAR_DET~".~"NRO_DOC~" 		) AND
			( ~"CNTAS_COBRAR_DET~".~"COD_ART~"			= ~"ARTICULO~".~"COD_ART~"					) AND
			( ~"FACTURA_EXPORTACION~".~"TIPO_DOC~" 		= ~"CNTAS_COBRAR~".~"TIPO_DOC~" 			) AND  
         ( ~"FACTURA_EXPORTACION~".~"NRO_DOC~" 		= ~"CNTAS_COBRAR~".~"NRO_DOC~" 			) AND
			
			( ~"FACTURA_EXPORTACION~".~"NRO_EMBARQUE~" = ~"EMBARQUE~".~"NRO_EMBARQUE~"         ) AND
			( ~"CNTAS_COBRAR~".~"COD_MONEDA~" 			= ~"MONEDA~".~"COD_MONEDA~"			 		) AND
			( ~"PROVEEDOR_A~".~"PROVEEDOR~"            = ~"CNTAS_COBRAR~".~"COD_RELACION~" (+) ) AND           
         ( ~"EMBARQUE~".~"PUERTO_ORIGEN~"           = ~"FL_PUERTOS_A~".~"PUERTO~" 		  (+)	) AND
         ( ~"EMBARQUE~".~"PUERTO_DESTINO~"          = ~"FL_PUERTOS_B~".~"PUERTO~" 		  (+)	) AND
         ( ~"EMBARQUE~".~"ASEGURADOR~" 		         = ~"PROVEEDOR_B~".~"PROVEEDOR~" 	  (+)	) AND
         ( ~"EMBARQUE~".~"DOC_COBR_BANCO~" 			= ~"BANCO~".~"COD_BANCO~" 		 	  (+)	) AND
			( ~"CNTAS_COBRAR~".~"FORMA_PAGO~"				= ~"FORMA_PAGO~".~"FORMA_PAGO~"	  (+)	) AND	
			( ~"CNTAS_COBRAR~".~"TIPO_DOC~"					= :as_tipo_doc   							) AND
			( ~"CNTAS_COBRAR~".~"NRO_DOC~"					= :as_nro_doc   							)
" arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
