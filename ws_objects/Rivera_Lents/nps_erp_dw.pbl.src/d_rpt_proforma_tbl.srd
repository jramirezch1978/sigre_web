 column=(type=char(40) updatewhereclause=yes name=desc_moneda dbname="moneda.desc_moneda" )
 retrieve="select p.*, pd.*, fp.desc_forma_pago, 
       u.nombre as nom_vendedor, m.descripcion as desc_moneda
from proforma p,
     proforma_det pd,
     forma_pago   fp,
     usuario      u,
	  moneda		   m
where p.nro_proforma = pd.nro_proforma     
  and p.forma_pago   = fp.forma_pago
  and p.vendedor     = u.cod_usr
  and p.cod_moneda	= m.cod_moneda
  and p.nro_proforma = :as_nro_proforma" arguments=(("as_nro_proforma", string)) )
