 retrieve="  SELECT OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_USR,   
         OC.FLAG_ESTADO,   
         OC.MONTO_TOTAL,   
         OC.OBSERVACION,   
         OC.FLAG_IMPORTACION,   
         OC.COD_MONEDA,   
         AMP.COD_ART,   
         AMP.PRECIO_UNIT,   
         AMP.DECUENTO,  
         A.DESC_ART,   
         A.UND,   
         trunc(AMP.FEC_PROYECT) as fec_proyect,   
         U.NOMBRE,   
         P.NOM_PROVEEDOR,
			P.RUC,
			TRIM(D.DIR_DEP_ESTADO) || ' ' || TRIM(D.DIR_DISTRITO) || ' ' || TRIM(D.DIR_DIRECCION) AS DIRECCION,
         sum(AMP.CANT_PROYECT) as cant_proyect,
			sum(round(amp.precio_unit * amp.cant_proyect,2)) as importe,
			sum(round((amp.precio_unit - NVL(amp.decuento, 0)) * amp.cant_proyect,2)) as valor_venta,
			sum(round(amp.decuento * amp.cant_proyect,2)) as s_descuento,
			sum(round( amp.cant_proyect * amp.impuesto, 2)) as s_impuesto,
			(select oc_dir_1 from logparam where reckey = '1') as dir_1,
			(select oc_dir_2 from logparam where reckey = '1') as dir_2,
			(select oc_dir_3 from logparam where reckey = '1') as dir_3,
			(select oc_dir_4 from logparam where reckey = '1') as dir_4,
			(select oc_dir_5 from logparam where reckey = '1') as dir_5
    FROM ORDEN_COMPRA 		OC,   
         ARTICULO_MOV_PROY AMP,   
         ARTICULO 			A,   
         USUARIO				U,   
         PROVEEDOR 			P,
			DIRECCIONES			D
   WHERE OC.COD_ORIGEN 	= AMP.COD_ORIGEN 
     and AMP.COD_ART 	= A.COD_ART 
     and OC.NRO_OC 		= AMP.NRO_DOC 
     and OC.COD_USR 		= U.COD_USR 
     and OC.PROVEEDOR 	= P.PROVEEDOR 
     and OC.COD_ORIGEN 	= :as_cod_origen 
     AND OC.NRO_OC 		= :as_nro_oc 
     AND OC.PROVEEDOR	= D.CODIGO (+)
     AND OC.DIRECCION_ITEM = D.ITEM (+)
     AND AMP.TIPO_DOC 	= (select doc_oc from logparam where reckey = '1') 
  group by OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_USR,   
         OC.FLAG_ESTADO,   
         OC.MONTO_TOTAL,   
         OC.OBSERVACION,   
         OC.FLAG_IMPORTACION,   
         OC.COD_MONEDA,   
         AMP.COD_ART,   
         AMP.PRECIO_UNIT,   
         AMP.DECUENTO,
         A.DESC_ART,   
         A.UND,   
         trunc(AMP.FEC_PROYECT),
         U.NOMBRE,   
         P.NOM_PROVEEDOR,
			P.RUC,
			TRIM(D.DIR_DEP_ESTADO) || ' ' || TRIM(D.DIR_DISTRITO) || ' ' || TRIM(D.DIR_DIRECCION)" arguments=(("as_cod_origen", string),("as_nro_oc", string)) )
