 column=(type=char(10) updatewhereclause=yes name=pln_cobranza_det_nro_planilla dbname="pln_cobranza_det.nro_planilla" )
 retrieve="SELECT distinct(cc.tipo_doc) as tipo_doc, cc.nro_doc, 
		 cc.fecha_documento, cc.cod_moneda, 
		 cc.importe_doc, decode(cc.cod_moneda,(select cod_dolares from logparam l where reckey='1'), 
		 NVL(cc.saldo_dol,0), NVL(cc.saldo_sol,0)) as saldo, 
		 USF_FIN_REFER_FACTURAS(cc.cod_relacion, cc.tipo_doc, cc.nro_doc) as referencias,
       pln.nro_planilla
FROM   cntas_cobrar cc, cntas_cobrar_det ccd, pln_cobranza_det pln
WHERE  cc.tipo_doc = ccd.tipo_doc and
       cc.nro_doc = ccd.nro_doc and
       cc.tipo_doc = pln.doc_cxc (+) and
       cc.nro_doc = pln.nro_doc_cxc (+) and
       cc.flag_estado <>'0' and
       ccd.tipo_ref=(select l.doc_ov from logparam l where reckey='1') and
       ccd.nro_ref = :as_nro_ov" arguments=(("as_nro_ov", string)) )
