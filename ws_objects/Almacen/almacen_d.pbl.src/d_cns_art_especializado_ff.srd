 retrieve="  SELECT A.COD_ART,   
         case
				when a.desc_clase_vehiculo is not null then
					a.full_desc_vehiculo
				else
					a.full_desc_art
			end as desc_art,
         A.UND, A.UND2,
         A.DESC_SUB_CAT,   
         A.DESC_CATEGORIA,   
         A.SLDO_TOTAL,   
         A.SLDO_POR_LLEGAR,   
         A.SLDO_SOLICITADO,   
         A.SLDO_DEVUELTO,   
         A.SLDO_PRESTAMO,   
         A.SLDO_CONSIGNACION,   
         A.COSTO_ULT_COMPRA,   
         A.COSTO_PROM_SOL,   
         A.COSTO_PROM_DOL,   
         A.FLAG_REPOSICION,   
         A.FLAG_CRITICO,   
         A.FLAG_OBSOLETO,   
         A.PESO_UND,   
         A.VOL_UND,   
         A.SLDO_RESERVADO,   
         A.FEC_ULT_COMPRA,   
         A.FEC_ULT_SALIDA,   
         A.NRO_ULTIMA_OC,   
         A.SLDO_MINIMO,   
         A.SLDO_MAXIMO,   
         A.DIAS_REPOSICION,   
         A.DIAS_REP_IMPORT,   
         A.CNT_COMPRA_REC,   
         A.FLAG_INVENTARIABLE,   
         A.FLAG_UND2,   
         A.FLAG_CNTRL_LOTE,   
         A.UND2,   
         A.FACTOR_CONV_UND,   
         A.SLDO_TOTAL_UND2,   
         A.FILE_IMAGEN,   
         A.SLDO_WARRANTEADO,   
         A.SLDO_TRANSPORTADO,   
			A.COD_SKU,
			a.nro_serie,
			a.nro_motor,
         A.FLAG_ESTADO
    FROM vw_ARTICULO				A
   WHERE a.cod_art		= :as_cod_art   
" arguments=(("as_cod_art", string)) )
