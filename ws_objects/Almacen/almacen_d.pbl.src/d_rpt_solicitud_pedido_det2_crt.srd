 retrieve="select spd.nro_item,
       spd.proveedor, p.nom_proveedor,
       a1.cat_art, a1.desc_categoria,
       spd.cod_sub_cat, a2.desc_sub_cat,
		a.cod_art, a.desc_art,
       spd.almacen, al.desc_almacen,
       spd.cantidad,
       spd.precio_unit,
       spd.nro_pedido_prov
from solicitud_pedido_det spd,
     solicitud_pedido     sp,
     articulo_sub_categ   a2,
     articulo_categ       a1,
	articulo        a,
     proveedor        p,
     almacen          al,
     moneda           m
where sp.nro_solicitud = spd.nro_solicitud 
	and spd.cod_art = a.cod_art
  and a.sub_cat_art =a2.cod_sub_cat
  and a2.cat_art      = a1.cat_art
  and spd.proveedor   = p.proveedor
  and spd.almacen     = al.almacen
  and sp.cod_moneda   = m.cod_moneda
  and sp.nro_solicitud = :as_nro_solicitud
" arguments=(("as_nro_solicitud", string))  sort="desc_categoria A nom_proveedor A desc_sub_cat A articulo_desc_art A " )
