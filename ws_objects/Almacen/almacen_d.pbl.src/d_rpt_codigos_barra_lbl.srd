 column=(type=char(2) updatewhereclause=yes name=cod_taco dbname="cod_taco" )
 retrieve="select a.desc_art, 
		 '*' || a.cod_sku || '*' as code_bar, 
		 a.cod_sku,
		 zl.flag_print_precio, 
       case
		 	when zl.flag_print_precio = '1' then
		 		PKG_LOGISTICA.of_soles(null)
			else
				null
		 end as moneda,
       case
		 	when zl.flag_print_precio = '1' then
		 		a.precio_vta_unidad
			else
				null
		 end as precio_vta_unidad,
		 (select max(vm.fec_registro)
  			 from vale_mov vm,
					articulo_mov am
			where vm.nro_Vale = am.nro_vale
 			  and am.origen_mov_proy = amp.cod_origen
        	  and am.nro_mov_proy    = amp.nro_mov
        	  and vm.flag_estado <> '0'
        	  and am.flag_estado <> '0'
        	  and vm.tipo_mov    like 'I%') as fec_ingreso,
		 a.sub_cat_art, 
		 a.cod_marca, 
		 a.cod_sub_linea, 
		 a.estilo, 
		 a.cod_suela, 
		 a.cod_acabado, 
		 a.color1, 
		 a.talla,
		 '' as siglas,
		 '' as linea1,
		 '' as linea2,
		 a.color2,
		 a.cod_taco
from articulo a,
     zc_linea zl,
     zc_sub_linea zs,
     articulo_mov_proy amp,
	  (select 1 from dual
		union
      select 2 from dual
	   union
		select 3 from dual) s
where amp.cod_art     = a.cod_art
  and a.cod_sub_linea = zs.cod_sub_linea (+)
  and zs.cod_linea    = zl.cod_linea     (+)
  and amp.nro_doc     = :as_nro_doc
  and amp.tipo_doc    = (select doc_oc from logparam where reckey = '1')
order by a.sub_cat_art, a.cod_marca, a.cod_sub_linea, a.estilo, a.cod_suela, a.cod_acabado, a.color1, a.talla
  " arguments=(("as_nro_doc", string),("as_empresa", string))  sort="linea1 A linea2 A talla A " )
