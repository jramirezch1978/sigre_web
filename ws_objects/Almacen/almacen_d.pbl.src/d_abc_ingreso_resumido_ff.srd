 retrieve="select a1.cat_art,
       a1.desc_categoria, 
       a1.abreviatura as abrev_categoria,
       a2.cod_sub_cat,
       a2.desc_sub_cat,
       a.cod_marca,
       m.nom_marca,
       zl.cod_linea,
       zl.desc_linea, 
		 zl.abreviatura as abrev_linea,
       a.cod_sub_linea, zsl.desc_sub_linea,
       a.cod_suela, zs.desc_suela,
       a.cod_acabado, za.desc_acabado,
       a.estilo,
       a.color1, c1.descripcion as color_primario,
       a.color2, c2.descripcion as color_secundario,
       a.cod_taco, zt.desc_taco,
       a.cod_art, a.desc_art,
       0.0 as min_talla,
		 0.0 as max_talla,
       0.0 as precio,
		 0.5 as intervalo,
		 a.und,
		 '' as desc_unidad,
		 a.cod_clase,
		 ac.desc_clase
from articulo a,
     zc_sub_linea zsl,
     zc_linea     zl,
     marca        m,
     articulo_categ a1,
     articulo_sub_categ a2,
     zc_suela           zs,
     zc_acabado         za,
     zc_taco            zt,
     color              c1,
     color              c2,
	  articulo_clase		ac
where a.cod_sub_linea = zsl.cod_sub_linea (+)     
  and zsl.cod_linea    = zl.cod_linea     (+)
  and a.cod_marca     = m.cod_marca      (+)
  and a.sub_cat_art   = a2.cod_sub_cat   (+)
  and a2.cat_art       = a1.cat_art      (+)
  and a.cod_suela      = zs.cod_suela    (+)
  and a.cod_acabado    = za.cod_acabado  (+)
  and a.color1         = c1.color        (+)
  and a.color2         = c2.color        (+)
  and a.cod_taco       = zt.cod_taco     (+)
  and a.cod_clase		  = ac.cod_clase	  (+)
order by desc_linea, desc_sub_linea, nom_marca  
" )
