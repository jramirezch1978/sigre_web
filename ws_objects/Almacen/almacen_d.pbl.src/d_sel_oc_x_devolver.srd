 retrieve="  SELECT DISTINCT OC.COD_ORIGEN,   
         OC.FEC_REGISTRO,   
         OC.PROVEEDOR,   
         P.NOM_PROVEEDOR,   
         OC.NRO_OC,   
         OC.FLAG_ESTADO  
    FROM ORDEN_COMPRA  			OC,   
         PROVEEDOR     			P,   
         ARTICULO_MOV_PROY  	AMP,
			ARTICULO_ALMACEN		AA
   WHERE OC.PROVEEDOR 	= P.PROVEEDOR 
	  and OC.COD_ORIGEN 	= AMP.COD_ORIGEN 
	  and AA.ALMACEN		= AMP.ALMACEN
  	  and AA.COD_ART		= AMP.COD_ART
	  and AMP.ALMACEN		= :as_almacen
	  and AMP.TIPO_DOC 	= (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1' )
     and OC.NRO_OC 		= AMP.NRO_DOC 
	  and NVL(AMP.CANT_PROCESADA,0) > 0
	  and AA.SLDO_TOTAL > 0 
     and OC.FLAG_ESTADO <> '0'
ORDER BY OC.FEC_REGISTRO DESC, OC.NRO_OC ASC   
" arguments=(("as_almacen", string)) )
