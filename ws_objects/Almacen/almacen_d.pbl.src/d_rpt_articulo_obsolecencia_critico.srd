 column=(type=number updatewhereclause=yes name=saldo dbname="saldo" )
 retrieve="SELECT a.cod_art, a.nom_articulo,            
         a.und, nvl(a.flag_critico,'0') as critico,   
         a.sldo_total, a.costo_prom_sol,   
         (a.sldo_total * a.costo_prom_sol) as tot_costo,   
         a.fec_ult_compra, a.fec_ult_salida,
			usf_alm_dif_dias(a.fec_ult_salida, :ad_fecha ) as saldo
    FROM ARTICULO a
   WHERE (a.fec_ult_salida is not null) and
         a.sldo_total >0 and
         a.costo_prom_sol > 0 and
         usf_alm_dif_dias(a.fec_ult_salida, :ad_fecha )  >= :an_dias
order by a.flag_critico, (a.sldo_total * a.costo_prom_sol)" update="ARTICULO" updatewhere=1 updatekeyinplace=no arguments=(("ad_fecha", date),("an_dias", number),("as_empresa", string),("as_usuario", string))  sort="saldo A " )
