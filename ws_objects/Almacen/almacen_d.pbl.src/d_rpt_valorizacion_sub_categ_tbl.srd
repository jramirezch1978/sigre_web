 column=(type=char(60) updatewhereclause=yes name=desc_sub_cat dbname="articulo_sub_categ.desc_sub_cat" )
 column=(type=number updatewhereclause=yes name=sldo_total dbname="sldo_total" )
 column=(type=number updatewhereclause=yes name=costo_prom_sol dbname="costo_prom_sol" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="select ac.cod_sub_cat,
       ac.desc_sub_cat,
		 sum(aa.sldo_total) as sldo_total,
		 sum(aa.sldo_total * aa.costo_prom_sol) / sum(aa.sldo_total) as costo_prom_sol,
       round(sum(aa.sldo_total * aa.costo_prom_sol),2) as Importe
from articulo_almacen   aa,
     articulo           a,
     articulo_sub_categ ac
where a.cod_art = aa.cod_art
  and ac.cod_sub_cat = a.sub_cat_art
  and a.flag_estado <> '0'
  and (aa.sldo_total > 0 or aa.sldo_total_und2 > 0)
  and ac.cod_sub_cat in (select cod_sub_cat from tt_alm_seleccion)
  and aa.almacen = :as_almacen
group by ac.cod_sub_cat, ac.desc_sub_cat

" arguments=(("as_almacen", string)) )
