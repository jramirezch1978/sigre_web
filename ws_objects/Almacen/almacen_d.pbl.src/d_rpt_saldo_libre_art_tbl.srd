 column=(type=char(60) updatewhereclause=yes name=almacen_desc_almacen dbname="almacen.desc_almacen" )
 retrieve="select aa.almacen, aa.cod_art, a.desc_art, a.und, 
       aa.sldo_total, aa.sldo_consignacion, aa.sldo_reservado, aa.sldo_por_llegar, 
       (aa.sldo_total - aa.sldo_reservado + aa.sldo_consignacion) as saldo_libre, 
       aa.sldo_solicitado, aa.costo_ult_compra, al.desc_almacen
from articulo_almacen aa, articulo a, almacen al
where (aa.cod_art = a.cod_art) and
      (aa.almacen=al.almacen) and
      (a.flag_inventariable = '1' and
       a.flag_estado <> '0' and
       al.flag_tipo_almacen='M') and
      (aa.sldo_total + aa.sldo_consignacion - aa.sldo_reservado) > 0 
order by aa.almacen, aa.cod_art     " )
