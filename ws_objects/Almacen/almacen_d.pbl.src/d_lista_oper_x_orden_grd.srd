 column=(type=char(10) updatewhereclause=yes name=cencos dbname="orden_trabajo.cencos_slc" )
 column=(type=datetime updatewhereclause=yes name=fec_inicio_est dbname="operaciones.fec_inicio_est" )
 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="operaciones.flag_estado" values="Anulado	0/Abierto	1/Cerrado	2/Planeado	3/" )
 retrieve="select distinct op.oper_sec, 
			op.desc_operacion, 
			ot.nro_orden, 
			ot.cencos_slc, 
		   op.FEC_INICIO_EST,
			op.flag_estado
from operaciones    op,
     orden_trabajo  ot
where ot.nro_orden    = op.nro_orden
  and op.flag_estado <> '0'
  and op.nro_orden = :as_nro_orden
  and op.flag_estado in ('1', '3')
  and USF_CMP_OPER_SEC_ACTIVO(op.oper_sec ) = 1
order by oper_sec asc" arguments=(("as_nro_orden", string)) )
