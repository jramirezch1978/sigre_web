 retrieve="SELECT 	AMP.COD_ART,   
         A.DESC_ART,   
         A.UND,   
         AMP.CANT_PROYECT,   
			AMP.CANT_PROCESADA,   
         AMP.COD_ORIGEN as org_amp,   
         AMP.NRO_MOV		as nro_amp,   
         AMP.PRECIO_UNIT,   
         AMP.DECUENTO,   
         AMP.IMPUESTO,   
         AMP.COD_MONEDA,   
         AMP.CENCOS,   
         AMP.CNTA_PRSP,   
         A.COSTO_PROM_SOL,   
         A.COSTO_PROM_DOL,
      	A.UND2,
      	A.FLAG_UND2,
      	A.FACTOR_CONV_UND,
      	vw.nro_pallet,
      	vw.nro_lote,
      	vw.anaquel,
      	vw.fila,
      	vw.columna,
			sum(
				case
					when decode(nvl(amp.cant_despacho,0), 0, amp.cant_proyect, amp.cant_despacho) - amp.cant_procesada > vw.saldo then
						vw.saldo
					else
						decode(nvl(amp.cant_despacho,0), 0, amp.cant_proyect, amp.cant_despacho) - amp.cant_procesada
				end 
			) as saldo_pendiente_und, 
      	sum(
				case
					when decode(nvl(amp.cant_despacho,0), 0, amp.cant_proyect, amp.cant_despacho) - amp.cant_procesada > vw.saldo then
						vw.saldo_und2
					else
						case
							when a.factor_conv_und > 0 then
								round((decode(nvl(amp.cant_despacho,0), 0, amp.cant_proyect, amp.cant_despacho) - amp.cant_procesada) * a.factor_conv_und,4)
						else
							0
					end
				end 
			) as saldo_pendiente_und2
    FROM ARTICULO_MOV_PROY     AMP,   
         ARTICULO              A,
         vw_alm_saldo_pallet   vw
   WHERE AMP.COD_ART    = A.COD_ART 
     and vw.cod_art     = amp.cod_art
     and vw.almacen     = amp.almacen
     and vw.saldo     > 0
     and AMP.TIPO_DOC   = (select doc_ov from logparam where reckey = '1')
     AND AMP.NRO_DOC    = :as_nro_doc 
     and amp.cod_origen	= :as_org_amp
	  and amp.nro_mov		= :an_nro_mov
     AND AMP.CANT_PROCESADA < decode(nvl(amp.cant_despacho,0), 0, amp.cant_proyect, amp.cant_despacho)
     and amp.flag_estado = '1'
     and vw.saldo > 0
     AND AMP.COD_ART IN (SELECT DISTINCT COD_ART 
                          FROM ARTICULO_ALMACEN
                         WHERE ALMACEN = :as_almacen
                           AND SLDO_TOTAL > 0)
group by AMP.COD_ART,   
         A.DESC_ART,   
         A.UND,   
         AMP.CANT_PROYECT,   
         AMP.COD_ORIGEN,   
         AMP.CANT_PROCESADA,   
         AMP.NRO_MOV,   
         AMP.PRECIO_UNIT,   
         AMP.DECUENTO,   
         AMP.IMPUESTO,   
         AMP.COD_MONEDA,   
         AMP.CENCOS,   
         AMP.CNTA_PRSP,   
         A.COSTO_PROM_SOL,   
         A.COSTO_PROM_DOL,
      	A.UND2,
      	A.FLAG_UND2,
      	A.FACTOR_CONV_UND,
      	vw.nro_pallet,
      	vw.nro_lote,
      	vw.anaquel,
      	vw.fila,
      	vw.columna
" arguments=(("as_nro_doc", string),("as_almacen", string),("as_org_amp", string),("an_nro_mov", number)) )
