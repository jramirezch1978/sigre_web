table(column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="vw_vta_reg_ventas_det.tipo_doc" )
 column=(type=char(4000) updatewhereclause=yes name=full_nro_doc dbname="vw_vta_reg_ventas_det.full_nro_doc" )
 column=(type=char(200) updatewhereclause=yes name=nom_vendedor dbname="vw_vta_reg_ventas_det.nom_vendedor" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="vw_vta_reg_ventas_det.cod_moneda" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="vw_vta_reg_ventas_det.cod_relacion" )
 column=(type=char(200) updatewhereclause=yes name=nom_cliente dbname="vw_vta_reg_ventas_det.nom_cliente" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="vw_vta_reg_ventas_det.ruc_dni" )
 column=(type=number updatewhereclause=yes name=imp_venta dbname="imp_venta" )
 column=(type=number updatewhereclause=yes name=peso_neto dbname="peso_neto" )
 retrieve="select vw.tipo_doc,
       vw.full_nro_doc,
       vw.nom_vendedor,
       vw.cod_moneda,
       vw.cod_relacion,
       vw.nom_proveedor as nom_cliente,
       vw.ruc_dni,
       sum(am.cant_procesada * vw.precio_vta) as imp_venta, 
       sum(am.peso_neto_tm) as peso_neto
  from guia                  g,
       guia_vale             gv,
       vale_mov              vm,
       articulo_mov          am,
       articulo              a,
       sunat_ubigeo          su,
       vw_vta_reg_ventas_det vw
 where g.nro_guia         = gv.nro_guia
   and gv.nro_vale        = vm.nro_vale
   and vm.nro_vale        = am.nro_Vale
   and am.cod_art         = a.cod_art
   and g.ubigeo_dst       = su.ubigeo
   and am.cod_origen      = vw.org_am     
   and am.nro_mov         = vw.nro_am   
   and g.nro_placa || '-' || g.nom_chofer in (select nro_placa || '-' || nom_chofer from TT_GUIA_PLACA_CHOFER)
   and g.nro_guia                in (select nro_guia from TT_GUIAS)
   and vw.tipo_doc || '-' || vw.nro_doc in (select tipo_doc || '-' || nro_doc from TT_COMPROBANTES)   
   and trunc(vm.fec_registro) between :ad_fecha1 and :ad_fecha2
   and vm.almacen         like :as_almacen
group by vw.tipo_doc,
         vw.full_nro_doc,
         vw.nom_vendedor,
         vw.cod_moneda,
         vw.cod_relacion,
         vw.nom_proveedor,
         vw.ruc_dni" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_almacen", string))  sort="nom_vendedor A tipo_doc A full_nro_doc A " )
