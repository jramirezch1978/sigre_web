table(column=(type=char(10) updatewhereclause=yes name=nro_otr dbname="orden_traslado.nro_otr" )
 column=(type=datetime updatewhereclause=yes name=fec_registro dbname="orden_traslado.fec_registro" )
 column=(type=char(6) updatewhereclause=yes name=almacen dbname="orden_traslado.almacen_org" )
 column=(type=char(60) updatewhereclause=yes name=desc_almacen dbname="almacen.desc_almacen" )
 column=(type=char(200) updatewhereclause=yes name=obs dbname="orden_traslado.obs" )
 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="orden_traslado.flag_estado" values="Anulado	0/Generado	1/Aprobado	2/" )
 column=(type=number updatewhereclause=yes name=stock dbname="stock" )
 retrieve="SELECT OT.NRO_OTR,
			 OT.FEC_REGISTRO,   
       OT.ALMACEN_ORG,
			 al.DESC_ALMACEN,
       ot.obs,
       OT.FLAG_ESTADO,
       nvl(sum(aa.sldo_total), 0) as stock  
  FROM ORDEN_TRASLADO OT,   
			 ALMACEN			  AL,
       ARTICULO_MOV_PROY AMP,
       articulo_almacen  aa
 WHERE OT.ALMACEN_ORG = AL.ALMACEN 
	 and OT.NRO_OTR     = AMP.NRO_DOC 
   and ot.almacen_org = aa.almacen  
   and amp.cod_art    = aa.cod_art  
   and nvl(aa.sldo_total,0 ) > 0
	and AMP.TIPO_DOC = (SELECT DOC_OTR FROM LOGPARAM WHERE RECKEY = '1'  ) 
   AND AMP.TIPO_MOV = (SELECT OPER_SAL_OTR FROM LOGPARAM WHERE RECKEY = '1') 
	AND AMP.CANT_PROYECT > AMP.CANT_PROCESADA 
   and AMP.CANT_PROYECT > 0
   AND OT.FLAG_ESTADO <>'0'
   AND AMP.FLAG_ESTADO <> '0'
   and OT.ALMACEN_ORG  = :as_almacen
   and amp.nro_doc	  like :as_nro_doc
group by OT.NRO_OTR,
			 OT.FEC_REGISTRO,   
       OT.ALMACEN_ORG,
			 al.DESC_ALMACEN,
       ot.obs,
       OT.FLAG_ESTADO    
ORDER BY OT.FEC_REGISTRO DESC,   
         OT.NRO_OTR ASC   
" arguments=(("as_almacen", string),("as_nro_doc", string)) )
