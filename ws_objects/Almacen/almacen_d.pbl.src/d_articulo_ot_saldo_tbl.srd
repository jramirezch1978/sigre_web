 retrieve="select aa.cod_art, a.desc_art 
   from articulo_almacen aa 
      inner join articulo a on aa.cod_art = a.cod_art
   where aa.sldo_total > 0
      and aa.almacen = :asi_almacen
      and aa.cod_art in (select amp.cod_art from articulo_mov_proy amp inner join operaciones o on amp.oper_sec = o.oper_sec where o.nro_orden = :asi_nro_orden)
	order by a.desc_art" arguments=(("asi_almacen", string),("asi_nro_orden", string)) )
