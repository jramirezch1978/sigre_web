table(column=(type=number updatewhereclause=yes name=id_foto dbname="imagen_articulo.id_foto" dbalias="I.id_foto" )
 column=(type=char(150) updatewhereclause=yes name=abrev_articulo dbname="imagen_articulo.abrev_articulo" dbalias="I.abrev_articulo" )
 column=(type=char(200) updatewhereclause=yes name=marca dbname="marca" )
 column=(type=char(20) updatewhereclause=yes name=estilo dbname="estilo" )
 column=(type=char(40) updatewhereclause=yes name=color1 dbname="color1" )
 column=(type=char(40) updatewhereclause=yes name=color2 dbname="color2" )
 column=(type=char(2) updatewhereclause=yes name=taco dbname="taco" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="fecha" )
 retrieve="   SELECT I.ID_FOTO,   
         I.ABREV_ARTICULO,            
         trim(m.nom_marca) marca, 
		trim(i.estilo)    estilo,         
         trim(c.descripcion) color1,
         trim(c2.descripcion) color2, 
         trim(i.cod_taco) taco  ,
		trunc(i.fecha_registro) fecha
    FROM fotos.IMAGEN_ARTICULO I,   
				fotos.MARCA M,   
				fotos.COLOR C,   
				fotos.COLOR C2  
   WHERE  I.COD_MARCA = M.COD_MARCA  and  
          C.COLOR= I.COLOR1  and  
          C2.COLOR = I.COLOR2  and 
		 i.abrev_articulo like '%' || replace(:filtro,' ','%') ||'%'
" arguments=(("filtro", string))  sort="abrev_articulo A " )
