 retrieve="SELECT  distinct ~"GUIA~".~"COD_ORIGEN~" ,
           ~"GUIA~".~"NRO_GUIA~" ,
           ~"GUIA~".~"ALMACEN~" ,
           ~"GUIA~".~"FEC_REGISTRO~" ,
           ~"GUIA~".~"FEC_IMPRESION~" ,
           ~"GUIA~".~"FLAG_ESTADO~" ,
           ~"GUIA~".~"MOTIVO_TRASLADO~" ,
           ~"GUIA~".~"CLIENTE~" ,
           ~"GUIA~".~"CENCOS~" ,
           ~"GUIA~".~"DESTINATARIO~" ,
           ~"GUIA~".~"NOM_CHOFER~" ,
           ~"GUIA~".~"NRO_BREVETE~" ,
           ~"GUIA~".~"NRO_PLACA~" ,
           ~"GUIA~".~"NRO_PLACA_CARRETA~" ,
           ~"GUIA~".~"DESTINO~" ,
           ~"GUIA~".~"TIPO_DOC~" ,
           ~"GUIA~".~"NRO_DOC~" ,
           ~"GUIA~".~"COD_USR~" ,
           ~"GUIA~".~"PROV_TRANSP~" ,
           ~"GUIA~".~"OBS~" ,
           ~"GUIA_VALE~".~"NRO_VALE~" ,
           ~"PROVEEDOR~".~"NOM_PROVEEDOR~" ,
           ~"MOTIVO_TRASLADO~".~"DESCRIPCION~"     
        FROM ~"GUIA~" ,
           ~"GUIA_VALE~" ,
           ~"PROVEEDOR~" ,
           ~"MOTIVO_TRASLADO~"     
        WHERE ~"GUIA~".~"COD_ORIGEN~" 		= ~"GUIA_VALE~".~"ORIGEN_GUIA~" (+)
		    and ~"GUIA~".~"NRO_GUIA~" 			= ~"GUIA_VALE~".~"NRO_GUIA~" (+)
			 and ~"GUIA~".~"CLIENTE~" 			= ~"PROVEEDOR~".~"PROVEEDOR~" 
			 and ~"GUIA~".~"MOTIVO_TRASLADO~" = ~"MOTIVO_TRASLADO~".~"MOTIVO_TRASLADO~" 
			 and TRUNC(~"GUIA~".~"FEC_REGISTRO~") between :ad_desde and :ad_hasta" arguments=(("ad_desde", date),("ad_hasta", date)) )
