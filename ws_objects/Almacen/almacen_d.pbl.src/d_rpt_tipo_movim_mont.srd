 retrieve="select e.cat_art, e.desc_categoria, trim(f.tipo_mov) ||' - '||f.desc_tipo_mov as movimiento,
       nvl(sum(b.cant_procesada * f.factor_sldo_total),0) as cantidad, 
       nvl(sum(b.cant_procesada * b.precio_unit * f.factor_sldo_total),0) as precio
from vale_mov a, articulo_mov b, articulo c, articulo_sub_categ d, articulo_categ e, 
		articulo_mov_tipo f
where a.nro_vale = b.nro_vale 
  and b.cod_art  = c.cod_art 
  and c.sub_cat_art = d.cod_sub_cat 
  and d.cat_art     = e.cat_art 
  and a.tipo_mov    = f.tipo_mov 
  and a.flag_estado <> '0' 
  and b.flag_estado <> '0'
  and (1 = :an_opc1 or c.cod_clase = :as_clase)
  and (1 = :an_opc2 or a.almacen = :as_alm) 
  and trunc(a.fec_registro) between trunc(:adt_fecini) and trunc(:adt_fecfin)
group by e.cat_art, e.desc_categoria, f.tipo_mov, f.desc_tipo_mov
order by e.cat_art, movimiento" arguments=(("as_clase", string),("an_opc1", number),("as_alm", string),("an_opc2", number),("adt_fecini", date),("adt_fecfin", date))  sort="articulo_categ_cat_art A articulo_categ_desc_categoria A " )
