 retrieve="select asm.cod_art, a.desc_art, a.und, a.cod_clase, ac.desc_clase, 
       asm.precio_prom_dol, asm.compras_totales, asm.valor_comp_total, 
       asm.total_mes_ingr, asm.valor_mes_ingr, asm.consumos_totales, asm.valor_cons_total, 
       asm.total_mes_egr, asm.valor_mes_egr , asm.sldo_total, asm.valor_saldo_total, 
       (asm.valor_saldo_total / :an_tipo_cambio) as valor_saldo_tot_dol,
		 (asm.valor_mes_egr / :an_tipo_cambio) as valor_mes_egr$,
		 (asm.valor_mes_ingr / :an_tipo_cambio) as valor_mes_ing$
from articulo_saldo_mensual asm, articulo a, articulo_clase ac
where (asm.cod_art=a.cod_art) 
  and (a.cod_clase=ac.cod_clase) 
  and asm.ano=:an_ano and asm.mes=:an_mes and asm.almacen=:as_almacen" arguments=(("an_ano", number),("an_mes", number),("as_almacen", string),("an_tipo_cambio", number)) )
