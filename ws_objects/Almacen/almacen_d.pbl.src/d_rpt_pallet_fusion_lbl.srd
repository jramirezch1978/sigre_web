 retrieve="select vm.almacen, 
       am.nro_pallet, 
       am.cod_art,
       a.desc_Art,
       a.und,
       a.und2,
       te.fec_produccion,
       te.fec_empaque,
       te.fec_reproceso,
       sum(am.cant_procesada) as cant_producida, 
       sum(am.cant_proc_und2) as total_caja,
     '' as titulo1,
     'C:\SIGRE\resources\PNG\qr_example.png' as codigo_qr
from vale_mov vm,
     articulo_mov am,
     articulo     a,
     tg_parte_empaque    te,
     tg_parte_empaque_und  teu
where vm.nro_vale  = am.nro_vale
  and am.cod_art   = a.cod_art
  and te.nro_parte   = teu.nro_parte
  and am.cus         = teu.codigo_cu
  and vm.flag_estado <> '0'
  and am.flag_estado <> '0'
  and vm.nro_vale    = :as_nro_vale
  and am.nro_pallet 	= :as_nro_pallet
  and vm.almacen   	= :as_almacen
group by vm.almacen, 
         am.nro_pallet, 
         am.cod_art,
         a.desc_Art,
         a.und,
         a.und2,
         te.fec_produccion,
         te.fec_empaque,
         te.fec_reproceso
--having sum(t.saldo) > 0       
