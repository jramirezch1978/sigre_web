table(column=(type=char(20) updatewhereclause=yes name=especie dbname="especie" )
 column=(type=char(20) updatewhereclause=yes name=cod_art dbname="articulo_cod_art" )
 column=(type=char(20) updatewhereclause=yes name=desc_art dbname="articulo_desc_art" )
 column=(type=char(20) updatewhereclause=yes name=und dbname="articulo_und" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="select a1.cat_art, a1.desc_categoria,
           a2.cod_sub_cat, a2.desc_sub_cat,
           VM.NRO_VALE as nro_vale,
           a.cod_art, a.desc_art,
           al.almacen, al.almacen || '-' || al.desc_almacen as desc_almacen,
           vm.tipo_mov,
           vm.tipo_mov || '-' || amt.desc_tipo_mov as desc_tipo_mov,
           a.und, a.und2,
           amt.flag_contabiliza,
           to_char(vm.fec_registro, 'yyyymm') as periodo,
           to_char(vm.fec_registro, 'YYYY') as año,
           to_char(vm.fec_registro, 'mm') as mes,
           cant_procesada * amt.factor_sldo_total as cantidad,
           cant_proc_und2 * amt.factor_sldo_total as cantidad_und2,
           decode(nvl(amt.FLAG_AJUSTE_VALORIZACION, '0'), '0', am.cant_procesada, 1) * am.precio_unit * amt.factor_sldo_total as importe,
           case
             when instr(a1.desc_categoria, 'ACEITE') > 0 then 'ACEITE DE PESCADO'
             when instr(a1.desc_categoria, 'HARINA') > 0 then 'HARINA RESIDUAL'
             when instr(a1.desc_categoria, 'MERLUZA') > 0 then 'MERLUZA'
             when instr(a1.desc_categoria, 'ANCHOVETA') > 0 then 'ANCHOVETA'
             when instr(a1.desc_categoria, 'CONCHA') > 0 then 'CONCHA DE ABANICO'
             when instr(a1.desc_categoria, 'PERICO') > 0 then 'PERICO'
             when instr(a1.desc_categoria, 'CALAMAR') > 0 then 'CALAMAR'
             when instr(a1.desc_categoria, 'POTA') > 0 then 'POTA'
             when instr(a1.desc_categoria, 'PAICHE') > 0 then 'PAICHE'
             ELSE
               'OTRAS ESPECIES'
           end as especie
    from vale_mov vm,
         articulo_mov am,
         articulo     a,
         articulo_mov_tipo amt,
         articulo_categ    a1,
         articulo_sub_categ a2,
         almacen            al
    where vm.nro_vale = am.nro_vale
      and am.cod_Art  = a.cod_art
      and vm.tipo_mov = amt.tipo_mov
      and a2.cod_sub_cat = a.sub_cat_art
      and a1.cat_art     = a2.cat_art
      and vm.almacen     = al.almacen
      and vm.flag_estado <> '0'
      and am.flag_estado <> '0'
      and a.cod_clase    like :as_clase
      and vm.almacen		 like :as_almacen
      AND trunc(vm.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
union all
    select a1.cat_art, a1.desc_categoria,
           a2.cod_sub_cat, a2.desc_sub_cat,
           VM.NRO_VALE as nro_vale,
           a.cod_art, a.desc_art,
           al.almacen, al.almacen || '-' || al.desc_almacen as desc_almacen,
           'I00',
           'I00' || '-' || 'SALDO ANTERIOR' as desc_tipo_mov,
           a.und, a.und2,
           '0' as flag_contabiliza,
           to_char(vm.fec_registro, 'yyyymm') as periodo,
           to_char(vm.fec_registro, 'YYYY') as año,
           to_char(vm.fec_registro, 'mm') as mes,
           sum(nvl(am.cant_procesada,0) * amt.factor_sldo_total) as cantidad,
           sum(nvl(am.cant_proc_und2,0) * amt.factor_sldo_total) as cantidad_und2,
           sum(decode(nvl(amt.FLAG_AJUSTE_VALORIZACION,'0'), '0', nvl(am.cant_procesada,0), 1)  * am.precio_unit * amt.factor_sldo_total) as importe,
           case
             when instr(a1.desc_categoria, 'ACEITE') > 0 then 'ACEITE DE PESCADO'
             when instr(a1.desc_categoria, 'HARINA') > 0 then 'HARINA RESIDUAL'
             when instr(a1.desc_categoria, 'MERLUZA') > 0 then 'MERLUZA'
             when instr(a1.desc_categoria, 'ANCHOVETA') > 0 then 'ANCHOVETA'
             when instr(a1.desc_categoria, 'CONCHA') > 0 then 'CONCHA DE ABANICO'
             when instr(a1.desc_categoria, 'PERICO') > 0 then 'PERICO'
             when instr(a1.desc_categoria, 'CALAMAR') > 0 then 'CALAMAR'
             when instr(a1.desc_categoria, 'POTA') > 0 then 'POTA'
             when instr(a1.desc_categoria, 'PAICHE') > 0 then 'PAICHE'
             ELSE
               'OTRAS ESPECIES'
           end as especie
    from vale_mov vm,
         articulo_mov am,
         articulo     a,
         articulo_mov_tipo amt,
         articulo_categ    a1,
         articulo_sub_categ a2,
         almacen            al
    where vm.nro_vale = am.nro_vale
      and am.cod_Art  = a.cod_art
      and vm.tipo_mov = amt.tipo_mov
      and a2.cod_sub_cat = a.sub_cat_art
      and a1.cat_art     = a2.cat_art
      and vm.almacen     = al.almacen
      and vm.flag_estado <> '0'
      and am.flag_estado <> '0'
      and a.cod_clase    like :as_clase
		and vm.almacen		 like :as_almacen
      AND trunc(vm.fec_registro) < :ad_fecha1
group by   a1.cat_art, 
           a1.desc_categoria,
           a2.cod_sub_cat, 
           a2.desc_sub_cat,
           VM.NRO_VALE,
           a.cod_art, a.desc_art,
           al.almacen, 
           al.almacen || '-' || al.desc_almacen,
           a.und, a.und2,
           to_char(vm.fec_registro, 'yyyymm'),
           to_char(vm.fec_registro, 'YYYY'),
           to_char(vm.fec_registro, 'mm'),
           case
             when instr(a1.desc_categoria, 'ACEITE') > 0 then 'ACEITE DE PESCADO'
             when instr(a1.desc_categoria, 'HARINA') > 0 then 'HARINA RESIDUAL'
             when instr(a1.desc_categoria, 'MERLUZA') > 0 then 'MERLUZA'
             when instr(a1.desc_categoria, 'ANCHOVETA') > 0 then 'ANCHOVETA'
             when instr(a1.desc_categoria, 'CONCHA') > 0 then 'CONCHA DE ABANICO'
             when instr(a1.desc_categoria, 'PERICO') > 0 then 'PERICO'
             when instr(a1.desc_categoria, 'CALAMAR') > 0 then 'CALAMAR'
             when instr(a1.desc_categoria, 'POTA') > 0 then 'POTA'
             when instr(a1.desc_categoria, 'PAICHE') > 0 then 'PAICHE'
             ELSE
               'OTRAS ESPECIES'
           end      
      
