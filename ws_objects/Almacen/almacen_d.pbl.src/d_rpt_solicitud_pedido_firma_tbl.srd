 retrieve="select sp.*,
       o.nombre as desc_origen,
       fp.desc_forma_pago,
       ua.nombre as nom_usr_solicita,
       ub.nombre as nom_usr_registro,
       m.descripcion as desc_moneda,
       spd.nro_item,
       spd.proveedor, p.nom_proveedor,
       spd.cod_sub_cat,
       spd.almacen, al.desc_almacen,
       spd.cantidad,
       spd.precio_unit,
       spd.nro_pedido_prov
from solicitud_pedido sp,
     solicitud_pedido_det spd,
     origen           o,
     usuario          ua,
     forma_pago       fp,
     usuario          ub,
     proveedor        p,
     almacen          al,
     moneda           m
where sp.nro_solicitud = spd.nro_solicitud 
  and sp.forma_pago   = fp.forma_pago
  and sp.cod_origen   = o.cod_origen
  and sp.usr_solicita = ua.cod_usr
  and sp.create_by    = ub.cod_usr
  and spd.proveedor   = p.proveedor
  and spd.almacen     = al.almacen
  and sp.cod_moneda   = m.cod_moneda
  and sp.nro_solicitud = :as_nro_solicitud" arguments=(("as_nro_solicitud", string)) )
