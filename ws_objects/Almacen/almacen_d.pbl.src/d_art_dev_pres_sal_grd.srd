 retrieve="  SELECT A.NRO_MOV_SAL,   
         B.TIPO_MOV,   
         D.DESC_TIPO_MOV,   
         B.COD_ART,   
         C.DESC_ART,   
         (B.CANT_SALIDA - B.CANT_INGRESO) AS CANTIDAD,
			A.NRO_MOV_ING,
			A.FLAG_REPLICACION 
    FROM ART_DEVOL_PREST_ENLACE  A,   
         ART_DEVOL_PRESTAMO  		B,   
         ARTICULO                C,   
         ARTICULO_MOV_TIPO       D
   WHERE A.NRO_MOV_SAL = B.NRO_MOV 
     and D.TIPO_MOV    = B.TIPO_MOV 
     and B.COD_ART     = C.COD_ART 
     and A.NRO_MOV_ING = :as_nro_mov_ing
     and B.FLAG_ESTADO <> '0'
ORDER BY A.NRO_MOV_SAL ASC   " update="ART_DEVOL_PREST_ENLACE" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_mov_ing", string)) )
