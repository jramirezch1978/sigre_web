 retrieve="select rownum,
       a.cod_art, a.desc_art, a.sldo_por_llegar,
       usf_alm_saldo_xllegar(a.cod_art) as sldo_xllegar_real,
       a.sldo_solicitado, 
       usf_alm_saldo_solic(a.cod_art) as sldo_solic_real,
		 a.Sldo_consignacion,
		 usf_alm_saldo_consig(a.cod_art) as sldo_consig_real,
		 a.sldo_reservado,
		 usf_alm_saldo_reservado(a.cod_art) as sldo_reservado_real
from articulo a
where a.flag_inventariable='1' and
		(  a.sldo_solicitado   <> usf_alm_saldo_solic(a.cod_art)   or 
			a.sldo_por_llegar   <> usf_alm_saldo_xllegar(a.cod_art) or 
			a.sldo_consignacion <> usf_alm_saldo_consig(a.cod_art)  or
			a.sldo_reservado 	  <> usf_alm_saldo_reservado(a.cod_art)  )" update="articulo" updatewhere=1 updatekeyinplace=no )
