 column=(type=number updatewhereclause=yes name=saldo_und2 dbname="saldo_und2" )
 column=(type=number updatewhereclause=yes name=saldo_importe dbname="saldo_importe" )
 retrieve="select vm.almacen,
       al.desc_almacen,
       am.cod_art,
       a.desc_art,
       a.und,
       a.und2,
       am.anaquel,
        am.fila,
        am.columna,
       sum(am.cant_procesada * amt.factor_sldo_total) as saldo_und,
       sum(am.cant_proc_und2 * amt.factor_sldo_total) as saldo_und2,
       sum(
          case
             when amt.flag_ajuste_valorizacion = '1' then
                am.precio_unit * amt.factor_sldo_total
             else
                am.precio_unit * am.cant_procesada * amt.factor_sldo_total
           end 
        ) as saldo_importe
  from vale_mov vm,
       articulo_mov am,
       articulo_mov_tipo amt,
       articulo          a,
       almacen           al
 where vm.nro_vale 		= am.nro_vale
   and vm.tipo_mov 		= amt.tipo_mov
   and vm.almacen  		= al.almacen
   and am.cod_art  		= a.cod_art
   and vm.flag_estado 	<> '0'
   and am.flag_estado 	<> '0'
   and vm.almacen			like :as_almacen
   and trunc(vm.fec_registro) <= trunc(:ad_fecha)
group by vm.almacen,
       al.desc_almacen,
       am.cod_art,
       a.desc_art,
       a.und,
       a.und2,
       am.anaquel,
        am.fila,
        am.columna" arguments=(("ad_fecha", date),("as_almacen", string))  sort="almacen A cod_art A " )
