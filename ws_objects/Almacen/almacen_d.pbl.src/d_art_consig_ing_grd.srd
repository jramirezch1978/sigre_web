 retrieve="  SELECT ~"ART_CONSIGNACION_ENLACE~".~"NRO_MOV_ING~",   
         ~"ARTICULO_CONSIGNACION~".~"TIPO_MOV~",   
         ~"ARTICULO_MOV_TIPO~".~"DESC_TIPO_MOV~",   
         ~"ARTICULO_CONSIGNACION~".~"COD_ART~",   
         ~"ARTICULO~".~"DESC_ART~",   
         NVL(~"ARTICULO_CONSIGNACION~".~"CANTIDAD~", 0) - 
			NVL(~"ARTICULO_CONSIGNACION~".~"CANTIDAD_LIQUIDADA~", 0) as CANTIDAD ,   
         ~"ART_CONSIGNACION_ENLACE~".~"NRO_MOV_SAL~",
  			~"ART_CONSIGNACION_ENLACE~".~"FLAG_REPLICACION~"
    FROM ~"ART_CONSIGNACION_ENLACE~",   
         ~"ARTICULO_CONSIGNACION~",   
         ~"ARTICULO~",   
         ~"ARTICULO_MOV_TIPO~"  
   WHERE ~"ART_CONSIGNACION_ENLACE~".~"NRO_MOV_ING~" = ~"ARTICULO_CONSIGNACION~".~"NRO_MOV~" 
     and ~"ARTICULO_MOV_TIPO~".~"TIPO_MOV~" = ~"ARTICULO_CONSIGNACION~".~"TIPO_MOV~" 
	  and ~"ARTICULO_CONSIGNACION~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" 
	  and ~"ART_CONSIGNACION_ENLACE~".~"NRO_MOV_SAL~" = :as_nro_mov_ing    
ORDER BY ~"ART_CONSIGNACION_ENLACE~".~"NRO_MOV_SAL~" ASC   
" update="ART_CONSIGNACION_ENLACE" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_mov_ing", string)) )
