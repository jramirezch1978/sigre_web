global function string f_rsocial_empresa (string as_empresa);// funcion que devuelve la direccion del codigo relacion
String ls_ruc

select e.nombre
  into :ls_ruc
  from empresa e
  where e.cod_empresa = (select g.cod_empresa from genparam g where reckey='1');



Return ls_ruc

end function

