 retrieve="select cp.tipo_doc, 
       cp.nro_doc, 
       d.nro_detraccion, 
       d.nro_deposito, 
       d.fecha_deposito, 
       dc.sldo_sol,
       dc.saldo_dol,
		 dc.flag_debhab, 
		 cp.origen,
		 dc.cod_moneda,
		 dc.flag_tabla
from cntas_pagar cp,
     detraccion  d,
     doc_pendientes_cta_cte dc
where cp.nro_detraccion = d.nro_detraccion and
      dc.nro_doc        = d.nro_detraccion and
      dc.cod_relacion   = cp.cod_relacion  and
      dc.tipo_doc       = :as_doc_det		 and
		dc.cod_relacion	= :as_codrel		 and
		trim(cp.tipo_doc) || trim(cp.nro_doc) in (:as_documentos)
union  
select cc.tipo_doc, 
       cc.nro_doc, 
       d.nro_detraccion, 
       d.nro_deposito, 
       d.fecha_deposito, 
       dc.sldo_sol,
       dc.saldo_dol,
		 dc.flag_debhab,
		 cc.origen,
		 dc.cod_moneda,
		 dc.flag_tabla
from cntas_cobrar cc,
     detraccion  d,
     doc_pendientes_cta_cte dc
where cc.nro_detraccion = d.nro_detraccion and
      dc.nro_doc        = d.nro_detraccion and
      dc.cod_relacion   = cc.cod_relacion  and
      dc.tipo_doc       = :as_doc_det		 and
		dc.cod_relacion	= :as_codrel	    and
		trim(cc.tipo_doc) || trim(cc.nro_doc) in (:as_documentos)" arguments=(("as_doc_det", string),("as_codrel", string),("as_documentos", stringlist)) )
