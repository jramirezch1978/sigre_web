 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="doc_pendientes_cta_cte.cod_relacion" )
 column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="doc_pendientes_cta_cte.tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="doc_pendientes_cta_cte.nro_doc" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="doc_pendientes_cta_cte.cod_moneda" )
 column=(type=number updatewhereclause=yes name=sldo_sol dbname="doc_pendientes_cta_cte.sldo_sol" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="doc_pendientes_cta_cte.saldo_dol" )
 column=(type=char(1) updatewhereclause=yes name=flag_tabla dbname="doc_pendientes_cta_cte.flag_tabla" )
 column=(type=char(180) updatewhereclause=yes name=observacion dbname="cntas_cobrar.observacion" )
 retrieve="SELECT 	cc.origen,
			dp.cod_relacion,
			dp.tipo_doc,
			dp.nro_doc,
			dp.cod_moneda,
			dp.sldo_sol,
			dp.saldo_dol,
			dp.flag_tabla,
			cc.observacion as observacion
  FROM doc_pendientes_cta_cte dp,
       cntas_cobrar cc
 WHERE dp.cod_relacion = cc.cod_relacion AND
       dp.tipo_doc     = cc.tipo_doc     AND
       dp.nro_doc      = cc.nro_doc      AND
       dp.flag_tabla   = '1'             AND
		 dp.cod_relacion = :as_cod_relacion
UNION
SELECT 	lc.origen,
			dp.cod_relacion,
			dp.tipo_doc,
			dp.nro_doc,
			dp.cod_moneda,
			dp.sldo_sol,
			dp.saldo_dol,
			dp.flag_tabla,
			lc.observacion as observacion 
  FROM doc_pendientes_cta_cte dp,
       letras_x_cobrar lc
 WHERE dp.cod_relacion = lc.cod_relacion AND
       dp.tipo_doc     = (select f.doc_letra_cobrar from finparam f where f.reckey = '1') AND
       dp.nro_doc      = lc.nro_letra    AND
       dp.flag_tabla   = '2'             AND
		 dp.cod_relacion = :as_cod_relacion
UNION
SELECT 	cp.origen,
			dp.cod_relacion,
			dp.tipo_doc,
			dp.nro_doc,
			dp.cod_moneda,
			dp.sldo_sol,
			dp.saldo_dol,
			dp.flag_tabla,
			cp.descripcion as observacion  
  FROM doc_pendientes_cta_cte dp,
       cntas_pagar cp
 WHERE dp.cod_relacion = cp.cod_relacion AND
       dp.tipo_doc     = cp.tipo_doc     AND
       dp.nro_doc      = cp.nro_doc      AND
       dp.flag_tabla   = '3'             AND
		 dp.cod_relacion = :as_cod_relacion 
UNION
SELECT 	lp.origen,
			dp.cod_relacion,
			dp.tipo_doc,
			dp.nro_doc,
			dp.cod_moneda,
			dp.sldo_sol,
			dp.saldo_dol,
			dp.flag_tabla,
			lp.observacion as observacion
  FROM doc_pendientes_cta_cte dp,
       letras_x_pagar lp
 WHERE dp.cod_relacion = lp.cod_relacion AND
       dp.tipo_doc     = (select f.doc_letra_pagar from finparam f where f.reckey = '1') AND
       dp.nro_doc      = lp.nro_letra    AND
       dp.flag_tabla   = '4'             AND
		 dp.cod_relacion = :as_cod_relacion" arguments=(("as_cod_relacion", string)) )
