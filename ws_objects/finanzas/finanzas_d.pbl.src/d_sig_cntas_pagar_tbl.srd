 retrieve="  SELECT ~"TT~".~"ORDEN~"		,
         ~"TT~".~"DESC_GRUPO~" ,   
			Sum(tt.imp_pend_inicial + tt.imp_pag_inicial) as cta_pagar_inicial, 
         Sum(tt.importe_compras) as compras ,
			Sum(tt.importe_pagos)   as pagos   ,   
         Sum( ( (tt.imp_pend_inicial + tt.imp_pag_inicial) + tt.importe_compras) - tt.importe_pagos) as cta_pagar_final   
    FROM ~"TT_SIG_DET_CNTAS_PAGAR~" ~"TT~"
GROUP BY ~"TT~".~"ORDEN~",   
         ~"TT~".~"DESC_GRUPO~"  
ORDER BY ~"TT~".~"ORDEN~" ASC   
" )
