 retrieve="SELECT DP.COD_RELACION, P.NOM_PROVEEDOR, 
		 DECODE(p.ruc, null, p.nro_doc_ident, p.ruc) as ruc,
		 DP.TIPO_DOC, DP.NRO_DOC, 
       DP.CNTA_CTBL, DP.FLAG_TABLA,
		 dp.fecha_doc,
		 dp.fecha_vencimiento, 
		 NVL((SELECT vta_dol_prom FROM calendario WHERE trunc(fecha) = trunc(sysdate)),0) as tasa_cambio,
       CASE WHEN 
                DP.FLAG_DEBHAB = 'D' AND 
                DP.COD_MONEDA = (select l.cod_soles from logparam l where l.reckey = '1') 
                THEN DP.SLDO_SOL ELSE 0 END  as SLDO_SOL_COBRAR,
       CASE WHEN 
                DP.FLAG_DEBHAB = 'H' AND 
                DP.COD_MONEDA = (select l.cod_soles from logparam l where l.reckey = '1') 
                THEN DP.SLDO_SOL ELSE 0 END  as SLDO_SOL_PAGAR,
       CASE WHEN 
                DP.FLAG_DEBHAB = 'D' AND 
                DP.COD_MONEDA <> (select l.cod_soles from logparam l where l.reckey = '1') 
                THEN DP.SALDO_DOL ELSE 0 END  as SLDO_DOL_COBRAR,
       CASE WHEN 
                DP.FLAG_DEBHAB = 'H' AND 
                DP.COD_MONEDA <> (select l.cod_soles from logparam l where l.reckey = '1') 
                THEN DP.SALDO_DOL ELSE 0 END as SLDO_DOL_PAGAR
FROM   DOC_PENDIENTES_CTA_CTE DP, 
		 PROVEEDOR 					P,
		 tt_fin_proveedor 		tp,
		 tt_fin_tipo_doc 			ttd
 WHERE DP.COD_RELACION  = P.PROVEEDOR      
   and DP.COD_RELACION  = tp.cod_proveedor 
	and dp.tipo_doc		 = ttd.tipo_doc     
	and dp.flag_tabla NOT IN ('V', 'X')
ORDER BY DP.COD_RELACION, DP.TIPO_DOC, DP.NRO_DOC"  sort="doc_pendientes_cta_cte_cod_relacion A proveedor_nom_proveedor A ruc A " )
