 retrieve="  SELECT ~"CP_DOC_DET_IMP~".~"COD_RELACION~",   
         ~"CP_DOC_DET_IMP~".~"TIPO_DOC~",   
         ~"CP_DOC_DET_IMP~".~"NRO_DOC~",   
         ~"CP_DOC_DET_IMP~".~"ITEM~",   
         ~"CP_DOC_DET_IMP~".~"TIPO_IMPUESTO~",   
         ~"IMPUESTOS_TIPO~".~"SIGNO~",   
         ~"IMPUESTOS_TIPO~".~"TASA_IMPUESTO~",   
         ~"CP_DOC_DET_IMP~".~"IMPORTE~",   
         ~"IMPUESTOS_TIPO~".~"CNTA_CTBL~",   
         ~"IMPUESTOS_TIPO~".~"DESC_IMPUESTO~",   
         ~"IMPUESTOS_TIPO~".~"FLAG_DH_CXP~",   
         ~"CNTBL_CNTA~".~"DESC_CNTA~",   
         ~"SOLICITUD_GIRO_LIQ_DET~".~"COD_MONEDA~",   
         ~"SOLICITUD_GIRO_LIQ_DET~".~"TASA_CAMBIO~",   
         ~"SOLICITUD_GIRO~".~"COD_MONEDA~" AS COD_MONEDA_CAB  
    FROM ~"CP_DOC_DET_IMP~",   
         ~"IMPUESTOS_TIPO~",   
         ~"SOLICITUD_GIRO_LIQ_DET~",   
         ~"CNTBL_CNTA~",   
         ~"SOLICITUD_GIRO~"  
   WHERE ( ~"CP_DOC_DET_IMP~".~"TIPO_IMPUESTO~" = ~"IMPUESTOS_TIPO~".~"TIPO_IMPUESTO~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"ITEM~" = ~"CP_DOC_DET_IMP~".~"ITEM~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"PROVEEDOR~" = ~"CP_DOC_DET_IMP~".~"COD_RELACION~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"TIPO_DOC~" = ~"CP_DOC_DET_IMP~".~"TIPO_DOC~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"NRO_DOC~" = ~"CP_DOC_DET_IMP~".~"NRO_DOC~" ) and  
         ( ~"IMPUESTOS_TIPO~".~"CNTA_CTBL~" = ~"CNTBL_CNTA~".~"CNTA_CTBL~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"ORIGEN~" = ~"SOLICITUD_GIRO~".~"ORIGEN~" ) and  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"NRO_SOLICITUD~" = ~"SOLICITUD_GIRO~".~"NRO_SOLICITUD~" ) and  
         ( ( ~"SOLICITUD_GIRO_LIQ_DET~".~"ORIGEN~" = :as_origen ) AND  
         ( ~"SOLICITUD_GIRO_LIQ_DET~".~"NRO_SOLICITUD~" = :as_nro_solicitud ) )    
" update="CP_DOC_DET_IMP" updatewhere=1 updatekeyinplace=yes arguments=(("as_origen", string),("as_nro_solicitud", string)) )
