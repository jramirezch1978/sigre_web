 retrieve="  SELECT ~"DOC_REFERENCIAS~".~"COD_RELACION~",   
         ~"DOC_REFERENCIAS~".~"TIPO_DOC~"    ,   
         ~"DOC_REFERENCIAS~".~"NRO_DOC~"     ,   
         ~"DOC_REFERENCIAS~".~"TIPO_MOV~"    ,   
         ~"DOC_REFERENCIAS~".~"ORIGEN_REF~"  ,   
         ~"DOC_REFERENCIAS~".~"TIPO_REF~"    ,   
         ~"DOC_REFERENCIAS~".~"NRO_REF~"     ,   
         ~"DOC_REFERENCIAS~".~"IMPORTE~"     ,   
         ~"DOC_REFERENCIAS~".~"FLAB_TABOR~"  ,   
         ~"VW_FIN_DOC_REF_X_PAG~".~"COD_MONEDA~" AS COD_MONEDA_DET,   
         ~"DOC_TIPO~".~"FLAG_SIGNO~"         ,   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~"     ,   
         ~"CNTAS_PAGAR~".~"COD_MONEDA~"  	  ,
			'OLD' AS FLAG_REF	
    FROM ~"DOC_REFERENCIAS~"      ,    
         ~"VW_FIN_DOC_REF_X_PAG~" ,   
         ~"DOC_TIPO~"             ,   
         ~"CNTAS_PAGAR~"  
   WHERE (( ~"DOC_REFERENCIAS~".~"ORIGEN_REF~"   = ~"VW_FIN_DOC_REF_X_PAG~".~"COD_ORIGEN~" )  AND
			 ( ~"DOC_REFERENCIAS~".~"TIPO_REF~"	   = ~"VW_FIN_DOC_REF_X_PAG~".~"TIPO_DOC~"   )  AND		
          ( ~"DOC_REFERENCIAS~".~"NRO_REF~"      = ~"VW_FIN_DOC_REF_X_PAG~".~"NRO_DOC~"    )) AND  
         (( ~"CNTAS_PAGAR~".~"COD_RELACION~"     = ~"DOC_REFERENCIAS~".~"COD_RELACION~"    )  AND  
          ( ~"CNTAS_PAGAR~".~"TIPO_DOC~"         = ~"DOC_REFERENCIAS~".~"TIPO_DOC~"        )  AND  
          ( ~"CNTAS_PAGAR~".~"NRO_DOC~"          = ~"DOC_REFERENCIAS~".~"NRO_DOC~"         )) AND  
			 ( ~"DOC_REFERENCIAS~".~"TIPO_DOC~"     = ~"DOC_TIPO~".~"TIPO_DOC~"               )  AND  
         (( ~"DOC_REFERENCIAS~".~"TIPO_DOC~"     = :as_tipo_doc                        )  AND  
          ( ~"DOC_REFERENCIAS~".~"NRO_DOC~"      = :as_nro_doc                         )  AND  
          ( ~"DOC_REFERENCIAS~".~"COD_RELACION~" = :as_cod_relacion                    ))    
" update="DOC_REFERENCIAS" updatewhere=1 updatekeyinplace=yes arguments=(("as_tipo_doc", string),("as_nro_doc", string),("as_cod_relacion", string)) )
