 retrieve="  SELECT ~"CNTAS_COBRAR~".~"TIPO_DOC~",   
         ~"CNTAS_COBRAR~".~"NRO_DOC~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~",   
         ~"CNTAS_COBRAR~".~"COD_RELACION~",   
         ~"CODIGO_RELACION~".~"NOMBRE~",   
         ~"CNTAS_COBRAR~".~"COD_MONEDA~",   
         ~"CNTAS_COBRAR_DET~".~"COD_ART~",   
         ~"CNTAS_COBRAR_DET~".~"DESCRIPCION~",   
         ~"CNTAS_COBRAR_DET~".~"CANTIDAD~",   
         ~"CNTAS_COBRAR_DET~".~"PRECIO_UNITARIO~",   
			~"CNTAS_COBRAR_DET~".~"DESCUENTO~",   
			~"CNTAS_COBRAR_DET~".~"REDONDEO_MANUAL~",   
         ~"CC_DOC_DET_IMP~".~"IMPORTE~"  
    FROM ~"CNTAS_COBRAR~",   
         ~"CNTAS_COBRAR_DET~",   
         ~"CODIGO_RELACION~",   
         ~"CC_DOC_DET_IMP~" 
  WHERE (( ~"CNTAS_COBRAR_DET~".~"TIPO_DOC~"   = ~"CNTAS_COBRAR~".~"TIPO_DOC~"        )  and  
         ( ~"CNTAS_COBRAR_DET~".~"NRO_DOC~"    = ~"CNTAS_COBRAR~".~"NRO_DOC~"         )  and  
         ( ~"CNTAS_COBRAR~".~"COD_RELACION~"   = ~"CODIGO_RELACION~".~"COD_RELACION~" )) and  
        (( ~"CNTAS_COBRAR~".~"FLAG_ESTADO~"    <> '0'                             )) and  
        (( ~"CC_DOC_DET_IMP~".~"TIPO_DOC~" (+) = ~"CNTAS_COBRAR_DET~".~"TIPO_DOC~"    )  and  
         ( ~"CC_DOC_DET_IMP~".~"NRO_DOC~"  (+) = ~"CNTAS_COBRAR_DET~".~"NRO_DOC~"     )  and  
         ( ~"CC_DOC_DET_IMP~".~"ITEM~"     (+) = ~"CNTAS_COBRAR_DET~".~"ITEM~"        )) and
        (( ~"CNTAS_COBRAR~".~"ORIGEN~"			 = :as_origen								)) and
		  (( trunc(~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~") between :ad_fecha_inicio AND :ad_fecha_final ))
" arguments=(("ad_fecha_inicio", date),("ad_fecha_final", date),("as_origen", string),("as_empresa", string),("as_user", string))  sort="cntas_cobrar_det_cod_art A fecha_documento A tipo_doc A " )
