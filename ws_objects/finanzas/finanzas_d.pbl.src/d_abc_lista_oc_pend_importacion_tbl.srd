 retrieve="  SELECT distinct OC.COD_ORIGEN       , 
			OC.NRO_OC          	,	   
         OC.FLAG_ESTADO      	, 
			OC.FLAG_COTIZACION 	,   
         OC.FLAG_IMPORTACION 	, 
			OC.PROVEEDOR       	,   
			p.nom_proveedor		,
         OC.NOM_VENDEDOR     	, 
			OC.OBSERVACION     	,   
         OC.FEC_REGISTRO     	, 
			OC.FORMA_PAGO      	,   
         OC.COD_MONEDA       	, 
			OC.MONTO_TOTAL     	,   
         OC.MONTO_FACTURADO  	, 
			OC.COD_USR				,   
         OC.SOL_COD_ORIGEN	 	, 
			OC.NRO_SOL_COMP		,   
         OC.NRO_PROGRAMA		, 
			OC.JOB,
			fp.desc_forma_pago,
		   fp.dias_vencimiento  
    FROM ORDEN_COMPRA  			OC,
			ARTICULO_MOV_PROY		AMP,
			forma_pago				fp,
			proveedor				p
   WHERE OC.NRO_OC       	=  AMP.NRO_DOC                            
	  and oc.proveedor		= p.proveedor
	  AND AMP.TIPO_DOC    	=  (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1')         
	  and oc.forma_pago	 	= fp.forma_pago
	  AND Nvl(AMP.CANT_PROCESADA,0) - Nvl(AMP.CANT_FACTURADA,0) > 0 
--	  AND OC.PROVEEDOR     =  :as_cod_proveedor                                           
	  and oc.flag_importacion	= '1'
" arguments=(("as_cod_proveedor", string))  sort="nro_oc D " )
