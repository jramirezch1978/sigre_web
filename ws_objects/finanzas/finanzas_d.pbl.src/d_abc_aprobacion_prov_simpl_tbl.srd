 retrieve="select a.regkey,
       a.tipo_doc,
       a.serie_cp,
       a.numero_cp,
       a.fec_emision,
       a.cod_usr,
		 a.cod_moneda,
		 u.nombre as generado_por,
		 a.base_imponible,
		 a.impuesto_igv,
		 a.adq_no_grav,
       a.base_imponible + a.impuesto_igv + a.adq_no_grav as total,
       a.fecha_aprob,
       a.usr_aprob,
		 up.nombre as aprobado_por,
       a.proveedor,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       a.concepto,
       a.forma_pago,
		 a.ano,
       a.mes
from fin_provision_simpl a,
     proveedor           p,
	  usuario				 u,
	  usuario				 up
where a.proveedor = p.proveedor
  and a.cod_usr	= u.cod_usr
  and a.usr_aprob = up.cod_usr (+)
  and (a.usr_aprob is null or a.fecha_aprob is null)
  " arguments=(("as_usuario", string))  sort="regkey A " )
