 retrieve="  SELECT ~"TT_FIN_OV_ADEL~".~"COD_RELACION~",   
         ~"TT_FIN_OV_ADEL~".~"TIPO_DOC~"    ,   
         ~"TT_FIN_OV_ADEL~".~"NRO_DOC~"     ,   
         ~"TT_FIN_OV_ADEL~".~"COD_MONEDA~"  ,   
         ~"TT_FIN_OV_ADEL~".~"SALDO_SOL~"   ,   
         ~"TT_FIN_OV_ADEL~".~"SALDO_DOL~"   ,   
         ~"TT_FIN_OV_ADEL~".~"FLAG_PROC~"   ,  
			~"TT_FIN_OV_ADEL~".~"ORIGEN~"      ,
			~"TT_FIN_OV_ADEL~".~"FLAG_TABOR~"	 ,	
         DECODE(tt_fin_ov_adel.flag_debhab,'H','+','-') as flag_cxp,   
			DECODE(tt_fin_ov_adel.cod_moneda,(select cod_soles from logparam where reckey ='1'),tt_fin_ov_adel.saldo_sol,tt_fin_ov_adel.saldo_dol) as saldo,
         DECODE(tt_fin_ov_adel.cod_moneda,(select cod_soles from logparam where reckey =1),null,' ') as flag_mon,
         ~"TT_FIN_OV_ADEL~".~"FLAG_DEBHAB~"  
    FROM ~"TT_FIN_OV_ADEL~"  
   WHERE ~"TT_FIN_OV_ADEL~".~"NRO_DOC~" = :as_nro_ov    
" arguments=(("as_nro_ov", string)) )
