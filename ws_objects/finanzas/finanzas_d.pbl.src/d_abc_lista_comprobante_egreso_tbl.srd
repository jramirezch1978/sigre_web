 retrieve="  SELECT ~"CNTAS_PAGAR~".~"COD_RELACION~",   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~",   
         ~"CNTAS_PAGAR~".~"NRO_DOC~",   
         ~"CNTAS_PAGAR~".~"COD_MONEDA~",   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~",   
         ~"CNTAS_PAGAR~".~"TOTAL_PAGAR~",   
         ~"CNTAS_PAGAR~".~"FECHA_EMISION~",   
         ~"CNTAS_PAGAR_DET~".~"CENCOS~",   
         ~"CNTAS_PAGAR_DET~".~"CNTA_PRSP~",   
         SUBSTR(~"CNTAS_PAGAR~".~"DESCRIPCION~",1,60) AS DESCRIPCION,
         ~"CODIGO_RELACION~".~"NOMBRE~",   
         ~"CNTAS_PAGAR~".~"ORIGEN~"  
    FROM ~"CNTAS_PAGAR~",   
         ~"CODIGO_RELACION~",   
         ~"CNTAS_PAGAR_DET~"  
   WHERE ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~" ) and  
         ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"CNTAS_PAGAR_DET~".~"COD_RELACION~" ) and  
         ( ~"CNTAS_PAGAR~".~"TIPO_DOC~" = ~"CNTAS_PAGAR_DET~".~"TIPO_DOC~" ) and  
         ( ~"CNTAS_PAGAR~".~"NRO_DOC~" = ~"CNTAS_PAGAR_DET~".~"NRO_DOC~" ) and  
         ( ( ~"CNTAS_PAGAR~".~"TIPO_DOC~" = (select comprobante_egr from finparam) ) AND  
         ( ~"CNTAS_PAGAR~".~"FLAG_ESTADO~" in ('1','2','3') ) AND  
         ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = :as_cod_relacion ) )   
GROUP BY ~"CNTAS_PAGAR~".~"COD_RELACION~",   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~",   
         ~"CNTAS_PAGAR~".~"NRO_DOC~",   
         ~"CNTAS_PAGAR~".~"COD_MONEDA~",   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~",   
         ~"CNTAS_PAGAR~".~"TOTAL_PAGAR~",   
         ~"CNTAS_PAGAR~".~"FECHA_EMISION~",   
         ~"CNTAS_PAGAR~".~"ORIGEN~",   
         ~"CNTAS_PAGAR~".~"DESCRIPCION~",   
         ~"CODIGO_RELACION~".~"NOMBRE~",   
         ~"CNTAS_PAGAR_DET~".~"CENCOS~",   
         ~"CNTAS_PAGAR_DET~".~"CNTA_PRSP~"   
" arguments=(("as_cod_relacion", string)) )
