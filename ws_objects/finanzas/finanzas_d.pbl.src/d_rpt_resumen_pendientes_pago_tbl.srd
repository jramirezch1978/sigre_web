table(column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=char(1) updatewhereclause=yes name=tipo_doc_ident dbname="proveedor.tipo_doc_ident" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=number updatewhereclause=yes name=saldo_sol dbname="saldo_sol" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 column=(type=number updatewhereclause=yes name=saldo_conv_sol dbname="saldo_conv_sol" )
 column=(type=number updatewhereclause=yes name=saldo_conv_dol dbname="saldo_conv_dol" )
 retrieve="select p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       sum(
         case 
           when cp.cod_moneda = pkg_logistica.of_soles(null) then
             cp.saldo_sol
           else
             0
         end 
       )as saldo_sol,
       sum(
         case 
           when cp.cod_moneda = pkg_logistica.of_dolares(null) then
             cp.saldo_dol
           else
             0
         end 
       )as saldo_dol,
       sum(
         case 
           when cp.cod_moneda = pkg_logistica.of_soles(null) then
             cp.saldo_sol 
           else
             cp.saldo_dol * cp.tasa_cambio
         end 
       )as saldo_conv_sol,
       sum(
			case 
           when cp.cod_moneda = pkg_logistica.of_dolares(null) then
				 cp.saldo_dol
			  else
             cp.saldo_sol / cp.tasa_cambio
         end 
       )as saldo_conv_dol
from cntas_pagar cp,
     proveedor   p
where cp.cod_relacion = p.proveedor
  and ((cp.cod_moneda = PKG_LOGISTICA.of_soles(null) and cp.saldo_sol > 0) or
       (cp.cod_moneda = PKG_LOGISTICA.of_dolares(null) and cp.saldo_dol > 0) )
  and cp.flag_estado <> '0'
  and (cp.flag_provisionado = 'R' 
   or (cp.flag_provisionado = 'D' and cp.flag_control_reg = '0') 
   or (cp.flag_provisionado = 'D' and cp.flag_control_reg = '1' and cp.flag_caja_bancos = '0'))
  and trunc(cp.fecha_emision) between trunc(:ad_fecha1) and  trunc(:ad_fecha2)
  and cp.origen like :as_origen
group by p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident)   
union
select p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       sum(
         case 
           when cc.cod_moneda = pkg_logistica.of_soles(null) then
             cc.saldo_sol
           else
             0
         end 
       )as saldo_sol,
       sum(
         case 
           when cc.cod_moneda = pkg_logistica.of_dolares(null) then
             cc.saldo_dol
           else
             0
         end 
       )as saldo_dol,
       sum(
         case 
           when cc.cod_moneda = pkg_logistica.of_soles(null) then
             cc.saldo_sol 
           else
             cc.saldo_dol * cc.tasa_cambio
         end 
       )as saldo_conv_sol,
       sum(
         case 
           when cc.cod_moneda = pkg_logistica.of_soles(null) then
             cc.saldo_sol / cc.tasa_cambio
           else
             cc.saldo_dol
         end 
       )as saldo_conv_dol       
from cntas_cobrar cc,
     proveedor     p
where cc.cod_relacion = p.proveedor
  and ((cc.cod_moneda = PKG_LOGISTICA.of_soles(null) and cc.saldo_sol > 0) or
       (cc.cod_moneda = PKG_LOGISTICA.of_dolares(null) and cc.saldo_dol > 0) )
  and cc.flag_estado <> '0'
  and (cc.flag_provisionado = 'D' and cc.flag_control_reg = '1' and cc.flag_caja_bancos = '1')
  and trunc(cc.fecha_documento) between trunc(:ad_fecha1) and  trunc(:ad_fecha2)
  and cc.origen like :as_origen
group by p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident)          " arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_origen", string))  sort="nom_proveedor A " )
