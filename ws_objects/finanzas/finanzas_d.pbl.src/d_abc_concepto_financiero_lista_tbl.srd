 retrieve="SELECT  CF.CONFIN          ,
        CF.DESCRIPCION     ,
        CF.MATRIZ_CNTBL    ,
        M.DESCRIPCION     as desc_matriz_cntbl,
        CF.GRUPO           ,
        cg.descripcion     as desc_grupo,
        CF.COD_FLUJO_CAJA  ,
        fc.descripcion     as desc_flujo_caja,
        CF.FLAG_ESTADO     ,
        CF.COFIN_REVERSO   ,
        CF.FLAG_REPLICACION,
        CF.FLAG_VISUAL_WINDOW
   FROM CONCEPTO_FINANCIERO   CF ,
        MATRIZ_CNTBL_FINAN    M,
        cofin_grupo           cg,
        codigo_flujo_caja     fc
  WHERE CF.MATRIZ_CNTBL = M.MATRIZ    (+)
    and cf.grupo        = cg.grupo    (+)
    and cf.cod_flujo_caja = fc.cod_flujo_caja (+)
order by cf.grupo, cf.confin    
    
  " update="CONCEPTO_FINANCIERO" updatewhere=0 updatekeyinplace=yes )
