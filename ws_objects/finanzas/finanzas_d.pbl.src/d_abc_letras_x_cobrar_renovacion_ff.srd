 retrieve="  SELECT ~"LETRAS_X_COBRAR~".~"NRO_LETRA~",   
         ~"LETRAS_X_COBRAR~".~"NRO_RENOVACION~",   
         ~"LETRAS_X_COBRAR~".~"COD_RELACION~",   
         ~"LETRAS_X_COBRAR~".~"FLAG_ESTADO~",   
         ~"LETRAS_X_COBRAR~".~"FLAG_SITUACION~",   
         ~"LETRAS_X_COBRAR~".~"COD_BANCO~",   
         ~"LETRAS_X_COBRAR~".~"FECHA_REGISTRO~",   
         ~"LETRAS_X_COBRAR~".~"FECHA_EMISION~",   
         ~"LETRAS_X_COBRAR~".~"VENCIMIENTO~",   
         ~"LETRAS_X_COBRAR~".~"COD_MONEDA~",   
         ~"LETRAS_X_COBRAR~".~"TASA_CAMBIO~",   
         ~"LETRAS_X_COBRAR~".~"LT_ORIGINAL~",   
         ~"LETRAS_X_COBRAR~".~"INTERESES~",   
         ~"LETRAS_X_COBRAR~".~"TOTAL_LETRA~",   
         ~"LETRAS_X_COBRAR~".~"TOTAL_PAGADO~",   
         ~"LETRAS_X_COBRAR~".~"OBSERVACION~",   
         ~"LETRAS_X_COBRAR~".~"CONFIN~",   
         ~"LETRAS_X_COBRAR~".~"ORIGEN~",   
			~"LETRAS_X_COBRAR~".~"ANO~",   
			~"LETRAS_X_COBRAR~".~"MES~",   
         ~"LETRAS_X_COBRAR~".~"NRO_LIBRO~",   
         ~"LETRAS_X_COBRAR~".~"NRO_ASIENTO~",   
         ~"LETRAS_X_COBRAR~".~"COD_USR~",  
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CONCEPTO_FINANCIERO~".~"DESCRIPCION~",   
         ~"CONCEPTO_FINANCIERO~".~"MATRIZ_CNTBL~",

  			0 as dias_a_cancelar
    FROM ~"LETRAS_X_COBRAR~",   
         ~"PROVEEDOR~",   
         ~"CONCEPTO_FINANCIERO~"  
   WHERE ( ~"LETRAS_X_COBRAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" ) and  
         ( ~"LETRAS_X_COBRAR~".~"CONFIN~" = ~"CONCEPTO_FINANCIERO~".~"CONFIN~" ) and 
         ( ~"LETRAS_X_COBRAR~".~"NRO_LETRA~" <> ~"LETRAS_X_COBRAR~".~"LT_ORIGINAL~" ) and
         ( ( ~"LETRAS_X_COBRAR~".~"ORIGEN~" = :as_cod_origen ) AND  
         ( ~"LETRAS_X_COBRAR~".~"NRO_LETRA~" = :as_nro_letra ) )    
" update="LETRAS_X_COBRAR" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_origen", string),("as_nro_letra", string)) )
