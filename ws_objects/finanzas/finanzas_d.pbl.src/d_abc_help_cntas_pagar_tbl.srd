 retrieve="SELECT  ~"CNTAS_PAGAR~".~"TIPO_DOC~",~"DOC_TIPO~".~"DESC_TIPO_DOC~"
  FROM  ~"CNTAS_PAGAR~" ,   
        ~"DOC_GRUPO_RELACION~"  ,
        ~"DOC_TIPO~"
 WHERE (~"CNTAS_PAGAR~".~"TIPO_DOC~"      = ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~" )  AND
      (( ~"DOC_GRUPO_RELACION~".~"GRUPO~" = '06'                            )) AND 
       (~"CNTAS_PAGAR~".~"TIPO_DOC~"      = ~"DOC_TIPO~".~"TIPO_DOC~"           ) AND
                (( ~"CNTAS_PAGAR~".~"COD_MONEDA~"   = :as_cod_moneda                                     ))    
GROUP BY ~"CNTAS_PAGAR~".~"TIPO_DOC~",~"DOC_TIPO~".~"DESC_TIPO_DOC~"" arguments=(("as_cod_moneda", string)) )
