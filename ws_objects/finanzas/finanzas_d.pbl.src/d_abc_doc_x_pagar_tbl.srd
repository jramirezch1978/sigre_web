 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="doc_pendientes_cta_cte.nro_doc" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="doc_pendientes_cta_cte.cod_moneda" )
 column=(type=number updatewhereclause=yes name=total_pagar dbname="total_pagar" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="doc_pendientes_cta_cte.cod_relacion" )
 column=(type=char(1) updatewhereclause=yes name=flag_tabla dbname="doc_pendientes_cta_cte.flag_tabla" )
 column=(type=char(1) updatewhereclause=yes name=flag_cxp dbname="flag_cxp" )
 column=(type=number updatewhereclause=yes name=tasa_cambio dbname="vw_fin_doc_general.tasa_cambio" )
 column=(type=char(2) updatewhereclause=yes name=origen dbname="vw_fin_doc_general.origen" )
 retrieve="SELECT dp.tipo_doc     ,        
       dp.nro_doc      , 
       dp.cod_moneda   ,
       DECODE(dp.cod_moneda,(SELECT cod_soles FROM logparam WHERE reckey = '1'),dp.sldo_sol,dp.saldo_dol) AS total_pagar, 
       dp.cod_relacion ,
		 dp.flag_tabla	  ,  
		 Decode(dp.flag_debhab,'H','+','-') as flag_cxp,
		 vw.tasa_cambio  ,
       vw.origen	
  FROM doc_pendientes_cta_cte dp,
		 vw_fin_doc_general		vw
 WHERE ((vw.tipo_doc 	 = dp.tipo_doc		  )  AND
		  (vw.nro_doc  	 = dp.nro_doc		  )  AND
		  (vw.cod_relacion = dp.cod_relacion  )) AND	
		  (dp.cod_relacion = :as_cod_relacion )  AND
		  (dp.tipo_doc		<> (SELECT doc_sol_giro FROM finparam WHERE reckey = '1' ))	
UNION
SELECT (SELECT doc_sol_giro FROM finparam WHERE reckey = '1' ) as tipo_doc ,
       sg.nro_solicitud  ,
       sg.cod_moneda     ,
       sg.importe        ,
       sg.cod_relacion   ,
       '6'   as flag_tabla  ,
		 '+'   as flag_cxp	 ,
		 0.000 as tasa_cambio ,
		 sg.origen	 
 FROM  solicitud_giro sg
WHERE (sg.cod_relacion = :as_cod_relacion )	AND
		(sg.flag_estado  = '2')
UNION
SELECT dp.tipo_doc     ,        
       dp.nro_doc      , 
       dp.cod_moneda   ,
       DECODE(dp.cod_moneda,(SELECT cod_soles FROM logparam WHERE reckey = '1'),dp.sldo_sol,dp.saldo_dol) AS total_pagar, 
       dp.cod_relacion ,
 		 dp.flag_tabla	  ,  
		 Decode(dp.flag_debhab,'H','+','-') as flag_cxp,
		 vw.tasa_cambio  ,
       vw.origen	
  FROM doc_pendientes_cta_cte dp,
	    vw_fin_doc_general		vw
 WHERE ((vw.tipo_doc 	 = dp.tipo_doc		  )  AND
		  (vw.nro_doc  	 = dp.nro_doc		  )  AND
		  (vw.cod_relacion = dp.cod_relacion  )) AND	
		  (dp.cod_relacion = :as_cod_relacion )  AND
		  (dp.tipo_doc		 = (SELECT doc_sol_giro FROM finparam WHERE reckey = '1' ))AND
        (vw.flag_estado  = '5'              )	" arguments=(("as_cod_relacion", string)) )
