table(column=(type=number updatewhereclause=yes name=ingresos dbname="ingresos" )
 column=(type=number updatewhereclause=yes name=egresos dbname="egresos" )
 column=(type=number updatewhereclause=yes name=saldo_anterior dbname="saldo_anterior" )
 retrieve="select nvl(sum(ingresos), 0) as ingresos,
       nvl(sum(egresos), 0) as egresos,
		 PKG_SIGRE_FINANZAS.of_fin_caja_saldo_anterior(:ad_fecha1, :as_origen, :as_moneda) as saldo_anterior
from (
		select 	af.orden as orden1,
         		af.desc_actividad,  
					gc.orden as orden2,
					fc.orden as orden3,
					gc.factor,
					case 
						when af.flag_tipo_flujo = 'E' then 'FLUJO ECONOMICO'
						when af.flag_tipo_flujo = 'F' then 'FLUJO FINANCIERO'
					END AS TIPO_FLUJO,
					case 
						when gc.factor = 'I' then 
							'01.Ingresos' 
						else 
							'02.Egresos'
					end as flag_ingr_egr,
					case 
						when gc.factor = 'I' then
							case 
								when cbd.cod_moneda = :as_moneda then 
									cbd.importe
								when cbd.cod_moneda = PKG_LOGISTICA.of_soles(null) then 
									cbd.importe / cb.tasa_cambio
								else
									cbd.importe * cb.tasa_cambio
							end - 
							-- Disminuyo la retención del IGV
							case
								when :as_moneda = PKG_LOGISTICA.of_soles(null) then
									nvl(cbd.impt_ret_igv, 0)
								else
									nvl(cbd.impt_ret_igv, 0) / cb.tasa_cambio
							end
						else
							0
					end * cbd.factor as ingresos,
					case 
						when gc.factor = 'E' then
							case 
								when cbd.cod_moneda = :as_moneda then 
									cbd.importe
								when cbd.cod_moneda = PKG_LOGISTICA.of_soles(null) then 
									cbd.importe / cb.tasa_cambio
								else
									cbd.importe * cb.tasa_cambio
							end - 
							-- Disminuyo la retención del IGV
							case
								when :as_moneda = PKG_LOGISTICA.of_soles(null) then
									nvl(cbd.impt_ret_igv, 0)
								else
									nvl(cbd.impt_ret_igv, 0) / cb.tasa_cambio
							end
						else
							0
					end * cbd.factor as egresos
		
			 from caja_bancos         		cb,
					caja_bancos_det     		cbd,
					codigo_flujo_caja     	fc,
					grupo_cod_flujo_caja   	gc,
				  fin_actividad_flujo  		af,
				  banco_cnta        			bc
			 where cb.origen         		= cbd.origen  
				and cb.nro_registro     	= cbd.nro_registro
				and cbd.cod_flujo_caja   	= fc.cod_flujo_caja 
				and fc.grp_flujo_caja    	= gc.grp_flujo_caja 
				and gc.cod_actividad     	= af.cod_actividad
				and cb.cod_ctabco      		= bc.cod_ctabco
				and cb.flag_estado      	<> '0'
				and bc.flag_flujo_caja  	= '1'
				and trunc(cb.fecha_emision) between :ad_fecha1 and :ad_fecha2
				and cb.origen          like :as_origen
	) s
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_origen", string),("as_moneda", string)) )
