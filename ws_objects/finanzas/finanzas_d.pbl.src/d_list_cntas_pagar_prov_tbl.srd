 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="cntas_pagar.flag_estado" dbalias="cp.flag_estado" values="Generado	1/Pagado Parcial	2/Pagado Total	3/Anulado	0/" )
 retrieve="select 	cp.cod_relacion, 
		 	cp.tipo_doc,
       	cp.nro_doc,
		 	p.nom_proveedor, 
       	decode(p.ruc, null, p.nro_doc_ident, p.ruc) as ruc_dni,
       	cp.fecha_registro,
       	cp.fecha_emision,
		 	cp.vencimiento,
       	cp.tasa_cambio,
       	trim(cp.origen) || trim(to_char(cp.ano, '0000')) || trim(to_char(cp.mes, '00')) || trim(to_char(cp.nro_libro, '00')) || trim(to_char(cp.nro_asiento, '000000')) as voucher,
       	cp.descripcion,
       	cp.cod_moneda,
       	cp.importe_doc,
			cp.ano, cp.mes,
			(select nvl(sum(f.importe),0)
				from flujo_caja_proy f
			  where f.proveedor_cxp = cp.cod_relacion
				 and f.tipo_doc_cxp  = cp.tipo_doc
				 and f.nro_doc_cxp	  = cp.nro_doc) as imp_asignado,
			case 
				when cp.cod_moneda = (select cod_soles from logparam where reckey = '1') then
					cp.saldo_sol - (select nvl(sum(f.importe),0)
										   from flujo_caja_proy f
									 	  where f.proveedor_cxp = cp.cod_relacion
											 and f.tipo_doc_cxp  = cp.tipo_doc
											 and f.nro_doc_cxp	  = cp.nro_doc)
				when cp.cod_moneda = (select cod_dolares from logparam where reckey = '1') then
					cp.saldo_dol - (select nvl(sum(f.importe),0)
										   from flujo_caja_proy f
									 	  where f.proveedor_cxp = cp.cod_relacion
											 and f.tipo_doc_cxp  = cp.tipo_doc
											 and f.nro_doc_cxp	  = cp.nro_doc)
			end as imp_pendiente,
		 	decode(cp.cod_moneda, (select cod_soles from logparam where reckey = '1'),cp.saldo_sol - (select nvl(sum(f.importe),0)
										  from flujo_caja_proy f
										 where f.proveedor_cxp = cp.cod_relacion
											and f.tipo_doc_cxp  = cp.tipo_doc
											and f.nro_doc_cxp	  = cp.nro_doc)) * dt.factor * -1 as pendiente_soles,
		 	decode(cp.cod_moneda, (select cod_dolares from logparam where reckey = '1'),cp.saldo_dol - (select nvl(sum(f.importe),0)
										  from flujo_caja_proy f
										 where f.proveedor_cxp = cp.cod_relacion
											and f.tipo_doc_cxp  = cp.tipo_doc
											and f.nro_doc_cxp	  = cp.nro_doc)) * dt.factor * -1 as pendiente_dolares,
			cp.saldo_sol,
			cp.saldo_dol,
       	cp.flag_estado
from cntas_pagar 	cp,
     proveedor   	p,
	  doc_tipo		dt
where cp.cod_relacion 	= p.proveedor
  and cp.tipo_doc			= dt.tipo_doc
  and cp.flag_provisionado = 'R'
  and ((cp.cod_moneda = (select cod_soles from logparam where reckey  = '1') and cp.saldo_sol > 0) 
    or (cp.cod_moneda = (select cod_dolares from logparam where reckey  = '1') and cp.saldo_dol > 0))
  and ((:as_flag = '1' and trunc(cp.fecha_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)) or
		 (:as_flag = '2' and trunc(cp.fecha_emision) between trunc(:ad_fecha1) and trunc(:ad_fecha2)) or
		  :as_flag = '3')
  and cp.flag_estado not in ('0', '4')
      " arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_flag", string)) )
