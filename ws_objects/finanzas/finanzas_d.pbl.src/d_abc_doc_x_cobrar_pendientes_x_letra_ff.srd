 retrieve="SELECT   cc.origen     ,   
         cc.tipo_doc   ,   
         cc.nro_doc    ,   
			DECODE(dp.flag_tabla,'1','C','2','C','3','P','4','P','6','P') as tipo_mov,
         dp.flag_tabla , 
			dp.cod_moneda ,
         DECODE(dp.cod_moneda,(SELECT cod_soles FROM logparam where reckey = '1'),dp.sldo_sol,dp.saldo_dol) AS SALDOS,
			dt.flag_signo ,	
         dp.sldo_sol   ,   
         dp.saldo_dol  
	
    FROM doc_pendientes_cta_cte dp,
			cntas_cobrar  			  cc,	
			cntas_cobrar_det		  ccd,
			doc_tipo					  dt	 
	WHERE ((cc.tipo_doc 		= ccd.tipo_doc    )  AND
			 (cc.nro_doc  		= ccd.nro_doc     )) AND 
			((cc.cod_relacion = dp.cod_relacion )	AND
 			 (cc.tipo_doc		= dp.tipo_doc		)	AND
			 (cc.nro_doc		= dp.nro_doc		))	AND
			 (cc.tipo_doc		= dt.tipo_doc		)  AND
			((ccd.tipo_ref		= :as_tipo_doc		)  AND
			 (ccd.nro_ref		= :as_nro_doc		))	" arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
