 retrieve="  SELECT ~"ORDEN_COMPRA~".~"COD_ORIGEN~"       , ~"ORDEN_COMPRA~".~"NRO_OC~"          ,   
         ~"ORDEN_COMPRA~".~"FLAG_ESTADO~"      , ~"ORDEN_COMPRA~".~"FLAG_COTIZACION~" ,   
         ~"ORDEN_COMPRA~".~"FLAG_IMPORTACION~" , ~"ORDEN_COMPRA~".~"PROVEEDOR~"       ,   
         ~"ORDEN_COMPRA~".~"NOM_VENDEDOR~"     , ~"ORDEN_COMPRA~".~"OBSERVACION~"     ,   
         ~"ORDEN_COMPRA~".~"FEC_REGISTRO~"     , ~"ORDEN_COMPRA~".~"FORMA_PAGO~"      ,   
         ~"ORDEN_COMPRA~".~"COD_MONEDA~"       , ~"ORDEN_COMPRA~".~"MONTO_TOTAL~"     ,   
         ~"ORDEN_COMPRA~".~"MONTO_FACTURADO~"  , ~"ORDEN_COMPRA~".~"COD_USR~"			,   
         ~"ORDEN_COMPRA~".~"SOL_COD_ORIGEN~"	 , ~"ORDEN_COMPRA~".~"NRO_SOL_COMP~"		,   
         ~"ORDEN_COMPRA~".~"NRO_PROGRAMA~"		 , ~"ORDEN_COMPRA~".~"JOB~"  
    FROM ~"ORDEN_COMPRA~"  ,~"ARTICULO_MOV_PROY~"
   WHERE ( ~"ORDEN_COMPRA~".~"NRO_OC~"           =  ~"ARTICULO_MOV_PROY~".~"NRO_DOC~"                            ) AND
         ( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~"    =  (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1')         ) AND
         ( Nvl(~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~",0) - Nvl(~"ARTICULO_MOV_PROY~".~"CANT_FACTURADA~",0) > 0 ) AND
         ( ~"ORDEN_COMPRA~".~"PROVEEDOR~"     =  :as_cod_proveedor                                           ) 
GROUP BY ~"ORDEN_COMPRA~".~"COD_ORIGEN~",   
         ~"ORDEN_COMPRA~".~"NRO_OC~",   
         ~"ORDEN_COMPRA~".~"FLAG_ESTADO~",   
         ~"ORDEN_COMPRA~".~"FLAG_COTIZACION~",   
         ~"ORDEN_COMPRA~".~"FLAG_IMPORTACION~",   
         ~"ORDEN_COMPRA~".~"PROVEEDOR~",   
         ~"ORDEN_COMPRA~".~"NOM_VENDEDOR~",   
         ~"ORDEN_COMPRA~".~"OBSERVACION~",   
         ~"ORDEN_COMPRA~".~"FEC_REGISTRO~",   
         ~"ORDEN_COMPRA~".~"FORMA_PAGO~",   
         ~"ORDEN_COMPRA~".~"COD_MONEDA~",   
         ~"ORDEN_COMPRA~".~"MONTO_TOTAL~",   
         ~"ORDEN_COMPRA~".~"MONTO_FACTURADO~",   
         ~"ORDEN_COMPRA~".~"COD_USR~",   
         ~"ORDEN_COMPRA~".~"SOL_COD_ORIGEN~",   
         ~"ORDEN_COMPRA~".~"NRO_SOL_COMP~",   
         ~"ORDEN_COMPRA~".~"NRO_PROGRAMA~",   
         ~"ORDEN_COMPRA~".~"JOB~"  " arguments=(("as_cod_proveedor", string)) )
