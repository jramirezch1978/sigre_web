 retrieve="  SELECT ~"FINANCIAMIENTO_DET~".~"ORIGEN~",   
         ~"FINANCIAMIENTO_DET~".~"NUM_REGISTRO~",   
         ~"FINANCIAMIENTO_DET~".~"NUM_CUOTA~",   
         ~"FINANCIAMIENTO_DET~".~"CONFIN~",   
         ~"FINANCIAMIENTO_DET~".~"IMP_CUOTA_CAPITAL~",   
         ~"FINANCIAMIENTO_DET~".~"TASA_INTERES~",   
         ~"FINANCIAMIENTO_DET~".~"FLAG_TASA_INTERES~",   
         ~"FINANCIAMIENTO_DET~".~"IMP_CUOTA_INTERES~",   
         ~"FINANCIAMIENTO_DET~".~"FECHA_VCTO~",   
         ~"FINANCIAMIENTO_DET~".~"TIPO_IMPUESTO~",   
         ~"FINANCIAMIENTO_DET~".~"IMP_IMPUESTO~",   
			~"FINANCIAMIENTO_DET~".~"TIPO_DOC~",
			~"FINANCIAMIENTO_DET~".~"NRO_DOC~",
         ~"FINANCIAMIENTO_DET~".~"FECHA_PAGO_INT~",   
         ~"FINANCIAMIENTO_DET~".~"FLAG_ESTADO~",   
         ~"CONCEPTO_FINANCIERO~".~"MATRIZ_CNTBL~",
         ~"IMPUESTOS_TIPO~".~"SIGNO~"  
    FROM ~"FINANCIAMIENTO_DET~",   
         ~"CONCEPTO_FINANCIERO~",   
         ~"IMPUESTOS_TIPO~"  
   WHERE ( financiamiento_det.tipo_impuesto 		= impuestos_tipo.tipo_impuesto (+)) AND
         ( ~"FINANCIAMIENTO_DET~".~"CONFIN~" 			= ~"CONCEPTO_FINANCIERO~".~"CONFIN~"  ) AND   
			((~"FINANCIAMIENTO_DET~".~"ORIGEN~" 			= :as_cod_origen					    ) AND
			 (~"FINANCIAMIENTO_DET~".~"NUM_REGISTRO~"	= :as_num_registro					 ))" update="FINANCIAMIENTO_DET" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_origen", string),("as_num_registro", string)) )
