 retrieve="  SELECT ~"CNTAS_COBRAR~".~"FECHA_VENCIMIENTO~",   
         ~"CNTAS_COBRAR~".~"TIPO_DOC~",   
         ~"CNTAS_COBRAR~".~"NRO_DOC~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~",   
         ~"CNTAS_COBRAR~".~"FECHA_PRESENTACION~",   
         ~"CNTAS_COBRAR~".~"COD_RELACION~",   
			rtrim(ltrim(~"CNTAS_COBRAR~".~"OBSERVACION~")),   
         ~"CNTAS_COBRAR~".~"COD_MONEDA~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_A_COBRAR~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_COBRADO~",   
         ~"CODIGO_RELACION~".~"NOMBRE~"  ,
			DECODE(~"CNTAS_COBRAR~".~"TIPO_DOC~",(SELECT NOTA_CREDITO FROM FINPARAM WHERE RECKEY = '1'),-~"CNTAS_COBRAR~".~"IMPORTE_A_COBRAR~",~"CNTAS_COBRAR~".~"IMPORTE_A_COBRAR~") AS IMP_COBRAR,
			DECODE(~"CNTAS_COBRAR~".~"TIPO_DOC~",(SELECT NOTA_CREDITO FROM FINPARAM WHERE RECKEY = '1'),-~"CNTAS_COBRAR~".~"IMPORTE_COBRADO~",~"CNTAS_COBRAR~".~"IMPORTE_COBRADO~") AS IMP_COBRADO,
			(SELECT SYSDATE FROM DUAL) AS FECHA_ACTUAL
    FROM ~"CNTAS_COBRAR~",   
         ~"CODIGO_RELACION~",  
			~"TT_FIN_ORIGENES~"
   WHERE ( ~"CNTAS_COBRAR~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~" )    AND
			( ~"CNTAS_COBRAR~".~"FLAG_ESTADO~" IN ('1','4') )   AND
			( ~"CNTAS_COBRAR~".~"ORIGEN~"      = ~"TT_FIN_ORIGENES~".~"COD_ORIGEN~" )

