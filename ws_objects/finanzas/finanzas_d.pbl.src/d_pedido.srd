 retrieve="SELECT OV.FEC_REGISTRO   ,OV.NRO_OV   ,VM.FEC_REGISTRO ,AM.NRO_VALE ,G.FEC_REGISTRO AS FECHA_GUIA,GV.NRO_GUIA,
       CC.FECHA_DOCUMENTO,CCD.TIPO_DOC,CCD.NRO_DOC     ,TRIM(CC.ORIGEN)||TRIM(TO_CHAR(CC.ANO))||TRIM(TO_CHAR(CC.MES))||TRIM(TO_CHAR(CC.NRO_LIBRO))||TRIM(TO_CHAR(CC.NRO_ASIENTO)) AS ASIENTO_CCOB,
       CB.FECHA_EMISION,TRIM(CB.ORIGEN)||TRIM(TO_CHAR(CB.ANO))||TRIM(TO_CHAR(CB.MES))||TRIM(TO_CHAR(CB.NRO_LIBRO))||TRIM(TO_CHAR(CB.NRO_ASIENTO)) AS ASIENTO_BANCO,
       TRIM(PCD.NRO_PLANILLA)||'-'||TRIM(TO_CHAR(PCD.ITEM_DEPOSITO)) AS PLANILLA
  FROM ORDEN_VENTA OV     ,ARTICULO_MOV_PROY AMP ,ARTICULO_MOV     AM  ,VALE_MOV     VM,
       GUIA G             ,GUIA_VALE         GV  ,CNTAS_COBRAR_DET CCD ,CNTAS_COBRAR CC,
       CAJA_BANCOS_DET CBD,CAJA_BANCOS       CB  ,PLN_COBRANZA_DEP PCD
 WHERE (TRIM(OV.NRO_OV)   = TRIM(AMP.NRO_DOC)  ) AND
       (AMP.TIPO_DOC      = (SELECT L.DOC_OV FROM LOGPARAM L WHERE L.RECKEY = '1')) AND
      ((AMP.COD_ORIGEN    = AM.ORIGEN_MOV_PROY    (+) )  AND
       (AMP.NRO_MOV       = AM.NRO_MOV_PROY       (+) )) AND
      ((AM.NRO_VALE       = VM.NRO_VALE           (+) )) AND 
      ((VM.COD_ORIGEN     = GV.ORIGEN_VALE        (+) )  AND
       (VM.NRO_VALE       = GV.NRO_VALE           (+) )) AND
      ((GV.ORIGEN_GUIA    = G.COD_ORIGEN          (+) )  AND
       (GV.NRO_GUIA       = G.NRO_GUIA            (+) )) AND
      ((OV.NRO_OV         = CCD.NRO_REF           (+) )) AND
      ((CCD.TIPO_DOC      = CC.TIPO_DOC           (+) )  AND
       (CCD.NRO_DOC       = CC.NRO_DOC            (+) )) AND
      ((CC.TIPO_DOC       = CBD.TIPO_DOC          (+) )  AND
       (CC.NRO_DOC        = CBD.NRO_DOC           (+) )  AND
       (CC.COD_RELACION   = CBD.COD_RELACION      (+) )) AND
      ((CBD.ORIGEN        = CB.ORIGEN             (+) )  AND
       (CBD.NRO_REGISTRO  = CB.NRO_REGISTRO       (+) )) AND
      ((CB.ORIGEN         = PCD.ORG_CAJA_BANC     (+) )  AND
       (CB.NRO_REGISTRO   = PCD.NRO_REG_CAJA_BANC (+) )) AND
      ((TO_CHAR(OV.FEC_REGISTRO,'yyyymmdd') >= :as_fecha_inicio )  AND
		 (TO_CHAR(OV.FEC_REGISTRO,'yyyymmdd') <= :as_fecha_final  ))
      
GROUP BY OV.FEC_REGISTRO   ,OV.NRO_OV   ,VM.FEC_REGISTRO ,AM.NRO_VALE ,G.FEC_REGISTRO ,GV.NRO_GUIA,
       CC.FECHA_DOCUMENTO,CCD.TIPO_DOC,CCD.NRO_DOC     ,TRIM(CC.ORIGEN)||TRIM(TO_CHAR(CC.ANO))||TRIM(TO_CHAR(CC.MES))||TRIM(TO_CHAR(CC.NRO_LIBRO))||TRIM(TO_CHAR(CC.NRO_ASIENTO)) ,
       CB.FECHA_EMISION,TRIM(CB.ORIGEN)||TRIM(TO_CHAR(CB.ANO))||TRIM(TO_CHAR(CB.MES))||TRIM(TO_CHAR(CB.NRO_LIBRO))||TRIM(TO_CHAR(CB.NRO_ASIENTO)) ,
       TRIM(PCD.NRO_PLANILLA)||'-'||TRIM(TO_CHAR(PCD.ITEM_DEPOSITO)) 
" arguments=(("as_fecha_inicio", string),("as_fecha_final", string))  sort="orden_venta_fec_registro A orden_venta_nro_ov A " )
