 column=(type=char(1) updatewhereclause=yes name=flag_debhab_inv dbname="flag_debhab_inv" )
 column=(type=number updatewhereclause=yes name=total_pagar dbname="total_pagar" )
 column=(type=char(1) updatewhereclause=yes name=flag_cxp dbname="flag_cxp" )
 retrieve="  SELECT  vw.origen       ,
          dp.cod_relacion ,   
			 dp.tipo_doc     ,   
			 dp.nro_doc      ,   
			 dp.flag_tabla   ,   
			 dp.cnta_ctbl    ,   
			 dp.cod_moneda   ,   
			 dp.flag_debhab  ,   
			 dp.sldo_sol     ,   
		    dp.saldo_dol    ,    
			 DECODE(dp.flag_debhab,'D','H','D') AS FLAG_DEBHAB_INV , 
		    DECODE(dp.cod_moneda,(SELECT cod_soles FROM logparam WHERE reckey = '1'),dp.sldo_sol,dp.saldo_dol) AS total_pagar, 
			 DECODE(dp.flag_debhab,'H','+','-') as flag_cxp
    FROM  doc_pendientes_cta_cte dp,
			 vw_fin_doc_general		vw
   WHERE ( dp.tipo_doc <> (select doc_sol_giro from finparam where reckey = '1') ) AND  
			((vw.tipo_doc     = dp.tipo_doc      )  AND
			 (vw.nro_doc		= dp.nro_doc		 )  AND
          (vw.cod_relacion = dp.cod_relacion  )) AND
         ( dp.cod_relacion = :as_cod_relacion )    
" arguments=(("as_cod_relacion", string)) )
