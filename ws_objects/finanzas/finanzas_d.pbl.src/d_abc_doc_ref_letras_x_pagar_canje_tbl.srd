 retrieve="  SELECT ~"DOC_REFERENCIAS~".~"COD_RELACION~",   
         ~"DOC_REFERENCIAS~".~"TIPO_DOC~"    ,   
         ~"DOC_REFERENCIAS~".~"NRO_DOC~"     ,   
         ~"DOC_REFERENCIAS~".~"TIPO_MOV~"    ,   
         ~"DOC_REFERENCIAS~".~"ORIGEN_REF~"  ,   
         ~"DOC_REFERENCIAS~".~"TIPO_REF~"    ,   
         ~"DOC_REFERENCIAS~".~"NRO_REF~"     ,   
         ~"DOC_REFERENCIAS~".~"IMPORTE~"     ,   
         ~"DOC_REFERENCIAS~".~"FLAB_TABOR~"  ,   
		   ~"LETRAS_X_PAGAR~".~"TASA_CAMBIO~"  ,   
         ~"LETRAS_X_PAGAR~".~"COD_MONEDA~"     AS COD_MONEDA_CAB ,   
         ~"VW_FIN_DOC_X_PAGAR~".~"COD_MONEDA~" AS COD_MONEDA_DET ,
			~"DOC_REFERENCIAS~".~"FLAG_RET_IGV~",
			~"DOC_REFERENCIAS~".~"IMP_RET_IGV~",
			NULL AS FLAG_IMPUESTO,
			~"DOC_TIPO~".~"FLAG_SIGNO~"
    FROM ~"DOC_REFERENCIAS~"    ,   
         ~"LETRAS_X_PAGAR~"     ,   
         ~"VW_FIN_DOC_X_PAGAR~" ,
			~"DOC_TIPO~"			   
   WHERE (( ~"DOC_REFERENCIAS~".~"COD_RELACION~"  = ~"LETRAS_X_PAGAR~".~"COD_RELACION~"     )  AND  
         (  ~"DOC_REFERENCIAS~".~"NRO_DOC~"       = ~"LETRAS_X_PAGAR~".~"NRO_LETRA~"        )) AND  
         (( ~"DOC_REFERENCIAS~".~"TIPO_REF~"      = ~"VW_FIN_DOC_X_PAGAR~".~"TIPO_DOC~"     )  AND  
         (  ~"DOC_REFERENCIAS~".~"NRO_REF~"       = ~"VW_FIN_DOC_X_PAGAR~".~"NRO_DOC~"      )  AND  
         (  ~"DOC_REFERENCIAS~".~"COD_RELACION~"  = ~"VW_FIN_DOC_X_PAGAR~".~"COD_RELACION~" )) AND  
			(( ~"DOC_REFERENCIAS~".~"TIPO_DOC~"		 = ~"DOC_TIPO~".~"TIPO_DOC~"					)) AND
         (( ~"DOC_REFERENCIAS~".~"TIPO_DOC~"      = :as_tipo_doc                        )  AND  
         (  ~"DOC_REFERENCIAS~".~"NRO_DOC~"       = :as_nro_doc                         )  AND  
         (  ~"DOC_REFERENCIAS~".~"COD_RELACION~"  = :as_cod_relacion                    )  AND  
         (  ~"LETRAS_X_PAGAR~".~"NRO_RENOVACION~" = 0                                   ))    
" update="DOC_REFERENCIAS" updatewhere=1 updatekeyinplace=yes arguments=(("as_tipo_doc", string),("as_nro_doc", string),("as_cod_relacion", string)) )
