 retrieve=" select cp.cod_relacion ,
		  p.nom_proveedor	,
		  p.flag_ret_igv,	
 		  cp.fecha_emision ,	
		  cp.tipo_doc	  ,
		  cp.nro_doc		  ,
		  dp.cnta_ctbl	  ,
		  dp.flag_debhab  ,
		  dp.sldo_sol	  ,
        dp.saldo_dol	  ,
		  cp.cod_usr		  ,
		  cp.cod_moneda,
		  cp.importe_doc,	
		 (select RTRIM(LTRIM(cb.origen))||RTRIM(LTRIM(TO_CHAR(cb.ano)))||RTRIM(LTRIM(TO_CHAR(cb.mes,'09')))||RTRIM(LTRIM(TO_CHAR(cb.nro_libro)))||RTRIM(LTRIM(TO_CHAR(cb.nro_asiento))) AS VOUCHER  from caja_bancos cb,caja_bancos_det cbd  
         where (cb.origen        = cbd.origen       ) and
               (cb.nro_registro  = cbd.nro_registro ) and
               (cb.flag_estado	= '1'					 ) and
					(cbd.factor       = 1                ) and
					
               (cb.nro_libro     in (select f.libro_pagos from finparam f where f.reckey = '1')) and
               (cbd.cod_relacion = cp.cod_relacion       ) and
               (cbd.tipo_doc     = cp.tipo_doc           ) and
               (cbd.nro_doc      = cp.nro_doc     			)) as voucher,
		 USF_FIN_REFER_PAGAR(cp.cod_relacion,cp.tipo_doc,cp.nro_doc) as referencia	 
  from cntas_pagar cp,doc_pendientes_cta_cte dp,proveedor p
 where (cp.cod_relacion		= p.proveedor			) and
		 (cp.cod_relacion    = dp.cod_relacion (+)) and
       (cp.tipo_doc        = dp.tipo_doc     (+)) and
       (cp.nro_doc         = dp.nro_doc      (+)) and
       (cp.tipo_doc in (select f.doc_anticipo_oc from finparam f where reckey = '1'
                        union
                        select f.doc_anticipo_os from finparam f where reckey = '1' )) "  sort="fecha_emision D " )
