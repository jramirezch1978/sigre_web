 retrieve="  SELECT ~"ORDEN_COMPRA~".~"COD_ORIGEN~",   
         ~"ORDEN_COMPRA~".~"NRO_OC~",   
         ~"ORDEN_COMPRA~".~"MONTO_FACTURADO~",   
         ~"ORDEN_COMPRA~".~"COD_MONEDA~",   
         ~"ORDEN_COMPRA~".~"COD_USR~",   
         usf_fin_adelantos_oc(cod_origen,nro_oc) as monto_adelantos,   
         ~"ORDEN_COMPRA~".~"MONTO_TOTAL~"  ,
			~"ORDEN_COMPRA~".~"PROVEEDOR~"  AS COD_RELACION
    FROM ~"ORDEN_COMPRA~"  
   WHERE ( ~"ORDEN_COMPRA~".~"FLAG_ESTADO~" in ('1','2','3','4') ) AND  
         ( ~"ORDEN_COMPRA~".~"PROVEEDOR~" = :as_cod_proveedor )    
" update="ORDEN_COMPRA" updatewhere=1 updatekeyinplace=no filter=" IF ( isNull( monto_facturado ),0, monto_facturado )  +  IF ( isNull( monto_adelantos  ),0,  monto_adelantos  )  <  monto_total " arguments=(("as_cod_proveedor", string)) )
