 retrieve="  SELECT ~"TT_FIN_CNS_PROG_PAGOS~".~"NRO_REGISTRO~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"IMPORTE~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"FECHA_PROG~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_CTABCO~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"DESCRIPCION~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_MONEDA~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_ORIGEN~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"TIPO_DOC~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"NRO_DOC~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_RELACION~",   
         ~"TT_FIN_CNS_PROG_PAGOS~".~"OBS~",   
         ~"CODIGO_RELACION~".~"NOMBRE~"  AS NOMBRES, 
			DECODE(~"TT_FIN_CNS_PROG_PAGOS~".~"COD_MONEDA~",(SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1'),IMPORTE,0.00) AS IMP_SOLES,
  			DECODE(~"TT_FIN_CNS_PROG_PAGOS~".~"COD_MONEDA~",(SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1'),IMPORTE,0.00) AS IMP_DOLARES
    FROM ~"TT_FIN_CNS_PROG_PAGOS~"	,   
         ~"CODIGO_RELACION~"       
   WHERE ( ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~"       ) AND
			( ~"TT_FIN_CNS_PROG_PAGOS~".~"COD_ORIGEN~"	  Like :as_cod_origen					   	 )" arguments=(("as_cod_origen", string),("as_empresa", string),("as_usuario", string))  sort="cod_origen A cod_relacion A tipo_doc A nro_doc A " )
