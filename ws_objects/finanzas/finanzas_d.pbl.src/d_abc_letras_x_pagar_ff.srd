         ~"LETRAS_X_PAGAR~".~"TOTAL_LETRA~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"  
    FROM ~"LETRAS_X_PAGAR~",   
         ~"PROVEEDOR~"  
   WHERE ( ~"LETRAS_X_PAGAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" ) and  
