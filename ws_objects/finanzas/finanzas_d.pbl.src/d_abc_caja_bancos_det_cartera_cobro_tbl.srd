 retrieve="  SELECT ~"CAJA_BANCOS_DET~".~"ORIGEN~",   
         ~"CAJA_BANCOS_DET~".~"NRO_REGISTRO~",   
         ~"CAJA_BANCOS_DET~".~"ITEM~",   
         ~"CAJA_BANCOS_DET~".~"TIPO_DOC~",   
         ~"CAJA_BANCOS_DET~".~"NRO_DOC~",   
         ~"CAJA_BANCOS_DET~".~"IMPORTE~",   
         ~"CAJA_BANCOS_DET~".~"FLAB_TABOR~", 
			~"CAJA_BANCOS_DET~".~"FLAG_CXP~", 
         ''  AS COD_MONEDA_CAB,   
         ~"VW_FIN_DOC_GENERAL_CB~".~"COD_MONEDA~"  AS COD_MONEDA_DET,   
			~"CAJA_BANCOS_DET~".~"ORIGEN_DOC~" ,
         ~"CAJA_BANCOS_DET~".~"IMPORTE~"   AS IMPORTE_DET,   
			DECODE(~"CAJA_BANCOS_DET~".~"TIPO_DOC~",(SELECT doc_sol_giro FROM finparam WHERE reckey = '1'),NULL,'1') AS T_TIPDOC,
			~"CAJA_BANCOS_DET~".~"COD_RELACION~",
         0.000 AS TASA_CAMBIO,
			~"CAJA_BANCOS_DET~".~"IMPT_RET_IGV~",
			~"CAJA_BANCOS_DET~".~"FLAG_RET_IGV~",
         ~"CAJA_BANCOS_DET~".~"FLAG_FLUJO_CAJA~"
    FROM ~"CAJA_BANCOS_DET~" 	,   
         ~"VW_FIN_DOC_GENERAL_CB~" 
   WHERE ((~"CAJA_BANCOS_DET~".~"COD_RELACION~" = ~"VW_FIN_DOC_GENERAL_CB~".~"COD_RELACION~" ) and  
         ( ~"CAJA_BANCOS_DET~".~"TIPO_DOC~"     = ~"VW_FIN_DOC_GENERAL_CB~".~"TIPO_DOC~"     ) and  
         ( ~"CAJA_BANCOS_DET~".~"NRO_DOC~"      = ~"VW_FIN_DOC_GENERAL_CB~".~"NRO_DOC~"      )) and
			( ~"CAJA_BANCOS_DET~".~"ORIGEN~"       = :as_origen									   ) and
			( ~"CAJA_BANCOS_DET~".~"NRO_REGISTRO~" = :al_nro_registro							   ) 
" update="CAJA_BANCOS_DET" updatewhere=1 updatekeyinplace=yes arguments=(("as_origen", string),("al_nro_registro", number)) )
