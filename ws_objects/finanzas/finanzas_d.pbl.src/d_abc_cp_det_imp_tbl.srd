 retrieve="  SELECT CI.COD_RELACION,   
         CI.TIPO_DOC,   
         CI.NRO_DOC,   
         CI.ITEM,   
         CI.TIPO_IMPUESTO,   
         CI.IMPORTE,   
         IT.CNTA_CTBL,   
         IT.DESC_IMPUESTO,   
         IT.TASA_IMPUESTO,   
         IT.SIGNO,   
         CC.DESC_CNTA,   
         IT.FLAG_DH_CXP,   
         null as flag,   
         CI.FLAG_REPLICACION,   
         IT.FLAG_IGV  
    FROM CP_DOC_DET_IMP CI,   
         IMPUESTOS_TIPO IT,   
         CNTBL_CNTA     CC
   WHERE CI.TIPO_IMPUESTO = IT.TIPO_IMPUESTO
     and CC.CNTA_CTBL     = IT.CNTA_CTBL 
     and CI.COD_RELACION = :as_cod_relacion 
     AND CI.TIPO_DOC = :as_tipo_doc 
     AND CI.NRO_DOC = :as_nro_doc    
" update="CP_DOC_DET_IMP" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_relacion", string),("as_tipo_doc", string),("as_nro_doc", string)) )
