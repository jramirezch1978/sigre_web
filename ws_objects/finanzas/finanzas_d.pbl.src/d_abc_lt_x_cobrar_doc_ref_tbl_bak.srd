 retrieve="select dr.cod_relacion, dr.tipo_doc, dr.nro_doc, dr.tipo_mov,
       dr.origen_ref, dr.tipo_ref, dr.nro_ref, dr.importe, dr.flab_tabor,
       v.origen , v.nro_libro, v.nro_provisional, v.cod_moneda, dt.flag_signo,
       cp.cnta_ctbl, 
       decode(cp.flag_debhab,'D','H','D') as flag_debhab
      
from   doc_referencias dr , vw_fin_doc_general v, doc_tipo dt,
       cntbl_pre_asiento_det cp 

where  dr.cod_relacion =:as_relacion and dr.tipo_doc =:as_tipo and dr.nro_doc =:as_nro and
       dr.tipo_mov =:as_mov and
       dr.cod_relacion = v.cod_relacion and dr.tipo_doc = v.tipo_doc and
       dr.nro_doc = v.nro_doc and dr.tipo_ref = dt.tipo_doc and 
       v.origen = cp.origen and v.nro_libro = cp.nro_libro and v.nro_provisional = cp.nro_provisional and
       dr.cod_relacion = cp.cod_relacion and dr.tipo_ref = cp.tipo_docref and 
       dr.nro_ref = cp.nro_docref1
" update="doc_referencias" updatewhere=1 updatekeyinplace=no arguments=(("as_relacion", string),("as_tipo", string),("as_nro", string),("as_mov", string)) )
