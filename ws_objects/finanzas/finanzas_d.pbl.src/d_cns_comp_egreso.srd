 retrieve="  SELECT ~"CNTAS_PAGAR~".~"COD_RELACION~",   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~",   
         ~"CNTAS_PAGAR~".~"NRO_DOC~",   
         ~"CNTAS_PAGAR~".~"FECHA_EMISION~",   
         ~"CNTAS_PAGAR~".~"VENCIMIENTO~",   
         ~"CNTAS_PAGAR~".~"FORMA_PAGO~",   
         ~"CNTAS_PAGAR~".~"COD_MONEDA~",   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~",   
         ~"CNTAS_PAGAR~".~"TOTAL_PAGAR~",   
         ~"CNTAS_PAGAR~".~"TOTAL_PAGADO~",   
         ~"CNTAS_PAGAR~".~"DESCRIPCION~",   
         ~"FORMA_PAGO~".~"DESC_FORMA_PAGO~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CNTAS_PAGAR_DET~".~"ITEM~",   
         ~"CNTAS_PAGAR_DET~".~"DESCRIPCION~",   
         ~"CNTAS_PAGAR_DET~".~"CANTIDAD~",   
         ~"CNTAS_PAGAR_DET~".~"IMPORTE~",   
         ~"CNTAS_PAGAR_DET~".~"CENCOS~",   
         ~"CNTAS_PAGAR_DET~".~"CNTA_PRSP~",   
         ~"CNTAS_PAGAR_DET~".~"COD_RELACION~",   
         ~"CNTAS_PAGAR_DET~".~"TIPO_DOC~",   
         ~"CNTAS_PAGAR_DET~".~"NRO_DOC~",   
         ~"CNTAS_PAGAR~".~"ORIGEN~",   
         ~"CNTAS_PAGAR~".~"FLAG_ESTADO~"  
    FROM ~"CNTAS_PAGAR~",   
         ~"CNTAS_PAGAR_DET~",   
         ~"FORMA_PAGO~",   
         ~"PROVEEDOR~"  
   WHERE ( ~"CNTAS_PAGAR_DET~".~"COD_RELACION~" = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
         ( ~"CNTAS_PAGAR_DET~".~"TIPO_DOC~" = ~"CNTAS_PAGAR~".~"TIPO_DOC~" ) and  
         ( ~"CNTAS_PAGAR_DET~".~"NRO_DOC~" = ~"CNTAS_PAGAR~".~"NRO_DOC~" ) and  
         ( ~"FORMA_PAGO~".~"FORMA_PAGO~" = ~"CNTAS_PAGAR~".~"FORMA_PAGO~" ) and  
         ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
         ( ( To_char(~"CNTAS_PAGAR~".~"FECHA_EMISION~",'yyyymmdd') >= :as_fechai ) AND  
         ( To_char(~"CNTAS_PAGAR~".~"FECHA_EMISION~",'yyyymmdd') <= :as_fechaf ) AND  
         ( ~"CNTAS_PAGAR~".~"TIPO_DOC~" = :as_tipdoc ) AND  
         ( ~"CNTAS_PAGAR~".~"ORIGEN~" = :as_origen ) )    
" arguments=(("as_fechai", string),("as_fechaf", string),("as_tipdoc", string),("as_origen", string))  sort="cntas_pagar_det_tipo_doc A cntas_pagar_det_nro_doc A cntas_pagar_det_cod_relacion A cntas_pagar_det_item A " )
