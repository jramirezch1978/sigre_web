 column=(type=decimal(2) updatewhereclause=yes name=monto_total dbname="orden_servicio.monto_total" dbalias="os.monto_total" )
 column=(type=char(6) updatewhereclause=yes name=forma_pago dbname="orden_servicio.forma_pago" dbalias="os.forma_pago" )
 column=(type=char(6) updatewhereclause=yes name=cod_usr dbname="orden_servicio.cod_usr" dbalias="os.cod_usr" )
 column=(type=char(60) updatewhereclause=yes name=desc_forma_pago dbname="forma_pago.desc_forma_pago" dbalias="fp.desc_forma_pago" )
 column=(type=decimal(0) updatewhereclause=yes name=dias_vencimiento dbname="forma_pago.dias_vencimiento" dbalias="fp.dias_vencimiento" )
 retrieve="select distinct 
		 os.cod_origen      ,os.nro_os     ,os.flag_estado     ,
		 os.fec_registro    ,os.proveedor  ,os.descripcion     ,
		 os.flag_cotizacion ,os.cod_moneda ,os.monto_facturado ,
		 os.monto_total     ,os.forma_pago ,os.cod_usr,
		 fp.desc_forma_pago, fp.dias_vencimiento
  from orden_servicio 		os,
       orden_servicio_det 	osd,
		 forma_pago				fp
 where os.nro_os       = osd.nro_os  
   and os.forma_pago	  = fp.forma_pago
   and os.nro_sol_serv is not null   
   and os.flag_estado  in ('1','3')  
   and os.proveedor	  = :as_cod_prov
union
select distinct 
		 os.cod_origen      ,os.nro_os	  ,os.flag_estado ,
		 os.fec_registro    ,os.proveedor  ,os.descripcion ,
		 os.flag_cotizacion ,os.cod_moneda ,os.monto_facturado ,
	    os.monto_total     ,os.forma_pago      ,os.cod_usr,
		 fp.desc_forma_pago, fp.dias_vencimiento
  from orden_servicio os,
       orden_servicio_det osd,
		 forma_pago				fp
 where os.nro_os           = osd.nro_os 
   and os.forma_pago	  = fp.forma_pago
   and osd.conformidad_usr is not null  
   and os.flag_estado in ('1','3')      
   and os.proveedor	  = :as_cod_prov  

        " arguments=(("as_cod_prov", string)) )
