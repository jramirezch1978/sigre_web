table(column=(type=char(4000) updatewhereclause=yes name=compute_0001 dbname="compute_0001" )
 retrieve="select '01' || '|' -- 01. Tipo de Compra
       || dt.cod_sunat || '|' --02. Tipo de Comprobante
       || case 
            when dt.cod_sunat in ('10', '12') then ''
				when cp.serie_cp is not null then
					cp.serie_cp
            when instr(replace(cp.nro_doc, '.', ''), '-') > 0 then
              	pkg_fact_electronica.of_get_serie(cp.nro_doc)
            else
             ''
          end || '|' -- 3.Serie del Documento
       || case 
            when dt.cod_sunat in ('50', '52', '53', '54') then ''
				when cp.numero_cp is not null then
					cp.numero_cp
				when instr(replace(cp.nro_doc, '.', ''), '-') > 0 then
              	pkg_fact_electronica.of_get_nro(cp.nro_doc)
            else 
					lpad(cp.numero_cp, 10, ' ')
          end || '|' -- 4.Nro del Documento
       || DECODE(p.flag_personeria, 'N', '01', 'J', '02', '03') || '|' -- 5.Tipo de Persona
       || p.tipo_doc_ident || '|' -- 6.Tipo de documento
       || DECODE(p.ruc, null, p.nro_doc_ident, p.ruc) || '|' -- 7.Nro de documento
       || case 
             when cb.tipo_doc in ('TRF', 'TLC') then '003'
             when cb.tipo_doc in ('CH') then '007'
             when cb.tipo_doc in ('CB') then '001'
             else '009'
          end || '|' -- 8. Medios de Pago
       || case 
             when cb.tipo_doc in ('TRF', 'TLC', 'CH', 'CB') then b.cod_banco_sunat
             else '  '
          end || '|' -- 9. Codigo del banco o entidad
       || case 
             when cb.tipo_doc in ('TRF', 'TLC', 'CH', 'CB') then 
					case 
						when b.cod_banco_sunat = '99' then trim(substr(b.nom_banco,1,30))
						else lpad(cb.nro_doc, 10, ' ')
					 end
             else lpad(' ', 10, ' ')
          end || '|' -- 10.Numero de operacion
       || case 
             when cb.tipo_doc in ('TRF', 'TLC', 'CH', 'CB') then to_char(cb.fecha_emision, 'dd/mm/yyyy')
             else lpad(' ', 10, ' ')
          end || '|' -- 11.Fecha de la operacion
       || case 
            when cbd.cod_moneda = (select cod_soles from logparam where reckey = '1') then
              trim(to_char(cbd.importe, '9999999990.00'))
            else
              trim(to_char(cbd.importe * cb.tasa_cambio, '9999999990.00'))
          end || '|' -- 12. Monto de la operacion
from caja_bancos     cb,
     caja_bancos_det cbd,
     cntas_pagar     cp,
     doc_tipo        dt,
     proveedor       p,
     banco_cnta      bc,
     banco           b,
	  doc_grupo_relacion dgr
where cb.origen      = cbd.origen
  and cb.nro_registro  = cbd.nro_registro
  and cbd.cod_relacion = cp.cod_relacion
  and cbd.tipo_doc     = cp.tipo_doc
  and cbd.nro_doc      = cp.nro_doc
  and cp.tipo_doc      = dt.tipo_doc
  and cp.cod_relacion  = p.proveedor
  and cb.cod_ctabco    = bc.cod_ctabco
  and bc.cod_banco     = b.cod_banco
  and dt.tipo_doc		  = dgr.tipo_doc
  and cbd.flag_provisionado = 'R'
  and dgr.grupo		  = '09'
  and cp.flag_estado   <> '0'
  and cb.flag_Estado   <> '0' 
  and cb.nro_libro     = 12  
  and cp.ano           = :an_year
  and cp.mes           = :an_mes
  and dt.cod_sunat is not null" arguments=(("an_year", number),("an_mes", number)) )
