 retrieve="  SELECT ~"CNTAS_COBRAR~".~"TIPO_DOC~",   
         ~"CNTAS_COBRAR~".~"NRO_DOC~",   
         ~"CNTAS_COBRAR~".~"COD_RELACION~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~",   
         ~"CNTAS_COBRAR~".~"FECHA_VENCIMIENTO~",   
         ~"CNTAS_COBRAR~".~"COD_MONEDA~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_A_COBRAR~",   
         ~"CNTAS_COBRAR~".~"ORIGEN~",   
         ~"CNTAS_COBRAR~".~"OBSERVACION~",   
         ~"CNTAS_COBRAR~".~"NRO_PROVISIONAL~",   
         ~"CNTAS_COBRAR~".~"FLAG_ESTADO~",   
         ~"CNTAS_COBRAR~".~"FECHA_REGISTRO~",   
         ~"CNTAS_COBRAR~".~"PUNTO_VENTA~",   
         ~"CNTAS_COBRAR~".~"TASA_CAMBIO~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_COBRADO~",   
         ~"CNTAS_COBRAR~".~"COD_USR~",   
         ~"CNTAS_COBRAR~".~"NRO_LIBRO~",   
         ~"CNTAS_COBRAR~".~"FORMA_PAGO~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         to_char(~"CNTAS_COBRAR~".~"FECHA_REGISTRO~",'mm')||'/'||to_char(~"CNTAS_COBRAR~".~"FECHA_REGISTRO~",'yyyy') as PERIODO,   
         ~"CNTAS_COBRAR~".~"ITEM_DIRECCION~",   
         ~"CNTAS_COBRAR~".~"MOTIVO~"  
    FROM ~"CNTAS_COBRAR~",   
         ~"PROVEEDOR~",   
         ~"DOC_GRUPO_RELACION~",   
         ~"FINPARAM~"  
   WHERE ( ~"CNTAS_COBRAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" ) and  
         ( ~"FINPARAM~".~"DOC_NTVNT~" = ~"DOC_GRUPO_RELACION~".~"GRUPO~" ) and  
         ( ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~" = ~"CNTAS_COBRAR~".~"TIPO_DOC~" )    
" update="CNTAS_COBRAR" updatewhere=1 updatekeyinplace=no )
