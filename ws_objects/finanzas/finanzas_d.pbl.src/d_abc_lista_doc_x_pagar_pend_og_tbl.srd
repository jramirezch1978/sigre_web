 column=(type=char(10) updatewhereclause=yes name=cencos dbname="cntas_pagar_det.cencos" )
 column=(type=char(10) updatewhereclause=yes name=cnta_prsp dbname="cntas_pagar_det.cnta_prsp" )
 retrieve="  SELECT dp.cod_relacion  ,
         dp.tipo_doc      ,
         dp.nro_doc       ,
         cp.cod_moneda    ,          
         cp.fecha_emision ,   
         cp.tasa_cambio   ,
         SUBSTR(cp.descripcion,1,60) AS DESCRIPCION   ,
         DECODE(cp.cod_moneda,(SELECT cod_soles FROM logparam WHERE reckey = '1'),dp.sldo_sol,dp.saldo_dol) as saldo,
			cr.nombre		  ,	
			cd.cencos		  ,
			cd.cnta_prsp
    FROM doc_pendientes_cta_cte dp,   
         cntas_pagar            cp,
			cntas_pagar_det		  cd,
			codigo_relacion		  cr
   WHERE (( dp.cod_relacion = cp.cod_relacion )  and  
          ( dp.tipo_doc     = cp.tipo_doc     )  and  
          ( dp.nro_doc      = cp.nro_doc      )) and  
         (( dp.cod_relacion = cr.cod_relacion )) and

			(( cp.cod_relacion = cd.cod_relacion )  and
			 ( cp.tipo_doc		 = cd.tipo_doc		 )  and
			 ( cp.nro_doc		 = cd.nro_doc		 )) and

         ((dp.flag_tabla    = '3'             )  and
          (dp.flag_debhab   = 'H'             )) and
			 ( dp.cod_relacion = :as_cod_relacion)

GROUP BY dp.cod_relacion  ,
         dp.tipo_doc      ,
         dp.nro_doc       ,
         cp.cod_moneda    ,          
         cp.fecha_emision ,   
         cp.tasa_cambio   ,
         cp.descripcion   ,
			dp.sldo_sol      ,
         dp.saldo_dol     ,
			cr.nombre		  ,	
			cd.cencos		  ,
			cd.cnta_prsp" arguments=(("as_cod_relacion", string)) )
