 column=(type=number updatewhereclause=yes name=item_prod dbname="embarque.item_prod" )
 retrieve="  SELECT ~"EMBARQUE~".~"NRO_OV~",   
			~"EMBARQUE~".~"DOC_COBR_TIPO~",   
			~"EMBARQUE~".~"DOC_COBR_NRO~",   
			~"LOTE_EMBARQUE~".~"NRO_LOTE~",   
         ~"LOTE_EMBARQUE~".~"NRO_EMBARQUE~",   
         ~"LOTE_EMBARQUE~".~"COD_ART~",   
         ~"ARTICULO~".~"NOM_ARTICULO~",   
         Nvl(lote_embarque.cant_maxima_permitida,0) - Nvl(lote_embarque.cant_liberada,0) as cantidad_disp,
			:al_item_prod as item_prod
    FROM ~"EMBARQUE~"      ,
			~"LOTE_EMBARQUE~" ,   
         ~"ARTICULO~"  
   WHERE ( ~"EMBARQUE~".~"NRO_EMBARQUE~" = ~"LOTE_EMBARQUE~".~"NRO_EMBARQUE~") and
			( ~"ARTICULO~".~"COD_ART~"      = ~"LOTE_EMBARQUE~".~"COD_ART~" ) and  
         ( Nvl(lote_embarque.cant_maxima_permitida,0) - Nvl(lote_embarque.cant_liberada,0) > 0 )  and
			( ~"LOTE_EMBARQUE~".~"COD_ART~" =  :as_cod_art ) 
" arguments=(("as_cod_art", string),("al_item_prod", number)) )
