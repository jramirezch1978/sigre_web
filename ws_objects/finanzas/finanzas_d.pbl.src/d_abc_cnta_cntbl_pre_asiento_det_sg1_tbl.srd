 retrieve="SELECT  ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"       ,
		  ~"CNTBL_ASIENTO_DET~".~"ANO~"   		 ,
		  ~"CNTBL_ASIENTO_DET~".~"MES~"   		 ,	
		  ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"    ,
		  ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~"  ,
		  ~"CNTBL_ASIENTO_DET~".~"ITEM~"         ,
        ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~"    ,
        ~"CNTBL_ASIENTO_DET~".~"FEC_CNTBL~"    ,
        ~"CNTBL_ASIENTO_DET~".~"DET_GLOSA~"    ,
        ~"CNTBL_ASIENTO_DET~".~"FLAG_GEN_AUT~" ,
        ~"CNTBL_ASIENTO_DET~".~"FLAG_DEBHAB~"  ,
        ~"CNTBL_ASIENTO_DET~".~"CENCOS~"       ,
        ~"CNTBL_ASIENTO_DET~".~"COD_CTABCO~"   ,
        ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~" ,
        ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"  ,
        ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF2~"  ,
        ~"CNTBL_ASIENTO_DET~".~"IMP_MOVSOL~"   ,
        ~"CNTBL_ASIENTO_DET~".~"IMP_MOVDOL~"   ,
        ~"CNTBL_ASIENTO_DET~".~"IMP_MOVAJU~"   ,
        ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~" ,
        '' as flag    
  FROM  ~"CNTBL_ASIENTO_DET~" ,
        ~"SOLICITUD_GIRO~"     
WHERE ( ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"      = ~"SOLICITUD_GIRO~".~"ORIGEN~"      ) AND
		( ~"CNTBL_ASIENTO_DET~".~"ANO~"         = ~"SOLICITUD_GIRO~".~"ANO~"         ) AND
		( ~"CNTBL_ASIENTO_DET~".~"MES~"	      = ~"SOLICITUD_GIRO~".~"MES~"	      ) AND
      ( ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"   = ~"SOLICITUD_GIRO~".~"NRO_LIBRO~"   ) AND
      ( ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~" = ~"SOLICITUD_GIRO~".~"NRO_ASIENTO~" ) AND
      ((~"SOLICITUD_GIRO~".~"ORIGEN~"         = :as_origen                     ) AND
       (~"SOLICITUD_GIRO~".~"NRO_SOLICITUD~"  = :as_nro_solicitud              ))  " update="CNTBL_ASIENTO_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_origen", string),("as_nro_solicitud", string)) )
