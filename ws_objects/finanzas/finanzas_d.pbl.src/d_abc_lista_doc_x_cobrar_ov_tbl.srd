         ~"TT_FIN_DOC_COBRAR~".~"NRO_OV~",   
         ~"TT_FIN_DOC_COBRAR~".~"FLAG_DEBHAB~",   
			~"TT_FIN_DOC_COBRAR~".~"ORIGEN~",   
			~"TT_FIN_DOC_COBRAR~".~"FLAB_TABOR~",   
	      DECODE(tt_fin_doc_cobrar.flag_debhab,'H','+','-') as flag_cxp  ,
			DECODE(tt_fin_doc_cobrar.cod_moneda,(select cod_soles from logparam where reckey =1),tt_fin_doc_cobrar.saldo_sol,tt_fin_doc_cobrar.saldo_dol) as saldo,
         DECODE(tt_fin_doc_cobrar.cod_moneda,(select cod_soles from logparam where reckey =1),null,' ') as flag_mon

    FROM ~"TT_FIN_DOC_COBRAR~"  
   WHERE ~"TT_FIN_DOC_COBRAR~".~"NRO_OV~" = :as_nro_ov    
" arguments=(("as_nro_ov", string)) )
