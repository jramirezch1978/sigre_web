 column=(type=number updatewhereclause=yes name=factor dbname="factor" )
 retrieve="  SELECT ~"CNTAS_PAGAR~".~"ORIGEN~"		  ,   
			~"CNTAS_PAGAR~".~"COD_RELACION~" ,   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~"	  ,   
         ~"CNTAS_PAGAR~".~"NRO_DOC~"		  , 
  			~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~",
			~"CNTAS_PAGAR~".~"COD_MONEDA~"	 , 
         ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"   ,   
         ~"CNTAS_PAGAR~".~"SALDO_DOL~"   ,
			DECODE(~"DOC_PENDIENTES_CTA_CTE~".~"FACTOR~",1,1,-1) AS FACTOR
    FROM ~"CNTAS_PAGAR~"  ,   
         ~"CNTAS_COBRAR~" ,
			~"DOC_PENDIENTES_CTA_CTE~" 
   WHERE  ( ~"CNTAS_COBRAR~".~"COD_RELACION~"   = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
          ( ~"CNTAS_COBRAR~".~"NRO_DETRACCION~" = ~"CNTAS_PAGAR~".~"NRO_DOC~"      ) and  
			 ( ~"CNTAS_PAGAR~".~"TIPO_DOC~"		  = ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" ) AND
			 ( ~"CNTAS_PAGAR~".~"NRO_DOC~"		  = ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"  ) AND
			 ( ~"CNTAS_PAGAR~".~"COD_RELACION~"	  = ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" ) AND
         (( ~"CNTAS_PAGAR~".~"TIPO_DOC~"        = 'DETR'                       ))    AND
			 ( ~"CNTAS_PAGAR~".~"FLAG_CAJA_BANCOS~" = '1' ) AND
		   ( ((~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"   =  (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1')  )    AND  
      		(~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"     >  0                                                    ))   OR  
		     ((~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"   =  (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1'))    AND  
		      (~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"    >  0                                                    )) )  AND
     		 
  			 ( ~"CNTAS_PAGAR~".~"COD_RELACION~"	  = :AS_COD_RELACION ) AND 
			 ( ~"CNTAS_COBRAR~".~"TIPO_DOC~"		  = :AS_TIPO_DOC 		) AND
			 ( ~"CNTAS_COBRAR~".~"NRO_DOC~"		  = :AS_NRO_DOC  		) 


     

" arguments=(("as_cod_relacion", string),("as_tipo_doc", string),("as_nro_doc", string)) )
