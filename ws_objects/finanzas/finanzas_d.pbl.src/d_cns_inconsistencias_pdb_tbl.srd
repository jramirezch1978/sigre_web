 column=(type=char(10) updatewhereclause=yes name=cntas_pagar_nro_doc dbname="cntas_pagar.nro_doc" )
 column=(type=char(3) updatewhereclause=yes name=cntas_pagar_cod_moneda dbname="cntas_pagar.cod_moneda" )
 column=(type=decimal(4) updatewhereclause=yes name=cntas_pagar_tasa_cambio dbname="cntas_pagar.tasa_cambio" )
 column=(type=number updatewhereclause=yes name=compute_0006 dbname="compute_0006" )
 column=(type=datetime updatewhereclause=yes name=cntas_pagar_fecha_emision dbname="cntas_pagar.fecha_emision" )
 column=(type=number updatewhereclause=yes name=total dbname="total" )
 column=(type=number updatewhereclause=yes name=igv dbname="igv" )
 retrieve="SELECT a.cod_relacion ,
       a.tipo_doc     ,
       a.nro_doc      ,
       a.cod_moneda   ,
       a.tasa_cambio  ,
       usf_fin_tasa_cambio(a.fecha_emision),
       a.fecha_emision ,
       SUM(Nvl(b.importe,0) ) as total,
       SUM(Nvl(c.importe,0) ) as igv

FROM  cntas_pagar           a, 
      cntas_pagar_det       b, 
      cp_doc_det_imp        c, 
      proveedor             d, 
      doc_tipo              dc,
      detraccion            dt,
      detr_bien_serv        dbs,
      credito_fiscal        e

WHERE   ((a.cod_relacion     = b.cod_relacion   )   
  AND    (a.tipo_doc         = b.tipo_doc       )   
  AND    (a.nro_doc          = b.nro_doc        ))  
  AND   ((b.cod_relacion     = c.cod_relacion   )   
  AND    (b.tipo_doc         = c.tipo_doc       )  
  AND    (b.nro_doc          = c.nro_doc        )  
  AND    (b.item             = c.item           )) 
  AND    (b.tipo_cred_fiscal  = e.tipo_cred_fiscal ) 
  AND    (dc.tipo_doc        = a.tipo_doc           )  
  AND    (a.cod_relacion     = d.proveedor          )  
  AND    (a.nro_detraccion   = dt.nro_detraccion (+))  
  AND    (a.bien_serv        = dbs.bien_serv     (+))  
  AND    (a.tipo_doc        NOT IN (select 'PZ' from dual ) ) 
  AND    (a.nro_libro        = (select libro_compras from finparam where reckey = '1' )    ) 
  AND    (b.tipo_cred_fiscal = (select tipo_cred_fiscal from finparam where reckey = '1' ) ) 
  AND    (c.tipo_impuesto    = (select cod_igv from logparam where reckey = '1' )          ) 
  AND    (a.ano              = :AS_YEAR                                                     ) 
  AND    (a.mes              = :AS_MES                                               ) 
  AND    length(nvl(substr(a.nro_doc,1,instr(a.nro_doc,'-',1,1) - 1),''))  <= 4
         
GROUP BY a.cod_relacion, 
         d.ruc, 
         a.nro_asiento, 
         a.tipo_doc, 
         dc.cod_sunat, 
         a.nro_doc,
         a.fecha_emision, 
         a.cod_moneda,
         a.tasa_cambio,  
         a.ano,a.mes,
         d.flag_personeria, 
         d.nom_proveedor,
         d.apellido_pat,
         d.apellido_mat,
         d.nombre1,
         d.nombre2,
         a.flag_detraccion ,
         dbs.bien_serv,
         dt.nro_deposito,
         a.flag_retencion,
         a.porc_ret_igv,
         b.tipo_cred_fiscal ,
         e.cod_sunat " arguments=(("AS_YEAR", string),("AS_MES", string)) )
