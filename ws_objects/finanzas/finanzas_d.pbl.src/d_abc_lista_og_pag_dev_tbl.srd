 column=(type=char(1) updatewhereclause=yes name=flag_tipo_gasto dbname="flag_tipo_gasto" )
 retrieve="  SELECT ~"SOLICITUD_GIRO~".~"COD_RELACION~"  ,   
         (select doc_sol_giro from finparam where reckey = '1' ) as tipo_doc,   
         ~"SOLICITUD_GIRO~".~"NRO_SOLICITUD~" as nro_doc,   
         ~"SOLICITUD_GIRO~".~"COD_MONEDA~"    ,   
         ~"SOLICITUD_GIRO~".~"FECHA_EMISION~" ,   
			usf_fin_tasa_cambio(~"SOLICITUD_GIRO~".~"FECHA_EMISION~")   as tasa_cambio,
         substr(descripcion,1,60)         as descripcion,   
         ~"SOLICITUD_GIRO~".~"IMPORTE~"       as saldo      ,   
         ~"CODIGO_RELACION~".~"NOMBRE~"       ,
			~"SOLICITUD_GIRO~".~"CENCOS~"        ,
			~"SOLICITUD_GIRO~".~"CNTA_PRSP~"		,
		   'G' AS FLAG_TIPO_GASTO
    FROM ~"SOLICITUD_GIRO~",   
         ~"CODIGO_RELACION~"  
   WHERE  ( ~"SOLICITUD_GIRO~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~" )  and  
         (( ~"SOLICITUD_GIRO~".~"FLAG_ESTADO~"  = '2'                              )) and
			 ( ~"SOLICITUD_GIRO~".~"COD_RELACION~" = :as_cod_relacion                 )
UNION
 SELECT ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",
		  ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"    ,
		  ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"     ,
		  ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"  ,
		  ~"DOC_PENDIENTES_CTA_CTE~".~"FECHA_DOC~"	  ,
		  usf_fin_tasa_cambio(~"DOC_PENDIENTES_CTA_CTE~".~"FECHA_DOC~")   as tasa_cambio,
		  'ORDEN DE GIRO POR DEVOLVER' AS DESCRIPCION,
		  DECODE(~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",(SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1'),~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~",~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~") AS SALDO,
		  ~"CODIGO_RELACION~".~"NOMBRE~"             ,
		  ~"SOLICITUD_GIRO~".~"CENCOS~"              ,
		  ~"SOLICITUD_GIRO~".~"CNTA_PRSP~"			  ,
		  'D' AS FLAG_TIPO_GASTO		
   FROM ~"DOC_PENDIENTES_CTA_CTE~" ,
		  ~"SOLICITUD_GIRO~"		   ,	
		  ~"CODIGO_RELACION~"			
  WHERE ((DOC_PENDIENTES_CTA_CTE.TIPO_DOC     = (SELECT DOC_SOL_GIRO FROM FINPARAM WHERE RECKEY = '1') )) AND
		   (DOC_PENDIENTES_CTA_CTE.NRO_DOC      = SOLICITUD_GIRO.NRO_SOLICITUD                           )  AND
		   (DOC_PENDIENTES_CTA_CTE.COD_RELACION = SOLICITUD_GIRO.COD_RELACION									 )  AND
		  ((DOC_PENDIENTES_CTA_CTE.COD_RELACION = CODIGO_RELACION.COD_RELACION									 )) AND
         (DOC_PENDIENTES_CTA_CTE.FLAG_DEBHAB  = 'H'                                                    )  AND
		   (DOC_PENDIENTES_CTA_CTE.COD_RELACION = :as_cod_relacion													 )                 

			 " arguments=(("as_cod_relacion", string)) )
