 retrieve="  SELECT ~"LETRAS_X_COBRAR~".~"ORIGEN~"           ,
			~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"  ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"   ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~",   
			~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",
         DECODE(~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",(SELECT cod_soles FROM logparam where reckey = '1'),~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~",~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"   ) AS SALDO,
         ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"  ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~" ,
			~"DOC_TIPO~".~"FLAG_SIGNO~"              , 
			~"LETRAS_X_COBRAR~".~"CONFIN~"				 ,
			~"CONCEPTO_FINANCIERO~".~"MATRIZ_CNTBL~" ,
			~"CONCEPTO_FINANCIERO~".~"DESCRIPCION~"
    FROM ~"DOC_PENDIENTES_CTA_CTE~",
			~"LETRAS_X_COBRAR~"       ,
			~"DOC_TIPO~"					,
			~"CONCEPTO_FINANCIERO~"
   WHERE ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = :as_cod_relacion ) AND  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" 		= (SELECT doc_letra_cobrar FROM finparam WHERE reckey = '1') )  AND  
			((~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = ~"LETRAS_X_COBRAR~".~"COD_RELACION~" )  AND
			 (~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"		= ~"LETRAS_X_COBRAR~".~"NRO_LETRA~"    )) AND
			 (~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"		= ~"DOC_TIPO~".~"TIPO_DOC~"            )  AND
			 (~"LETRAS_X_COBRAR~".~"CONFIN~"					= ~"CONCEPTO_FINANCIERO~".~"CONFIN~"   )" arguments=(("as_cod_relacion", string)) )
