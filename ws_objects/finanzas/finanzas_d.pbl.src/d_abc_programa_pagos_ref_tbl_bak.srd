 column=(type=char(3) update=yes updatewhereclause=yes name=cod_moneda dbname="doc_pendientes_cta_cte.cod_moneda" )
 retrieve="SELECT  D.COD_RELACION, D.TIPO_DOC, D.NRO_DOC, C.VENCIMIENTO, 
        DECODE (D.COD_MONEDA,(SELECT L.COD_SOLES FROM LOGPARAM L WHERE L.RECKEY = '1'),
                D.SLDO_SOL, D.SALDO_DOL) AS IMP_SALDO ,
        D.COD_MONEDA
  FROM  DOC_PENDIENTES_CTA_CTE D ,  
        CNTAS_PAGAR C   
 WHERE  D.COD_RELACION = C.COD_RELACION and  
        D.TIPO_DOC     = C.TIPO_DOC     and  
        D.NRO_DOC      = C.NRO_DOC and  
        D.FLAG_TABLA   = '3'  
" update="DOC_PENDIENTES_CTA_CTE" updatewhere=1 updatekeyinplace=no )
