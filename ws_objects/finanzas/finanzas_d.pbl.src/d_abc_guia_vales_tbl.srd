 retrieve="SELECT DISTINCT  ~"GUIA~".~"NRO_GUIA~" ,
       ~"GUIA~".~"COD_ORIGEN~"   ,
       ~"GUIA~".~"FEC_REGISTRO~" ,
       ~"GUIA~".~"COD_USR~"      
  FROM ~"VALE_MOV~"  ,
       ~"GUIA_VALE~" ,
       ~"GUIA~"      ,
		   ~"MOTIVO_TRASLADO~",
       ~"LOGPARAM~"  
 WHERE ~"GUIA_VALE~".~"ORIGEN_VALE~"            = ~"VALE_MOV~".~"COD_ORIGEN~"            
   and ~"GUIA_VALE~".~"NRO_VALE~"               = ~"VALE_MOV~".~"NRO_VALE~"              
   and ~"GUIA_VALE~".~"ORIGEN_GUIA~"             = ~"GUIA~".~"COD_ORIGEN~"                
   and ~"GUIA_VALE~".~"NRO_GUIA~"               = ~"GUIA~".~"NRO_GUIA~"                  
   and ~"GUIA~".~"MOTIVO_TRASLADO~"             = ~"MOTIVO_TRASLADO~".~"MOTIVO_TRASLADO~"
	and ~"MOTIVO_TRASLADO~".~"FLAG_FACTURACION~" = '1'                                
	and ~"GUIA~".~"FLAG_ESTADO~"                 = '1'                                
	and ~"VALE_MOV~".~"TIPO_REFER~"               IS NULL                             
   and ~"GUIA~".~"CLIENTE~"                     = :as_cliente                         
" arguments=(("as_cliente", string)) )
