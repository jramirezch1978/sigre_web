table(column=(type=char(20) updatewhereclause=yes name=nom_proveedor dbname="proveedor_nom_proveedor" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="select p.nom_proveedor,
       case 
			when cp.ano is null or cp.mes is null or cp.nro_asiento is null then
				to_char(cp.fecha_emision, 'yyyy-mm') 
			else
				to_char(cp.ANO)||'-'||  trim(to_char(cp.mes,'00')) 
		 end as mes,
		 nvl(sum(case 
						when :asi_moneda = PKG_LOGISTICA.of_soles(null) then
							cp.saldo_sol
						else
							cp.saldo_dol
					 end),0) as saldo_dol
from cntas_pagar cp,
     proveedor   p
where cp.cod_relacion = p.proveedor
  and ((cp.cod_moneda = PKG_LOGISTICA.of_soles(null)   and cp.saldo_sol > 0) 
    or (cp.cod_moneda = PKG_LOGISTICA.of_dolares(null) and cp.saldo_dol > 0))
  and cp.flag_estado <> '0'
  and (cp.flag_provisionado = 'R' 
	 or (cp.flag_provisionado = 'D' and cp.flag_control_reg = '0') 
	 or (cp.flag_provisionado = 'D' and cp.flag_control_reg = '1' and cp.flag_caja_bancos = '0'))
  and ((:asi_opcion = '1' and trunc(cp.fecha_emision) between trunc(:adi_fecha1) and  trunc(:adi_fecha2))
    or (:asi_opcion = '2' and trunc(cp.vencimiento) between trunc(:adi_fecha1) and  trunc(:adi_fecha2)))
  and cp.origen like :asi_origen
group by p.nom_proveedor,
       case 
			when cp.ano is null or cp.mes is null or cp.nro_asiento is null then
				to_char(cp.fecha_emision, 'yyyy-mm') 
			else
				to_char(cp.ANO)||'-'||  trim(to_char(cp.mes,'00')) 
		 end" arguments=(("asi_opcion", string),("adi_fecha1", date),("adi_Fecha2", date),("asi_origen", string),("asi_moneda", string))  sort="nom_proveedor A " )
