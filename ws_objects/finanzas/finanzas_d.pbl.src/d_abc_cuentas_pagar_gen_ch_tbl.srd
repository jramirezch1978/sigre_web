 retrieve="  SELECT ~"CNTAS_PAGAR~".~"COD_RELACION~",   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~",   
         ~"CNTAS_PAGAR~".~"NRO_DOC~",   
         DECODE(~"CNTAS_PAGAR~".~"COD_MONEDA~",(SELECT COD_SOLES FROM LOGPARAM),~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~",~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~") AS IMPORTE,
         ~"CNTAS_PAGAR~".~"VENCIMIENTO~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~"  AS FLAB_TABOR  ,
			   'C' AS ORIGEN_DOC ,
         '' AS CONFIN   ,
			   ~"CNTAS_PAGAR~".~"ORIGEN~",
			   '1' AS FLAG_EDIT,
			   ~"CODIGO_RELACION~".~"NOMBRE~",
				substr(~"CNTAS_PAGAR~".~"DESCRIPCION~",1,60) AS OBS,
			   0 AS NRO,
			   null as flag_ret,
			   0 as serie_cr,
			   null as nro_cr  ,
			   0.00 as monto_ret
				
   FROM ~"CNTAS_PAGAR~"           ,   
    		~"DOC_PENDIENTES_CTA_CTE~",   
		   	~"CODIGO_RELACION~"       ,
        ~"DOC_GRUPO_RELACION~"  
   WHERE ((~"CNTAS_PAGAR~".~"COD_RELACION~"            = ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" )  AND
			    (~"CNTAS_PAGAR~".~"TIPO_DOC~"                = ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"     )  AND
			 (~"CNTAS_PAGAR~".~"NRO_DOC~"                 = ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"      )) AND
			 (~"CNTAS_PAGAR~".~"TIPO_DOC~"                = ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~"         )  AND 
         ((~"DOC_GRUPO_RELACION~".~"GRUPO~"            = '06'                                    )) AND
			((~"CNTAS_PAGAR~".~"COD_RELACION~"            = ~"CODIGO_RELACION~".~"COD_RELACION~"        )) AND
			 (~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_DEBHAB~"  = 'H'                                     )"  sort="tipo_doc A vencimiento A " )
