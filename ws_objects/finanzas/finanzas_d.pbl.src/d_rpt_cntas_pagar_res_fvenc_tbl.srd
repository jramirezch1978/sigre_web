 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="  SELECT ~"PROVEEDOR~".~"NOM_PROVEEDOR~", 
		   
			to_char(~"CNTAS_PAGAR~".~"VENCIMIENTO~",'YYYY/MM') AS ANO_MES,
         DECODE(~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",(SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1'),~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~" ,~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~") * (~"DOC_PENDIENTES_CTA_CTE~".~"FACTOR~" * -1) as saldo_Dol
    FROM ~"CNTAS_PAGAR~",   
         ~"PROVEEDOR~",   
         ~"FORMA_PAGO~",   
         ~"DOC_PENDIENTES_CTA_CTE~"  
   WHERE (  ~"PROVEEDOR~".~"PROVEEDOR~"                              = ~"CNTAS_PAGAR~".~"COD_RELACION~"            ) and  
         (  forma_pago.forma_pago (+)                            = cntas_pagar.forma_pago                              ) and  
         (  ~"CNTAS_PAGAR~".~"COD_RELACION~"                         = ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" ) and  
         (  ~"CNTAS_PAGAR~".~"TIPO_DOC~"                             = ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"     ) and  
         (  ~"CNTAS_PAGAR~".~"NRO_DOC~"                              = ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"      ) and  
         ( ( To_char(~"CNTAS_PAGAR~".~"VENCIMIENTO~",'yyyymmdd') >= :as_dia_inicio                          )  AND  
			  ( To_char(~"CNTAS_PAGAR~".~"VENCIMIENTO~",'yyyymmdd') <= :as_dia_final	                          )) AND
           (~"CNTAS_PAGAR~".~"ORIGEN~"										  Like :as_origen										  )  AND
           ( ~"CNTAS_PAGAR~".~"FLAG_PROVISIONADO~"                  = 'R'                                                    )    AND
        ( ((~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"               =  (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1')   )    AND  
           (~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"                 >  0                                                     ))   OR  
          ((~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"               =  (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1') )    AND  
           (~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"                >  0                                                     )) ) 
ORDER BY to_char(~"CNTAS_PAGAR~".~"VENCIMIENTO~",'YYYYMM')" arguments=(("as_dia_inicio", string),("as_dia_final", string),("as_origen", string))  sort="nom_proveedor A " )
