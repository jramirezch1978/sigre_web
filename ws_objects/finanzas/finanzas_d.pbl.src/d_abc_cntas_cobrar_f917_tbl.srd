 retrieve="select t.tipo_doc, 
       t.nro_doc,
       pkg_fact_electronica.of_get_full_nro(t.nro_doc) as full_nro_doc,
       t.cod_relacion,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       t.fecha_documento,
       t.fecha_vencimiento,
       t.forma_pago,
		 t.flag_mercado,
       fp.desc_forma_pago,
		 fp.dias_vencimiento,
		 t.medio_pago,
		 fmp.desc_medio_pago,
       t.tasa_cambio,
       t.importe_doc,
       t.saldo_sol,
       t.saldo_dol,
		 t.cod_moneda,
		 t.flag_estado
from cntas_cobrar 	t,
     proveedor    	p,
     forma_pago   	fp,
	  fin_medios_pago	fmp
where t.cod_relacion = p.proveedor
  and t.forma_pago   = fp.forma_pago
  and t.medio_pago	= fmp.medio_pago	(+)
  --and t.nro_libro is not null
  --and t.nro_asiento is not null
  and ((:as_filtro = '1' and to_char(t.fecha_documento, 'yyyymm') <= trim(to_char(:an_year, '0000')) ||  trim(to_char(:an_mes, '00'))) or
		 (:as_filtro = '2' and t.cod_relacion like :as_proveedor ))
order by t.cod_relacion, t.tipo_doc, full_nro_doc" update="cntas_cobrar" updatewhere=0 updatekeyinplace=yes arguments=(("as_filtro", string),("an_year", number),("an_mes", number),("as_proveedor", string)) )
