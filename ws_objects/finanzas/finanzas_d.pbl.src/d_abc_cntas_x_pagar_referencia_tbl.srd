 retrieve="SELECT ~"DOC_REFERENCIAS~".~"COD_RELACION~" ,
       ~"DOC_REFERENCIAS~".~"TIPO_DOC~" ,
       ~"DOC_REFERENCIAS~".~"NRO_DOC~" ,
       ~"DOC_REFERENCIAS~".~"TIPO_MOV~" ,
       ~"DOC_REFERENCIAS~".~"ORIGEN_REF~" ,
       ~"DOC_REFERENCIAS~".~"TIPO_REF~" ,
       ~"DOC_REFERENCIAS~".~"NRO_REF~" ,
       ~"DOC_REFERENCIAS~".~"IMPORTE~" ,
       ~"DOC_REFERENCIAS~".~"FLAB_TABOR~" ,
 		 ~"DOC_REFERENCIAS~".~"FLAG_RET_IGV~" ,
		 ~"DOC_REFERENCIAS~".~"IMP_RET_IGV~" ,
       ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~" ,
       ~"CNTBL_ASIENTO_DET~".~"FLAG_DEBHAB~" ,
       ~"LETRAS_X_PAGAR~".~"COD_MONEDA~"     ,
		 NULL AS FLAG_IMPUESTO
  FROM ~"DOC_REFERENCIAS~" ,
       ~"LETRAS_X_PAGAR~" ,
       ~"CNTBL_ASIENTO_DET~"     
 WHERE (~"DOC_REFERENCIAS~".~"COD_RELACION~" = ~"LETRAS_X_PAGAR~".~"COD_RELACION~"    ) and
       (~"DOC_REFERENCIAS~".~"NRO_REF~"      = ~"LETRAS_X_PAGAR~".~"NRO_LETRA~"       ) and
       (~"LETRAS_X_PAGAR~".~"ORIGEN~"        = ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"       ) and
		 (~"LETRAS_X_PAGAR~".~"ANO~"           = ~"CNTBL_ASIENTO_DET~".~"ANO~"          ) and
		 (~"LETRAS_X_PAGAR~".~"MES~"           = ~"CNTBL_ASIENTO_DET~".~"MES~"          ) and	 
       (~"LETRAS_X_PAGAR~".~"NRO_LIBRO~"     = ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"    ) and
       (~"LETRAS_X_PAGAR~".~"NRO_ASIENTO~"   = ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~"  ) and
       (~"DOC_REFERENCIAS~".~"COD_RELACION~" = ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~" ) and
       (~"DOC_REFERENCIAS~".~"TIPO_REF~"     = ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~" ) and
       (~"DOC_REFERENCIAS~".~"NRO_REF~"      = ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"  ) and
       ((~"DOC_REFERENCIAS~".~"COD_RELACION~"= :as_relacion ) and  
        (~"DOC_REFERENCIAS~".~"TIPO_DOC~"    = :as_tipo_doc ) and      
        (~"DOC_REFERENCIAS~".~"NRO_DOC~"     = :as_nro_doc  ) and     
        (~"DOC_REFERENCIAS~".~"TIPO_MOV~"    = :as_tipo_mov ))  
" update="DOC_REFERENCIAS" updatewhere=1 updatekeyinplace=yes arguments=(("as_relacion", string),("as_tipo_doc", string),("as_nro_doc", string),("as_tipo_mov", string)) )
