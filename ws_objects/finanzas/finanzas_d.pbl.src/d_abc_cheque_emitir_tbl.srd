 retrieve="  SELECT ~"CHEQUE_EMITIR~".~"NRO_REGISTRO~",   
         ~"CHEQUE_EMITIR~".~"COD_CTABCO~",   
         ~"CHEQUE_EMITIR~".~"NRO_CHEQUE~",   
         ~"CHEQUE_EMITIR~".~"COD_USR~",   
         ~"CHEQUE_EMITIR~".~"FLAG_ESTADO~",   
         ~"CHEQUE_EMITIR~".~"FECHA~",   
         ~"CHEQUE_EMITIR~".~"FECHA_VALUTA~",   
         ~"CHEQUE_EMITIR~".~"FECHA_IMPRESION~",   
         ~"CHEQUE_EMITIR~".~"NRO_IMPRESION~",   
         ~"CHEQUE_EMITIR~".~"IMPORTE~",   
         ~"CHEQUE_EMITIR~".~"AFAVOR~",   
         ~"CHEQUE_EMITIR~".~"TIPO_DOC~",   
         ~"CHEQUE_EMITIR~".~"COD_ORIGEN~",   
         ~"CHEQUE_EMITIR~".~"NRO_REGISTRO_REF~",   
         ~"BANCO_CNTA~".~"DESCRIPCION~",   
         ~"BANCO~".~"COD_BANCO~",   
         ~"BANCO~".~"NOM_BANCO~",   
         ~"BANCO_CNTA~".~"COD_MONEDA~"  
    FROM ~"CHEQUE_EMITIR~",   
         ~"BANCO_CNTA~",   
         ~"BANCO~"  
   WHERE ( ~"BANCO_CNTA~".~"COD_CTABCO~" = ~"CHEQUE_EMITIR~".~"COD_CTABCO~" ) and  
         ( ~"BANCO~".~"COD_BANCO~" = ~"BANCO_CNTA~".~"COD_BANCO~" ) and  
			( ~"CHEQUE_EMITIR~".~"FLAG_ESTADO~" = '1' ) and   
         ( ( ~"CHEQUE_EMITIR~".~"COD_CTABCO~" like :as_ctabco ) AND  
         ( trunc(cheque_emitir.fecha) between :ad_fecha_inicio and :ad_fecha_final )            )    
" update="CHEQUE_EMITIR" updatewhere=1 updatekeyinplace=yes arguments=(("as_ctabco", string),("ad_fecha_inicio", date),("ad_fecha_final", date))  sort="banco_cod_banco A cod_ctabco A nro_cheque A " )
