 retrieve=" SELECT ~"LETRAS_X_PAGAR~".~"COD_RELACION~"   ,   
         ~"LETRAS_X_PAGAR~".~"NRO_LETRA~"      ,   
         ~"LETRAS_X_PAGAR~".~"NRO_RENOVACION~" ,   
         ~"LETRAS_X_PAGAR~".~"NRO_INTERNO~"    ,   
         ~"LETRAS_X_PAGAR~".~"FLAG_ESTADO~"    ,   
         ~"LETRAS_X_PAGAR~".~"FLAG_TIPO~"      ,   
         ~"LETRAS_X_PAGAR~".~"FECHA_REGISTRO~" ,   
         ~"LETRAS_X_PAGAR~".~"FECHA_EMISION~"  ,   
         ~"LETRAS_X_PAGAR~".~"VENCIMIENTO~"    ,   
         ~"LETRAS_X_PAGAR~".~"COD_MONEDA~"     ,   
         ~"LETRAS_X_PAGAR~".~"COD_BANCO~"      ,   
         ~"LETRAS_X_PAGAR~".~"TASA_CAMBIO~"    ,   
         ~"LETRAS_X_PAGAR~".~"LT_ORIGINAL~"    ,   
         ~"LETRAS_X_PAGAR~".~"INTERESES~"      ,   
         ~"LETRAS_X_PAGAR~".~"TOTAL_LETRA~"    ,   
         ~"LETRAS_X_PAGAR~".~"TOTAL_PAGADO~"   ,   
         ~"LETRAS_X_PAGAR~".~"OBSERVACION~"    ,   
         ~"LETRAS_X_PAGAR~".~"FLAG_SITUACION~" ,   
         ~"LETRAS_X_PAGAR~".~"CONFIN~"         ,   
         ~"LETRAS_X_PAGAR~".~"ORIGEN~"         ,   
		   ~"LETRAS_X_PAGAR~".~"ANO~"	          ,   
			~"LETRAS_X_PAGAR~".~"MES~"	          ,   
         ~"LETRAS_X_PAGAR~".~"NRO_LIBRO~"      ,   
         ~"LETRAS_X_PAGAR~".~"NRO_ASIENTO~"	 ,   
         ~"LETRAS_X_PAGAR~".~"COD_USR~"        ,   
         ~"BANCO~".~"NOM_BANCO~"               ,   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"       ,   
         ~"CONCEPTO_FINANCIERO~".~"DESCRIPCION~" ,
			0 as dias_a_cancelar
    FROM ~"BANCO~",   
         ~"CONCEPTO_FINANCIERO~",   
         ~"LETRAS_X_PAGAR~",   
         ~"PROVEEDOR~"  
   WHERE ( ~"BANCO~".~"COD_BANCO~" (+)= ~"LETRAS_X_PAGAR~".~"COD_BANCO~" ) and  
         ( ~"LETRAS_X_PAGAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" ) and  
         ( ~"LETRAS_X_PAGAR~".~"CONFIN~" = ~"CONCEPTO_FINANCIERO~".~"CONFIN~" ) and  
         ( ( ~"LETRAS_X_PAGAR~".~"NRO_LETRA~" <> ~"LETRAS_X_PAGAR~".~"LT_ORIGINAL~" ) )    
" )
