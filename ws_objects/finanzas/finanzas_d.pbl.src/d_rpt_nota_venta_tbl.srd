         ~"CNTAS_COBRAR_DET~".~"CANTIDAD~",   
         ~"CNTAS_COBRAR_DET~".~"PRECIO_UNITARIO~",   
         ~"CNTAS_COBRAR_DET~".~"REDONDEO_MANUAL~",   
         ~"CNTAS_COBRAR_DET~".~"DESCUENTO~",   
         ~"CNTAS_COBRAR_DET~".~"TIPO_REF~",   
         ~"CNTAS_COBRAR_DET~".~"NRO_REF~",   
         ~"CNTAS_COBRAR_DET~".~"TIPO_CRED_FISCAL~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"PROVEEDOR~".~"RUC~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_A_COBRAR~",   
         ~"CC_DOC_DET_IMP~".~"IMPORTE~",   
         ~"MONEDA~".~"DESCRIPCION~",   
         ~"IMPUESTOS_TIPO~".~"TASA_IMPUESTO~",   
         NVL(~"DIRECCIONES~".~"DIR_DIRECCION~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_NUMERO~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_MNZ~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_LOTE~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_URBANIZACION~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_DISTRITO~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_CIUDAD~",'')||' '||NVL(~"DIRECCIONES~".~"DIR_PROVINCIA~",'')||' - '||NVL(~"DIRECCIONES~".~"DIR_DEP_ESTADO~",'') AS DIRE_CLIENTE,   
         ~"CC_DOC_DET_IMP~".~"ITEM~"  
    FROM ~"CNTAS_COBRAR~",   
         ~"CNTAS_COBRAR_DET~",   
         ~"PROVEEDOR~",   
         ~"DIRECCIONES~",   
         ~"CC_DOC_DET_IMP~",   
         ~"MONEDA~",   
         ~"IMPUESTOS_TIPO~"  
   WHERE ( cntas_cobrar.cod_relacion (+) = direcciones.codigo) and  
         ( cntas_cobrar.item_direccion (+) = direcciones.item) and  
         ( cntas_cobrar.cod_relacion = proveedor.proveedor (+)) and  
         ( cntas_cobrar.cod_moneda = moneda.cod_moneda (+)) and  
         ( cc_doc_det_imp.tipo_impuesto = impuestos_tipo.tipo_impuesto (+)) and  
         ( ~"CNTAS_COBRAR_DET~".~"TIPO_DOC~" = ~"CNTAS_COBRAR~".~"TIPO_DOC~" ) and  
         ( ~"CNTAS_COBRAR_DET~".~"NRO_DOC~" = ~"CNTAS_COBRAR~".~"NRO_DOC~" ) and  
         ( ~"CNTAS_COBRAR~".~"TIPO_DOC~" = ~"CC_DOC_DET_IMP~".~"TIPO_DOC~" ) and  
         ( ~"CNTAS_COBRAR~".~"NRO_DOC~" = ~"CC_DOC_DET_IMP~".~"NRO_DOC~" ) and  
         ( ( ~"CNTAS_COBRAR~".~"TIPO_DOC~" = :as_tipo_doc ) AND  
         ( ~"CNTAS_COBRAR~".~"NRO_DOC~" = :as_nro_documento ) )    
