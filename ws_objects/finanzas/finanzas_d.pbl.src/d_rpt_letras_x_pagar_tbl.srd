 retrieve="SELECT  cp.cod_relacion,  
        cp.nro_doc, 
        cp.flag_estado, 
        cp.vencimiento, 
        cp.fecha_emision, 
        cp.cod_moneda, 
        cp.tasa_cambio, 
        cp.importe_doc, 
        cp.saldo_sol, 
        cp.saldo_dol, 
        cp.cod_usr, 
        ( cp.origen ||'-'|| to_char(cp.ano) ||'-'|| to_char(cp.mes)||'-'|| trim(to_char(cp.nro_libro)) ||'-'|| trim(to_char(cp.nro_asiento)) ) as voucher ,
        p.nom_proveedor 
   FROM cntas_pagar cp, 
        proveedor p
  WHERE ( cp.cod_relacion = p.proveedor ) and 
		  ( cp.tipo_doc = (select f.doc_letra_pagar from finparam f where f.reckey='1')) and 
        ( cp.origen like :as_origen ) and 
        ( cp.ano = :an_ano ) and 
        ( cp.mes >= :an_mes_ini and cp.mes <= :an_mes_fin )  
ORDER BY cp.cod_relacion, cp.ano, cp.mes, cp.nro_libro, cp.nro_asiento " arguments=(("as_origen", string),("an_ano", number),("an_mes_ini", number),("an_mes_fin", number)) )
