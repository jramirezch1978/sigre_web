 retrieve="  SELECT ~"VW_FIN_DOC_X_PAGAR~".~"ORIGEN~"          ,
			~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"     ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~"  ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"  ,
         DECODE(~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",(SELECT cod_soles FROM logparam where reckey = '1'),~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~",~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"   ) AS TOTAL_PAGAR,   
			~"DOC_TIPO~".~"FLAG_SIGNO~"
    FROM ~"VW_FIN_DOC_X_PAGAR~",   
         ~"DOC_PENDIENTES_CTA_CTE~",
			~"DOC_TIPO~"  
   WHERE (( ~"VW_FIN_DOC_X_PAGAR~".~"TIPO_DOC~"     = ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"     )  AND
          ( ~"VW_FIN_DOC_X_PAGAR~".~"NRO_DOC~"      = ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"      )  AND
          ( ~"VW_FIN_DOC_X_PAGAR~".~"COD_RELACION~" = ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" )) AND
			 ( ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" = ~"DOC_TIPO~".~"TIPO_DOC~"                   )  AND
		    ( ~"VW_FIN_DOC_X_PAGAR~".~"COD_RELACION~" = :as_cod_relacion								)  
" arguments=(("as_cod_relacion", string)) )
