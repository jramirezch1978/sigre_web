 column=(type=char(20) updatewhereclause=yes name=proveedor_razon_social dbname="proveedor_razon_social" )
 column=(type=number updatewhereclause=yes name=flujo_caja_proy_fec_programada dbname="flujo_caja_proy_fec_programada" )
 column=(type=number updatewhereclause=yes name=imp_soles dbname="imp_soles" )
 retrieve="select DECODE(g.factor, 1, '01.INGRESOS', '02.EGRESOS') as Segmento,
		 g.descripcion as desc_grupo,
		 cf.descripcion as desc_flujo_caja,
		 t.proveedor_cxp, p.nom_proveedor as razon_social,
		 'Semana ' || TO_CHAR(t.fec_programada, 'WW') AS SEMANA,
		 to_char(t.fec_programada, 'yyyy') || '-' || case 
			when to_char(t.fec_programada, 'mm') = '01' then '01(Enero)'
			when to_char(t.fec_programada, 'mm') = '02' then '02(Febrero)'
			when to_char(t.fec_programada, 'mm') = '03' then '03(Marzo)'
			when to_char(t.fec_programada, 'mm') = '04' then '04(Abril)'
			when to_char(t.fec_programada, 'mm') = '05' then '05(Mayo)'
			when to_char(t.fec_programada, 'mm') = '06' then '06(Junio)'
			when to_char(t.fec_programada, 'mm') = '07' then '07(Julio)'
			when to_char(t.fec_programada, 'mm') = '08' then '08(Agosto)'
			when to_char(t.fec_programada, 'mm') = '09' then '09(Setiembre)'  
			when to_char(t.fec_programada, 'mm') = '10' then '10(Octubre)'
			when to_char(t.fec_programada, 'mm') = '11' then '11(Noviembre)'
			when to_char(t.fec_programada, 'mm') = '12' then '12(Diciembre)'  
		 end as mes,
		 sum(decode(t.cod_moneda, (select cod_soles from logparam where reckey = '1'), t.importe * g.factor, 0)) as imp_soles,
		 sum(decode(t.cod_moneda, (select cod_dolares from logparam where reckey = '1'), t.importe * g.factor, 0)) as imp_dolares

from flujo_caja_proy t, 
	  codigo_flujo_caja cf,
	  grupo_cod_flujo_caja g,
	  proveedor            p
where g.grp_flujo_caja = cf.grp_flujo_caja
  and cf.cod_flujo_caja  = t.cod_flujo_caja
  and t.proveedor_cxp    = p.proveedor (+)
  and trunc(t.fec_programada) between :ad_Fecha1 and :ad_Fecha2
group by DECODE(g.factor, 1, '01.INGRESOS', '02.EGRESOS'),
		 g.descripcion,
		 cf.descripcion,
		 t.proveedor_cxp, p.nom_proveedor,
		 'Semana ' || TO_CHAR(t.fec_programada, 'WW'),
		 to_char(t.fec_programada, 'yyyy') || '-' || case 
			when to_char(t.fec_programada, 'mm') = '01' then '01(Enero)'
			when to_char(t.fec_programada, 'mm') = '02' then '02(Febrero)'
			when to_char(t.fec_programada, 'mm') = '03' then '03(Marzo)'
			when to_char(t.fec_programada, 'mm') = '04' then '04(Abril)'
			when to_char(t.fec_programada, 'mm') = '05' then '05(Mayo)'
			when to_char(t.fec_programada, 'mm') = '06' then '06(Junio)'
			when to_char(t.fec_programada, 'mm') = '07' then '07(Julio)'
			when to_char(t.fec_programada, 'mm') = '08' then '08(Agosto)'
			when to_char(t.fec_programada, 'mm') = '09' then '09(Setiembre)'  
			when to_char(t.fec_programada, 'mm') = '10' then '10(Octubre)'
			when to_char(t.fec_programada, 'mm') = '11' then '11(Noviembre)'
			when to_char(t.fec_programada, 'mm') = '12' then '12(Diciembre)'  
		 end" arguments=(("ad_fecha1", date),("ad_fecha2", date))  sort="segmento A grupo_cod_flujo_caja_desc_grupo A codigo_flujo_caja_desc_flujo_caja A flujo_caja_proy_proveedor_cxp A proveedor_razon_social A " )
