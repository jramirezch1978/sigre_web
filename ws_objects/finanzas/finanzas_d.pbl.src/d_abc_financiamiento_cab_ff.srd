 retrieve="  SELECT ~"FINANCIAMIENTO~".~"ORIGEN~",   
         ~"FINANCIAMIENTO~".~"NUM_REGISTRO~",   
         ~"FINANCIAMIENTO~".~"COD_CTABCO~",   
         ~"FINANCIAMIENTO~".~"FECHA_EMISION~",   
         ~"FINANCIAMIENTO~".~"NRO_DOC~",   
         ~"FINANCIAMIENTO~".~"COD_RELACION~",   
         ~"FINANCIAMIENTO~".~"COD_AVAL~",   
         ~"FINANCIAMIENTO~".~"IMP_CAPITAL~",   
         ~"FINANCIAMIENTO~".~"IMP_INTERES~",   
         ~"FINANCIAMIENTO~".~"SLDO_CAPITAL~",   
         ~"FINANCIAMIENTO~".~"SLDO_INTERES~",   
         ~"FINANCIAMIENTO~".~"DOC_GARANTIA~",   
         ~"FINANCIAMIENTO~".~"FLAG_INTERES~",   
         ~"FINANCIAMIENTO~".~"COD_MONEDA~",   
         ~"FINANCIAMIENTO~".~"NRO_LIBRO~",   
         ~"FINANCIAMIENTO~".~"COD_USR~",   
         ~"FINANCIAMIENTO~".~"ANO~",   
         ~"FINANCIAMIENTO~".~"MES~",   
         ~"FINANCIAMIENTO~".~"NRO_ASIENTO~",   
         ~"FINANCIAMIENTO~".~"TASA_CAMBIO~",   
         ~"FINANCIAMIENTO~".~"OBSERVAC~",   
         ~"PROVEEDOR_A~".~"NOM_PROVEEDOR~",   
         ~"PROVEEDOR_B~".~"NOM_PROVEEDOR~"  AS NOM_AVAL
    FROM ~"FINANCIAMIENTO~",   
         ~"PROVEEDOR~" ~"PROVEEDOR_A~",   
         ~"PROVEEDOR~" ~"PROVEEDOR_B~"  
   WHERE ( ~"FINANCIAMIENTO~".~"COD_RELACION~" = ~"PROVEEDOR_A~".~"PROVEEDOR~" ) and  
         ( financiamiento.cod_aval = proveedor_b.proveedor (+)) and  
         ( ( ~"FINANCIAMIENTO~".~"ORIGEN~" = :as_cod_origen ) AND  
         ( ~"FINANCIAMIENTO~".~"NUM_REGISTRO~" = :as_nro_registro ) )    
" update="FINANCIAMIENTO" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_origen", string),("as_nro_registro", string)) )
