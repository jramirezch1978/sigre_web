 retrieve="SELECT cb.origen       ,
       cb.nro_registro ,
		 cb.flag_estado  ,	
       cb.fecha_emision,
       cb.ano          ,
       cb.mes          ,
       cb.nro_libro    ,
       cb.nro_asiento  ,
       cbd.tipo_doc	  ,
		 RTRIM(LTRIM(cb.origen))||RTRIM(LTRIM(TO_CHAR(cb.ano)))||RTRIM(LTRIM(TO_CHAR(cb.mes,'09')))||RTRIM(LTRIM(TO_CHAR(cb.nro_libro)))||RTRIM(LTRIM(TO_CHAR(cb.nro_asiento))) AS VOUCHER		
  FROM caja_bancos cb,
       caja_bancos_det cbd
 WHERE ((cb.origen       = cbd.origen       )  and
        (cb.nro_registro = cbd.nro_registro )) and
        (cbd.tipo_doc    = (select doc_letra_pagar from finparam where reckey = '1' )) and
		  (cb.flag_tiptran = :as_flag_tiptran )
GROUP BY cb.origen       ,
         cb.nro_registro ,
		   cb.flag_estado  ,	
         cb.fecha_emision,
         cb.ano          ,
         cb.mes          ,
         cb.nro_libro    ,
         cb.nro_asiento  ,
         cbd.tipo_doc" arguments=(("as_flag_tiptran", string)) )
