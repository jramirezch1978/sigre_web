 retrieve="  SELECT ~"CNTAS_COBRAR~".~"COD_RELACION~",   
			~"CODIGO_RELACION~".~"NOMBRE~"   ,   
         ~"CNTAS_COBRAR~".~"TIPO_DOC~"    ,   
         ~"CNTAS_COBRAR~".~"NRO_DOC~"  
    FROM ~"CNTAS_COBRAR~",
		   ~"CODIGO_RELACION~"
   WHERE (~"CNTAS_COBRAR~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~") AND
			(~"CNTAS_COBRAR~".~"COD_RELACION~" = :as_cod_relacion					  ) AND
			(~"CNTAS_COBRAR~".~"TIPO_DOC~"		= :as_tipo_doc							  ) AND
			(~"CNTAS_COBRAR~".~"NRO_DOC~"		= :as_nro_doc							  ) 

   UNION   
  SELECT ~"CNTAS_PAGAR~".~"COD_RELACION~",   
			~"CODIGO_RELACION~".~"NOMBRE~"  ,   
         ~"CNTAS_PAGAR~".~"TIPO_DOC~"	 ,   
         ~"CNTAS_PAGAR~".~"NRO_DOC~"  
    FROM ~"CNTAS_PAGAR~",
			~"CODIGO_RELACION~"
   WHERE (~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"CODIGO_RELACION~".~"COD_RELACION~") AND
			(~"CNTAS_PAGAR~".~"COD_RELACION~" = :as_cod_relacion					 ) AND
			(~"CNTAS_PAGAR~".~"TIPO_DOC~"		= :as_tipo_doc							 ) AND
			(~"CNTAS_PAGAR~".~"NRO_DOC~"		= :as_nro_doc							 ) 

