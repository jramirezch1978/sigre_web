table(column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="cntas_pagar.tipo_doc" dbalias="cp.tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="cntas_pagar.nro_doc" dbalias="cp.nro_doc" )
 column=(type=datetime updatewhereclause=yes name=fecha_emision dbname="cntas_pagar.fecha_emision" dbalias="cp.fecha_emision" )
 column=(type=decimal(4) updatewhereclause=yes name=tasa_cambio dbname="cntas_pagar.tasa_cambio" dbalias="cp.tasa_cambio" )
 column=(type=char(180) updatewhereclause=yes name=descripcion dbname="cntas_pagar.descripcion" dbalias="cp.descripcion" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_pagar.cod_moneda" dbalias="cp.cod_moneda" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="select cp.tipo_doc,
       cp.nro_doc,
       cp.fecha_emision,
       cp.tasa_cambio,
       cp.descripcion,
       cp.cod_moneda,
       sum(case 
            when cp.cod_moneda = pkg_logistica.of_soles(null) then
                 cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1) 
            when cp.cod_moneda = pkg_logistica.of_dolares(null) then
                 cad.imp_movdol * decode(cad.flag_debhab, 'D', 1, -1)
           end) as importe
from cntbl_asiento ca,
     cntbl_Asiento_det cad,
     cntas_pagar       cp
where ca.origen        	= cad.origen
  and ca.ano           	= cad.ano
  and ca.mes           	= cad.mes
  and ca.nro_libro     	= cad.nro_libro
  and ca.nro_asiento   	= cad.nro_asiento     
  and cad.cod_relacion 	= cp.cod_relacion
  and cad.tipo_docref1 	= cp.tipo_doc
  and cad.nro_docref1  	= cp.nro_doc
  and cp.nro_asiento      is not null
  and cp.cod_relacion  	= :as_proveedor
  and cad.cnta_ctbl    in (:as_cnta_cntbl_ant1, :as_cnta_cntbl_ant2, :as_cnta_cntbl_ant3, '43201106')
  and ca.nro_libro 	  not in (10, 11)  --No debo considerar los libros de pago o cobro para nada
group by cp.tipo_doc,
       cp.nro_doc,
       cp.fecha_emision,
       cp.tasa_cambio,
       cp.descripcion,
       cp.cod_moneda 
having sum(case 
            when cp.cod_moneda = pkg_logistica.of_soles(null) then
                 cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1) 
            when cp.cod_moneda = pkg_logistica.of_dolares(null) then
                 cad.imp_movdol * decode(cad.flag_debhab, 'D', 1, -1)
           end) > 0     " arguments=(("as_proveedor", string),("as_cnta_cntbl_ant1", string),("as_cnta_cntbl_ant2", string),("as_cnta_cntbl_ant3", string)) )
