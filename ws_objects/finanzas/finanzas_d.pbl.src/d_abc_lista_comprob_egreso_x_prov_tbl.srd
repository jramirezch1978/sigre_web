table(column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="cntas_pagar.cod_relacion" )
 column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="cntas_pagar.tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="cntas_pagar.nro_doc" )
 column=(type=number updatewhereclause=yes name=total_pagar dbname="total_pagar" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_pagar.cod_moneda" )
 column=(type=char(60) updatewhereclause=yes name=nombre dbname="codigo_relacion.nombre" )
 column=(type=char(2) updatewhereclause=yes name=origen dbname="cntas_pagar.origen" )
 retrieve="SELECT cp.cod_relacion ,   
       cp.tipo_doc	  ,   
       cp.nro_doc      ,   
       (Nvl(cp.total_pagar,0.00) - 	Nvl(cp.total_pagado,0.00))  as total_pagar,
       cp.cod_moneda	  ,   
		 cr.nombre		  ,
		 cp.origen
  FROM cntas_pagar       cp,   
         finparam        fi,    
			codigo_relacion cr
   WHERE (fi.comprobante_egr = cp.tipo_doc     )  AND
			(cp.cod_relacion    = cr.cod_relacion )  AND
        ((cp.flag_estado     IN ('1','2')             )) AND
		  ((cp.cod_relacion	  = :as_cod_relacion))
UNION
  SELECT v.cod_relacion ,
			v.tipo_doc	   ,
			v.nro_doc	   ,
         DECODE(d.cod_moneda,(select cod_soles from logparam where reckey = '1' ),d.sldo_sol,d.saldo_dol ) ,         
			d.cod_moneda  ,
         c.nombre      ,              
         v.origen     
   FROM vw_fin_doc_x_asiento_diario v,
        doc_pendientes_cta_cte      d,
        codigo_relacion             c
 WHERE ((v.cod_relacion = :as_cod_relacion )) AND
		 ((d.flag_tabla   = 'V'              )) AND
		 ((d.flag_debhab  = 'H'              )) AND
       ((d.cod_relacion = v.cod_relacion   )  AND
        (d.tipo_doc     = v.tipo_doc       )  AND
        (d.nro_doc      = v.nro_doc        )) AND
       ((d.cod_relacion = c.cod_relacion   )) 

		 
			" arguments=(("as_cod_relacion", string)) )
