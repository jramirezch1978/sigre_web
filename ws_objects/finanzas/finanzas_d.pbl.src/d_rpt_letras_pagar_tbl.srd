 retrieve="SELECT ~"CNTAS_PAGAR~".~"COD_RELACION~",   
       ~"CNTAS_PAGAR~".~"NRO_DOC~",   
       ~"CNTAS_PAGAR~".~"FECHA_EMISION~",   
       ~"CNTAS_PAGAR~".~"VENCIMIENTO~",   
       ~"CNTAS_PAGAR~".~"COD_MONEDA~",   
       ~"CNTAS_PAGAR~".~"TASA_CAMBIO~",   
       ~"CNTAS_PAGAR~".~"TOTAL_PAGADO~",   
       ~"CNTAS_PAGAR~".~"DESCRIPCION~",
       ~"CNTAS_PAGAR~".~"ORIGEN~",   
       ~"CNTAS_PAGAR~".~"ANO~",   
       ~"CNTAS_PAGAR~".~"MES~",   
       ~"CNTAS_PAGAR~".~"NRO_LIBRO~",   
       ~"CNTAS_PAGAR~".~"NRO_ASIENTO~",   
       ~"CNTAS_PAGAR~".~"IMPORTE_DOC~",   
       ~"CODIGO_RELACION~".~"NOMBRE~",   
       usf_fin_refer_letras( cntas_pagar.cod_relacion, cntas_pagar.nro_doc) as doc_refer,   
       ~"MONEDA~".~"DESCRIPCION~"  
  FROM ~"CNTAS_PAGAR~",   
       ~"CODIGO_RELACION~",   
       ~"MONEDA~"  
 WHERE ( ~"CNTAS_PAGAR~".~"COD_RELACION~"  = ~"CODIGO_RELACION~".~"COD_RELACION~" ) AND
       ( ~"CNTAS_PAGAR~".~"COD_MONEDA~"    = ~"MONEDA~".~"COD_MONEDA~"            ) AND
       ( ~"CNTAS_PAGAR~".~"TIPO_DOC~"      = (select f.doc_letra_pagar from finparam f) ) AND
       ((~"CNTAS_PAGAR~".~"NRO_DOC~"       = :as_nro_doc                      ) AND  
        ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = :as_cod_relacion                 ))    " arguments=(("as_nro_doc", string),("as_cod_relacion", string)) )
