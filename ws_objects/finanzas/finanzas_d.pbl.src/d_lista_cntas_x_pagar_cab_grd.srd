         ~"CNTAS_PAGAR~".~"TOTAL_PAGAR~",   
         ~"CNTAS_PAGAR~".~"TOTAL_PAGADO~",   
         ~"CNTAS_PAGAR~".~"COD_USR~",   
         ~"CNTAS_PAGAR~".~"ORIGEN~",   
         ~"CNTAS_PAGAR~".~"NRO_LIBRO~",   
         ~"CNTAS_PAGAR~".~"NRO_PROVISIONAL~",   
         ~"CNTAS_PAGAR~".~"DESCRIPCION~",   
         ~"FORMA_PAGO~".~"DESC_FORMA_PAGO~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CNTAS_PAGAR~".~"JOB~",   
         to_char(~"CNTAS_PAGAR~".~"FECHA_REGISTRO~",'mm')||'/'||to_char(~"CNTAS_PAGAR~".~"FECHA_REGISTRO~",'yyyy') as PERIODO  
    FROM ~"CNTAS_PAGAR~",   
         ~"FORMA_PAGO~",   
         ~"PROVEEDOR~"  
   WHERE ( cntas_pagar.forma_pago = forma_pago.forma_pago (+)) and  
         ( ~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" )    
