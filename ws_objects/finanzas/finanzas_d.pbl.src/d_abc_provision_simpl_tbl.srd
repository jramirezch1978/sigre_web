 retrieve="select t.regkey,
       t.cod_origen,
       t.fec_emision,
       t.tipo_doc,
       t.serie_cp,
       t.numero_cp,
       t.proveedor, p.nom_proveedor, 
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       t.forma_pago, fp.desc_forma_pago,
       t.concepto,
       t.base_imponible,
       t.impuesto_igv,
       t.adq_no_grav,
       t.cod_moneda,
       t.tasa_cambio,
       t.cod_usr,
       t.ano,
       t.mes,
       t.confin,
		 cf.descripcion as desc_confin,
       t.cencos,
       t.cnta_prsp,
       t.centro_benef,
       t.flag_estado,
       t.usr_aprob,
       t.fecha_aprob,
		 t.fec_registro,
		 t.flag_pago,
		 t.fec_pago,
		 t.COD_CTABCO,
		 bc.descripcion as desc_banco_cnta,
		 cp.origen || trim(to_Char(cp.ano, '0000')) || trim(to_Char(cp.mes, '00')) || trim(to_Char(cp.nro_libro, '00')) || trim(to_Char(cp.nro_asiento, '000000')) as voucher
  from fin_provision_simpl t,
       proveedor           p,
       forma_pago          fp,
		 banco_cnta				bc,
		 concepto_financiero	cf,
		 cntas_pagar			cp
 where t.proveedor   		= p.proveedor
   and t.forma_pago  		= fp.forma_pago
   and t.confin				= cf.confin
   and t.COD_CTABCO			= bc.COD_CTABCO  	(+)
   and t.cod_relacion_cxp	= cp.cod_relacion	(+)
   and t.tipo_doc_cxp		= cp.tipo_doc		(+)
   and t.nro_doc_cxp			= cp.nro_doc		(+)
   and t.ano					= :ani_year
	and t.mes					= :ani_mes
   and t.cod_usr			like :as_usuario
order by t.fec_emision, p.nom_proveedor
 " update="fin_provision_simpl" updatewhere=0 updatekeyinplace=yes arguments=(("ani_year", number),("ani_mes", number),("as_usuario", string)) )
