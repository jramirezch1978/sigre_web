 column=(type=char(10) updatewhereclause=yes name=cencos dbname="maestro.cencos" dbalias="m.cencos" )
 column=(type=char(80) updatewhereclause=yes name=desc_cencos dbname="centros_costo.desc_cencos" dbalias="cc.desc_cencos" )
 column=(type=char(12) updatewhereclause=yes name=centro_benef dbname="maestro.centro_benef" dbalias="m.centro_benef" )
 column=(type=char(100) updatewhereclause=yes name=desc_centro dbname="centro_beneficio.desc_centro" dbalias="cb.desc_centro" )
 retrieve="select p.proveedor,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       m.cencos,
       cc.desc_cencos,
       m.centro_benef,
       cb.desc_centro
from proveedor p,
     maestro   m,
     centros_costo cc,
     centro_beneficio cb
where p.proveedor = m.cod_trabajador    (+)
  and m.cencos    = cc.cencos           (+)
  and m.centro_benef = cb.centro_benef  (+)
  and p.flag_estado <> '0'
order by p.nom_proveedor" )
