 retrieve="  SELECT ~"CHEQUE_EMITIR~".~"NRO_CHEQUE~",   
			~"CAJA_BANCOS~".~"FECHA_EMISION~" ,
         ~"CAJA_BANCOS~".~"OBS~",   
			~"CAJA_BANCOS~".~"ORIGEN~" ,
			~"CAJA_BANCOS~".~"NRO_ASIENTO~" ,
         ~"CHEQUE_EMITIR~".~"AFAVOR~",   
         ~"CAJA_BANCOS~".~"COD_MONEDA~",   
         ~"CAJA_BANCOS~".~"TASA_CAMBIO~",
			   
         DECODE(CAJA_BANCOS.COD_MONEDA,'S/.',CHEQUE_EMITIR.IMPORTE,ROUND(CHEQUE_EMITIR.IMPORTE * CAJA_BANCOS.TASA_CAMBIO,2)) AS SOLES  ,
			DECODE(CAJA_BANCOS.COD_MONEDA,'US$',CHEQUE_EMITIR.IMPORTE,ROUND(CHEQUE_EMITIR.IMPORTE / CAJA_BANCOS.TASA_CAMBIO,2)) AS DOLARES  
    FROM ~"CAJA_BANCOS~",   
         ~"CHEQUE_EMITIR~"  
   WHERE ( ~"CAJA_BANCOS~".~"REG_CHEQUE~" = ~"CHEQUE_EMITIR~".~"NRO_REGISTRO~" ) and  
         ( ( ~"CAJA_BANCOS~".~"FLAG_ESTADO~" = '1' ) )    and
			( ~"CAJA_BANCOS~".~"ANO~" = 2003 ) and  
			( ~"CAJA_BANCOS~".~"MES~" = 2 )
"  sort="caja_bancos_origen A caja_bancos_nro_asiento A " )
