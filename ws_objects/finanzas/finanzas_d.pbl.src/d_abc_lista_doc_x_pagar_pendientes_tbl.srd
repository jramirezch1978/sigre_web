 retrieve="  SELECT ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~",   
         ~"CNTAS_PAGAR~".~"FECHA_EMISION~",   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~",   
         SUBSTR(~"CNTAS_PAGAR~".~"DESCRIPCION~",1,60) AS DESCRIPCION,   
         decode(doc_pendientes_cta_cte.cod_moneda,(select cod_soles from logparam where reckey = '1'),doc_pendientes_cta_cte.sldo_sol,doc_pendientes_cta_cte.saldo_dol) as saldo  ,
         ~"CNTAS_PAGAR_DET~".~"CENCOS~",   
         ~"CNTAS_PAGAR_DET~".~"CNTA_PRSP~"
    FROM ~"DOC_PENDIENTES_CTA_CTE~",   
         ~"CNTAS_PAGAR~"      ,
         ~"CNTAS_PAGAR_DET~"  
   WHERE (( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" = ~"CNTAS_PAGAR~".~"TIPO_DOC~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~" = ~"CNTAS_PAGAR~".~"NRO_DOC~" )) and  
         
         ((~"CNTAS_PAGAR~".~"COD_RELACION~" = ~"CNTAS_PAGAR_DET~".~"COD_RELACION~" ) AND
          (~"CNTAS_PAGAR~".~"TIPO_DOC~" = ~"CNTAS_PAGAR_DET~".~"TIPO_DOC~" ) AND
          (~"CNTAS_PAGAR~".~"NRO_DOC~" = ~"CNTAS_PAGAR_DET~".~"NRO_DOC~" )) AND
          
         ( ( ~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~" = '3' ) AND  
           ( ~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_DEBHAB~" = 'H' ) )AND
         (( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = :as_cod_relacion ) AND  
          ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~" = :as_cod_moneda ) )    

    
GROUP BY ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",   
         ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"     ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"  ,   
         ~"CNTAS_PAGAR~".~"FECHA_EMISION~"          ,   
         ~"CNTAS_PAGAR~".~"TASA_CAMBIO~"            ,   
         ~"CNTAS_PAGAR~".~"DESCRIPCION~"            ,   
         doc_pendientes_cta_cte.sldo_sol         ,
         doc_pendientes_cta_cte.sAldo_Dol,
         ~"CNTAS_PAGAR_DET~".~"CENCOS~",   
         ~"CNTAS_PAGAR_DET~".~"CNTA_PRSP~"" arguments=(("as_cod_relacion", string),("as_cod_moneda", string)) )
