 retrieve="select dr.cod_relacion ,
	    dr.tipo_doc     ,
		 dr.nro_doc      ,
       dr.tipo_mov     ,
       dr.origen_ref   ,
       dr.tipo_ref     ,
       dr.nro_ref      ,
       dr.importe      ,
       dr.flab_tabor   ,
       v.origen        ,
       v.nro_libro     ,
       v.nro_asiento   ,
       v.cod_moneda    ,
       dt.flag_signo   
from   doc_referencias     dr,
       vw_fin_doc_general   v,
       doc_tipo            dt
where ((dr.cod_relacion = v.cod_relacion)   and
       (dr.tipo_doc     = v.tipo_doc    )   and
       (dr.nro_doc      = v.nro_doc     ))  and
      ((dr.tipo_ref     = dt.tipo_doc    )) and 
		((dr.cod_relacion = :as_relacion   ) and
	    (dr.tipo_doc     = :as_tipo       ) and
       (dr.nro_doc      = :as_nro        ) and
       (dr.tipo_mov     = :as_mov        )) 
" update="doc_referencias" updatewhere=0 updatekeyinplace=yes arguments=(("as_relacion", string),("as_tipo", string),("as_nro", string),("as_mov", string)) )
