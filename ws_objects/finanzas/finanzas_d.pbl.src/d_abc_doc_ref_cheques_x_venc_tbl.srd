 retrieve="SELECT DOC_REFERENCIAS.COD_RELACION ,   
       DOC_REFERENCIAS.TIPO_DOC     ,   
       DOC_REFERENCIAS.NRO_DOC      ,   
       DOC_REFERENCIAS.TIPO_MOV     ,   
       DOC_REFERENCIAS.ORIGEN_REF   ,   
       DOC_REFERENCIAS.TIPO_REF     ,   
       DOC_REFERENCIAS.NRO_REF      ,   
       DOC_REFERENCIAS.IMPORTE      ,   
       DOC_REFERENCIAS.FLAB_TABOR   ,    
		 CTA_COB_LET.TASA_CAMBIO      ,   
       CTA_COB_LET.COD_MONEDA  AS COD_MONEDA_CAB ,   
       CTA_COB_REF.COD_MONEDA  AS COD_MONEDA_DET ,
	  	 DOC_REFERENCIAS.FLAG_RET_IGV ,
		 DOC_REFERENCIAS.IMP_RET_IGV  ,
		 DOC_REFERENCIAS.FACTOR       ,
		 NULL AS FLAG_IMPUESTO		   ,
		 (SELECT COD_SOLES   FROM LOGPARAM WHERE RECKEY = '1' ) AS SOLES  ,
		 (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1' ) AS DOLARES,
		 DOC_REFERENCIAS.FLAG_REPLICACION,
		 DOC_REFERENCIAS.CONFIN
  FROM DOC_REFERENCIAS          ,   
       CNTAS_COBRAR CTA_COB_LET ,   
  	    CNTAS_COBRAR CTA_COB_REF        
 WHERE (( DOC_REFERENCIAS.COD_RELACION  = CTA_COB_LET.COD_RELACION )  AND  
		  ( DOC_REFERENCIAS.TIPO_DOC	    = CTA_COB_LET.TIPO_DOC	    )  AND
        ( DOC_REFERENCIAS.NRO_DOC       = CTA_COB_LET.NRO_DOC      )) AND  
       (( DOC_REFERENCIAS.COD_RELACION  = CTA_COB_REF.COD_RELACION )  AND
		  ( DOC_REFERENCIAS.TIPO_REF      = CTA_COB_REF.TIPO_DOC     )  AND  
        ( DOC_REFERENCIAS.NRO_REF       = CTA_COB_REF.NRO_DOC      )) AND  
       (( CTA_COB_LET.FLAG_TIPO_LTR		 = :as_tipo_ltr            )) AND  
	     (( DOC_REFERENCIAS.COD_RELACION = :as_cod_relacion               )  AND  
        ( DOC_REFERENCIAS.TIPO_DOC      = :as_tipo_doc                    )  AND  
        ( DOC_REFERENCIAS.NRO_DOC       = :as_nro_doc             )) 
UNION
SELECT DOC_REFERENCIAS.COD_RELACION ,   
       DOC_REFERENCIAS.TIPO_DOC     ,   
       DOC_REFERENCIAS.NRO_DOC      ,   
       DOC_REFERENCIAS.TIPO_MOV     ,   
       DOC_REFERENCIAS.ORIGEN_REF   ,   
       DOC_REFERENCIAS.TIPO_REF     ,   
       DOC_REFERENCIAS.NRO_REF      ,   
       DOC_REFERENCIAS.IMPORTE     ,   
       DOC_REFERENCIAS.FLAB_TABOR  ,   
		 CTA_COB_LET.TASA_CAMBIO     ,   
       CTA_COB_LET.COD_MONEDA  AS COD_MONEDA_CAB ,   
       CTA_PAG_REF.COD_MONEDA  AS COD_MONEDA_DET ,
		 DOC_REFERENCIAS.FLAG_RET_IGV,
		 DOC_REFERENCIAS.IMP_RET_IGV ,
		 DOC_REFERENCIAS.FACTOR      ,
		 NULL AS FLAG_IMPUESTO		 ,
		 (SELECT COD_SOLES   FROM LOGPARAM WHERE RECKEY = '1' ) AS SOLES,
		 (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1' ) AS DOLARES,
		 DOC_REFERENCIAS.FLAG_REPLICACION,
		DOC_REFERENCIAS.CONFIN
  FROM DOC_REFERENCIAS          ,   
       CNTAS_COBRAR CTA_COB_LET ,   
	    CNTAS_PAGAR  CTA_PAG_REF
 WHERE (( DOC_REFERENCIAS.COD_RELACION  = CTA_COB_LET.COD_RELACION )  AND  
        ( DOC_REFERENCIAS.TIPO_DOC	    = CTA_COB_LET.TIPO_DOC	    )  AND
        ( DOC_REFERENCIAS.NRO_DOC       = CTA_COB_LET.NRO_DOC      )) AND  
	    (( DOC_REFERENCIAS.COD_RELACION  = CTA_PAG_REF.COD_RELACION )  AND
	     ( DOC_REFERENCIAS.TIPO_REF      = CTA_PAG_REF.TIPO_DOC     )  AND  
        ( DOC_REFERENCIAS.NRO_REF       = CTA_PAG_REF.NRO_DOC      )) AND  
       (( CTA_COB_LET.FLAG_TIPO_LTR		 = :as_tipo_ltr                     )) AND  
	    (( DOC_REFERENCIAS.COD_RELACION  = :as_cod_relacion               )  AND  
        ( DOC_REFERENCIAS.TIPO_DOC      = :as_tipo_doc                    )  AND  
        ( DOC_REFERENCIAS.NRO_DOC       = :as_nro_doc             ))    " update="DOC_REFERENCIAS" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_relacion", string),("as_tipo_doc", string),("as_nro_doc", string),("as_tipo_ltr", string)) )
