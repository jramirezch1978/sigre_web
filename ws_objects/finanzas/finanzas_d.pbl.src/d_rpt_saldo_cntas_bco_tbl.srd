 column=(type=char(40) updatewhereclause=yes name=descripcion dbname="banco_cnta.descripcion" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="banco_cnta.cod_moneda" )
 column=(type=number updatewhereclause=yes name=saldo dbname="saldo" )
 column=(type=char(3) updatewhereclause=yes name=cod_dolares dbname="cod_dolares" )
 column=(type=number updatewhereclause=yes name=tasa_cambio dbname="tasa_cambio" )
 retrieve="    SELECT ca.cod_ctabco, bc.descripcion, 
           bc.cod_moneda,
           nvl(sum(DECODE(ca.flag_debhab, 'D', 
            decode(bc.cod_moneda,(select cod_soles from logparam where reckey = '1'),ca.imp_movsol,ca.imp_movdol), 
            0)),0) -
           nvl(sum(DECODE(ca.flag_debhab, 'D', 0, 
            decode(bc.cod_moneda,(select cod_soles from logparam where reckey = '1'),ca.imp_movsol,ca.imp_movdol))),0) as saldo,
			  (select cod_dolares from logparam where reckey = '1') as cod_dolares,
			  usf_fin_tasa_cambio(:ad_fecha) as tasa_cambio

    FROM cntbl_asiento     c ,
         caja_bancos       cb,
         cntbl_asiento_det ca,
         banco_cnta        bc
   WHERE c.origen        = cb.origen
     AND c.ano           = cb.ano
     AND c.mes           = cb.mes
     AND c.nro_libro     = cb.nro_libro
     AND c.nro_asiento   = cb.nro_asiento
     AND c.origen        = ca.origen
     AND c.ano           = ca.ano
     AND c.mes           = ca.mes
     AND c.nro_libro     = ca.nro_libro
     AND c.nro_asiento   = ca.nro_asiento
     and ca.cod_ctabco   = bc.cod_ctabco
     and cb.flag_estado  <> '0'
     and trunc(cb.fecha_emision) <= :ad_fecha
     and bc.flag_flujo_caja	= '1'
group by ca.cod_ctabco, bc.descripcion, bc.cod_moneda" arguments=(("ad_fecha", date))  sort="descripcion A " )
