 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="cntas_pagar.flag_estado" values="Anulado	0/Activo	1/Pago Parcial	2/Pago Total	3/" )
 retrieve="select t.tipo_doc, 
       t.nro_doc,
		 t.cod_relacion,
       t.serie_cp,
       t.numero_cp,
       case
          when t.serie_cp is not null and t.numero_cp is not null then
            t.serie_cp || '-' || t.numero_cp
          else
            t.nro_doc
       end as full_nro_doc,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       t.fecha_emision,
       t.vencimiento,
       t.forma_pago,
       fp.desc_forma_pago,
       fp.dias_vencimiento,
       t.tasa_cambio,
       t.importe_doc,
       t.saldo_sol,
       t.saldo_dol,
		 t.flag_estado
from cntas_pagar t,
     proveedor    p,
     forma_pago   fp
where t.cod_relacion = p.proveedor
  and t.forma_pago   = fp.forma_pago
  and ((:as_filtro = '1' and t.ano = :an_year and t.mes = :an_mes) or
		 (:as_filtro = '2' and t.cod_relacion like :as_proveedor ))
order by t.cod_relacion, t.tipo_doc, full_nro_doc" update="cntas_pagar" updatewhere=0 updatekeyinplace=no arguments=(("as_filtro", string),("an_year", number),("an_mes", number),("as_proveedor", string)) )
