 column=(type=decimal(0) updatewhereclause=yes name=dias_vencimiento dbname="forma_pago.dias_vencimiento" )
 column=(type=decimal(4) updatewhereclause=yes name=tasa_cambio dbname="cntas_pagar.tasa_cambio" )
