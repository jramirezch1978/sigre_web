        ~"CNTAS_COBRAR~".~"COD_RELACION~"      ,
        ~"CNTAS_COBRAR~".~"FLAG_ESTADO~"       ,
        ~"CNTAS_COBRAR~".~"FECHA_REGISTRO~"    ,
        ~"CNTAS_COBRAR~".~"PUNTO_VENTA~"       ,
        ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~"   ,
        ~"CNTAS_COBRAR~".~"FECHA_VENCIMIENTO~" ,
        ~"CNTAS_COBRAR~".~"COD_MONEDA~"        ,
        ~"CNTAS_COBRAR~".~"TASA_CAMBIO~"       ,
        ~"CNTAS_COBRAR~".~"COD_USR~"           ,
        ~"CNTAS_COBRAR~".~"ORIGEN~"            ,
		  ~"CNTAS_COBRAR~".~"ANO~"               ,
		  ~"CNTAS_COBRAR~".~"MES~"               ,
        ~"CNTAS_COBRAR~".~"NRO_LIBRO~"         ,
        ~"CNTAS_COBRAR~".~"NRO_ASIENTO~"       ,
        ~"CNTAS_COBRAR~".~"FORMA_PAGO~"        ,
        ~"CNTAS_COBRAR~".~"OBSERVACION~"       ,
        ~"PROVEEDOR~".~"NOM_PROVEEDOR~"        ,
        ~"CNTAS_COBRAR~".~"SALDO_SOL~"       	 ,
		  ~"CNTAS_COBRAR~".~"SALDO_DOL~"       	 
   FROM ~"CNTAS_COBRAR~"       ,
        ~"FINPARAM~"           ,
        ~"DOC_GRUPO_RELACION~" ,
        ~"PROVEEDOR~"     
  WHERE ( ~"CNTAS_COBRAR~".~"TIPO_DOC~"     = ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~" ) and
        ( ~"FINPARAM~".~"DOC_VENTAS~"       = ~"DOC_GRUPO_RELACION~".~"GRUPO~"    ) and
