 retrieve="SELECT DISTINCT  ~"GUIA~".~"NRO_GUIA~" ,
       ~"GUIA~".~"COD_ORIGEN~"   ,
       ~"GUIA~".~"FEC_REGISTRO~" ,
       ~"GUIA~".~"COD_USR~"      
  FROM ~"VALE_MOV~"  ,
       ~"GUIA_VALE~" ,
       ~"GUIA~"      ,
		 ~"MOTIVO_TRASLADO~",
       ~"LOGPARAM~"  
 WHERE (~"GUIA_VALE~".~"ORIGEN_VALE~"             = ~"VALE_MOV~".~"COD_ORIGEN~"            ) and
       ( ~"GUIA_VALE~".~"NRO_VALE~"               = ~"VALE_MOV~".~"NRO_VALE~"              ) and
       ( ~"GUIA_VALE~".~"ORIGEN_GUIA~"            = ~"GUIA~".~"COD_ORIGEN~"                ) and
       ( ~"GUIA_VALE~".~"NRO_GUIA~"               = ~"GUIA~".~"NRO_GUIA~"                  ) and
		 ( ~"GUIA~".~"MOTIVO_TRASLADO~"             = ~"MOTIVO_TRASLADO~".~"MOTIVO_TRASLADO~") and
       ( ~"VALE_MOV~".~"TIPO_REFER~"              = ~"LOGPARAM~".~"DOC_OV~"                ) and
		 ( ~"MOTIVO_TRASLADO~".~"FLAG_FACTURACION~" = '1'                                ) and
      (( ~"VALE_MOV~".~"ORIGEN_REFER~"            = :as_origen                         ) and
       ( ~"VALE_MOV~".~"NRO_REFER~"               = :as_orden_venta                    ) and
       ( ~"GUIA~".~"FLAG_ESTADO~"                 = '1'                                ))  " arguments=(("as_origen", string),("as_orden_venta", string)) )
