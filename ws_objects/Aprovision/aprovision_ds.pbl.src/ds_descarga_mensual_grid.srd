table(column=(type=char(2) updatewhereclause=yes name=mes dbname="mes" )
 column=(type=number updatewhereclause=yes name=peso_bruto dbname="peso_bruto" )
 retrieve="select to_char(b.fecha_descarga, 'mm') as mes, sum(a.peso_bruto) as peso_bruto
from ap_pd_descarga_det a,
     ap_pd_descarga b,
     tg_naves c
where a.nro_parte = b.nro_parte
  and c.proveedor = a.proveedor     
  and c.flag_tipo_flota = :as_tflota
  and substr(b.nro_parte,1,2) = :as_origen
  and to_char(b.fecha_descarga, 'yyyy') = :as_ano
group by to_char(b.fecha_descarga, 'mm')  " arguments=(("as_tflota", string),("as_origen", string),("as_ano", string)) )
