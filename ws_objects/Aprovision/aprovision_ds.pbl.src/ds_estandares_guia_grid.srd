 retrieve="select a.cod_estandar, 
		 c.estandar_desc, 
       a.calidad_det_valor,
		 c.lectura_min,
 		 c.lectura_max
from 	ap_guia_recepcion_calidad a,
		ap_guia_recepcion_det b,
      tg_estandar c
where b.cod_maquina = a.cod_maquina
  and b.nro_reporte = a.nro_reporte
  and c.cod_estandar = a.cod_estandar
  and b.cod_guia_rec = :as_guia
  and a.cod_maquina = :as_maq
  and a.nro_reporte = :as_ticket
" arguments=(("as_guia", string),("as_maq", string),("as_ticket", string)) )
