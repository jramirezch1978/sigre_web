 retrieve="SELECT apdd.proveedor as cod_prov, 
	    p.nom_proveedor as nom_prov, 
       tump.descripcion as uso, 
       apdd.especie as cod_esp, 
       te.descr_especie as desc_esp, 
       sum(apdd.peso_bruto + decode(apdd.flag_variacion, 'C', (peso_variacion * -1), apdd.peso_variacion)) as peso, sum((apdd.peso_bruto + decode(apdd.flag_variacion, 'C', (peso_variacion * -1), apdd.peso_variacion)) * apdd.precio_venta) as precio

FROM 
		 ap_pd_descarga_det        apdd,
		 ap_pd_descarga			   apd,
       proveedor                 p,
       tg_usos_materia_prima     tump,
       tg_especies               te

WHERE 
		apdd.proveedor       	= p.proveedor 
  and apd.nro_parte			   = apdd.nro_parte
  and apdd.cod_uso_ref     	= tump.cod_uso
  and apdd.especie         	= te.especie 

  and trim(apd.cod_origen)	= trim(:asi_cod_origen)
  and trunc(apdd.inicio_descarga) between :adi_ini and :adi_fin

GROUP BY apdd.proveedor, p.nom_proveedor , tump.descripcion, apdd.especie, te.descr_especie
ORDER BY p.nom_proveedor, apdd.proveedor, tump.descripcion" arguments=(("asi_cod_origen", string),("adi_ini", date),("adi_fin", date))  sort="cod_prov A nom_prov A " )
