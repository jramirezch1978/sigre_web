 retrieve="SELECT  A.DESC_ALMACEN,
        VM.FEC_REGISTRO,
        A.NOM_ARTICULO,
        A.UND,
        AM.CANT_PROCESADA,
        ROUND(AM.CANT_PROCESADA * USF_FL_CONV_MON(AM.PRECIO_UNIT, 
                AM.COD_MONEDA, (SELECT COD_SOLES FROM LOGPARAM WHERE 
              RECKEY = '1'),VM.FEC_REGISTRO),2) AS IMPOR_CANT 
  
FROM   VALE_MOV                VM,
       ARTICULO_MOV            AM,
       ARTICULO_MOV_TIPO       AMT,
       ARTICULO                A,
       ALMACEN                 A
       
WHERE  VM.COD_ORIGEN                 = AM.COD_ORIGEN
  AND  VM.NRO_VALE                   = AM.NRO_VALE
  AND  VM.TIPO_MOV                   = AMT.TIPO_MOV
  AND  AM.COD_ART                    = A.COD_ART
  AND  VM.ALMACEN                    = A.ALMACEN
  AND  NVL(AMT.FACTOR_SLDO_TOTAL, 0) = -1
  AND  TO_CHAR(VM.FEC_REGISTRO,'yyyymmdd') BETWEEN :AS_FEC_INI AND :AS_FEC_FIN
  AND  VM.PROVEEDOR = :AS_PROVEEDOR" arguments=(("AS_FEC_INI", string),("AS_FEC_FIN", string),("AS_PROVEEDOR", string)) )
