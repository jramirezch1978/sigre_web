 retrieve="  SELECT A.NRO_PARTE,   
         A.ITEM,   
         A.FORMATO,   
         A.COD_MAQUINA,   
         A.ATRIBUTO,   
         A.HORA_INICIO,   
         A.HORA_FIN,   
         A.NRO_LECTURA,   
			A.NRO_CONTROL,
         A.VALOR_LECTURA,   
         B.DESC_MAQ,   
         C.DESCRIPCION,   
         D.MEDICION_MIN,   
         D.MEDICION_MAX,
			E.DESC_UNIDAD  
    FROM TG_PARTE_PISO_DET A,   
         MAQUINA B,   
         TG_MED_ACT_ATRIBUTO C,   
         TG_FMT_MED_ACT_DET D,
			UNIDAD E  
   WHERE A.COD_MAQUINA = B.COD_MAQUINA and  
         A.ATRIBUTO    = C.ATRIBUTO and  
         A.COD_MAQUINA = D.COD_MAQUINA AND
         A.FORMATO     = D.FORMATO AND
         A.ATRIBUTO    = D.ATRIBUTO AND
			E.UND			  = C.UND AND
         A.NRO_PARTE = :as_parte 
ORDER BY A.COD_MAQUINA ASC,   
         A.ATRIBUTO ASC,
         A.NRO_LECTURA ASC      " arguments=(("as_parte", string)) )
