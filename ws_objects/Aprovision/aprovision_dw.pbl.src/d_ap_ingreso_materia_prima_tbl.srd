 retrieve="SELECT PDD.INICIO_DESCARGA,
       GR.COD_GUIA_REC,
		 AM.NRO_VALE,
       P.NOM_PROVEEDOR,
       E.DESCR_ESPECIE,
       USF_AP_CONV_UND(PDD.UND,:AS_COD_UND, GRD.PESO_VENTA) AS PESO,
       USF_FL_CONV_MON(GRD.PRECIO_UNITARIO, GR.COD_MONEDA, :AS_COD_MONEDA, PDD.INICIO_DESCARGA) AS PRECIO

FROM   AP_PD_DESCARGA_DET      PDD,
       AP_GUIA_RECEPCION       GR,
       AP_GUIA_RECEPCION_DET   GRD,
       PROVEEDOR               P,
       TG_ESPECIES             E,
       ARTICULO_MOV            AM

WHERE  PDD.NRO_PARTE       = GRD.NRO_PARTE
  AND  PDD.ITEM            = GRD.ITEM_PARTE
  AND  GRD.COD_GUIA_REC    = GR.COD_GUIA_REC
  AND  PDD.ESPECIE         = E.ESPECIE
  AND  PDD.PROVEEDOR       = P.PROVEEDOR
  AND  GRD.NRO_MOV         = AM.NRO_MOV
  AND  TRUNC(PDD.INICIO_DESCARGA) BETWEEN TRUNC(:ad_fecha_ini) AND TRUNC(:ad_fecha_fin)
  AND  PDD.ESPECIE     LIKE :AS_COD_ESPECIE         /*Para indicar la especies */
  AND  INSTR(:AS_COD_ORIGEN, GR.ORIGEN) > 0   	    /*Para indicar el origen   */

/*ORDER BY PDD.INICIO_DESCARGA,
         GR.COD_GUIA_REC*/
" arguments=(("AS_COD_UND", string),("AS_COD_MONEDA", string),("AD_FECHA_INI", date),("AD_FECHA_FIN", date),("AS_COD_ESPECIE", string),("AS_COD_ORIGEN", string)) )
