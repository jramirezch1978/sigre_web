 retrieve="SELECT PDD.INICIO_DESCARGA,
       GD.COD_GUIA_REC,
       AM.NRO_VALE,
		 PDD.NRO_TICKET,
       E.DESCR_ESPECIE,
		 ROUND(USF_AP_CONV_UND(PDD.UND, :AS_COD_UND, GD.PESO_VENTA),3) as PESO_VENTA,
       ROUND(USF_FL_CONV_MON(USF_AP_CONV_PRECIO(PDD.UND, :AS_COD_UND, (NVL(GD.PRECIO_UNITARIO,0) + NVL(GD.PRECIO_UNITARIO_CASTIGO,0) +
                             NVL(GD.PRECIO_REPRESENTACION,0) + NVL(GD.PRECIO_AJUSTE_CNTBL,0))),
             GC.COD_MONEDA,:AS_COD_MON,PDD.INICIO_DESCARGA),2) AS PRECIO_UNIT,
       TU.DESCRIPCION,
       GC.PROVEEDOR,
       P.NOM_PROVEEDOR,
       P.RUC,
       N.NOMB_NAVE,
       N.MATRICULA

FROM   AP_PD_DESCARGA_DET     PDD,
       AP_GUIA_RECEPCION_DET  GD,
       AP_GUIA_RECEPCION      GC,
       TG_ESPECIES            E,
       TG_USOS_MATERIA_PRIMA  TU,
       ARTICULO_MOV           AM,
       PROVEEDOR              P,
       TG_NAVES               N

WHERE  GC.PROVEEDOR          = :AS_PROVEEDOR
  AND  PDD.NRO_PARTE         = GD.NRO_PARTE
  AND  PDD.ITEM              = GD.ITEM_PARTE
  AND  GD.COD_GUIA_REC       = GC.COD_GUIA_REC
  AND  PDD.ESPECIE           = E.ESPECIE
  AND  PDD.COD_USO_REF       = TU.COD_USO
  AND  GD.ORIGEN_MOV         = AM.COD_ORIGEN
  AND  GD.NRO_MOV            = AM.NRO_MOV
  AND  GC.PROVEEDOR          = N.PROVEEDOR (+)
  AND  GC.PROVEEDOR          = P.PROVEEDOR
  AND  TRUNC(PDD.INICIO_DESCARGA) BETWEEN TRUNC(:AD_FECHA_INICIO) AND TRUNC(:AD_FECHA_FIN)
  AND  GC.FLAG_ESTADO        <> '0'
  AND	 PDD.COD_ORIGEN		  IN (:ASA_COD_ORIGEN)" arguments=(("AS_PROVEEDOR", string),("AD_FECHA_INICIO", date),("AD_FECHA_FIN", date),("AS_COD_UND", string),("AS_COD_MON", string),("AS_ORIGEN", string),("ASA_COD_ORIGEN", stringlist))  sort="inicio_descarga A " )
