 retrieve="Select apd.especie, te.descr_especie, apd.inicio_descarga, apd.fin_descarga, 
       apd.proveedor, 
		 p1.nom_proveedor as proveedor, 
		 apd.prov_Transporte, 
       p2.nom_proveedor as transportista, 
       apd.nro_placa,
		 Nvl(apd.peso_estimado,0) as Toneladas
  From ap_pd_descarga_det apd, tg_especies te, proveedor p1, proveedor p2  
 Where apd.especie = te.especie
   and apd.proveedor = p1.proveedor (+)
   and apd.prov_Transporte = p2.proveedor (+)
   and trunc(apd.inicio_descarga) = :ad_fecha
	and apd.especie = :as_especie
 Order by apd.especie, apd.inicio_descarga" arguments=(("ad_fecha", date),("as_especie", string))  sort="especie A ap_pd_descarga_det_inicio_descarga A " )
