       Sum(Nvl(apd.peso_bruto,0)) as total
  From ap_pd_descarga_det apd, ap_pd_descarga ap, proveedor p
 Where apd.nro_parte = ap.nro_parte
   and apd.prov_transporte = p.proveedor(+)
   and apd.especie = :as_especie
   and trunc(apd.inicio_descarga)BetWeen :ad_fecha_ini and :ad_fecha_fin
