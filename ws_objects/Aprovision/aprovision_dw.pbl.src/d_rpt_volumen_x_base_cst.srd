table(column=(type=char(20) updatewhereclause=yes name=desc_base dbname="ap_bases_desc_base" )
 column=(type=decimal(0) updatewhereclause=yes name=ap_recibo_cajas_cant_cajas dbname="ap_recibo_cajas_cant_cajas" )
 retrieve="select apb.desc_base, 
		 'SEMANA' || to_char(a.semana, '00') as semana, 
		 apt.tipo_caja || '-' || apt.desc_tipo_caja as tipo_caja, apr.cant_cajas
from ap_control_cosecha     a,
     ap_control_cosecha_det b,
     ap_recibo_cajas        apr,
     ap_empacadora          ape,
     ap_sectores            aps,
     ap_bases               apb,
     ap_tipo_caja           apt
where a.nro_parte = b.nro_parte
  and b.nro_parte = apr.nro_parte
  and b.nro_certificacion = apr.nro_certificacion     
  and a.cod_empacadora    = ape.cod_empacadora
  and ape.cod_sector      = aps.cod_sector
  and aps.cod_base        = apb.cod_base   
  and apr.tipo_caja       = apt.tipo_caja
  and a.flag_estado		  <> '0'
  and apr.flag_estado	  <> '0'
  and ((:as_flag = '1' and trunc(a.fec_parte) between trunc(:ad_fecha1) and trunc(:ad_fecha2)) or :as_flag <> '1')
  and ((:as_flag = '2' and trim(to_char(a.ano)) || trim(to_char(a.semana, '00'))  between to_char(:ai_year1) || trim(to_char(:ai_week1, '00')) and to_char(:ai_year2) || trim(to_char(:ai_week2, '00'))) or :as_flag <> '2')
" arguments=(("as_flag", string),("ad_fecha1", date),("ad_fecha2", date),("ai_year1", number),("ai_year2", number),("ai_week1", number),("ai_week2", number))  sort="desc_base A " )
