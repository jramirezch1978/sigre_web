 retrieve="SELECT PD.NRO_PARTE,
		 PDD.INICIO_DESCARGA, 
       PDD.PROVEEDOR, 
       USF_AP_NOM_PROVEEDOR(PDD.PROVEEDOR) AS NOM_EMBARCACION,
       N.MATRICULA,
       USF_AP_CONV_UND(PDD.UND,(SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),PDD.PESO_BRUTO) AS PESO_TN,
       ROUND(USF_FL_CONV_MON(PDD.PRECIO_VENTA, PDD.COD_MONEDA, (select cod_dolares from logparam where reckey = '1'),
                       PDD.INICIO_DESCARGA),2) as PRECIO_$,
       (SELECT NVL(C.VTA_DOL_PROM ,0) FROM CALENDARIO C
  	      WHERE TRUNC(C.FECHA) = TRUNC(PDD.INICIO_DESCARGA )) AS TIPO_CAMBIO
FROM  AP_PD_DESCARGA      PD,
      AP_PD_DESCARGA_DET  PDD,
      PROVEEDOR           P,
      TG_NAVES            N      
WHERE PD.NRO_PARTE  = PDD.NRO_PARTE
  AND PDD.PROVEEDOR = P.PROVEEDOR
  AND PDD.PROVEEDOR = N.PROVEEDOR (+)
  AND PD.NRO_PARTE = :as_nro_parte" arguments=(("as_nro_parte", string)) )
