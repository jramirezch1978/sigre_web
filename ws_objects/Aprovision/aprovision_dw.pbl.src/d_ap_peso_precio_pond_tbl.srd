 retrieve="SELECT TO_CHAR(pdd.inicio_descarga, 'dd') AS fecha,
       e.descr_especie,

       SUM(DECODE(n.flag_tipo_flota, 'P', 
           USF_AP_CONV_UND(pdd.und,(SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
           NVL(gd.peso_venta,0)),0)) AS peso_P   ,                     
       
       SUM( DECODE(n.flag_tipo_flota, 'T', 
            USF_AP_CONV_UND(pdd.und, (SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
            NVL(gd.peso_venta, 0)), 0)) AS peso_T ,      

		 SUM( DECODE(n.flag_tipo_flota, NULL, 
            USF_AP_CONV_UND(pdd.und, (SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
            NVL(gd.peso_venta, 0)), 0)) AS peso_O ,                  
       
       DECODE(SUM(DECODE(n.flag_tipo_flota, 'P', NVL(gd.peso_venta, 0), 0)), 0, 0,      
              ROUND(SUM(DECODE(n.flag_tipo_flota, 'P', NVL(gd.peso_venta, 0) * 
		 	     USF_FL_CONV_MON(NVL(gd.precio_unitario, 0) + NVL(gd.precio_unitario_castigo,0) + 
			     NVL(gd.precio_representacion, 0), gc.cod_moneda, 
              (SELECT cod_dolares FROM logparam WHERE reckey = '1'), pdd.inicio_descarga))) / 
              SUM(DECODE(n.flag_tipo_flota, 'P', NVL(gd.peso_venta, 0), 0)), 2)) AS Precio_p, 
       
       DECODE(SUM(DECODE(n.flag_tipo_flota, 'T', NVL(gd.peso_venta, 0), 0)), 0, 0,      
              ROUND(SUM(DECODE(n.flag_tipo_flota, 'T', NVL(gd.peso_venta, 0) * 
				  USF_FL_CONV_MON(NVL(gd.precio_unitario, 0) + NVL(gd.precio_unitario_castigo,0) + 
			     NVL(gd.precio_representacion, 0), gc.cod_moneda, 
              (SELECT cod_dolares FROM logparam WHERE reckey = '1'), pdd.inicio_descarga))) / 
              SUM(DECODE(n.flag_tipo_flota, 'T', NVL(gd.peso_venta, 0), 0)), 2)) AS Precio_T, 
      
       DECODE(SUM(DECODE(n.flag_tipo_flota, NULL, NVL(gd.peso_venta, 0), 0)), 0, 0,      
              ROUND(SUM(DECODE(n.flag_tipo_flota, NULL, NVL(gd.peso_venta, 0) * 
				  USF_FL_CONV_MON(NVL(gd.precio_unitario, 0) + NVL(gd.precio_unitario_castigo,0) + 
			     NVL(gd.precio_representacion, 0), gc.cod_moneda, 
              (SELECT cod_dolares FROM logparam WHERE reckey = '1'), pdd.inicio_descarga))) / 
              SUM(DECODE(n.flag_tipo_flota, NULL, NVL(gd.peso_venta, 0), 0)), 2)) AS Precio_O, 
                          
       SUM(USF_AP_CONV_UND(pdd.und,(SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
            NVL(gd.peso_venta,0))) AS peso_Tot,           
       
       DECODE(SUM(NVL(gd.peso_venta,0)),0,0,
             ROUND(SUM(NVL(gd.peso_venta,0) * USF_FL_CONV_MON(NVL(gd.precio_unitario,0) + 
             NVL(gd.precio_unitario_castigo,0) + NVL(gd.precio_representacion,0), 
             gc.cod_moneda,(SELECT cod_dolares FROM logparam WHERE reckey = '1'), 
             pdd.inicio_descarga)) / SUM(NVL(gd.peso_venta,0)),2)) AS Precio_Tot      
      
FROM  ap_pd_descarga_det      pdd,				/* Detalle del parte diario */
      ap_guia_recepcion_det   gd,				/* Detalle de Guia de Recepcion */
      ap_guia_recepcion       gc,				/* Cabecera de Guia de Recepcion */
      tg_naves                n,					/* Tabla de naves */
      tg_especies             e           /* Tabla especies */

WHERE  gd.nro_parte                          = pdd.nro_parte
  AND  gd.item_parte                         = pdd.item
  AND  pdd.proveedor                         = n.proveedor (+)
  AND  gd.cod_guia_rec                       = gc.cod_guia_rec
  AND  pdd.especie                           = e.especie
  AND  To_char(pdd.inicio_descarga, 'yyyy')  = :as_year  
  AND  To_char(pdd.inicio_descarga, 'MM')  = :as_mes
  AND  pdd.especie                           LIKE :as_cod_especie  /*Para indicar la especies */
  AND  INSTR(:as_cod_origen, gc.origen) > 0  	                   /*Para indicar el origen   */

GROUP BY TO_CHAR(pdd.inicio_descarga, 'dd'),
         e.descr_especie

ORDER BY  TO_CHAR(pdd.inicio_descarga, 'dd')" arguments=(("as_year", string),("as_mes", string),("as_cod_especie", string),("as_cod_origen", string)) )
