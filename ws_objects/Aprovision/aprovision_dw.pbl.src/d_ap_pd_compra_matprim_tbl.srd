 column=(type=char(4000) updatewhereclause=yes name=nom_embarcacion dbname="nom_embarcacion" )
 column=(type=char(12) updatewhereclause=yes name=matricula dbname="matricula" )
 column=(type=char(0) updatewhereclause=yes name=patron_lancha dbname="compute_0005" )
 column=(type=number updatewhereclause=yes name=peso_venta dbname="peso_venta" )
 column=(type=number updatewhereclause=yes name=precio_unitario dbname="precio_unitario" )
 column=(type=number updatewhereclause=yes name=precio_castigo dbname="precio_castigo" )
 retrieve="SELECT gc.proveedor as cod_prov,
       p.nom_proveedor as proveedor,
       USF_AP_NOM_PROVEEDOR(pdd.proveedor) AS NOM_EMBARCACION,
       n.matricula,
		 '',
       sum(round(USF_AP_CONV_UND(pdd.UND,
					 (SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
					 gd.peso_venta),4)) as peso_venta, 								/*Peso en toneladas*/

       round(sum(gd.peso_venta * USF_FL_CONV_MON(gd.precio_unitario, 
					 gc.cod_moneda, (select cod_dolares from logparam where 
					 reckey = '1'), pdd.inicio_descarga)) / sum(gd.peso_venta),2) 
					 as Precio_unitario,													/* Precio unitario en dolares */

		round(sum(gd.peso_venta * USF_FL_CONV_MON(gd.precio_unitario_castigo, 
					 gc.cod_moneda, (select cod_dolares from logparam where 
					 reckey = '1'), pdd.inicio_descarga)) / sum(gd.peso_venta),2) 
					 as precio_castigo													/* Precio castigo en dolares */

	/*	round(sum(gd.peso_venta * USF_FL_CONV_MON(gd.precio_representacion, 
					 gc.cod_moneda, (select cod_dolares from logparam where 
					 reckey = '1'), pdd.inicio_descarga))/ sum(gd.peso_venta),2) 
					 as precio_representacion											 Precio representacion en dolares */

FROM ap_guia_recepcion     gc,   /* cabecera de la guia */
     ap_guia_recepcion_det gd,    /* detalle de la guia */
     proveedor              p,    /* datos del proveedor */
     ap_pd_descarga_det     pdd,  /* detalle del parte diario de descarga */
     tg_naves               n    /* tabla de naves */

WHERE trunc(pdd.inicio_descarga) BETWEEN trunc(:ad_fecha_ini) AND trunc(:ad_fecha_fin)
  AND gc.cod_guia_rec 	= gd.cod_guia_rec
  AND gd.nro_parte    	= pdd.nro_parte
  AND gd.item_parte   	= pdd.item
  AND pdd.proveedor   	= p.proveedor
  AND pdd.proveedor   	= n.proveedor (+)
  AND pdd.especie     LIKE :as_cod_especie      /*Para indicar la especies */
  AND INSTR(:as_cod_origen, gc.origen) > 0   	/*Para indicar el origen   */

GROUP BY gc.proveedor,
       p.nom_proveedor,
       USF_AP_NOM_PROVEEDOR(pdd.proveedor),
       n.matricula

ORDER BY gc.proveedor
" arguments=(("as_cod_origen", string),("as_cod_especie", string),("ad_fecha_ini", date),("ad_fecha_fin", date),("an_tipo_cambio", number)) )
