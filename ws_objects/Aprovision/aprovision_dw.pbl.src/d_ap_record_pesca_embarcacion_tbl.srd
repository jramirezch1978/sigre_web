 column=(type=number updatewhereclause=yes name=peso_venta_tn dbname="peso_venta_tn" )
 retrieve="SELECT PDD.PROVEEDOR, 
       N.NOMB_NAVE,
       N.MATRICULA,
       N.CAPAC_BODEGA,
       SUM(ROUND(USF_AP_CONV_UND(PDD.UND, (SELECT UND_TM FROM AP_PARAM WHERE ORIGEN = 'XX'),
					 GD.PESO_VENTA),4)) as PESO_VENTA_TN								/*Peso en toneladas*/
      
FROM  AP_PD_DESCARGA_DET      PDD,
      TG_NAVES                N,
      AP_GUIA_RECEPCION_DET   GD,
      TG_ESPECIES             E

WHERE PDD.PROVEEDOR = N.PROVEEDOR
  AND PDD.ESPECIE   = E.ESPECIE
  AND PDD.NRO_PARTE = GD.NRO_PARTE
  AND PDD.ITEM      = GD.ITEM_PARTE
  AND TRUNC(PDD.INICIO_DESCARGA) BETWEEN TRUNC(:AD_FECHA_INI) AND TRUNC(:AD_FECHA_FIN)
  AND INSTR(:AS_COD_ORIGEN, PDD.COD_ORIGEN) > 0
  AND E.FLAG_TIPO_MATPRIM        			  = 'H'        /* Solo filtra especies Hidrobiologicas */
  AND N.FLAG_TIPO_FLOTA       				 LIKE :AS_FLOTA

GROUP BY PDD.PROVEEDOR, 
         N.NOMB_NAVE,
         N.MATRICULA,
         N.CAPAC_BODEGA


" arguments=(("AD_FECHA_INI", date),("AD_FECHA_FIN", date),("AS_COD_ORIGEN", string),("AS_FLOTA", string))  sort="peso_venta_tn D " )
