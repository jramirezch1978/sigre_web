 retrieve="SELECT  rownum,
        gd.cod_guia_rec, 
        round(USF_FL_CONV_MON(gd.precio_unitario + gd.precio_unitario_castigo + gd.precio_representacion,
        gc.cod_moneda, am.cod_moneda, pdd.inicio_descarga),4)  as pre_uni_guia,
        usf_ap_conv_precio(a.und, pdd.und, am.precio_unit) 		as pre_uni_mov,
        gd.peso_venta   	 as peso_guia,
        usf_ap_conv_und(a.und, pdd.und, am.cant_procesada)     as peso_mov,
        pdd.inicio_descarga as fecha_guia,
        vm.fec_registro		 as fecha_mov,
        gd.almacen_dst      as Alm_dest_guia,
        vm.almacen          as Alm_dest_mov
FROM   ap_guia_recepcion_det gd,				     /* Detalle de la Guia de Recepcion */
       ap_pd_descarga_det 	 pdd,				     /* Detalle del Parte diario de descarga */
       ap_guia_recepcion 		 gc,				  /* Cabecera de la Guia de Recepcion */
       vale_mov 					   vm,			  /* Cabecera del movimiento */
       articulo_mov 			   am,				  /* Detalle del movimiento */
       articulo              a                 /* Articulos  */
WHERE  pdd.nro_parte 	 = gd.nro_parte
  AND  pdd.item 			 = gd.item_parte 
  AND  gc.cod_guia_rec = gd.cod_guia_rec
  AND  gd.nro_mov 		 = am.nro_mov
  AND  gd.origen_mov	 = am.cod_origen
  AND  vm.nro_vale 	   = am.nro_vale
  AND  am.cod_art      = a.cod_art
  AND  (vm.fec_registro <> pdd.inicio_descarga or 
        USF_AP_CONV_UND(a.und, pdd.und,am.cant_procesada) <> gd.peso_venta or 
        USF_AP_CONV_PRECIO(pdd.und, a.und, round(USF_FL_CONV_MON(gd.precio_unitario + gd.precio_unitario_castigo + gd.precio_representacion,
        gc.cod_moneda, am.cod_moneda, pdd.inicio_descarga),4)) <> am.precio_unit or
        gd.almacen_dst <> vm.almacen)
 AND  Trunc(pdd.inicio_descarga) BETWEEN trunc(:ad_fecha_ini) AND trunc(:ad_fecha_fin)" arguments=(("ad_fecha_ini", date),("ad_fecha_fin", date)) )
