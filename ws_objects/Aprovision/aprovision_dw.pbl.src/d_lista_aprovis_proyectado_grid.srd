 retrieve="  SELECT A.ORIGEN             ,   
			   A.ANO                ,
         A.ESPECIE            ,   
         B.DESCR_ESPECIE      ,   
			   A.FLAG_PROVEEDOR     ,
         A.FLAG_ZONA_DESCARGA ,   
         sum(A.CANTIDAD_ESTIMADA) AS cantidad_estimada  ,   
         sum(A.CANTIDAD_REAL) AS cantidad_real      ,   
         A.UNIDAD_PESO        ,
         C.DESC_UNIDAD        
    FROM AP_PROYECCION A,   
         TG_ESPECIES B,   
         UNIDAD C   
   WHERE B.ESPECIE 	= A.ESPECIE
	  and C.UND 		= A.UNIDAD_PESO 
	  and a.ano 		= :ai_year
GROUP BY A.ORIGEN             ,   
			   A.ANO                ,
         A.ESPECIE            ,   
         B.DESCR_ESPECIE      ,   
			   A.FLAG_PROVEEDOR     ,
         A.FLAG_ZONA_DESCARGA ,   
         A.UNIDAD_PESO        ,
         C.DESC_UNIDAD        
   ORDER BY a.origen, A.FLAG_PROVEEDOR 
" arguments=(("ai_year", number)) )
