 retrieve="select apdd.proveedor as cod_prov, cr.nombre as nom_prov, tump.descripcion as uso, apdd.especie as cod_esp, te.descr_especie as desc_esp, sum(apdd.peso_bruto) as peso, sum((apdd.peso_bruto * agrd.precio_unitario) - (apdd.peso_bruto * agrd.precio_unitario_castigo) + agrd.precio_representacion) as precio
   from ap_guia_recepcion_det agrd
      inner join ap_guia_recepcion agr on agrd.cod_guia_rec = agr.cod_guia_rec
      inner join ap_pd_descarga_det apdd on agrd.nro_parte = apdd.nro_parte and agrd.item_parte = apdd.item
      inner join codigo_relacion cr on apdd.proveedor = cr.cod_relacion
      inner join tg_usos_materia_prima tump on apdd.cod_uso_ref = tump.cod_uso
      inner join tg_especies te on apdd.especie = te.especie
   where trunc(apdd.fin_descarga) between :adi_ini and :adi_fin
      and trim(apdd.cod_origen) = trim(:asi_cod_origen)
   group by apdd.proveedor, cr.nombre, tump.descripcion, apdd.especie, te.descr_especie
   order by cr.nombre, apdd.proveedor, tump.descripcion, te.descr_especie, apdd.especie" arguments=(("asi_cod_origen", string),("adi_ini", date),("adi_fin", date))  sort="cod_prov A nom_prov A " )
