 retrieve="  SELECT TT.ALMACEN,
			TT.COD_ART,   
         TT.FECHA,   
         TT.INGRESOS,   
         TT.SALIDAS,   
         TT.SALDO,   
         TT.DISPONIBLE,   
         TT.POR_COMPRAR,   
         A.DESC_ART,   
         A.UND,   
         TT.NRO_ITEM,   
         TT.NRO_MOV,   
         TT.TIPO_MOV,   
         TT.OPERACION,   
         TT.CENCOS,   
         TT.CNTA_PRSP,   
         TT.STOCK,
  			TT.FLAG_ESTADO,
			TT.comp_sugerida,
			TT.OPER_SEC,
			TT.origen_mov_proy,
			tt.nro_mov_proy,
			tt.tipo_doc,
			tt.nro_doc
    FROM TT_COMPRAS_SUGERIDAS TT,   
         ARTICULO 				A
   WHERE TT.COD_ART 				= A.COD_ART 
     and TT.TIPO_MOV = (Select OPER_CONS_INTERNO from logparam where reckey = '1')
	  and NVL(A.FLAG_INVENTARIABLE, '0') = '1'
     and TT.POR_COMPRAR > 0 
     and trim(tt.origen_mov_proy) || trim(to_char(tt.nro_mov_proy)) not in (
				SELECT trim(amp_origen) || trim(to_char(amp_nro)) 
				FROM prog_compras_amp  	a,
					  prog_compras_det	b
				WHERE a.nro_programa = b.nro_programa
              and a.nro_item		= b.nro_item
				  and b.flag_estado = '1'
				  and b.cant_proyect > NVL(b.cant_procesada,0))
ORDER BY TT.ALMACEN ASC,
			A.COD_ART ASC,   
         TT.NRO_ITEM ASC   

" )
