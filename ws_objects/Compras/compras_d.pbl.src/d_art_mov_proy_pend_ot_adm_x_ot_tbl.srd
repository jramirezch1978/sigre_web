 retrieve="  SELECT A.COD_ART,   
         A.NOM_ARTICULO,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP.FEC_PROYECT,
			AMP.CANT_PROCESADA,   
         AMP.CANT_PROYECT - AMP.CANT_PROCESADA as cant_pendiente,   
         AMP.TIPO_DOC,   
         AMP.NRO_DOC,   
         AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.OPER_SEC,   
         AMP.ALMACEN,   
         AMP.FEC_REGISTRO,
			AMP.CANT_RESERVADO,
         AALM.SLDO_CONSIGNACION,   
         NVL(AALM.SLDO_TOTAL,0) + NVL(AALM.SLDO_CONSIGNACION,0) as sldo_fisico,
         AALM.SLDO_POR_LLEGAR,   
         AALM.SLDO_SOLICITADO,
         MIN(AMPOC.NRO_DOC) as nro_doc_oc,
         NVL(SUM(CASE WHEN AMPOC.FLAG_ESTADO = 1 THEN NVL(AMPOC.CANT_PROYECT,0)
                      WHEN AMPOC.FLAG_ESTADO = 2 THEN NVL(AMPOC.CANT_PROCESADA,0)
                      END), 0 ) as 	qnt_comp,   
         NVL(SUM(AMPOC.CANT_PROCESADA), 0) as qnt_recib,
			COUNT(AMPOC.NRO_DOC) as cant_oc 
    FROM ARTICULO A,   
         ARTICULO_MOV_PROY AMP,   
         ARTICULO_ALMACEN AALM,   
         ALMACEN ALM,
			ORDEN_TRABAJO O,
         (SELECT * FROM ARTICULO_MOV_PROY WHERE TIPO_DOC = :as_doc_oc AND FLAG_ESTADO IN ('1','2')) AMPOC  
   WHERE ( AMP.COD_ART = A.COD_ART ) and  
         ( AMP.COD_ART = AALM.COD_ART ) and  
         ( AMP.ALMACEN = AALM.ALMACEN ) and  
         ( ALM.ALMACEN = AMP.ALMACEN ) and
         ( O.NRO_ORDEN = AMP.NRO_DOC) and
			( AMP.COD_ORIGEN = AMPOC.ORG_AMP_REF(+) and AMP.NRO_MOV = AMPOC.NRO_AMP_REF(+) ) and   
         ( ( AMP.FLAG_ESTADO = '1' ) AND  
         ( AMP.CANT_PROYECT > AMP.CANT_PROCESADA ) AND  
         ( trunc(AMP.FEC_PROYECT) between :ad_inicio and :ad_fin ) AND  
         ( AMP.TIPO_DOC = :as_doc_ot ) AND  
         ( AMP.TIPO_MOV = :as_oper ) AND  
         ( A.FLAG_INVENTARIABLE = '1' ) AND  
         ( ALM.COD_ORIGEN in ( :as_origen ) ) AND
         ( AALM.FLAG_REPOSICION in ( :as_reposicion ) ) AND
         ( O.OT_ADM = :as_ot_adm) ) 
GROUP BY A.COD_ART,   
         A.NOM_ARTICULO,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP.FEC_PROYECT,   
         AMP.CANT_PROYECT,
      	AMP.CANT_PROCESADA,   
         AMP.CENCOS,   
         AMP.TIPO_DOC,   
         AMP.NRO_DOC,   
         AMP.COD_ORIGEN,
         AMP.NRO_MOV,   
         AMP.OPER_SEC,   
         AMP.ALMACEN,   
         AMP.FEC_REGISTRO,
         AMP.COD_USR,
         AMP.CANT_RESERVADO,
			AALM.SLDO_TOTAL,
			AALM.SLDO_CONSIGNACION,   
         AALM.SLDO_POR_LLEGAR,   
         AALM.COSTO_ULT_COMPRA,   
         AALM.FEC_ULT_COMPRA,   
         AALM.SLDO_SOLICITADO   
" arguments=(("as_doc_ot", string),("as_doc_oc", string),("as_oper", string),("ad_inicio", date),("ad_fin", date),("as_origen", stringlist),("as_reposicion", stringlist),("as_ot_adm", string))  sort="tipo_doc A nro_doc A cod_art A fec_proyect A " )
