 retrieve="  SELECT oc.COD_ORIGEN,   
         oc.NRO_OC,   
         oc.FLAG_ESTADO,   
         oc.FLAG_IMPORTACION,   
         oc.PROVEEDOR,   
         oc.FEC_REGISTRO,   
         oc.FORMA_PAGO,   
         oc.COD_MONEDA,   
         am.COD_ART,   
         am.FEC_PROYECT,   
         am.CANT_PROYECT,   
         am.CANT_PROCESADA,   
         am.CANT_FACTURADA,   
         am.CENCOS,   
         p.NOM_PROVEEDOR,   
         fp.DESC_FORMA_PAGO,   
         a.NOM_ARTICULO,   
         a.UND  
    FROM ARTICULO_MOV_PROY am,   
         ORDEN_COMPRA oc,   
         PROVEEDOR p,   
         FORMA_PAGO fp,   
         ARTICULO a  
   WHERE ( oc.COD_ORIGEN = am.COD_ORIGEN ) and  
         ( oc.NRO_OC = am.NRO_DOC ) and  
         ( oc.PROVEEDOR = p.PROVEEDOR ) and  
         ( oc.FORMA_PAGO = fp.FORMA_PAGO ) and  
         ( am.COD_ART = a.COD_ART ) and  
         ( ( am.TIPO_DOC = (  SELECT l.DOC_OC  
                                                   FROM LOGPARAM l  
                                                  WHERE l.RECKEY = '1'  )) AND  
         ( TRUNC(oc.FEC_REGISTRO) >= :ad_desde ) AND  
         ( TRUNC(oc.FEC_REGISTRO) <= :ad_hasta ) )    
" arguments=(("ad_desde", date),("ad_hasta", date)) )
