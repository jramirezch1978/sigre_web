 column=(type=number updatewhereclause=yes name=items dbname="items" )
 retrieve="select u.cod_usr, 
		 u.nombre, 
       a.cod_art,
       a.desc_art,
       a.und,
		 p.proveedor,
		 p.nom_proveedor,
       SUM(amp.cant_proyect) AS cantidad,
		 sum( usf_fl_conv_mon(amp.cant_proyect * (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), fec_proyect) ) as monto_dol,
       sum( usf_fl_conv_mon(amp.cant_proyect * (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), fec_proyect) ) / Sum(amp.cant_proyect) as precio_prom,
		 count(*) as items
from articulo_mov_proy amp,
     orden_compra      oc,
     articulo          a,
	  proveedor			  p,
     usuario           u
where u.cod_usr = amp.cod_usr
  and oc.nro_oc = amp.nro_doc
  and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
  AND amp.cod_art = a.cod_art
  and oc.proveedor = p.proveedor
  and amp.flag_Estado <> '0'
  and oc.flag_estado <> '0'
  and trunc(oc.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
  and oc.cod_origen like :as_origen
  and amp.cod_usr = :as_user
  and amp.cod_art = :as_cod_art
group by u.cod_usr, 
         u.nombre, 
         a.cod_art,
         a.desc_art,
         a.und,
			p.proveedor,
		 	p.nom_proveedor
ORDER BY monto_dol desc" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_origen", string),("as_user", string),("as_cod_art", string)) )
