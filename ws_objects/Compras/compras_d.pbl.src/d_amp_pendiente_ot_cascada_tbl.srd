 retrieve="  SELECT A.COD_ART,   
         A.NOM_ARTICULO,
         A.DIAS_REPOSICION,
         A.UND, 
         AMP.FEC_PROYECT,   
         AMP.CANT_PROYECT - AMP.CANT_PROCESADA as cant_pendiente,   
         AMP.CENCOS,   
         AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.OPER_SEC,
         AMP.FEC_REGISTRO,   
         AMP.ALMACEN,
         AMP.COD_USR,
         AMP.OPER_SEC,    
         AMPOC.NRO_DOC as nro_doc_oc,
         AMPOC.CANT_PROYECT as qnt_comp,
         AMPOC.CANT_PROCESADA as qnt_recib,
         OT.OT_ADM  
    FROM ARTICULO A,   
         ARTICULO_MOV_PROY AMP,   
         ARTICULO_ALMACEN AALM,
			(SELECT NRO_DOC, OPER_SEC, COD_ART, CANT_PROYECT, CANT_PROCESADA FROM ARTICULO_MOV_PROY WHERE TIPO_DOC = :as_doc_oc)  AMPOC,
         ORDEN_TRABAJO OT   
   WHERE ( AMP.COD_ART = A.COD_ART ) and  
         ( AMP.COD_ART = AALM.COD_ART ) and  
         ( AMP.ALMACEN = AALM.ALMACEN ) and
         ( AMP.OPER_SEC = AMPOC.OPER_SEC(+) and AMP.COD_ART = AMPOC.COD_ART(+) ) and
         ( AMP.NRO_DOC = OT.NRO_ORDEN) AND  
         ( ( AMP.FLAG_ESTADO = '1' ) AND  
         ( AMP.CANT_PROYECT > AMP.CANT_PROCESADA ) AND 
         ( AMP.NRO_DOC = :as_nro_ot ) AND  
         ( AMP.TIPO_DOC = :as_doc_ot ) AND  
         ( AMP.TIPO_MOV = :as_oper ) )   
ORDER BY A.COD_ART ASC,   
         AMP.FEC_PROYECT ASC   
" arguments=(("as_doc_ot", string),("as_doc_oc", string),("as_oper", string),("as_nro_ot", string)) )
