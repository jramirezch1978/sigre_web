 retrieve="select tt.proveedor, p.nom_proveedor, 
		 sum(tt.cant_proyect*(tt.precio_unit-tt.descuento)) as total_dol, 
       sum(tt.cant_proyect*(tt.precio_unit-tt.descuento)*100/:ad_total) as porc
