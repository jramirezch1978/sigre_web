 retrieve="  SELECT DISTINCT 
			PC.COD_ORIGEN,
      	PC.NRO_PROGRAMA,
			PC.OBSERVACIONES,
			PC.FLAG_ESTADO,
         PC.FEC_REGISTRO,
         PC.COD_USR,
         PC.FLAG_REPLICACION  
  FROM PROG_COMPRAS 		   PC,
		 PROG_COMPRAS_DET 	PCD,
		 ARTICULO				A,
		 COTIZACION				C1,
		 COTIZACION_PROVEE	C2,
		 COTIZACION_PROVEE_BIEN_DET C3
  WHERE PCD.NRO_PROGRAMA = PC.NRO_PROGRAMA
    and A.COD_ART 		 = PCD.COD_ART
	 and C1.NRO_COTIZA	 = C2.NRO_COTIZA
	 AND C2.NRO_COTIZA	 = C3.NRO_COTIZA
    AND C2.PROVEEDOR	    = C3.PROVEEDOR
    AND C3.COD_ART		 = PCD.COD_ART
	 AND NVL(C1.FLAG_ESTADO, '0') = '1'
    AND NVL(C2.COTIZO, '0') = '1'
	 AND NVL(C3.FLAG_GANADOR, '0') = '1'
    AND NVL(C3.PRECIO_UNIT, 0) > 0
	 AND C1.NRO_COTIZA LIKE :as_nro_cotiza
    and PCD.FLAG_ESTADO  = '1'
    and NVL(PCD.cant_proyect,0) - NVL(PCD.cant_procesada,0)  > 0
	 and TRUNC(PCD.FEC_REQUERIDA) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
	 and PCD.COD_ART     	like :as_cod_art
    and UPPER(A.DESC_ART)  like :as_desc_art
	 and PCD.ALMACEN	   	like :as_almacen
    and A.SUB_CAT_ART	  	like :as_cod_sub_cat
    and PC.nro_programa 	like :as_nro_programa
    and ( PCD.NRO_AMP_OT_REF is null or 
			( PCD.NRO_AMP_OT_REF is not null and 
				USF_ALM_CANT_LIBRE_OT(PCD.ORG_AMP_OT_REF, PCD.NRO_AMP_OT_REF) > 0 ) )
    and pcd.comprador  = :as_user
  ORDER BY PC.NRO_PROGRAMA ASC   
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_cod_art", string),("as_desc_art", string),("as_almacen", string),("as_nro_programa", string),("as_cod_sub_cat", string),("as_nro_cotiza", string),("as_user", string)) )
