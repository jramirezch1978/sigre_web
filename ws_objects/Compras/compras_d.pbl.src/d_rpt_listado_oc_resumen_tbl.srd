 column=(type=char(60) updatewhereclause=yes name=nom_aprobador dbname="usuario.nom_aprobador" dbalias="u.nom_aprobador" )
 column=(type=datetime updatewhereclause=yes name=fecha_aprob dbname="orden_compra.fecha_aprob" dbalias="oc.fecha_aprob" )
 column=(type=char(2000) updatewhereclause=yes name=motivo_rechazo dbname="orden_compra.motivo_rechazo" dbalias="oc.motivo_rechazo" )
 retrieve=" SELECT  OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.FEC_REGISTRO,   
         OC.PROVEEDOR,   
         P.NOM_PROVEEDOR,   
         OC.FORMA_PAGO,   
         OC.COD_MONEDA,   
         OC.MONTO_TOTAL,   
         OC.MONTO_FACTURADO,   
         OC.NOM_VENDEDOR,   
         OC.OBSERVACION,   
         OC.FLAG_ESTADO,   
         u.nombre as nom_aprobador,
         oc.fecha_aprob,
         oc.motivo_rechazo
    FROM ORDEN_COMPRA       oc,     
         PROVEEDOR          p,
         usuario            u
   WHERE OC.PROVEEDOR   = P.PROVEEDOR 
     and oc.aprobador   = u.cod_usr (+)
     and trunc(OC.FEC_REGISTRO ) BETWEEN TRUNC(:ad_fecha_ini ) AND  TRUNC( :ad_fecha_fin ) 
ORDER BY OC.NRO_OC ASC   
" arguments=(("ad_fecha_ini", date),("ad_fecha_fin", date)) )
