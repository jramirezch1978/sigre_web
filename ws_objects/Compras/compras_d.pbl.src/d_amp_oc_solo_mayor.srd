 retrieve="  SELECT R.COD_ORIGEN, R.NRO_MOV, R.COD_ART, A.NOM_ARTICULO, R.ALMACEN, R.CANT_PROYECT, R.CANT_PROCESADA, R.CANT_RESERVADO, R.OPER_SEC, R.TIPO_DOC, R.NRO_DOC, L.SLDO_TOTAL, L.SLDO_RESERVADO,
         SUM( O.CANT_PROYECT) as OC_CNT_PRY, SUM( O.CANT_PROCESADA) as OC_CNT_PRS, COUNT(O.NRO_DOC) as OC_CNT_DOC
    FROM ARTICULO_MOV_PROY R,
         ARTICULO_MOV_PROY O,
         ARTICULO A,
         ARTICULO_ALMACEN L
   WHERE R.COD_ART  = A.COD_ART AND
         R.OPER_SEC = O.OPER_SEC AND
         R.COD_ART  = O.COD_ART AND
         R.ALMACEN  = O.ALMACEN AND
         R.COD_ART  = L.COD_ART AND
         R.ALMACEN  = L.ALMACEN AND
         R.TIPO_DOC = :as_doc_ot AND
         O.TIPO_DOC = :as_doc_oc AND
         R.FLAG_ESTADO = '1' AND
			O.FLAG_ESTADO in ('1', '2') 
GROUP BY R.COD_ORIGEN, R.NRO_MOV, R.COD_ART, A.NOM_ARTICULO, R.ALMACEN, R.CANT_PROYECT, R.CANT_PROCESADA, R.CANT_RESERVADO, R.OPER_SEC, R.TIPO_DOC, R.NRO_DOC, L.SLDO_TOTAL, L.SLDO_RESERVADO
  HAVING SUM( O.CANT_PROYECT) > R.CANT_PROYECT" arguments=(("as_doc_ot", string),("as_doc_oc", string))  sort="nro_doc A oper_sec A cod_art A almacen A nro_mov A " )
