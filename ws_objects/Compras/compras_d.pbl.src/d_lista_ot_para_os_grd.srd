 column=(type=datetime updatewhereclause=yes name=fec_inicio dbname="orden_trabajo.fec_inicio" )
 column=(type=datetime updatewhereclause=yes name=fec_registro dbname="orden_trabajo.fec_registro" )
 column=(type=char(10) updatewhereclause=yes name=ot_adm dbname="orden_trabajo.ot_adm" )
 column=(type=char(800) updatewhereclause=yes name=descripcion dbname="orden_trabajo.descripcion" )
 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="orden_trabajo.flag_estado" values="Anulado	0/Abierto	1/Terminado	2/Planeado	3/No modificable	5/" )
 column=(type=char(1) updatewhereclause=yes name=flag_estructura dbname="orden_trabajo.flag_estructura" )
 column=(type=datetime updatewhereclause=yes name=fecha_mayor dbname="fecha_mayor" )
 column=(type=datetime updatewhereclause=yes name=fecha_menor dbname="fecha_menor" )
 retrieve="select ot.nro_orden    , 
		 ot.fec_inicio   , 
		 ot.fec_registro , 
		 ot.ot_adm       , 
		 ot.descripcion  ,
		 ot.flag_estado  ,
		 ot.flag_estructura,
		 MAX(trunc(op.fec_inicio)) as fecha_mayor,
		 MIN(trunc(op.fec_inicio)) as fecha_menor
  from operaciones    op,
       ejecutor       ej,
       orden_trabajo  ot
where ot.nro_orden    = op.nro_orden
  and ej.cod_ejecutor = op.cod_ejecutor
  and ej.flag_externo = '1'
  and op.flag_estado	 = '1'
  and USF_CMP_OPER_SEC_ACTIVO(op.oper_sec ) = 1
  and trunc(op.fec_inicio) between trunc(:as_fecha_inicio) and trunc(:as_fecha_final)
  and op.oper_sec not in (select oper_sec from orden_servicio_det where flag_estado <> '0' and oper_sec is not null)
group by ot.nro_orden    , 
		 ot.fec_inicio     , 
		 ot.fec_registro   , 
		 ot.ot_adm         , 
		 ot.descripcion    ,
		 ot.flag_estado    ,
		 ot.flag_estructura
order by ot.fec_registro desc, ot.nro_orden asc" arguments=(("as_fecha_inicio", date),("as_fecha_final", date)) )
