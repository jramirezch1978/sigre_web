 column=(type=char(3) updatewhereclause=yes name=und dbname="articulo.und" )
 retrieve="SELECT distinct a.COD_ART, a.desc_art, a.und
 FROM ARTICULO_MOV_PROY amp,
		ORDEN_COMPRA 		oc,
		ARTICULO				a
WHERE amp.COD_ORIGEN = oc.COD_ORIGEN  
  and amp.NRO_DOC 	= oc.NRO_OC  
  and amp.tipo_doc 	= (select doc_oc from logparam where reckey = '1')
  and amp.cod_art   	= a.cod_art
  and amp.FLAG_ESTADO <> '0'  
  AND oc.flag_estado	 <> '0'
  and trunc(oc.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
