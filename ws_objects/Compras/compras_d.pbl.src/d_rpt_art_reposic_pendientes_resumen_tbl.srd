 column=(type=number updatewhereclause=yes name=cant_pendiente dbname="cant_pendiente" )
 retrieve="SELECT A.COD_ART,   
       A.NOM_ARTICULO,   
       A.UND,          
       AMP.ALMACEN,
       NVL(AA.SLDO_TOTAL,0) AS SLDO_TOTAL,
       NVL(AA.SLDO_POR_LLEGAR,0) AS SLDO_POR_LLEGAR,
       NVL(AA.COSTO_ULT_COMPRA,0) AS COSTO_ULT_COMPRA,
       NVL(AA.SLDO_MINIMO,0) AS SLDO_MINIMO, 
       NVL(AA.SLDO_MAXIMO,0) AS SLDO_MAXIMO,
       SUM((AMP.CANT_PROYECT - AMP.CANT_PROCESADA)) as cant_pendiente       
  FROM ARTICULO_MOV_PROY AMP,   
       ARTICULO A,   
       ARTICULO_ALMACEN AA
   WHERE ( AMP.COD_ART = A.COD_ART ) and  
         ( AMP.ALMACEN = AA.ALMACEN and
           AMP.COD_ART = AA.COD_ART ) and  
         ( AMP.FLAG_ESTADO = '1' ) AND  
         ( AMP.CANT_PROYECT > AMP.CANT_PROCESADA ) AND  
         ( TRUNC(AMP.FEC_PROYECT) <= :ad_fecha ) AND  
         ( AMP.TIPO_DOC = (select l.doc_ot from logparam l where reckey='1') AND 
			( AMP.TIPO_MOV=(select l.oper_cons_interno from logparam l where reckey='1')) AND
         ( A.FLAG_REPOSICION = '1' OR AA.SLDO_MINIMO > 0 ) )
GROUP BY  A.COD_ART,   
       A.NOM_ARTICULO,   
       A.UND,          
       AMP.ALMACEN,
       AA.SLDO_TOTAL,   
       AA.SLDO_POR_LLEGAR,   
       AA.COSTO_ULT_COMPRA,
       AA.SLDO_MINIMO, 
       AA.SLDO_MAXIMO
ORDER BY A.COD_ART          
" arguments=(("ad_fecha", date)) )
