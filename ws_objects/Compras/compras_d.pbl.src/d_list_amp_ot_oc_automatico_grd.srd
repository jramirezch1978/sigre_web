 retrieve="select amp.cod_art			,a.desc_art          ,
		 a.und               ,amp.oper_sec        , 
       USF_ALM_CANT_LIBRE_OT(amp.cod_origen,amp.nro_mov ) -
		 USF_ALM_CANT_PROG_PEND(amp.cod_origen, amp.nro_mov) as cant_pendiente,
		 usf_cmp_prec_ult_compra(amp.cod_art, amp.almacen) as precio_unit   , 
		 amp.almacen         ,
		 ot.cod_origen       , 
		 ot.ot_adm				,
       (select doc_ot from logparam where reckey = '1') as tipo_doc, 
		 ot.nro_orden        ,
		 a.dias_reposicion   ,
		 a.dias_rep_import	,
		 amp.cod_origen		,
		 amp.nro_mov			,
		 a.flag_reposicion	,
		 amp.fec_proyect		,
		 a2.cod_sub_cat		,
		 a2.desc_sub_cat		,
		 usf_alm_saldo_total_alm(amp.cod_Art, amp.almacen) as saldo_almacen
  from articulo_mov_proy  	amp,
       orden_trabajo      	ot,
       articulo		     	a,
		 articulo_sub_categ 	a2,
		 cotizacion				c1,
		 cotizacion_provee 	c2,
		 cotizacion_provee_bien_det c3 
 where amp.nro_doc		= ot.nro_orden      
   and a.cod_art	      = amp.cod_art
   and a2.cod_sub_cat	= a.sub_cat_art
   and c1.nro_cotiza		= c2.nro_cotiza
   and c2.nro_cotiza		= c3.nro_cotiza
   and c2.proveedor		= c3.proveedor
   and c3.cod_art			= amp.cod_art
   and NVL(c1.flag_estado, '0') 	= '1'
   and NVL(c2.cotizo, '0') 		= '1'
   and NVL(c3.precio_unit, 0) 	> 0
   and NVL(c3.flag_ganador, '0') = '1'
	and NVL(amp.flag_estado, '0')  		= '1'
   and Nvl(a.flag_inventariable, '0')  = '1'
	and USF_ALM_CANT_LIBRE_OT(amp.cod_origen,amp.nro_mov ) -
		 USF_ALM_CANT_PROG_PEND(amp.cod_origen, amp.nro_mov) > 0
   and amp.tipo_doc  		= :as_tipo_doc
   and amp.tipo_mov     	= :as_tipo_mov
   and amp.nro_doc      	= :as_nro_doc
	and a.cod_art				LIKE :as_cod_art
	and upper(a.desc_art)	LIKE :as_nom_art
   and amp.almacen         like :as_almacen
   and a2.cod_sub_cat		like :as_sub_cat
   and c1.nro_cotiza 		like :as_nro_cotiza
   and trunc(amp.fec_proyect) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
   " arguments=(("as_tipo_doc", string),("as_nro_doc", string),("ad_fecha1", date),("ad_fecha2", date),("as_cod_art", string),("as_nom_art", string),("as_tipo_mov", string),("as_almacen", string),("as_sub_cat", string),("as_nro_cotiza", string)) )
