         ~"TT_COMPRAS_SUGERIDAS~".~"INGRESOS~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"SALIDAS~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"SALDO~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"DISPONIBLE~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"POR_COMPRAR~",   
         ~"ARTICULO~".~"DESC_ART~",   
         ~"ARTICULO~".~"UND~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"NRO_ITEM~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"NRO_MOV~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"OPERACION~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"CENCOS~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"CNTA_PRSP~",   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~",   
         ~"TT_COMPRAS_SUGERIDAS~".~"STOCK~",
  			~"TT_COMPRAS_SUGERIDAS~".~"FLAG_ESTADO~",
			~"TT_COMPRAS_SUGERIDAS~".~"COMP_SUGERIDA~",
			~"TT_COMPRAS_SUGERIDAS~".~"LN_TEMP~"
    FROM ~"TT_COMPRAS_SUGERIDAS~",   
         ~"ARTICULO~",   
         ~"CENTROS_COSTO~"  
   WHERE ( tt_compras_sugeridas.cencos = centros_costo.cencos (+)) and  
         ( ~"TT_COMPRAS_SUGERIDAS~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" )   
ORDER BY ~"ARTICULO~".~"COD_ART~" ASC,   
         ~"TT_COMPRAS_SUGERIDAS~".~"NRO_ITEM~" ASC   
" )
