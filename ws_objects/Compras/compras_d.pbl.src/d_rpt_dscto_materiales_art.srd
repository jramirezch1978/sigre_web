 retrieve="select to_char(t.fec_registro,'dd/mm/yyyy') as fecha_oc , p.nro_doc , t.proveedor , j.nom_proveedor , p.cod_art , 
       d.nom_articulo , d.und , p.cant_proyect , p.almacen , b.cod_origen , 
       trunc(decode (p.cod_moneda , 'S/.' , p.decuento , 'US$' , p.decuento * usf_tc(t.fec_registro,'V') ) , 4)  as  soles    ,
       trunc(decode (p.cod_moneda , 'US$' , p.decuento , 'S/.' , p.decuento / usf_tc(t.fec_registro,'V') ) , 4)  as  dolares            
from orden_compra t , articulo_mov_proy p , proveedor j , articulo d , almacen b , tt_articulos_cmp tt1
where t.nro_oc = p.nro_doc and  t.proveedor = j.proveedor and p.cod_art = d.cod_art 
      and p.almacen = b.almacen 
      and p.tipo_doc=(select doc_oc from logparam where reckey = '1') 
      and p.tipo_mov=(select oper_ing_venta from logparam where reckey = '1')
      and p.decuento > 0 and t.flag_estado<>'0'
      and p.cod_art = tt1.cod_art 
      and trunc(t.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
