table(column=(type=char(10) updatewhereclause=yes name=nro_proforma dbname="proforma.nro_proforma" dbalias="p.nro_proforma" )
 column=(type=datetime updatewhereclause=yes name=fec_emision dbname="fec_emision" )
 column=(type=decimal(0) updatewhereclause=yes name=nro_item dbname="proforma_det.nro_item" dbalias="pd.nro_item" )
 column=(type=char(6) updatewhereclause=yes name=almacen dbname="proforma_det.almacen" dbalias="pd.almacen" )
 column=(type=number updatewhereclause=yes name=saldo_almacen dbname="saldo_almacen" )
 column=(type=char(12) updatewhereclause=yes name=cod_art dbname="articulo.cod_art" dbalias="a.cod_art" )
 column=(type=char(3000) updatewhereclause=yes name=desc_art dbname="articulo.desc_art" dbalias="a.desc_art" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="proforma_det.cantidad" dbalias="pd.cantidad" )
 column=(type=char(3) updatewhereclause=yes name=und dbname="articulo.und" dbalias="a.und" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="proforma.cod_moneda" dbalias="p.cod_moneda" )
 column=(type=number updatewhereclause=yes name=precio_venta dbname="precio_venta" )
 column=(type=char(12) updatewhereclause=yes name=centro_benef dbname="centro_benef" )
 column=(type=char(10) updatewhereclause=yes name=cencos dbname="cencos" )
 column=(type=number updatewhereclause=yes name=compromiso_compra dbname="compromiso_compra" )
 column=(type=number updatewhereclause=yes name=pend_compra dbname="pend_compra" )
 retrieve="select p.nro_proforma,
       trunc(p.fec_registro) as fec_emision,
       pd.nro_item,
     pd.almacen,
     (select nvl(sum(aa.sldo_total), 0)
       from articulo_almacen aa
      where aa.cod_art  = pd.cod_art
        and aa.almacen  = pd.almacen) as saldo_almacen,
       a.cod_art,
       a.desc_art,
       pd.cantidad,
       a.und,
       p.cod_moneda,
       pd.precio_vta + pd.igv as precio_venta,
       (select max(cba.centro_benef)
          from centro_benef_articulo cba
         where cba.cod_art = pd.cod_art) as centro_benef,
       (select max(av.cencos_ingreso)
          from articulo_venta av
         where av.cod_art = pd.cod_art) as cencos,
       (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
          from orden_compra oc,
               articulo_mov_proy amp
         where oc.nro_oc = amp.nro_doc
           and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
           and amp.nro_proforma  = pd.nro_proforma
           and amp.item_proforma = pd.nro_item
           and amp.flag_estado   <> '0') as compromiso_compra,
       pd.cantidad - (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
                        from orden_compra oc,
                             articulo_mov_proy amp
                       where oc.nro_oc = amp.nro_doc
                         and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
                         and amp.nro_proforma  = pd.nro_proforma
                         and amp.item_proforma = pd.nro_item
                         and amp.flag_estado   <> '0') as pend_compra
  from proforma p,
       proforma_det pd,
       articulo     a
 where p.nro_proforma = pd.nro_proforma
   and pd.cod_art     = a.cod_art
   and p.flag_estado  = '1'
   and pd.cod_art     not in (select ac.cod_art
                                from articulo_composicion ac)
   and pd.cantidad - (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
                        from orden_compra oc,
                             articulo_mov_proy amp
                       where oc.nro_oc = amp.nro_doc
                         and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
                         and amp.nro_proforma  = pd.nro_proforma
                         and amp.item_proforma = pd.nro_item
                         and amp.flag_estado   <> '0') > 0
  and trunc(p.fec_registro)   = trunc(:ad_fecha)
  and p.cod_moneda        = :as_moneda
union
select  p.nro_proforma,
      trunc(p.fec_registro) as fec_emision,
      pd.nro_item,
      pd.almacen,
      (select nvl(sum(aa.sldo_total), 0)
        from articulo_almacen aa
        where aa.cod_art  = pd.cod_art
         and aa.almacen = pd.almacen) as saldo_almacen,
      a.cod_art,
      a.desc_art,
      pd.cantidad * ac.cantidad as cantidad,
      a.und,
      p.cod_moneda,
      pd.precio_vta + pd.igv as precio_venta,
      (select max(cba.centro_benef)
          from centro_benef_articulo cba
         where cba.cod_art = pd.cod_art) as centro_benef,
      (select max(av.cencos_ingreso)
          from articulo_venta av
         where av.cod_art = pd.cod_art) as cencos,
      (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
        from orden_compra oc,
            articulo_mov_proy amp
        where oc.nro_oc = amp.nro_doc
         and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
         and amp.nro_proforma  = pd.nro_proforma
         and amp.item_proforma = pd.nro_item
         and amp.flag_estado   <> '0') as compromiso_compra,
      pd.cantidad * ac.cantidad - (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
                           from orden_compra oc,
                              articulo_mov_proy amp
                          where oc.nro_oc = amp.nro_doc
                            and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
                            and amp.nro_proforma  = pd.nro_proforma
                            and amp.item_proforma = pd.nro_item
                            and amp.flag_estado   <> '0') as pend_compra
   from proforma             p,
      proforma_det         pd,
      articulo_composicion ac,
      articulo             a
  where p.nro_proforma = pd.nro_proforma
    and pd.cod_art     = ac.cod_art
    and ac.componente  = a.cod_art
    and p.flag_estado  = '1'
    and pd.cantidad * ac.cantidad - (select nvl(sum(case when amp.flag_estado = '1' then amp.cant_proyect else amp.cant_procesada end),0)
                           from orden_compra oc,
                              articulo_mov_proy amp
                          where oc.nro_oc = amp.nro_doc
                            and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
                            and amp.nro_proforma  = pd.nro_proforma
                            and amp.item_proforma = pd.nro_item
                            and amp.flag_estado   <> '0')  > 0                         
     and trunc(p.fec_registro)  = trunc(:ad_fecha)
     and p.cod_moneda       = :as_moneda" arguments=(("ad_fecha", date),("as_moneda", string))  sort="fec_emision D " )
