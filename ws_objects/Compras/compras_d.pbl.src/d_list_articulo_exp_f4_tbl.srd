 column=(type=char(0) updatewhereclause=yes name=desc_almacen dbname="almacen" )
 column=(type=number updatewhereclause=yes name=stock dbname="stock" )
 retrieve="select a1.desc_categoria,
       a2.desc_sub_cat,
       a.desc_art,
       a.und,
       ac.desc_clase,
		 a.cod_sku,
       m.nom_marca,
		 '' as almacen,
		 0 as stock
from articulo           a,
     articulo_sub_categ a2,
     articulo_Categ     a1,
     articulo_clase     ac,
     marca              m,
     color              c,
     tipo_carroceria    tc,
     clase_vehiculo     cv,
     tipo_combustible   co
where a.sub_cat_art     = a2.cod_sub_cat     (+)
  and a2.cat_art        = a1.cat_art         (+)
  and a.cod_clase       = ac.cod_clase       (+)
  and a2.cod_marca      = m.cod_marca        (+)
  and a2.cod_color      = c.color            (+)
  and a2.cod_carroceria = tc.cod_carroceria  (+)
  and a2.cod_clase_vehiculo = cv.cod_clase_vehiculo (+)
  and a2.cod_comb           = co.cod_comb           (+)" )
