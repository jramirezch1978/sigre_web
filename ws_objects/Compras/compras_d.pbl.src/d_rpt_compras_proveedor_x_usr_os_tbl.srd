 column=(type=char(60) updatewhereclause=yes name=usuario_nombre dbname="usuario.nombre" )
 column=(type=char(8) updatewhereclause=yes name=proveedor dbname="proveedor.proveedor" )
 column=(type=char(50) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=number updatewhereclause=yes name=monto_dol dbname="monto_dol" )
 column=(type=number updatewhereclause=yes name=items dbname="items" )
 retrieve="select u.cod_usr, 
		 u.nombre, 
		 p.proveedor,
		 p.nom_proveedor,
		 sum( usf_fl_conv_mon(osd.importe - osd.decuento, os.cod_moneda, (select cod_dolares from logparam where reckey = '1'), osd.fec_proyect) ) as monto_dol,
		 count(*) as items
from orden_servicio_det osd,
     orden_servicio		os,
     usuario           	u,
	  proveedor				p
where u.cod_usr = os.cod_usr
  and os.nro_os = osd.nro_os
  and os.proveedor = p.proveedor
  and osd.flag_Estado <> '0'
  and os.flag_estado <> '0'
  and trunc(os.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
  and os.cod_origen like :as_origen
  and os.cod_usr = :as_usuario
group by u.cod_usr, 
			u.nombre, 
			p.proveedor,
		 	p.nom_proveedor
order by monto_dol desc" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_origen", string),("as_usuario", string)) )
