 column=(type=number updatewhereclause=yes name=monto_dol dbname="monto_dol" )
 column=(type=number updatewhereclause=yes name=items dbname="items" )
 retrieve="select oc.cod_usr, 
		 u.nombre, 
		 sum( usf_fl_conv_mon(amp.cant_proyect * (amp.precio_unit - amp.decuento), oc.cod_moneda, 'US$', fec_proyect) ) as monto_dol,
		 count(*) as items
from articulo_mov_proy amp,
     orden_compra      oc,
     usuario           u
where u.cod_usr = oc.cod_usr
  and oc.nro_oc = amp.nro_doc
  and amp.tipo_doc = 'OC'
  and amp.flag_Estado <> '0'
  and trunc(oc.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
group by oc.cod_usr, u.nombre" arguments=(("ad_fec_ini", date),("ad_fec_fin", date)) )
