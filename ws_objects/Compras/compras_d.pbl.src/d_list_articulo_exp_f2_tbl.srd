 retrieve="select a1.desc_categoria,
       a2.desc_sub_cat,
       a.desc_art,
       a.und,
       m.nom_marca,
       a.precio_vta_unidad,
       a.precio_vta_mayor,
       a.precio_vta_min
from articulo           a,
     articulo_sub_categ a2,
     articulo_Categ     a1,
     marca              m
where a.sub_cat_art     = a2.cod_sub_cat    (+)
