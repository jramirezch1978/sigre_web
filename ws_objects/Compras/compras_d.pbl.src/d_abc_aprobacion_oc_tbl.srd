 column=(type=char(11) updatewhereclause=yes name=ruc dbname="proveedor.ruc" dbalias="p.ruc" )
 column=(type=datetime updatewhereclause=yes name=fec_registro dbname="orden_compra.fec_registro" dbalias="oc.fec_registro" )
 column=(type=char(2000) updatewhereclause=yes name=observacion dbname="orden_compra.observacion" dbalias="oc.observacion" )
 column=(type=char(300) updatewhereclause=yes name=lugar_entrega dbname="orden_compra.lugar_entrega" dbalias="oc.lugar_entrega" )
