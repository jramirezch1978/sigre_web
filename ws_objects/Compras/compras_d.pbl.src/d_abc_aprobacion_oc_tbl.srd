 column=(type=decimal(2) updatewhereclause=yes name=monto_total dbname="orden_compra.monto_total" dbalias="oc.monto_total" )
 column=(type=char(60) updatewhereclause=yes name=generado_por dbname="usuario.nom_usuario" dbalias="u.nom_usuario" )
 column=(type=char(100) updatewhereclause=yes name=email dbname="usuario.email" dbalias="u.email" )
 column=(type=datetime updatewhereclause=yes name=fecha_aprob dbname="orden_compra.fecha_aprob" dbalias="oc.fecha_aprob" )
 column=(type=char(6) update=yes updatewhereclause=yes name=aprobador dbname="orden_compra.aprobador" dbalias="oc.aprobador" )
 column=(type=char(60) updatewhereclause=yes name=aprobado_por dbname="usuario.nom_aprobador" dbalias="uap.nom_aprobador" )
 column=(type=number updatewhereclause=yes name=monto_compra dbname="monto_compra" )
 column=(type=number updatewhereclause=yes name=impuestos dbname="impuestos" )
 retrieve="select oc.nro_oc,
       p.nom_proveedor,
       p.ruc,
		 oc.fec_registro,
       oc.observacion,
       oc.lugar_entrega,
       oc.cod_moneda,
       oc.monto_total,
       u.nombre as nom_usuario,
       u.email,
       oc.fecha_aprob, 
		 oc.aprobador, 
		 uap.nombre as nom_aprobador,
       sum(amp.precio_unit * amp.cant_proyect) as monto_compra,
       sum(nvl(amp.impuesto, 0) + nvl(amp.impuesto2,0)) as impuestos
from orden_compra oc,
     articulo_mov_proy amp,
     proveedor    p,
     usuario      u,
     usuario      uap
where oc.proveedor 	= p.proveedor     
  and oc.nro_oc    	= amp.nro_doc
  and amp.tipo_doc 	= (select doc_oc from logparam where reckey = '1')
  and oc.aprobador 	= uap.cod_usr (+)
  and oc.cod_usr   	= u.cod_usr
  and (oc.flag_estado = '3' or amp.flag_estado = '3')
group by oc.nro_oc,
       p.nom_proveedor,
       p.ruc,
		 oc.fec_registro,
       oc.observacion,
       oc.lugar_entrega,
       oc.cod_moneda,
       oc.monto_total,
       u.nombre,
       u.email,
       oc.fecha_aprob, 
		 oc.aprobador, 
		 uap.nombre
" update="orden_compra" updatewhere=0 updatekeyinplace=yes arguments=(("as_usuario", string))  sort="nro_oc A " )
