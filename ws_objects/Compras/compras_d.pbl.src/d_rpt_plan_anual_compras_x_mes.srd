 retrieve="Select m.cod_art, a.nom_articulo, a.und,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '01', m.cant_proyect,0)) as m_ene,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '02', m.cant_proyect,0)) as m_feb,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '03', m.cant_proyect,0)) as m_mar,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '04', m.cant_proyect,0)) as m_abr,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '05', m.cant_proyect,0)) as m_may,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '06', m.cant_proyect,0)) as m_jun,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '07', m.cant_proyect,0)) as m_jul,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '08', m.cant_proyect,0)) as m_ago,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '09', m.cant_proyect,0)) as m_set,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '10', m.cant_proyect,0)) as m_oct,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '11', m.cant_proyect,0)) as m_nov,
   SUM( DECODE( TO_CHAR(m.fec_proyect, 'MM'), '12', m.cant_proyect,0)) as m_dic
    From articulo_mov_proy m, articulo a, tt_alm_seleccion t
    Where  m.tipo_mov = 'S01' and t.cod_art = m.cod_art and 
      to_char(m.fec_proyect,'IYYY') = :as_ano  and
      m.flag_estado = '1' and m.cod_art = a.cod_art
    Group by m.cod_art, a.nom_articulo, a.und
    Order By m.cod_art" arguments=(("as_ano", string)) )
