 retrieve="  SELECT OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_USR,   
         OC.FLAG_ESTADO,   
         OC.MONTO_TOTAL,   
         OC.OBSERVACION,   
         OC.FLAG_IMPORTACION,   
         OC.COD_MONEDA,   
         AMP.COD_ART,   
         AMP.PRECIO_UNIT,   
         trim(A.DESC_ART) || case when amp.marca is null or trim(amp.marca) = '' then '' else ' Obs: ' || trim(amp.marca) end as desc_art,   
         A.UND,   
         trunc(AMP.FEC_PROYECT) as fec_proyect,   
         U.NOMBRE,   
         oc.aprobador,
         ap.nombre as nom_usuario_aprob,
         P.NOM_PROVEEDOR,
         P.RUC,
         P.EMAIL,
         OC.Lugar_entrega,
         b.nom_banco, oc.nro_cuenta,
         m.cod_moneda, m.simbolo,
         pkg_logistica.of_get_direccion(oc.proveedor, OC.DIRECCION_ITEM) AS DIRECCION,
         sum(AMP.CANT_PROYECT) as cant_proyect,
         sum(round(amp.precio_unit * amp.cant_proyect,2)) as importe,
         sum(round((amp.precio_unit - NVL(amp.decuento, 0)) * amp.cant_proyect,2)) as valor_venta,
         sum(round(nvl(amp.decuento,0) * amp.cant_proyect,2)) as importe_dscto,
         sum(case
                when amp.tipo_impuesto1 is not null and amp.tipo_impuesto1 = 'IGV18' then
                   amp.impuesto
                else 
                   0
             end +
             case
                when amp.tipo_impuesto2 is not null and amp.tipo_impuesto2 = 'IGV18' then
                   amp.impuesto2
                else 
                   0
             end) as importe_igv,
         sum(case
                when amp.tipo_impuesto1 is not null and amp.tipo_impuesto1 = 'ISC' then
                   amp.impuesto
                else 
                   0
             end +
             case
                when amp.tipo_impuesto2 is not null and amp.tipo_impuesto2 = 'ISC' then
                   amp.impuesto2
                else 
                   0
             end) as importe_isc,
         sum(case
                when amp.tipo_impuesto1 is not null and amp.tipo_impuesto1 in ('IPER1', 'IPERC') then
                   amp.impuesto
                else 
                   0
             end +
             case
                when amp.tipo_impuesto2 is not null and amp.tipo_impuesto2 in ('IPER1', 'IPERC') then
                   amp.impuesto2
                else 
                   0
             end) as importe_iperc,
         sum(case
                when amp.tipo_impuesto1 is not null and amp.tipo_impuesto1 not in ('IGV18', 'ISC', 'IPER1', 'IPERC') then
                   amp.impuesto
                else 
                   0
             end +
             case
                when amp.tipo_impuesto2 is not null and amp.tipo_impuesto2 not in ('IGV18', 'ISC', 'IPER1', 'IPERC') then
                   amp.impuesto2
                else 
                   0
             end) as otros_impuestos,
         (select oc_dir_1 from logparam where reckey = '1') as dir_1,
         (select oc_dir_2 from logparam where reckey = '1') as dir_2,
         (select oc_dir_3 from logparam where reckey = '1') as dir_3,
         (select oc_dir_4 from logparam where reckey = '1') as dir_4,
         (select oc_dir_5 from logparam where reckey = '1') as dir_5
    FROM ORDEN_COMPRA       OC,   
         ARTICULO_MOV_PROY AMP,   
         ARTICULO         A,   
         USUARIO          U,   
         PROVEEDOR        P,
         banco            b,
         moneda           m,
         usuario          ap
   WHERE OC.NRO_OC      = AMP.NRO_DOC 
     AND AMP.TIPO_DOC   = (select doc_oc from logparam where reckey = '1') 
     and AMP.COD_ART    = A.COD_ART 
     and OC.COD_USR     = U.COD_USR 
     and OC.PROVEEDOR   = P.PROVEEDOR 
     and oc.aprobador   = ap.cod_usr  (+)
     and OC.COD_ORIGEN  = :as_cod_origen 
     AND OC.NRO_OC      = :as_nro_oc
     and oc.Cod_banco   = b.cod_banco (+)
     and oc.cod_moneda  = m.cod_moneda (+)
  group by OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_USR,   
         OC.FLAG_ESTADO,   
         OC.MONTO_TOTAL,   
         OC.OBSERVACION,   
         OC.FLAG_IMPORTACION,   
         OC.COD_MONEDA,   
         AMP.COD_ART,   
         AMP.PRECIO_UNIT,   
         trim(A.DESC_ART) || case when amp.marca is null or trim(amp.marca) = '' then '' else ' Obs: ' || trim(amp.marca) end,   
         A.UND,   
         trunc(AMP.FEC_PROYECT),
         U.NOMBRE,   
      oc.aprobador,
         ap.nombre,
         P.NOM_PROVEEDOR,
         P.RUC,
         P.EMAIL,
         OC.Lugar_entrega,
         b.nom_banco, oc.nro_cuenta,
         m.cod_moneda, m.simbolo,
         pkg_logistica.of_get_direccion(oc.proveedor, OC.DIRECCION_ITEM)
" arguments=(("as_cod_origen", string),("as_nro_oc", string))  sort="cod_art A " )
