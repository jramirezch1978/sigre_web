 column=(type=number updatewhereclause=yes name=monto_sol dbname="monto_sol" )
 retrieve="select   p.proveedor,
		   p.nom_proveedor,
 		   sum( usf_fl_conv_mon(amp.cant_proyect * (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), fec_proyect) ) as monto_dol,
			sum( usf_fl_conv_mon(amp.cant_proyect * (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_soles from logparam where reckey = '1'), fec_proyect) ) as monto_sol
from articulo_mov_proy 	amp,
     orden_compra      	oc,
  	  proveedor			   p,
	  tt_alm_seleccion	tt	
  where oc.nro_oc = amp.nro_doc
  and amp.tipo_doc = (select doc_oc from logparam where reckey = '1')
  and oc.proveedor = p.proveedor
  and tt.proveedor = p.proveedor
  and amp.flag_Estado <> '0'
  and oc.flag_estado <> '0'
  and trunc(oc.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
group by p.proveedor,
		 	p.nom_proveedor
ORDER BY monto_dol desc" arguments=(("ad_fec_ini", date),("ad_fec_fin", date)) )
