 retrieve="SELECT a.cod_art, 
       a.desc_art,
       a.und,
       aa.sldo_total,
       aa.sldo_minimo,
       aa.sldo_maximo,
       aa.sldo_solicitado,
       aa.sldo_por_llegar,
       aa.flag_reposicion,
		 aa.sldo_consignacion,
		 al.almacen,
		 al.desc_almacen
FROM articulo_almacen aa,
     almacen          al,
     articulo         a
WHERE aa.almacen = al.almacen
  AND a.cod_art  = aa.cod_art
  AND aa.sldo_total <= NVL(aa.sldo_minimo,0)
  AND aa.flag_reposicion = '1'
  AND al.cod_origen = :as_origen
order by al.almacen, a.cod_art
" arguments=(("as_origen", string)) )
