 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="operaciones.flag_estado" values="Anulado	0/Abierto	1/Cerrado	2/Planeado	3/" )
 retrieve="select distinct op.oper_sec      , 
					 op.desc_operacion, 
                ot.nro_orden     , 
					 ot.cencos_slc    , 
                la.cnta_prsp_3ro , 
					 ej.cod_ejecutor  , 
                le.costo_unitario, 
					 op.fec_inicio,
					 s.servicio,
					 s.descripcion,
					 op.obs,
					 ot.centro_benef,
					 cb.desc_centro,
                op.flag_estado
           from operaciones    		op, 
					 labor          		la,
                ejecutor       		ej, 
					 orden_trabajo  		ot,
                labor_ejecutor      le, 
					 servicios				s,
					 tt_ope_ot_aprobadas tt,
					 centro_beneficio		cb
          where op.cod_labor    = la.cod_labor   
			   and op.servicio	  = s.servicio (+)
				and ot.centro_benef = cb.centro_benef (+)
            and ot.nro_orden    = op.nro_orden   
            and le.cod_labor    = op.cod_labor   
            and le.cod_ejecutor = op.cod_ejecutor
            and ej.cod_ejecutor = le.cod_ejecutor
            and op.flag_estado  in ('1', '3')    
            and op.nro_orden    = tt.nro_orden   
            and usf_cmp_oper_sec_activo(op.oper_sec ) = 1
            and op.oper_sec not in (select oper_Sec 
													from orden_Servicio_det 
												  where flag_estado <> '0' 
													 and oper_sec is not null)
            and ej.flag_externo = '1'               
            and trunc(op.fec_inicio) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
order by op.oper_sec asc" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
