 retrieve="Select m.cencos, m.cod_art, a.nom_articulo, a.und, ac.cat_art, ac.desc_categoria,
   sc.cod_sub_cat, sc.desc_sub_cat, cc.desc_cencos,
     SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'01', m.cant_proyect,0 )) as ene,  
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'02', m.cant_proyect,0 )) as feb,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'03', m.cant_proyect,0 )) as mar,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'04', m.cant_proyect,0 )) as abr,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'05', m.cant_proyect,0 )) as may,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'06', m.cant_proyect,0 )) as jun,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'07', m.cant_proyect,0 )) as jul,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'08', m.cant_proyect,0 )) as ago,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'09', m.cant_proyect,0 )) as seti,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'10', m.cant_proyect,0 )) as oct,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'11', m.cant_proyect,0 )) as nov,
   SUM( DECODE(TO_CHAR(m.fec_proyect, 'MM'),'12', m.cant_proyect,0 )) as dic
    From articulo_mov_proy m, tt_alm_seleccion t, articulo a, articulo_categ ac,
     articulo_sub_categ sc, centros_costo cc
    Where t.cencos = m.cencos and m.tipo_mov = 'S01' and
      to_char(m.fec_proyect,'IYYY') = :as_anio and
      m.flag_estado <> '0' and m.cod_art = a.cod_art and
      a.flag_inventariable = '1' and a.sub_cat_art = sc.cod_sub_cat and
      sc.cat_art = ac.cat_art and m.cencos = cc.cencos
    Group By m.cencos, m.cod_art, a.nom_articulo, a.und, 
       ac.cat_art, ac.desc_categoria,
   sc.cod_sub_cat, sc.desc_sub_cat, cc.desc_cencos
   Order by m.cencos, ac.cat_art, sc.cod_sub_cat, m.cod_art" arguments=(("as_anio", string)) )
