 retrieve="SELECT  PCD.NRO_PROGRAMA     ,PCD.NRO_ITEM         ,
        PCD.FLAG_ESTADO      ,PCD.FEC_REGISTRO     ,
		  PCD.FEC_REQUERIDA    ,PCD.FLAG_PRIORIDAD   ,
        PCD.COD_ART		     ,A.DESC_ART			   ,
        A.UND 				     ,PCD.CANT_PROYECT     ,
        PCD.CANT_PROCESADA   ,PCD.COMPRADOR 	      ,
		  PCD.ALMACEN			  ,
		  (select nvl(sum(aa.sldo_total),0)
			  from articulo_almacen aa	
			where aa.cod_art = pcd.cod_art
 			  and aa.almacen = pcd.almacen) as saldo_almacen,
		  PCD.ORG_AMP_OT_REF   ,
		  PCD.NRO_AMP_OT_REF	  ,PCD.FLAG_REPLICACION ,
		  AMP.NRO_DOC			  ,
		  PCD.cant_ingresada
   FROM PROG_COMPRAS_DET  PCD ,
        ARTICULO          A   ,
		  ARTICULO_MOV_PROY AMP
  WHERE (PCD.COD_ART        = A.COD_ART      )
	 AND (PCD.ORG_AMP_OT_REF = AMP.COD_ORIGEN (+) )
	 AND (PCD.NRO_AMP_OT_REF = AMP.NRO_MOV    (+) )
    AND (PCD.NRO_PROGRAMA   = :as_nro_programa)
ORDER BY PCD.NRO_ITEM" update="PROG_COMPRAS_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_programa", string)) )
