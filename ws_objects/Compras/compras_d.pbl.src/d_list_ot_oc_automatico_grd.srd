 column=(type=char(2) updatewhereclause=yes name=orden_trabajo_cod_origen dbname="orden_trabajo.cod_origen" )
 column=(type=char(10) updatewhereclause=yes name=cencos_slc dbname="orden_trabajo.cencos_slc" )
 column=(type=char(800) updatewhereclause=yes name=descripcion dbname="orden_trabajo.descripcion" )
 column=(type=char(10) updatewhereclause=yes name=ot_adm dbname="orden_trabajo.ot_adm" )
 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="orden_trabajo.flag_estado" values="Anulado	0/Abierto	1/Cerrado	2/Planeado	3/No Modificable	5/" )
 retrieve="select ot.nro_orden   ,ot.cod_origen  ,
		 ot.cencos_slc	 ,ot.descripcion ,
		 ot.ot_adm		 ,ot.flag_estado 
  from orden_trabajo      	ot,
       articulo_mov_proy  	amp,
	    articulo			  	a,
		 articulo_sub_categ 	a2,
		 cotizacion				c1,
		 cotizacion_provee	c2,
		 cotizacion_provee_bien_det c3	
where a2.cod_sub_cat								= a.sub_cat_art
  and amp.nro_doc                         = ot.nro_orden      
  and amp.cod_art                         = a.cod_art         
  and c1.nro_cotiza 								= c2.nro_cotiza
  and c2.nro_cotiza 								= c3.nro_cotiza
  and c2.proveedor  								= c3.proveedor
  and c3.cod_art	  								= amp.cod_art
  and NVL(c1.flag_estado, '0') 				= '1'
  and NVL(c2.cotizo, '0') 						= '1'
  and NVL(c3.precio_unit, 0) 					> 0
  and NVL(c3.flag_ganador, '0') 				= '1'
  and Nvl(amp.flag_estado, '0')           = '1'               
  and Nvl(a.flag_inventariable, '0')      = '1'               
  and Nvl(a.flag_estado, '0')             = '1'              
  and USF_ALM_CANT_LIBRE_OT(amp.cod_origen,amp.nro_mov ) -
		 USF_ALM_CANT_PROG_PEND(amp.cod_origen, amp.nro_mov) > 0
  and trunc(amp.fec_proyect) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
  and amp.tipo_doc       = :as_tipo_doc
  and amp.tipo_mov		 = :as_tipo_mov
  and a.cod_art		    like :as_cod_art 
  and upper(a.desc_art)  like :as_nom_art
  and amp.nro_doc        like :as_nro_doc
  and ot.ot_adm          like :as_ot_adm
  and amp.almacen        like :as_almacen
  and a2.cod_sub_cat		 like :as_sub_cat
  and c1.nro_cotiza		 like :as_nro_cotiza
group by ot.nro_orden   ,ot.cod_origen  ,
		   ot.cencos_slc	,ot.descripcion ,
		 	ot.ot_adm		,ot.flag_estado 
" arguments=(("ad_fecha1", date),("ad_Fecha2", date),("as_cod_art", string),("as_nom_art", string),("as_tipo_doc", string),("as_nro_doc", string),("as_ot_adm", string),("as_tipo_mov", string),("as_almacen", string),("as_sub_cat", string),("as_nro_cotiza", string)) )
