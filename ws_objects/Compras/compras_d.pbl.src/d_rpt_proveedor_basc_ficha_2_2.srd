 retrieve="select nom_proveedor,  ruc,
       usf_get_colinvarchar('select telefono from telefonos where codigo = '''||proveedor||'''') as telefonos,
       nvl(USF_AP_DIREC_PROV(proveedor,'1'),USF_AP_DIREC_PROV(proveedor,'3')) as direcciones,
       gerente, usf_get_colinvarchar('select nom_rep from representante where proveedor = '''||proveedor||'''') as representantes,
       (select a.descripcion from proveedor_tipo a where a.tipo_proveedor = p.tipo_proveedor) as prod_serv,
       basc_contacto, basc_contacto_cargo, p.BASC_CERTIFICACION, p.BASC_TIPO_CERTIFICACION, p.BASC_SUSCRIPCIONES,
       case when ruc is null then '0' else '1' end as registrada_sunat,
       p.BASC_PAGINA_WEB, p.BASC_FLAG_EMAIL_PROPIO, p.BASC_FLAG_BUENAS_REF, p.BASC_DESC_BUENAS_REF,
       p.BASC_FLAG_BUEN_FIN, p.BASC_DESC_ESTADO_FIN
  from proveedor p where proveedor = :as_proveedor" arguments=(("as_proveedor", string)) )
