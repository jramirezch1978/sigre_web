 retrieve="  SELECT A.COD_ART,   
         AC1.CAT_ART,   
         A.SUB_CAT_ART, 
        A.NOM_ARTICULO,  
         A.DESC_ART,   
         A.FLAG_ESTADO,   
         A.FLAG_CRITICO,   
         A.FLAG_OBSOLETO,   
         A.FLAG_INVENTARIABLE,   
         A.UND,   
         A.COD_CLASE,   
         A.COD_ORIGEN,   
         A.PESO_UND,   
         A.VOL_UND,   
         A.SLDO_TOTAL,   
         A.SLDO_POR_LLEGAR,   
         A.SLDO_SOLICITADO,   
         A.SLDO_DEVUELTO,   
         A.SLDO_PRESTAMO,   
         A.SLDO_CONSIGNACION,   
         A.SLDO_RESERVADO,   
         A.COSTO_PROM_SOL,   
         A.COSTO_PROM_DOL, 
			A.MONEDA_COMPRA,  
         A.COSTO_ULT_COMPRA,   
         A.FEC_ULT_COMPRA,   
         A.FEC_ULT_SALIDA,   
         A.NRO_ULTIMA_OC,   
         A.SLDO_MINIMO,   
         A.SLDO_MAXIMO,   
         A.DIAS_REPOSICION,   
         A.DIAS_REP_IMPORT,   
         A.CNT_COMPRA_REC,   
         A.FILE_IMAGEN,   
         AC1.DESC_CATEGORIA,   
         AC2.DESC_SUB_CAT,   
         A.FLAG_UND2,   
         A.FLAG_CNTRL_LOTE,   
         A.SLDO_TOTAL_UND2,   
         U.DESC_UNIDAD,   
         AC.DESC_CLASE,   
         A.FLAG_REPLICACION,   
         A.FLAG_IQPF,   
         A.SLDO_WARRANTEADO,   
         A.SLDO_TRANSPORTADO,   
         A.FLAG_CNTRL_REQ,   
         A.NIVEL_APROB,   
         A.FLAG_REPOSICION,   
         A.UND2,   
         A.FACTOR_CONV_UND,   
         A.FLAG_PERCEPCION,   
         A.COD_MARCA,   
         M.NOM_MARCA,   
         A.COD_SKU,
         A.COD_CUBSO,
         a.ano_modelo,
         a.ano_fabricacion,
         a.nro_chasis,
         a.nro_serie,
         a.nro_motor,
         a.nro_dua,
         a.nro_item_dua,
         a.nro_placa,
         a.nro_lote,
        a.fec_registro,
        a.cod_usr,
         a.estilo,
         a.cod_talla, at.desc_talla,
         sc.desc_bien_serv_obra,
         a.color1, co1.descripcion as desc_color1,
         a.color2, co2.descripcion as desc_color2,
			a.nomenclatura
    FROM ARTICULO            A,   
         ARTICULO_CATEG      AC1,   
         ARTICULO_SUB_CATEG  AC2,   
         UNIDAD              U,   
         ARTICULO_CLASE      AC,   
         MARCA               M,
         SUNAT_CUBSO         SC,
         articulo_talla      at,
         color               co1,
         color               co2
   WHERE a.und          = U.und           (+) 
     and A.COD_CLASE    = AC.cod_clase    (+)
     and A.cod_marca    = M.cod_marca     (+)
     and a.cod_cubso    = sc.cod_cubso    (+)
     and a.cod_talla    = at.cod_talla    (+)
     and a.color1       = co1.color       (+)
     and a.color2       = co2.color       (+)
     and A.SUB_CAT_ART  = AC2.COD_SUB_CAT 
     and AC2.CAT_ART    = AC1.CAT_ART
     and A.COD_ART    = :as_cod_art 
" update="ARTICULO" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_art", string)) )
