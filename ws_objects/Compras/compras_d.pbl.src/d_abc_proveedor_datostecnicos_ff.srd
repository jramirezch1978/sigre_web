 retrieve="SELECT  p.proveedor ,
		  p.FLAG_ESTADO ,
		  p.FLAG_CLIE_PROV ,
		  p.FLAG_TIPO_PRECIO ,
		  p.TIPO_PROVEEDOR ,
		  p.NOM_PROVEEDOR ,
		  p.EMAIL ,
		  p.RUC ,
		  p.GERENTE ,
		  p.ACTIV_EMPRESA ,
		  p.ANO_EXISTENCIA ,
		  p.UNID_MOVILES ,
		  p.STK_REPUESTOS ,
		  p.VEND_CAPAC ,
		  p.PROG_EXTENS ,
		  p.DISPONIBILIDAD ,
		  p.AREA_CONTROL ,
		  p.NRO_PERS_CC ,
		  p.NRO_PERS_MTTO ,
		  C.COD_CAMPO ,
		  C.DESC_CAMPO ,
		  p.FLAG_PERSONERIA ,
		  p.COD_USR ,
		  p.FLAG_RET_IGV ,
		  p.DOC_IDENT_TRANSP ,
		  p.FLAG_DETRACION ,
		  p.NOMBRE2 ,
		  p.NOMBRE1 ,
		  p.APELLIDO_PAT ,
		  p.APELLIDO_MAT ,
		  p.FLAG_REPLICACION ,
		  p.FLAG_NAC_EXT  ,
		  p.TIPO_DOC_IDENT  ,
		  p.NRO_DOC_IDENT  ,
		  p.DETR_CTA_BCO   ,
		  p.FLO_ID,
		  p.CNPJ  
	FROM PROVEEDOR	p ,
		  CAMPO		c     
  WHERE p.PROVEEDOR = C.PROVEEDOR (+)
	 and p.PROVEEDOR = :as_proveedor
  ORDER BY p.NOM_proveedor          ASC  " update="PROVEEDOR" updatewhere=0 updatekeyinplace=yes arguments=(("as_proveedor", string)) )
