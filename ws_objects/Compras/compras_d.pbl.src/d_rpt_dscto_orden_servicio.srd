 retrieve="select os.cod_origen , o.nro_orden as nro_ot , trunc (ota.fecha_aprob) as fecha_aprob, os.nro_os , 
       trunc(os.fec_registro) as fecha_os, os.proveedor ,
       p.nom_proveedor , fp.desc_forma_pago , os.cod_usr, round((os.fec_registro - ota.fecha_aprob),0) as t_log_ser,
       sum(trunc(decode (os.cod_moneda , :as_soles , osd.importe , :as_dolar , osd.importe * usf_tc(os.fec_registro,'V') ) , 4))  as monto_soles    ,
       sum(trunc(decode (os.cod_moneda , :as_dolar , osd.importe , :as_soles , osd.importe / usf_tc(os.fec_registro,'V') ) , 4))  as monto_dolares  ,          
       sum(trunc(decode (os.cod_moneda , :as_soles , osd.decuento , :as_dolar , osd.decuento * usf_tc(os.fec_registro,'V') ) , 4))  as dscto_soles    ,
       sum(trunc(decode (os.cod_moneda , :as_dolar , osd.decuento , :as_soles , osd.decuento / usf_tc(os.fec_registro,'V') ) , 4))  as dscto_dolares            
from orden_servicio os, orden_servicio_det osd, operaciones o, proveedor p, ot_aprobaciones ota, forma_pago fp, tt_proveedor tp
where os.cod_origen = osd.cod_origen
  and  os.nro_os = osd.nro_os
  and  os.proveedor = p.proveedor
  and  p.proveedor = tp.proveedor
  and  osd.oper_sec = o.oper_sec  (+)
  and o.nro_aprob = ota.nro_aprob (+)
  and os.forma_pago = fp.forma_pago
  and os.flag_estado <> 0
  and osd.decuento > 0
  and os.fec_registro >= :ad_fec_ini 
  and os.fec_registro <= :ad_fec_fin
group by os.cod_origen , o.nro_orden , trunc (ota.fecha_aprob) , os.nro_os , 
       trunc(os.fec_registro) , os.proveedor, p.nom_proveedor , fp.desc_forma_pago , os.cod_usr, round((os.fec_registro - ota.fecha_aprob),0) 
" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_soles", string),("as_dolar", string))  sort="orden_servicio_proveedor A " )
