 column=(type=char(40) updatewhereclause=yes name=centros_costo_desc_cencos dbname="centros_costo.desc_cencos" )
 retrieve="  SELECT ~"ARTICULO_MOV_PROY~".~"CENCOS~",   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~"  
    FROM ~"ARTICULO_MOV_PROY~",   
         ~"CENTROS_COSTO~"  
   WHERE ( ~"ARTICULO_MOV_PROY~".~"CENCOS~" = ~"CENTROS_COSTO~".~"CENCOS~" ) and  
         ( ( ~"ARTICULO_MOV_PROY~".~"TIPO_MOV~" = :as_tipo_mov ) AND  
         ( to_char( ~"ARTICULO_MOV_PROY~".~"FEC_PROYECT~", 'YYYY') = :as_anio ) AND  
			( ~"ARTICULO_MOV_PROY~".~"FLAG_ESTADO~" <> '0') AND 
         ( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~" is NULL ) OR  
         ( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~" = (  SELECT ~"SEMPARAM~".~"TIPO_DOC_HAB~"  
                                                 FROM ~"SEMPARAM~"  
                                                WHERE ~"SEMPARAM~".~"RECKEY~" = '1'  )) OR
			( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~" = (  SELECT ~"PROD_PARAM~".~"DOC_OT~"  
                                                 FROM ~"PROD_PARAM~"  
                                                WHERE ~"PROD_PARAM~".~"RECKEY~" = '1'  ))
 )   
GROUP BY ~"ARTICULO_MOV_PROY~".~"CENCOS~",   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~"  
ORDER BY ~"ARTICULO_MOV_PROY~".~"CENCOS~" ASC   
" arguments=(("as_tipo_mov", string),("as_anio", string)) )
