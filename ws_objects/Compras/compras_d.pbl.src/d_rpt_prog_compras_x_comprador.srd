 retrieve="  SELECT ~"PROG_COMPRAS_DET~".~"NRO_ITEM~",   
         ~"PROG_COMPRAS_DET~".~"FEC_REGISTRO~",   
         ~"PROG_COMPRAS_DET~".~"FLAG_PRIORIDAD~",   
         ~"PROG_COMPRAS_DET~".~"COD_ART~",   
         ~"ARTICULO~".~"NOM_ARTICULO~",   
         ~"ARTICULO~".~"UND~",   
         ~"PROG_COMPRAS_DET~".~"ALMACEN~",   
         ~"PROG_COMPRAS_DET~".~"COMPRADOR~",   
         ~"PROG_COMPRAS_DET~".~"FEC_REQUERIDA~",   
         ~"PROG_COMPRAS_DET~".~"NRO_PROGRAMA~",   
         (NVL(~"PROG_COMPRAS_DET~".~"CANT_PROYECT~",0) - NVL(~"PROG_COMPRAS_DET~".~"CANT_PROCESADA~",0)) as cant_pendiente,   
         ~"ARTICULO_MOV_PROY~".~"NRO_DOC~",
			~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~" - ~"ARTICULO_MOV_PROY~".~"CANT_PROCESADA~" AS PENDIENTE    
    FROM ~"ARTICULO~"         ,   
         ~"PROG_COMPRAS_DET~" ,   
         ~"ARTICULO_MOV_PROY~"   
   WHERE ( ~"PROG_COMPRAS_DET~".~"COD_ART~"        = ~"ARTICULO~".~"COD_ART~"             ) and  
         ( ~"PROG_COMPRAS_DET~".~"ORG_AMP_OT_REF~" = ~"ARTICULO_MOV_PROY~".~"COD_ORIGEN~" ) and  
         ( ~"PROG_COMPRAS_DET~".~"NRO_AMP_OT_REF~" = ~"ARTICULO_MOV_PROY~".~"NRO_MOV~"    ) and  
         ( ( ~"PROG_COMPRAS_DET~".~"FLAG_ESTADO~"  = '1'                              ) and
         ( ~"PROG_COMPRAS_DET~".~"COMPRADOR~"      = :as_comprador                    ) and  
         ( NVL(~"PROG_COMPRAS_DET~".~"CANT_PROYECT~",0) > NVL(~"PROG_COMPRAS_DET~".~"CANT_PROCESADA~",0) ) )    
" arguments=(("as_comprador", string))  sort="prog_compras_det_nro_programa A nro_item A " )
