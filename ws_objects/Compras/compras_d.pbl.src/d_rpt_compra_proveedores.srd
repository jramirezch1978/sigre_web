 column=(type=number updatewhereclause=yes name=imp_dol dbname="imp_dol" )
 retrieve="Select cp.cod_relacion, p.nom_proveedor, p.ruc, 
       (Select trim(max(d.dir_provincia||' - '||d.dir_ciudad||' - '||d.dir_distrito||' - '||d.dir_direccion))
          From direcciones d Where d.codigo = cp.cod_relacion And d.flag_uso = '1' )  As direccion,
       Sum(decode(cp.cod_moneda,:as_sol,cp.importe_doc,
                                        ROUND(cp.importe_doc * cp.tasa_cambio,2))) As imp_sol,
       Sum(decode(cp.cod_moneda,:as_dol,cp.importe_doc,
                                        ROUND(cp.importe_doc / cp.tasa_cambio,2))) As imp_dol
  From cntas_pagar cp, proveedor p
 Where cp.cod_relacion         = p.proveedor
   And trunc(cp.fecha_emision) >= :ad_fec_ini
   And trunc(cp.fecha_emision) <= :ad_fec_fin
   And cp.flag_estado          <> '0'
   And cp.tipo_doc             = (Select doc_fact_pagar From finparam Where reckey = '1')
   and cp.origen					 like :as_origen
 Group By cp.cod_relacion, p.nom_proveedor, p.ruc
 Order By p.nom_proveedor" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_origen", string),("as_sol", string),("as_dol", string),("as_empresa", string),("as_user", string)) )
