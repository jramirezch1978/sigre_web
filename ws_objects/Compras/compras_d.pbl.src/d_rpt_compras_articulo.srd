 column=(type=number updatewhereclause=yes name=precio_soles dbname="precio_soles" )
 column=(type=number updatewhereclause=yes name=precio_dolares dbname="precio_dolares" )
 column=(type=number updatewhereclause=yes name=monto_soles dbname="monto_soles" )
 column=(type=number updatewhereclause=yes name=monto_dolares dbname="monto_dolares" )
 retrieve="select ac1.cod_sub_cat, ac1.desc_sub_cat,
       ac2.cat_art, ac2.desc_categoria,
       a.cod_art, a.desc_art, a.und,
       oc.fec_registro, oc.nro_oc,
       p.proveedor, p.nom_proveedor,
       amp.cant_proyect,
       usf_fl_conv_mon( (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_soles from logparam where reckey = '1'), oc.fec_registro) as precio_soles,
       usf_fl_conv_mon( (amp.precio_unit - amp.decuento), oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), oc.fec_registro) as precio_dolares,
       usf_fl_conv_mon( (amp.precio_unit - amp.decuento) * amp.cant_proyect, oc.cod_moneda, (select cod_soles from logparam where reckey = '1'), oc.fec_registro) as monto_soles,
       usf_fl_conv_mon( (amp.precio_unit - amp.decuento) * amp.cant_proyect, oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), oc.fec_registro) as monto_dolares
from orden_compra       oc,
     articulo_mov_proy  amp,
     articulo           a,
     articulo_sub_categ ac1,
     articulo_categ     ac2,
     proveedor          p,
	  tt_articulos_cmp   tt1
where oc.nro_oc       = amp.nro_doc  
  and amp.tipo_doc    = (select doc_oc from logparam where reckey = '1')
  and a.cod_art       = amp.cod_art
  and ac1.cod_sub_cat = a.sub_cat_art
  and ac2.cat_art     = ac1.cat_art   
  and p.proveedor     = oc.proveedor
  and ltrim(rtrim(tt1.codigo)) = ltrim(rtrim(a.cod_art))
  and oc.flag_estado <> '0'
  and amp.flag_estado <> '0'
  and trunc(oc.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
