 retrieve="SELECT o.nombre, tt.cod_origen, aca.desc_categoria,
		 sum(tt.cant_proyect*tt.precio_unit) as total_dol
  FROM TT_CMP_COMPRAS_RESUMEN tt, 
		 articulo 					a, 
		 articulo_sub_categ 		asu, 
		 articulo_categ 			aca,
		 origen						o
 WHERE tt.cod_art    = a.cod_art 
   AND a.sub_cat_art = asu.cod_sub_cat 
	AND asu.cat_art   = aca.cat_art 
   and o.cod_origen	= tt.cod_origen
GROUP BY o.nombre, tt.cod_origen, aca.desc_categoria
ORDER BY tt.cod_origen, total_dol desc" )
