 column=(type=number updatewhereclause=yes name=comp_sugerida dbname="comp_sugerida" )
 retrieve="  SELECT AMP.ALMACEN,
			AMP.COD_ART,   
         AMP.FEC_PROYECT AS FECHA,   
         AMP.CANT_PROYECT,   
			AMP.COD_ORIGEN,
			AMP.nro_mov,
			AMP.tipo_doc,
			AMP.nro_doc,
			A.DESC_ART,
			A.UND,
			AMP.OPER_SEC,
			NVL(AMP.CANT_PROYECT,0) - NVL(AMP.CANT_PROCESADA,0) as comp_sugerida
    FROM ARTICULO_MOV_PROY AMP,   
         ARTICULO 			A
   WHERE AMP.COD_ART 				= A.COD_ART 
     and AMP.TIPO_MOV = (Select OPER_CONS_INTERNO from logparam where reckey = '1')
	  and NVL(A.FLAG_INVENTARIABLE, '0') = '1'
     and NVL(AMP.CANT_PROYECT,0) - NVL(AMP.CANT_PROCESADA,0) > 0 
	  and AMP.ALMACEN like :as_almacen
	  and TRUNC(AMP.FEC_PROYECT) <= TRUNC(:ad_fecha)
	  and AMP.TIPO_DOC = (SELECT DOC_OT FROM LOGPARAM WHERE RECKEY = '1')
     and AMP.NRO_DOC like :as_nro_orden
     and trim(AMP.COD_ORIGEN) || trim(to_char(AMP.nro_mov)) not in (
				SELECT trim(amp_origen) || trim(to_char(amp_nro)) 
				FROM prog_compras_amp  	a,
					  prog_compras_det	b
				WHERE a.nro_programa = b.nro_programa
              and a.nro_item		= b.nro_item
				  and b.flag_estado = '1'
				  and b.cant_proyect > NVL(b.cant_procesada,0))
ORDER BY AMP.ALMACEN ASC,
			A.COD_ART ASC

" arguments=(("as_almacen", string),("as_nro_orden", string),("ad_fecha", date)) )
