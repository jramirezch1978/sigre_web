 column=(type=char(1) updatewhereclause=yes name=flag_estado dbname="articulo_mov_proy.flag_estado" values="Anulado	0/Aprobado	1/Cerrado	2/Proyectado	3/" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 retrieve=" select a.cod_art, a.und, 
		  a.desc_art, 
        ot.ot_adm, ot.nro_orden, amp.almacen, 
		  al.desc_almacen,
		  amp.fec_proyect,
        o.oper_sec,
		  ota.descripcion,
		  amp.flag_estado,
        decode(:as_func, '1', usf_alm_cant_libre_ot(amp.cod_origen, amp.nro_mov), amp.cant_proyect) as cantidad
  from articulo_mov_proy  amp, 
       operaciones        o, 
       orden_trabajo      ot, 
       articulo           a, 
       almacen				  al,
		 ot_administracion  ota
 where ota.ot_adm			 	 = ot.ot_adm
   and amp.oper_sec         = o.oper_sec 
   and amp.tipo_doc         = (select doc_ot from logparam where reckey = '1') 
   AND amp.tipo_mov         = (select oper_cons_interno from logparam where reckey = '1')
   AND INSTR(:as_estado, amp.flag_estado) > 0    
   and amp.cod_art          = a.cod_art 
   and o.nro_orden          = ot.nro_orden 
   and amp.almacen			 = al.almacen
   AND a.flag_estado        = '1' 
   and a.flag_inventariable = '1' 
   and to_number(to_char(amp.fec_proyect,'yyyy')) = :an_year
   AND al.almacen        LIKE :as_almacen
   AND a.cod_art         = :as_cod_art
   AND ot.ot_adm         LIKE :as_ot_adm
   AND ot.nro_orden      LIKE :as_nro_ot
   and decode(:as_func, '1', usf_alm_cant_libre_ot(amp.cod_origen, amp.nro_mov), amp.cant_proyect) > 0
order by ot.nro_orden" arguments=(("as_cod_art", string),("as_almacen", string),("as_ot_adm", string),("as_nro_ot", string),("as_estado", string),("as_func", string),("an_year", number)) )
