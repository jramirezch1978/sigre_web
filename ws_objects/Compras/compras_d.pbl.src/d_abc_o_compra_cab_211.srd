 retrieve="  SELECT OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.FLAG_IMPORTACION,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_MONEDA,   
         OC.MONTO_TOTAL,   
         OC.COD_USR,   
         OC.NRO_PROGRAMA,   
         OC.JOB,   
         OC.FLAG_COTIZACION,   
         OC.OBSERVACION,   
         OC.FLAG_REPLICACION,   
         OC.OT_GASTOS,   
         OC.DIRECCION_ITEM,   
         pkg_logistica.of_get_direccion(oc.proveedor, oc.direccion_item) AS DIRECCION,   
         OC.APROBADOR,   
         OC.FECHA_APROB,   
         OC.COD_BANCO,   
         BANCO.NOM_BANCO,   
         OC.UO_DST_PAGO,   
         OC.NRO_CUENTA,   
         OC.FLAG_SOLICITA_DUA,   
         OC.FLAG_ESTADO,   
         OC.NRO_COTIZACION,   
         OC.LUGAR_ENTREGA,   
         OC.IMP_FREIGHT,   
         OC.IMP_INSURANCE,   
         OC.IMP_OTHER,   
         P.NOM_PROVEEDOR  
    FROM ORDEN_COMPRA	OC,   
         PROVEEDOR		P,   
         BANCO  
   WHERE banco.cod_banco (+) = OC.cod_banco
     and OC.PROVEEDOR 	= P.PROVEEDOR 
     AND OC.NRO_OC 		= :as_nro
" update="ORDEN_COMPRA" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro", string)) )
