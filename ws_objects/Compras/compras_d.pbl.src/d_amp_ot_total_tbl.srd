 retrieve="  SELECT A.COD_ART,   
         A.NOM_ARTICULO,
         A.UND,
			A.FLAG_REPOSICION,
			AMP.COD_ORIGEN,
			AMP.NRO_MOV, 
         AMP.FEC_PROYECT,
         AMP.FEC_REGISTRO,
			AMP.CANT_PROCESADA,
   		AMP.CANT_PROYECT,
			AMP.CANT_PROCESADA,
			AMP.CANT_RESERVADO,
         AMP.FLAG_ESTADO,   
         AMP.CENCOS,   
         AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.OPER_SEC,
         AMP.FEC_REGISTRO,   
         AMP.ALMACEN,
         AMP.COD_USR,
			AMP.NRO_APROB,
         MIN(AMPOC.NRO_DOC) as nro_doc_oc,
         NVL(SUM(CASE WHEN AMPOC.FLAG_ESTADO = 1 THEN NVL(AMPOC.CANT_PROYECT,0)
                      WHEN AMPOC.FLAG_ESTADO = 2 THEN NVL(AMPOC.CANT_PROCESADA,0)
                      END), 0 ) as 	qnt_comp,   
         NVL(SUM(AMPOC.CANT_PROCESADA),0) as qnt_recib,
         MIN(AMPOC.FEC_REGISTRO) as FEC_OC,
			COUNT(AMPOC.NRO_DOC) as cant_oc 
    FROM ARTICULO A,   
         ARTICULO_MOV_PROY AMP,   
			ARTICULO_MOV_PROY AMPOC  
   WHERE ( AMP.COD_ART = A.COD_ART ) and  
         ( AMP.COD_ORIGEN = AMPOC.ORG_AMP_REF(+) and AMP.NRO_MOV = AMPOC.NRO_AMP_REF(+) ) and   
         ( ( AMP.NRO_DOC = :as_nro_ot ) AND  
         ( AMP.TIPO_DOC = :as_doc_ot ) AND  
         ( AMP.TIPO_MOV = :as_oper ) )   
GROUP BY A.COD_ART,   
         A.NOM_ARTICULO,
			A.UND,
         A.FLAG_REPOSICION,
			AMP.COD_ORIGEN,
			AMP.NRO_MOV,    
         AMP.FEC_PROYECT,
         AMP.FEC_REGISTRO,
			AMP.CANT_PROCESADA,
   		AMP.CANT_PROYECT,
			AMP.CANT_PROCESADA,
			AMP.CANT_RESERVADO,
         AMP.FLAG_ESTADO,   
         AMP.CENCOS,   
         AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.OPER_SEC,
         AMP.FEC_REGISTRO,   
         AMP.ALMACEN,
         AMP.COD_USR,
			AMP.NRO_APROB" arguments=(("as_doc_ot", string),("as_oper", string),("as_nro_ot", string))  sort="cod_art A fec_proyect A " )
