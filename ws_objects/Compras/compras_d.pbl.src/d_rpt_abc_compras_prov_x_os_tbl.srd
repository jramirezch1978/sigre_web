 retrieve="Select p.proveedor,
		 p.nom_proveedor,
		 sum( usf_fl_conv_mon(osd.importe - osd.decuento, os.cod_moneda, (select cod_dolares from logparam where reckey = '1'), osd.fec_proyect) ) as monto_dol,
		 count(*) as items,
		 (SELECT COUNT(s.proveedor)
               FROM orden_servicio s
              WHERE trunc(s.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
  				    AND s.cod_origen like :as_origen
  					 AND s.proveedor = p.proveedor)
from orden_servicio_det osd,
     orden_servicio		os,
     proveedor				p
where os.nro_os = osd.nro_os
  and os.proveedor = p.proveedor
  and osd.flag_Estado <> '0'
  and os.flag_estado <> '0'
  and trunc(os.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
  and os.cod_origen like :as_origen
group by p.proveedor,
		 	p.nom_proveedor
order by monto_dol desc

