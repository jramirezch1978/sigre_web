 column=(type=number updatewhereclause=yes name=cant_oc dbname="cant_oc" )
 column=(type=char(10) updatewhereclause=yes name=ot_adm dbname="ot_adm" )
 retrieve="  SELECT A.COD_ART,   
         A.desc_art,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP_OT.FEC_PROYECT,
         AMP_OT.CANT_PROYECT,
			AMP_OT.CANT_PROCESADA,   
         round(NVL(AMP_OT.CANT_PROYECT,0) - NVL(AMP_OT.CANT_PROCESADA,0) - 
		   NVL(AMP_OT.CANT_RESERVADO, 0) -
		   NVL(SUM(DECODE(AMP_OC.flag_estado, '1', AMP_OC.CANT_PROYECT - AMP_OC.CANT_PROCESADA,0)),0) -
		   NVL(SUM(AMP_OC.CANT_TRANSPORTADA),0),3) as pend_recib,   
         AMP_OT.CENCOS,   
         AMP_OT.TIPO_DOC,   
         AMP_OT.NRO_DOC,   
         AMP_OT.COD_ORIGEN,   
         AMP_OT.NRO_MOV,   
         AMP_OT.OPER_SEC,   
         AMP_OT.ALMACEN,   
         AMP_OT.FEC_REGISTRO,
         AMP_OT.COD_USR,
			AMP_OT.CANT_RESERVADO,
         AA.SLDO_CONSIGNACION,
         NVL(AA.SLDO_TOTAL,0) + NVL(AA.SLDO_CONSIGNACION,0) + NVL(AA.SLDO_TRANSPORTADO,0) as sldo_fisico,
			NVL(AA.SLDO_SOLICITADO,0) - (NVL(AA.SLDO_TOTAL,0) + NVL(AA.SLDO_CONSIGNACION,0) + NVL(AA.SLDO_POR_LLEGAR,0) + NVL(AA.SLDO_TRANSPORTADO,0)) as SLDO_PEND_ALM,     
         AA.SLDO_POR_LLEGAR,   
         AA.COSTO_ULT_COMPRA,   
         AA.FEC_ULT_COMPRA,   
         AA.SLDO_SOLICITADO,
         AA.SLDO_RESERVADO,
         AL.ALMACEN,
         PCD.NRO_PROGRAMA,
			MIN(AMP_OC.NRO_DOC) as nro_doc_oc,
         NVL(SUM(DECODE(AMP_OC.FLAG_ESTADO, 2, AMP_OC.CANT_PROCESADA, AMP_OC.CANT_PROYECT)), 0)    as qnt_comp,   
         NVL(SUM(AMP_OC.CANT_PROCESADA),0) as qnt_recib,
			NVL(SUM(AMP_OC.CANT_TRANSPORTADA),0) as qnt_transp,
			COUNT(AMP_OC.NRO_DOC) as cant_oc,
			OT.OT_ADM
    FROM ARTICULO 			A,   
         ARTICULO_MOV_PROY AMP_OT,   
         ARTICULO_ALMACEN 	AA,   
         ALMACEN 				AL,
         (SELECT * 
				FROM ARTICULO_MOV_PROY 
			  WHERE TIPO_DOC = (select doc_oc from logparam where reckey = '1')
   			 AND FLAG_ESTADO in ('1', '2')) AMP_OC ,
		   (SELECT * 
				FROM PROG_COMPRAS_DET 
			  WHERE FLAG_ESTADO = '1') 	PCD,
			ORDEN_TRABAJO		OT
   WHERE AMP_OT.COD_ART 		= A.COD_ART 
     and AMP_OT.COD_ART 		= AA.COD_ART 
	  and AMP_OT.ALMACEN 		= AA.ALMACEN 
	  and AA.ALMACEN 				= Al.ALMACEN 
	  and AMP_OT.COD_ORIGEN 	= AMP_OC.ORG_AMP_REF(+) 
	  and AMP_OT.NRO_MOV 		= AMP_OC.NRO_AMP_REF(+) 
	  and AMP_OT.COD_ORIGEN 	= PCD.ORG_AMP_OT_REF(+) 
	  and AMP_OT.NRO_MOV 		= PCD.NRO_AMP_OT_REF(+) 
	  and AMP_OT.FLAG_ESTADO 	= '1' 
	  AND AMP_OT.CANT_PROYECT 	> AMP_OT.CANT_PROCESADA 
	  AND OT.NRO_ORDEN			= AMP_OT.NRO_DOC
	  AND trunc(AMP_OT.FEC_PROYECT) between trunc(:ad_inicio) and trunc(:ad_fin)
	  AND AMP_OT.TIPO_DOC 		= (select doc_ot from logparam where reckey = '1')
	  AND OT.OT_ADM	 			like :as_ot_adm
	  AND AMP_OT.TIPO_MOV 		= (select oper_cons_interno from logparam where reckey = '1')
	  AND A.FLAG_INVENTARIABLE = '1' 
	  AND AL.ALMACEN 				like :as_almacen
	  and A.FLAG_REPOSICION 	in ( :as_reposicion ) 
	  and (NVL(AA.SLDO_TOTAL,0) + NVL(AA.SLDO_CONSIGNACION,0)) < AA.SLDO_SOLICITADO 
GROUP BY A.COD_ART,   
         A.desc_art,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP_OT.FEC_PROYECT,   
         AMP_OT.CANT_PROYECT,
      	AMP_OT.CANT_PROCESADA,  
         AMP_OT.CENCOS,   
         AMP_OT.TIPO_DOC,   
         AMP_OT.NRO_DOC,   
         AMP_OT.COD_ORIGEN,
         AMP_OT.NRO_MOV,   
         AMP_OT.OPER_SEC,   
         AMP_OT.ALMACEN,   
         AMP_OT.FEC_REGISTRO,
         AMP_OT.COD_USR,
			AMP_OT.CANT_RESERVADO,
         AA.SLDO_CONSIGNACION,
			AA.SLDO_TOTAL,
         AA.SLDO_POR_LLEGAR,   
         AA.COSTO_ULT_COMPRA,   
         AA.FEC_ULT_COMPRA,   
         AA.SLDO_SOLICITADO,
         AA.SLDO_RESERVADO,
			AA.SLDO_TRANSPORTADO,
         AL.ALMACEN,
         PCD.NRO_PROGRAMA,
			OT.OT_ADM
 HAVING round(NVL(AMP_OT.CANT_PROYECT,0) - NVL(AMP_OT.CANT_PROCESADA,0) - 
		  NVL(AMP_OT.CANT_RESERVADO, 0) -
		  NVL(SUM(DECODE(AMP_OC.flag_estado, '1', AMP_OC.CANT_PROYECT - AMP_OC.CANT_PROCESADA,0)),0) -
		  NVL(SUM(AMP_OC.CANT_TRANSPORTADA),0),3) > 0
                                                                                                     
                                                                                                 
" arguments=(("ad_inicio", date),("ad_fin", date),("as_almacen", string),("as_reposicion", stringlist),("as_ot_adm", string))  sort="flag_reposicion A cod_art A nro_doc A oper_sec A " )
