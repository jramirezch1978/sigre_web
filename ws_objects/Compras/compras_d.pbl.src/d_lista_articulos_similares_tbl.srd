 retrieve="select a.cod_art,
       a1.desc_categoria, a2.desc_sub_cat,
       a.desc_art,
		 a.und,
       a.cod_sku,
		 a.cod_marca,
		 m.nom_marca,
       a.cod_sub_linea,
       a.cod_acabado,
       a.color1,
       a.color2,
       a.cod_taco,
       a.talla,
       a.estilo,
       a.flag_genero
from articulo 				a,
     articulo_sub_categ a2,
     articulo_categ     a1,	
	  marca					m
where a.sub_cat_art 	= a2.cod_sub_cat
  and a2.cat_art  	= a1.cat_art
  and a.cod_marca		= m.cod_marca
  and trim(a.cod_art)	<> :as_cod_art
  and a.cod_sub_linea 	= :as_cod_sub_linea
  and a.cod_acabado   	= :as_cod_acabado
  and a.cod_taco      	= :as_cod_taco
  and trim(NVL(a.color1, 'XX'))  = trim(nvl(:as_color1, 'XX'))
  and NVL(a.color2, 'XX')  = nvl(:as_color2, 'XX')
  and a.estilo        	= :as_estilo
  and a.flag_genero   	= :as_flag_genero
  and a.cod_marca		 	= :as_cod_marca
" arguments=(("as_cod_Art", string),("as_cod_sub_linea", string),("as_cod_acabado", string),("as_cod_taco", string),("as_color1", string),("as_color2", string),("as_estilo", string),("as_flag_genero", string),("as_cod_marca", string)) )
