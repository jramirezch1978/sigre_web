 retrieve="  SELECT DISTINCT OC.COD_ORIGEN,   
         OC.NRO_OC,   
         OC.FLAG_ESTADO,   
         OC.FLAG_COTIZACION,   
         OC.FLAG_IMPORTACION,   
         OC.PROVEEDOR,   
         OC.NOM_VENDEDOR,   
         OC.OBSERVACION,   
         OC.FEC_REGISTRO,   
         OC.FORMA_PAGO,   
         OC.COD_MONEDA,   
         OC.MONTO_TOTAL,   
         OC.MONTO_FACTURADO,   
         OC.COD_USR,   
         OC.SOL_COD_ORIGEN,   
         OC.NRO_SOL_COMP,   
         OC.NRO_PROGRAMA,   
         OC.JOB  
    FROM ORDEN_COMPRA  		OC,
			ARTICULO_MOV_PROY AMP,
			ARTICULO				A
   WHERE AMP.COD_ART 	= A.COD_ART
	  AND OC.NRO_OC      =  AMP.NRO_DOC                    
	  AND AMP.TIPO_DOC 	=  (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1') 
	  AND ( NVL(AMP.FLAG_ESTADO, '0') = '1' 
			  OR (NVL(AMP.FLAG_ESTADO, '0') = '2' AND AMP.CANT_PROCESADA > 0 ) )
     and trunc(oc.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
     and amp.cod_art		    like :as_cod_art 
  	  and upper(a.desc_art)  like :as_desc_art
  	  and oc.nro_oc          like :as_nro_doc
     and amp.almacen        like :as_almacen


" arguments=(("ad_fecha1", date),("ad_Fecha2", date),("as_cod_art", string),("as_desc_art", string),("as_nro_doc", string),("as_almacen", string)) )
