 retrieve="SELECT OC.NRO_OC,
       OC.COD_ORIGEN,
       OC.OBSERVACION,
       AMP.COD_ART,
       A.DESC_ART,
       A.UND,
       U.DESC_UNIDAD,
       AMP.CANT_PROYECT,
       OC.PROVEEDOR,
       P.NOM_PROVEEDOR,
       SUM(USF_FL_CONV_MON(AMP.CANT_PROYECT * (AMP.PRECIO_UNIT - AMP.DECUENTO), OC.COD_MONEDA,(SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1'), FEC_PROYECT)) AS MONTO_DOL
  
  FROM ARTICULO_MOV_PROY AMP,
       ORDEN_COMPRA      OC,
       PROVEEDOR         P,
       ARTICULO          A,
       UNIDAD            U

 WHERE OC.NRO_OC = AMP.NRO_DOC
   AND AMP.TIPO_DOC = (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1')
   AND OC.PROVEEDOR = P.PROVEEDOR
   AND AMP.COD_ART = A.COD_ART
   AND U.UND = A.UND
   AND AMP.FLAG_ESTADO <> '0'
   AND OC.FLAG_ESTADO <> '0'
   and trunc(oc.fec_registro) between trunc(:ad_fec_ini) and trunc(:ad_fec_fin)
   and oc.cod_origen like :as_origen
   AND oc.proveedor = :as_proveedor
   and amp.cod_art   = :as_cod_art
 GROUP BY OC.NRO_OC,OC.COD_ORIGEN,
       OC.OBSERVACION,
       AMP.COD_ART,
       A.DESC_ART,
       A.UND,
       U.DESC_UNIDAD,
       AMP.CANT_PROYECT,
       OC.PROVEEDOR,
       P.NOM_PROVEEDOR
 ORDER BY MONTO_DOL DESC" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_origen", string),("as_proveedor", string),("as_cod_art", string)) )
