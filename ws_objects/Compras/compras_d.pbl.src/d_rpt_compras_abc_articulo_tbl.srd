 retrieve="select tt.cod_art, a.desc_art, a.und, 
		 sum(tt.cant_proyect) as cant_total, 
		 sum(tt.cant_proyect*(tt.precio_unit-tt.descuento)) as total_dol, 
       sum(tt.cant_proyect*(tt.precio_unit-tt.descuento)*100/:ad_total) as porc 
from tt_cmp_compras_resumen tt, articulo a
where tt.cod_art=a.cod_art 
group by tt.cod_art, a.desc_art, a.und
order by sum(tt.cant_proyect*(tt.precio_unit-tt.descuento)) desc " arguments=(("ad_total", number)) )
