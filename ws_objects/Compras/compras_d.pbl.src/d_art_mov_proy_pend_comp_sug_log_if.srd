 retrieve="  SELECT A.COD_ART,   
         A.NOM_ARTICULO,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP.FEC_PROYECT,
         AMP.CANT_PROYECT,
			AMP.CANT_PROCESADA,   
         NVL(AMP.CANT_PROYECT,0) - NVL(AMP.CANT_PROCESADA,0) - NVL(AMP.CANT_RESERVADO,0) as pend_recib,   
         AMP.CENCOS,   
         AMP.TIPO_DOC,   
         AMP.NRO_DOC,   
         AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.OPER_SEC,   
         AMP.ALMACEN,   
         AMP.FEC_REGISTRO,
         AMP.COD_USR,
			AMP.CANT_RESERVADO,
         AALM.SLDO_CONSIGNACION,
         NVL(AALM.SLDO_TOTAL,0) + NVL(AALM.SLDO_CONSIGNACION,0) as sldo_fisico,
			NVL(AALM.SLDO_SOLICITADO,0) - (NVL(AALM.SLDO_TOTAL,0) + NVL(AALM.SLDO_CONSIGNACION,0) + NVL(AALM.SLDO_POR_LLEGAR,0)) as SLDO_PEND_ALM,     
         AALM.SLDO_POR_LLEGAR,   
         AALM.COSTO_ULT_COMPRA,   
         AALM.FEC_ULT_COMPRA,   
         AALM.SLDO_SOLICITADO,
         AALM.SLDO_RESERVADO,
         ALM.ALMACEN,
			ALM.COD_ORIGEN,
         PCD.NRO_PROGRAMA,
			usf_cmp_comprador_x_articulo(alm.cod_origen, a.cod_art) as comprador,
			MIN(AMPOC.NRO_DOC) as nro_doc_oc,
         NVL(SUM(CASE WHEN AMPOC.FLAG_ESTADO = 1 THEN AMPOC.CANT_PROYECT
                      WHEN AMPOC.FLAG_ESTADO = 2 THEN AMPOC.CANT_PROCESADA
                      END),0) as 	qnt_comp,   
         NVL(SUM(AMPOC.CANT_PROCESADA),0) as qnt_recib,
			COUNT(AMPOC.NRO_DOC) as cant_oc
    FROM ARTICULO A,   
         ARTICULO_MOV_PROY AMP,   
         ARTICULO_ALMACEN AALM,   
         ALMACEN ALM,
         ARTICULO_MOV_PROY AMPOC,
         (SELECT * 
				FROM PROG_COMPRAS_DET 
			  WHERE FLAG_ESTADO = '1') PCD   
   WHERE ( AMP.COD_ART = A.COD_ART ) and  
         ( AMP.COD_ART = AALM.COD_ART ) and  
         ( AMP.ALMACEN = AALM.ALMACEN ) and  
         ( ALM.ALMACEN = AMP.ALMACEN ) and
         ( AMP.COD_ORIGEN = AMPOC.ORG_AMP_REF(+) and AMP.NRO_MOV = AMPOC.NRO_AMP_REF(+) ) and
         ( AMP.COD_ORIGEN = PCD.ORG_AMP_OT_REF(+) and AMP.NRO_MOV = PCD.NRO_AMP_OT_REF(+) ) and  
         ( ( AMP.FLAG_ESTADO = '1' ) AND  
         ( AMP.CANT_PROYECT > AMP.CANT_PROCESADA ) AND  
         ( trunc(AMP.FEC_PROYECT) between :ad_inicio and :ad_fin ) AND  
         ( AMP.TIPO_DOC = :as_doc_ot ) AND  
         ( AMP.TIPO_MOV = :as_oper ) AND  
         ( A.FLAG_INVENTARIABLE = '1' ) AND  
         ( ALM.ALMACEN in (:as_almacen) ) and
         ( A.FLAG_REPOSICION in ( :as_reposicion ) ) and
			( usf_cmp_comprador_x_articulo(alm.cod_origen, a.cod_art) in ( :as_comprador ) ) and
         ( (NVL(AALM.SLDO_TOTAL,0) + NVL(AALM.SLDO_CONSIGNACION,0)) < AALM.SLDO_SOLICITADO ))   
GROUP BY A.COD_ART,   
         A.NOM_ARTICULO,
			A.UND,
         A.FLAG_REPOSICION,
         A.DIAS_REPOSICION,   
         AMP.FEC_PROYECT,   
         AMP.CANT_PROYECT,
      	AMP.CANT_PROCESADA,   
         AMP.CENCOS,   
         AMP.TIPO_DOC,   
         AMP.NRO_DOC,   
         AMP.COD_ORIGEN,
         AMP.NRO_MOV,   
         AMP.OPER_SEC,   
         AMP.ALMACEN,   
         AMP.FEC_REGISTRO,
         AMP.COD_USR,
			AMP.CANT_RESERVADO,
         AALM.SLDO_CONSIGNACION,
			AALM.SLDO_TOTAL,
         AALM.SLDO_POR_LLEGAR,   
         AALM.COSTO_ULT_COMPRA,   
         AALM.FEC_ULT_COMPRA,   
         AALM.SLDO_SOLICITADO,
         AALM.SLDO_RESERVADO,
         ALM.ALMACEN,
			ALM.COD_ORIGEN,
         PCD.NRO_PROGRAMA   
 HAVING (NVL(AMP.CANT_PROYECT,0) - NVL(AMP.CANT_PROCESADA,0) - NVL(AMP.CANT_RESERVADO,0)) > NVL(SUM(CASE WHEN AMPOC.FLAG_ESTADO = 1 THEN NVL(AMPOC.CANT_PROYECT,0) END), 0) 
                                                                                                     
                                                                                                 
" arguments=(("as_doc_ot", string),("as_oper", string),("ad_inicio", date),("ad_fin", date),("as_almacen", stringlist),("as_reposicion", stringlist),("as_comprador", stringlist))  sort="flag_reposicion A comprador A cod_art A nro_doc A oper_sec A " )
