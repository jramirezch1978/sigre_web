 retrieve="SELECT tt.cod_art, a.desc_art, a.und, 
		 sum(tt.cant_proyect* tt.precio_unit)/sum(tt.cant_proyect) as precio_prom,
		 sum(tt.cant_proyect) as total_cant,
		 sum(tt.cant_proyect* tt.precio_unit) as total_compra,
		 sum(tt.cant_proyect*tt.descuento) as total_dcto, 
		 sum(tt.cant_proyect*tt.descuento/:an_total_descuento*100) as porc
from TT_CMP_COMPRAS_RESUMEN 	tt, 
	  proveedor 					p, 
	  articulo 						a
where tt.proveedor=p.proveedor 
  and tt.cod_art = a.cod_art 
  and tt.descuento > 0 
group by tt.cod_art, a.desc_art, a.und 
order by sum(tt.cant_proyect*tt.descuento) desc
" arguments=(("an_total_descuento", number)) )
