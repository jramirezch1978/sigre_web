 column=(type=char(6) updatewhereclause=yes name=cod_sub_cat dbname="articulo_sub_categ.cod_sub_cat" )
 column=(type=char(60) updatewhereclause=yes name=desc_sub_cat dbname="articulo_sub_categ.desc_sub_cat" )
 column=(type=number updatewhereclause=yes name=monto_soles dbname="monto_soles" )
 column=(type=number updatewhereclause=yes name=monto_dolares dbname="monto_dolares" )
 retrieve="select ac2.cat_art, ac2.desc_categoria,
		 ac1.cod_sub_cat, ac1.desc_sub_cat,
       sum(usf_fl_conv_mon( (amp.precio_unit - amp.decuento) * amp.cant_proyect, oc.cod_moneda, (select cod_soles from logparam where reckey = '1'), oc.fec_registro)) as monto_soles,
       sum(usf_fl_conv_mon( (amp.precio_unit - amp.decuento) * amp.cant_proyect, oc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), oc.fec_registro)) as monto_dolares
from orden_compra       oc,
     articulo_mov_proy  amp,
     articulo           a,
     articulo_sub_categ ac1,
	  articulo_categ     ac2,	
	  tt_articulos_cmp	tt1
where oc.nro_oc       = amp.nro_doc  
  and amp.tipo_doc    = (select doc_oc from logparam where reckey = '1')
  and a.cod_art       = amp.cod_art
  and ac1.cod_sub_cat = a.sub_cat_art
  and ac2.cat_art		 = ac1.cat_art
  and ltrim(rtrim(tt1.codigo)) = ltrim(rtrim(ac1.cod_sub_cat))
  and trunc(oc.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
  and oc.flag_estado <> '0'
  and amp.flag_estado <> '0'
group by ac2.cat_art, ac2.desc_categoria, ac1.cod_sub_cat, ac1.desc_sub_cat  
" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
