 retrieve="select 	cpd.tipo_doc as tipo_doc_oc, 
       	cpd.nro_doc as nro_doc_oc,
       	cp.fecha_emision as fecha_emision_cp_oc,
       	cp.tasa_cambio as tasa_cambio_cp_oc,
       	cp.cod_moneda as moneda_cp_oc,
       	cpd.importe as importe_cp_oc,
		 	case 
				when cp.cod_moneda = (select cod_soles from logparam where reckey = '1') then
					cpd.importe
				else
					cpd.importe * cp.tasa_cambio
			end as imp_soles,
		 	case 
				when cp.cod_moneda = (select cod_dolares from logparam where reckey = '1') then
					cpd.importe
				else
					cpd.importe / cp.tasa_cambio
			end as imp_dolares
		
		
from cntas_pagar         cp,
     cntas_pagar_det     cpd
where cpd.cod_relacion   = cp.cod_relacion
  and cpd.tipo_doc       = cp.tipo_doc
  and cpd.nro_doc        = cp.nro_doc
  and cpd.org_amp_ref	 = :as_org_amp
  and cpd.nro_amp_ref	 = :an_nro_amp
order by cpd.org_amp_ref, cpd.nro_amp_ref  " arguments=(("as_org_amp", string),("an_nro_amp", number))  sort="fecha_emision_cp_oc A " )
