 retrieve="select b.nro_aprob,
		 amp.cod_origen,
		 amp.nro_mov,
		 amp.flag_estado,
		 amp.cod_art, a.desc_art, a.und, 
	    amp.fec_proyect,
       NVL(amp.cant_proyect,0) - NVL(amp.cant_procesada,0) as cant_pendiente,
       amp.tipo_doc,
       amp.nro_doc,
       amp.oper_sec,
       c.fecha_emis, 
       c.usr_creacion, 
       u1.nombre as nom_usr_creacion,
       c.flag_activacion,
       c.fecha_aprob,
       c.usr_aprob,
       u2.nombre as nom_usr_aprob      
from articulo_mov_proy amp,
     ot_aprobacion_amp b,
     articulo          a,
     ot_aprobaciones   c,
     usuario           u1,
     usuario           u2
where b.cod_origen = amp.cod_origen
  and b.nro_mov    = amp.nro_mov
  and a.cod_art    = amp.cod_art
  and c.nro_aprob  = b.nro_aprob
  and c.usr_creacion = u1.cod_usr (+)
  and c.usr_aprob    = u2.cod_usr (+)
  and trunc(c.fecha_emis) >= :ad_fecha1
  and trunc(c.fecha_emis) <= :ad_fecha2
  and NVL(amp.cant_proyect,0) > NVL(amp.cant_procesada,0)
order by c.nro_aprob, amp.cod_art" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
