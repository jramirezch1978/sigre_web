 retrieve="  SELECT PCD.NRO_ITEM,   
         PCD.FEC_REGISTRO,   
         PCD.FLAG_PRIORIDAD,   
         PCD.COD_ART,   
         PCD.COMPRADOR,   
         PCD.FEC_REQUERIDA,   
         PCD.NRO_PROGRAMA,   
         (NVL(PCD.CANT_PROYECT,0) - NVL(PCD.CANT_PROCESADA,0)) as CANT_PENDIENTE,   
         AMP.NRO_DOC,
	 		AMP.CANT_PROYECT - AMP.CANT_PROCESADA AS PENDIENTE,
         AMP.ALMACEN,
	 		A.NOM_ARTICULO,   
         A.UND,
	 		COUNT(AMPC.COD_ORIGEN) AS CNT_OC,
	 		SUM(NVL(AMPC.CANT_PROYECT,0)) AS QNT_COMP,
         SUM(NVL(AMPC.CANT_PROCESADA,0)) AS QNT_ING
    FROM ARTICULO A,   
         PROG_COMPRAS_DET PCD,   
         ARTICULO_MOV_PROY AMP,
         ARTICULO_MOV_PROY AMPC
   WHERE ( PCD.COD_ART = A.COD_ART ) and  
         ( PCD.ORG_AMP_OT_REF = AMP.COD_ORIGEN )   and ( PCD.NRO_AMP_OT_REF = AMP.NRO_MOV ) and
         ( :as_doc_prc = AMPC.TIPO_REF(+) and PCD.NRO_PROGRAMA = AMPC.REFERENCIA(+) and PCD.NRO_ITEM = AMPC.ITEM_REF(+)) and 
	 		( ( PCD.FLAG_ESTADO = '1' ) AND  
         ( NVL(PCD.CANT_PROYECT,0) > NVL(PCD.CANT_PROCESADA,0) ) ) AND
	 		(AMP.TIPO_DOC	= :as_doc_ot )	AND
 	 		(AMP.NRO_DOC = :as_nro_doc	)    
GROUP BY PCD.NRO_ITEM,   
         PCD.FEC_REGISTRO,   
         PCD.FLAG_PRIORIDAD,   
         PCD.COD_ART,   
         PCD.COMPRADOR,   
         PCD.FEC_REQUERIDA,   
         PCD.NRO_PROGRAMA,   
         PCD.CANT_PROYECT,
	 		PCD.CANT_PROCESADA,  
         AMP.NRO_DOC,
	 		AMP.CANT_PROYECT,
	 		AMP.CANT_PROCESADA,
         AMP.ALMACEN,
	 		A.NOM_ARTICULO,   
         A.UND" arguments=(("as_doc_prc", string),("as_doc_ot", string),("as_nro_doc", string))  sort="nro_programa A nro_item A " )
