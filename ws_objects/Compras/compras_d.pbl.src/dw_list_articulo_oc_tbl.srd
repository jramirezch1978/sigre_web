 retrieve="SELECT DISTINCT 
		 A.COD_ART,
       A.DESC_ART,
       A.UND
       
FROM   ARTICULO A,
       ARTICULO_MOV_PROY  AMP,
       ORDEN_COMPRA       OC

WHERE  A.COD_ART    = AMP.COD_ART
  AND  AMP.TIPO_DOC = (SELECT DOC_OC FROM LOGPARAM WHERE RECKEY = '1')
  AND  OC.NRO_OC    = AMP.NRO_DOC
  AND  AMP.FLAG_ESTADO <> '0'
  AND  OC.FLAG_ESTADO <> '0'
  AND  OC.FEC_REGISTRO BETWEEN :AD_FECHA_INI  AND :AD_FECHA_FIN
  AND  OC.COD_ORIGEN LIKE :AS_ORIGEN" arguments=(("AD_FECHA_INI", date),("AD_FECHA_FIN", date),("AS_ORIGEN", string))  sort="cod_art A " )
