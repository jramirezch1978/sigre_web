 column=(type=char(40) updatewhereclause=yes name=tg_plantas_desc_planta dbname="tg_plantas.desc_planta" )
 retrieve="SELECT DISTINCT pp.nro_parte,
       pp.documento_desc,
       pp.fecha_parte,
       pp.cod_usr,
       pp.formato,
		   pp.nro_version,
       f.descripcion,
       pp.cod_planta,
       p.desc_planta

  FROM tg_parte_piso 		pp,
       tg_fmt_med_act 		f,
       tg_plantas 			p

 WHERE pp.formato              			= f.formato(+)
       AND pp.nro_version		    		= f.nro_version(+)
       AND pp.cod_planta        			= p.cod_planta(+)
       AND TRIM(NVL(PP.FORMATO,''))	like :as_formato
       and pp.nro_version			      = :ai_nro_version
		 AND trim(nvl(PP.COD_PLANTA,''))  like :as_cod_planta
       AND trunc(pp.fecha_parte) BETWEEN trunc(:ad_fecha1) AND trunc(:ad_fecha2)
  " arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_cod_planta", string),("as_formato", string),("ai_nro_version", number)) )
