 retrieve="SELECT distinct CP.COD_CERTIFICADO, 
       CP.PROVEEDOR, 
       P.NOM_PROVEEDOR,
       CPD.COD_SERVICIO||' '||C.DESC_SERVICIO AS SERVICIO,
       CP.FEC_INICIO, 
       CPD.FEC_VENCIMIENTO  
       
  FROM CERTIFICADO_PLANTA       CP,
       CERTIFICADO_PLANTA_DET   CPD,
       PROVEEDOR                P,
       CERTIFICADO_SERVICIO     C
 WHERE CP.COD_CERTIFICADO = CPD.COD_CERTIFICADO
   AND CP.PROVEEDOR       = P.PROVEEDOR
   AND CP.COD_PLANTA      = :as_cod_planta

ORDER BY CP.COD_CERTIFICADO ASC" arguments=(("as_cod_planta", string)) )
