 column=(type=char(60) updatewhereclause=yes name=codigo_relacion_nombre dbname="codigo_relacion.nombre" )
 column=(type=char(58) updatewhereclause=yes name=linea_destajo dbname="linea_destajo" )
 column=(type=char(3) updatewhereclause=yes name=unidad_destajo dbname="de_lineas.unidad_destajo" )
 column=(type=number updatewhereclause=yes name=cantidad_destajada dbname="cantidad_destajada" )
 column=(type=number updatewhereclause=yes name=total_costo dbname="total_costo" )
 retrieve="select pda.cod_trabajador, 
		 cr.nombre, 
       dl.linea_destajo || ' - ' || dl.descr_linea as Linea_destajo,
		 dl.unidad_destajo,
       Sum(pda.cant_destajada) as cantidad_destajada, 
     	 Sum(pda.cant_destajada * op.costo_unit) as total_costo
from pd_ot_asist_destajo pda,
     codigo_relacion     cr,
     pd_ot               pd,
     operaciones         op,
     pd_ot_det           pdd,
     de_lineas           dl
where cr.cod_relacion = pda.cod_trabajador     
  and pd.nro_parte    = pda.nro_parte
  and pdd.nro_parte   = pda.nro_parte
  and pdd.nro_item    = pda.nro_item
  and op.oper_sec     = pdd.oper_sec
  and dl.linea_destajo = pda.linea_destajo
  and to_char(pd.FECHA, 'yyyymmdd') >= to_char(:ad_fecha1, 'yyyymmdd')
  AND to_char(pd.FECHA, 'yyyymmdd') <= to_char(:ad_fecha2, 'yyyymmdd')
  AND pd.ot_adm = :as_ot_adm
  AND SUBSTR(pd.NRO_PARTE,1,2) LIKE :as_origen
  AND pd.TURNO LIKE :as_turno
group by dl.linea_destajo || ' - ' || dl.descr_linea, pda.cod_trabajador, cr.nombre, dl.unidad_destajo
order by dl.linea_destajo || ' - ' || dl.descr_linea, pda.cod_trabajador
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string),("as_origen", string),("as_turno", string)) )
