 retrieve="  SELECT ~"PROD_CONSUMO_ENER~".~"NRO_CONSUMO~",   
         ~"PROD_CONSUMO_ENER~".~"COD_PLANTA~",   
         ~"TG_PLANTAS~".~"DESC_PLANTA~",   
         ~"PROD_CONSUMO_ENER~".~"ANO~",   
         ~"PROD_CONSUMO_ENER~".~"MES~",   
         ~"PROD_CONSUMO_ENER~".~"COD_ORIGEN~",   
         ~"ORIGEN~".~"NOMBRE~",   
         ~"PROD_CONSUMO_ENER~".~"FECHA_REGISTRO~",   
         ~"PROD_CONSUMO_ENER~".~"COD_USR~",   
         ~"PROD_CONSUMO_ENER~".~"FACTOR_HP~",   
         ~"PROD_CONSUMO_ENER~".~"FACTOR_FHP~",   
         ~"PROD_CONSUMO_ENER~".~"FACTOR_CLIENTE~",   
         ~"PROD_CONSUMO_ENER~".~"IMP_TOTAL_APROX~",   
         ~"PROD_CONSUMO_ENER~".~"TOTAL_AJUSTE~",   
         ~"PROD_CONSUMO_ENER~".~"NRO_DIAS_HABILES~",
			 ~"PROD_CONSUMO_ENER~".~"COD_MONEDA~",
			m.descripcion
 
    FROM ~"ORIGEN~",   
         ~"PROD_CONSUMO_ENER~",   
         ~"TG_PLANTAS~",
			 MONEDA M
 
   WHERE ( prod_consumo_ener.cod_origen (+) = origen.cod_origen) and  
         ( tg_plantas.cod_planta (+) = prod_consumo_ener.cod_planta) and  
         ( ~"PROD_CONSUMO_ENER~".~"NRO_CONSUMO~" = :as_nro_consumo ) AND
         ( M.COD_MONEDA(+) = ~"PROD_CONSUMO_ENER~".~"COD_MONEDA~")
   
" update="PROD_CONSUMO_ENER" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_consumo", string)) )
