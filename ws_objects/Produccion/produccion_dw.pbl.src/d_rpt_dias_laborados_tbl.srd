table(column=(type=char(8) updatewhereclause=yes name=maestro_cod_trabajador dbname="maestro.cod_trabajador" )
 column=(type=char(123) updatewhereclause=yes name=nombre dbname="nombre" )
 column=(type=char(10) updatewhereclause=yes name=maestro_cencos dbname="maestro.cencos" )
 column=(type=char(40) updatewhereclause=yes name=centros_costo_desc_cencos dbname="centros_costo.desc_cencos" )
 column=(type=char(2) updatewhereclause=yes name=maestro_cod_afp dbname="maestro.cod_afp" )
 column=(type=char(30) updatewhereclause=yes name=admin_afp_desc_afp dbname="admin_afp.desc_afp" )
 column=(type=number updatewhereclause=yes name=horas dbname="horas" )
 column=(type=number updatewhereclause=yes name=dias dbname="dias" )
 column=(type=number updatewhereclause=yes name=dias_totales dbname="dias_totales" )
 retrieve="SELECT m.cod_trabajador, 
       m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 AS nombre,
       m.cencos, c.desc_cencos,
       m.cod_afp,
       f.desc_afp,
       Round(SUM((a.fec_hasta - a.fec_desde)*24),0)  AS horas,
       SUM(Round(((a.fec_hasta - a.fec_desde)*24)/8,0)) AS dias,   
 CASE WHEN (SUM(Round(((a.fec_hasta - a.fec_desde)*24)/8,0))) >= 3 THEN 
      SUM(Round(((a.fec_hasta - a.fec_desde)*24)/8,0)) + 1
      END AS DIAS_TOTALES
      
 FROM maestro           m,
       centros_costo     c,
       admin_afp         f,
       asistencia        a
       
 WHERE m.cencos          = c.cencos
   AND m.cod_afp         = f.cod_afp(+)
   AND m.tipo_trabajador = :as_tipo
   AND a.ot_adm          like :as_ot_adm
   AND a.cod_trabajador  = m.cod_trabajador
   AND M.FLAG_ESTADO     <> 0
   AND trunc(a.fec_movim)  BETWEEN trunc(:ad_ini) AND trunc(:ad_fin)
 
 GROUP BY
       m.cod_trabajador, 
       m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2,
       m.cencos, c.desc_cencos,
       m.cod_afp,
       f.desc_afp" arguments=(("as_tipo", string),("as_ot_adm", string),("ad_ini", date),("ad_fin", date)) )
