 retrieve="select sc.supervisor, 
       trim(m1.apel_paterno || ' ' || m1.apel_materno || ', ' || m1.nombre1 || ' ' || decode(m1.nombre2, null, '', substr(m1.nombre2,1,1))) as nom_supervisor,
       sc.cosechador1, 
       trim(m2.apel_paterno || ' ' || m2.apel_materno || ', ' || m2.nombre1 || ' ' || decode(m2.nombre2, null, '', substr(m2.nombre2,1,1))) as nom_cosechador1,
       sc.cosechador2, 
       trim(m3.apel_paterno || ' ' || m3.apel_materno || ', ' || m3.nombre1 || ' ' || decode(m3.nombre2, null, '', substr(m2.nombre2,1,1))) as nom_cosechador2
from superv_campo sc,
     maestro m1,
     maestro m2,
    maestro m3
where sc.supervisor = m1.cod_trabajador
  and sc.cosechador1 = m2.cod_trabajador
  and sc.cosechador2 = m3.cod_trabajador
order by sc.supervisor, sc.cosechador1" )
