 retrieve="SELECT TP.COD_PROD || ' - ' || TP.DESC_PROD AS PRODUCTO,
       TRUNC(OT.FECHA)   AS FECHA,
       SUM(PF.CANTIDAD)  AS CANTIDAD,
       E.DESC_ESTADO
       
  FROM PD_OT_PROD_FINAL PF, 
       PD_OT            OT,
       LABOR            L,
       TIPO_PRODUCTO    TP,
       PD_OT_DET        OTD,
       ESTADO_PRODUCTO  E
       
 WHERE TP.COD_PROD      = PF.COD_PRODUCTO
   AND OT.NRO_PARTE     = OTD.NRO_PARTE
   AND OTD.NRO_PARTE    = PF.NRO_PARTE
   AND OTD.NRO_ITEM     = PF.NRO_ITEM
   AND L.COD_LABOR      = OTD.COD_LABOR
   AND E.COD_ESTADO     = PF.COD_ESTADO
   AND OT.OT_ADM        = :as_ot_adm
   AND SUBSTR(OT.NRO_PARTE,1,2) = :as_origen
   AND trunc(OT.FECHA)  BETWEEN(:ad_fecha1) AND (:ad_fecha2)
GROUP BY TP.COD_PROD || ' - ' || TP.DESC_PROD,
       TRUNC(OT.FECHA),
       L.DESC_LABOR,
       E.DESC_ESTADO" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string),("as_origen", string)) )
