 retrieve="SELECT PD.NRO_PARTE,
       PD.ITEM,
       FM.FORMATO,
       FM.NRO_ITEM,
		 FM.NRO_ATRIBUTO,
       FM.COD_MAQUINA,
       NVL(DECODE(FM.DESC_CABECERA, null, M.DESC_MAQ, FM.DESC_CABECERA),null) as ~"MAQUINA / CABECERA~",
       FM.ATRIBUTO,
       A.DESCRIPCION,
       DECODE(A.FLAG_TIPO_DATO,'C',NULL, '1')   AS FLAG_TEXTO,
       DECODE(A.FLAG_TIPO_DATO,'N',NULL, '1')   AS FLAG_NUMERO,
       DECODE(A.FLAG_TIPO_DATO,'H',NULL, '1')   AS FLAG_HORA,
		 DECODE(A.FLAG_TIPO_DATO,'C','TEXTO', 'N', 'NUMERO', 'H', 'HORA') AS TIPO_DATO,
       A.FLAG_TIPO_DATO,
       PD.VALOR_LECTURA,
		 PD.VALOR_LECTURA_TEXTO,
	    PD.VALOR_LECT_FEC_HORA,
       PD.VALOR_LECT_NUMERICO,
       PD.NRO_CONTROL,
       PD.NRO_LECTURA,
		 PD.HORA_INICIO,
		 PD.HORA_FIN,
       PD.FLAG_REPLICACION,
		 PD.NRO_VERSION
       
  FROM TG_PARTE_PISO_DET   PD,
       TG_FMT_MED_ACT_DET  FM,
       TG_MED_ACT_ATRIBUTO A,
       MAQUINA             M
       
 WHERE FM.FORMATO(+)          = PD.FORMATO
   AND FM.NRO_VERSION(+)   	= PD.NRO_VERSION
   AND FM.NRO_ITEM(+)         = PD.NRO_ITEM
   AND FM.ATRIBUTO            = A.ATRIBUTO(+)
   AND M.COD_MAQUINA(+)       = FM.COD_MAQUINA
   AND PD.NRO_PARTE           = :as_nro_parte
   AND PD.NRO_CONTROL         = :an_control
   AND PD.NRO_LECTURA         = :an_lectura
   
ORDER BY FM.NRO_ATRIBUTO" update="TG_PARTE_PISO_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string),("an_control", number),("an_lectura", number)) )
