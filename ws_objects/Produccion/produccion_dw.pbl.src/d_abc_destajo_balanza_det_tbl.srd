 retrieve="select pd.cod_cuadrilla,
       pd.regkey,
       pd.cod_origen,
       pd.cod_cliente,
       pd.fec_parte,
       pd.fec_registro,
       pd.cod_usr,
       pd.turno,
       pd.cod_especie,
       pd.cod_presentacion,
       pd.cod_tarea,
       pd.nro_tarjeta,
       pd.cod_trabajador,
       pd.cant_producida,
       pd.und,
		 vw.nro_doc_ident_rtps,
       vw.nom_trabajador
from pd_destajo_balanza pd,
     vw_pr_trabajador  vw
where pd.cod_trabajador 	= vw.COD_TRABAJADOR   
  and trunc(pd.fec_parte) 	= trunc(:ad_fec_parte)
  and pd.cod_cliente			= :as_cliente
  and pd.cod_Cuadrilla		= :as_cuadrilla
  and pd.turno					= :as_turno
  and pd.cod_especie			= :as_especie
  and pd.cod_presentacion  = :as_presentacion
  and pd.cod_tarea			= :as_tarea
order by nom_trabajador" arguments=(("ad_fec_parte", date),("as_cliente", string),("as_cuadrilla", string),("as_turno", string),("as_especie", string),("as_presentacion", string),("as_tarea", string))  sort="nom_trabajador A " )
