 retrieve="SELECT DISTINCT    F.DESCRIPCION,
                     PP.FECHA_PARTE,
                     T.DESCRIPCION,
                     M.DESC_MAQ,
                     A.DESCRIPCION,
                     TRIM(PPD.VALOR_LECTURA_TEXTO)||TRIM(TO_CHAR(PPD.VALOR_LECT_FEC_HORA,'HH24:MM:SS'))||TRIM(to_char(PPD.VALOR_LECT_NUMERICO, '999,999.000')) AS VALOR_LECTURA
                    
FROM       TG_PARTE_PISO_DET   PPD,
           TG_MED_ACT_ATRIBUTO A,
           MAQUINA             M,
           TG_FMT_MED_ACT_DET  FD,
           TG_FMT_MED_ACT      F,
           TG_PARTE_PISO       PP,
           TURNO               T
           
     WHERE PPD.NRO_PARTE      = PP.NRO_PARTE
       AND PPD.FORMATO        = FD.FORMATO
       AND PPD.NRO_ITEM       = FD.NRO_ITEM
       AND FD.ATRIBUTO        = A.ATRIBUTO(+)
       AND FD.COD_MAQUINA     = M.COD_MAQUINA(+)
       AND PP.TURNO           = T.TURNO(+)
       AND FD.FORMATO         = F.FORMATO
       AND PPD.NRO_PARTE      = :as_nro_parte
       AND PPD.NRO_CONTROL    = :an_control
       AND PPD.NRO_LECTURA    = :an_lectura
" arguments=(("as_nro_parte", string),("an_control", number),("an_lectura", number)) )
