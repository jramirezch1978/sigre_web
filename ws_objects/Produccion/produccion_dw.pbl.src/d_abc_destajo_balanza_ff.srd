 retrieve="select pp.cod_cuadrilla, tc.desc_cuadrilla,
       pp.cod_especie, e.descr_especie,
       pp.cod_presentacion, p.desc_presentacion,
       pp.cod_tarea, ta.desc_tarea,
       pp.cod_usr, pp.fec_parte, pp.fec_registro, 
       pp.und,
       pp.cod_cliente, p.nom_proveedor as nom_cliente,
       pp.turno, tu.descripcion as desc_turno
from PD_DESTAJO_BALANZA pp,
     tg_presentacion    p,
     tg_tareas          ta,
     tg_cuadrillas      tc,
     tg_tarifario       tf,
     turno              tu,
     tg_especies        e,
     proveedor          p
where pp.cod_cliente      = p.proveedor         (+)
  and pp.cod_cuadrilla    = tc.cod_cuadrilla    (+)
  and pp.cod_especie      = e.especie           (+)
  and pp.cod_presentacion = p.cod_presentacion  (+)
  and pp.cod_tarea        = ta.cod_tarea        (+)
  and pp.cod_especie      = tf.cod_especie      (+)
  and pp.cod_presentacion = tf.cod_presentacion (+)
  and pp.cod_tarea        = tf.cod_tarea          (+)
  and pp.turno            = tu.turno              (+)" )
