 retrieve="select a.fec_movim, a.cod_trabajador, cr.nombre, a.ot_adm, oa.descripcion, a.turno, t.descripcion, t.hora_inicio_norm, t.hora_inicio_sab, t.hora_inicio_dom, t.hora_final_norm, t.hora_final_sab, t.hora_final_dom, a.fec_desde,  a.fec_hasta, to_number(a.fec_hasta - a.fec_desde) * 24 as horas
   from asistencia a
      inner join codigo_relacion cr on a.cod_trabajador = cr.cod_relacion
      inner join ot_administracion oa on a.ot_adm = oa.ot_adm
      inner join turno t on a.turno = t.turno
      inner join maestro m on a.cod_trabajador = m.cod_trabajador
   where trunc(a.fec_desde) between :adi_ini and :adi_fin
      and trim(m.cod_origen) = trim(:asi_cod_origen)
      and m.cod_trabajador like (:as_cod_trabajador)
      and a.ot_adm in (select distinct oau.ot_adm from ot_adm_usuario oau where oau.cod_usr = :asi_cod_usr)
   order by cr.nombre, oa.descripcion, t.descripcion, a.fec_movim, a.fec_desde" arguments=(("asi_cod_origen", string),("adi_ini", date),("adi_fin", date),("asi_cod_usr", string),("as_cod_trabajador", string)) )
