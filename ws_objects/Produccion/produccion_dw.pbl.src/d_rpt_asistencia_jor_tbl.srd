 retrieve="Select trunc(a.fec_movim) as fecha, a.cod_trabajador,
       m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 as Trabajador,
       Nvl(Sum(a.hor_diu_nor),0) as hor_diu_nor,
       Nvl(Sum(a.hor_ext_diu_1),0) as hor_ext_diu_1,
       Nvl(Sum(a.hor_ext_diu_2),0) as hor_ext_diu_2,
       Nvl(Sum(a.hor_noc_nor),0) as hor_noc_nor,
       Nvl(Sum(a.hor_ext_noc_1),0) as hor_ext_noc_1,
       Nvl(Sum(a.hor_ext_noc_2),0) as hor_ext_noc_2
  from asistencia a, maestro m
 Where a.cod_trabajador =m.cod_trabajador
	and a.cod_trabajador like :as_trabajador
   and trunc(a.fec_movim) Between :adi_fecha1 and :adi_fecha2
Group by trunc(a.fec_movim), a.cod_trabajador, m.apel_paterno, m.apel_materno, m.nombre1, m.nombre2" arguments=(("adi_fecha1", date),("adi_fecha2", date),("as_trabajador", string))  sort="fecha A trabajador A " )
