 retrieve="  SELECT A.LINEA_DESTAJO,   
         A.DESCR_LINEA,   
		   A.COD_TRABAJADOR,   
         A.NOMBRE,   
         SUM(A.CANT_DESTAJADA) AS CANT_DESTAJADA,   
         SUM(A.TOTAL_COSTO) / SUM(A.CANT_DESTAJADA)  AS COSTO_UNIT,   
         SUM(A.TOTAL_COSTO) AS TOTAL_COSTO  
    FROM VW_PR_TRAB_DESTAJO_X_OT A  
   WHERE A.NRO_ORDEN = :as_orden   
  GROUP BY A.LINEA_DESTAJO, A.DESCR_LINEA, A.COD_TRABAJADOR, A.NOMBRE
