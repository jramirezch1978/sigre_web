 retrieve="SELECT a.cod_trabajador, 
       m.apel_paterno||' '||m.apel_materno||', '||m.nombre1||' '||m.nombre2 AS trabajador,
       a.fec_desde,
       trim(t.descripcion) || ' / ' || to_char(t.hora_inicio_norm, 'hh24:mi') || ' - ' || to_char(t.hora_final_norm, 'hh24:mi') AS turno, 
       a.cod_tipo_mov, 
       tm.desc_movimi, 
       a.cod_usr, 
		 a.ot_adm
       
  FROM asistencia           a,
       tipo_mov_asistencia  tm,
       maestro              m,
       turno                t

 WHERE a.cod_tipo_mov       = tm.cod_tipo_mov
   AND m.cod_trabajador     = a.cod_trabajador
   AND t.turno              = a.turno
   AND trunc(a.fec_desde) BETWEEN trunc(:ad_fecha1) AND trunc(:ad_fecha2)
   AND a.cod_trabajador     =:as_cod_trabajador
   AND a.cod_tipo_mov		 like :as_tipo_m" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_cod_trabajador", string),("as_tipo_m", string)) )
