 retrieve="SELECT m.cod_trabajador,
       m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 AS nombre,
       m.cencos, c.desc_cencos,
       m.cod_afp,
       f.desc_afp,
		 COUNT(distinct(p.nro_parte)) AS dias_parte

 FROM  maestro              m,
       centros_costo        c,
       PD_OT_ASIST_DESTAJO  AD,
       PD_OT                P,
       admin_afp            f
        
 WHERE m.cencos             = c.cencos
   AND m.cod_afp            = f.cod_afp(+)
   AND M.COD_TRABAJADOR     = AD.COD_TRABAJADOR
   AND p.nro_parte          = ad.nro_parte
   AND m.tipo_trabajador    = :as_tipo
   AND p.ot_adm             like :as_ot_adm
   AND M.FLAG_ESTADO        <> 0
   AND trunc(p.fecha) between trunc(:ad_fecha_1) and trunc(:ad_fecha_2)
 
 GROUP BY
       m.cod_trabajador,
       m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2,
       m.cencos, c.desc_cencos,
       m.cod_afp,
       f.desc_afp

ORDER BY m.cod_trabajador DESC" arguments=(("ad_fecha_1", date),("ad_fecha_2", date),("as_tipo", string),("as_ot_adm", string)) )
