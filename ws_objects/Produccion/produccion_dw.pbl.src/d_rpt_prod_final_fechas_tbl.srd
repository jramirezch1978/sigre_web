 retrieve="  SELECT A.COD_ART,   
         B.NOM_ARTICULO,  
			B.UND,
			C.NRO_PARTE,
			C.FECHA,
			usf_pr_costo_pd_ot(A.NRO_PARTE) as TOTAL_PARTE,
         SUM(A.CANTIDAD) AS CANTIDAD
    FROM PD_OT_PROD_FINAL A,   
         ARTICULO B,   
         PD_OT C  
   WHERE A.COD_ART = B.COD_ART 
	  and A.NRO_PARTE = C.NRO_PARTE 
	  and to_char(C.FECHA, 'yyyymmdd') >= to_char(:ad_fecha1, 'yyyymmdd')
	  AND to_char(C.FECHA, 'yyyymmdd') <= to_char(:ad_fecha2, 'yyyymmdd')
	  AND C.ot_adm = :as_ot_adm
	  AND SUBSTR(C.NRO_PARTE, 1, 2) LIKE :as_origen
	  and C.TURNO LIKE :as_turno
   GROUP BY A.COD_ART, B.NOM_ARTICULO, B.UND, C.NRO_PARTE, C.FECHA, usf_pr_costo_pd_ot(A.NRO_PARTE)
   ORDER BY A.COD_ART
  
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string),("as_origen", string),("as_turno", string)) )
