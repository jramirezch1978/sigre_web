 retrieve="select m.cod_trabajador,
       trim(m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1 || ' ' || 
       decode(length(m.nombre2), 0, '', substr(m.nombre2,1,1) || '.')) as nom_trabajador,
		 a.cod_area, a.desc_area,
		 s.cod_seccion, s.desc_seccion
from 	maestro 	m,
		area		a,
		seccion	s
where a.cod_area = s.cod_area
  and s.cod_seccion = m.cod_seccion
  and a.cod_area	  = m.cod_area
  and m.cod_area like :as_area
  and m.cod_seccion like :as_seccion
  and m.flag_estado = '1'
order by m.cod_trabajador" arguments=(("as_area", string),("as_seccion", string)) )
