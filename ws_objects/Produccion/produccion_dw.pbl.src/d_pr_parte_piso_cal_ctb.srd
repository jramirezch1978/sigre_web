 retrieve="select mad.nro_atributo, initcap(trim(m.desc_maq)) as maquina, ppd.atributo, initcap(trim(maa.descripcion)) as desc_atributo, trim(to_char(mad.medicion_min, '999,999,990.00')) || ' - ' || trim(to_char(mad.medicion_max, '999,999,990.00')) as limites, ppd.nro_lectura, ppd.nro_control, ppd.valor_lectura
   from tg_parte_piso_det ppd
   left outer join maquina m on ppd.cod_maquina = m.cod_maquina
   left outer join tg_med_act_atributo maa on ppd.atributo = maa.atributo
   inner join tg_parte_piso pp  on pp.nro_parte = ppd.nro_parte
   inner join tg_fmt_med_act_det mad on ppd.formato = mad.formato 
      and ppd.cod_maquina = mad.cod_maquina
      and ppd.atributo = mad.atributo
   where ppd.nro_parte = :asi_nro_parte
      and ppd.nro_control = 0
   order by nro_atributo, maquina, desc_atributo" arguments=(("asi_nro_parte", string))  sort="nro_control A " )
