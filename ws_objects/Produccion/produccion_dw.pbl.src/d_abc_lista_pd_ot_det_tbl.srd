 retrieve="SELECT  PD_OT_DET.NRO_PARTE ,
        PD_OT_DET.NRO_ITEM ,
        PD_OT_DET.TIPO_DOC ,
        PD_OT_DET.COD_LABOR ,
        PD_OT_DET.COD_EJECUTOR ,
        PD_OT_DET.PROVEEDOR ,
        PD_OT_DET.DESC_LABOR ,
        PD_OT_DET.COD_MAQUINA ,
        PD_OT_DET.CANT_LABOR ,
        PD_OT_DET.UND_LABOR ,
        PD_OT_DET.CANT_AVANCE ,
        PD_OT_DET.UND_AVANCE ,
        PD_OT_DET.FLAG_TERMINADO ,
        PD_OT_DET.FLAG_CONFORMIDAD ,
        PD_OT_DET.HORA_INICIO ,
        PD_OT_DET.HORA_FIN ,
        PD_OT_DET.HORAS_INCIDEN ,
        PD_OT_DET.COSTO_LABOR ,
        PD_OT_DET.OPER_SEC ,
        PD_OT_DET.CONFIN ,
        LABOR.UND ,
		  LABOR.FLAG_MAQ_MO,
        EJECUTOR.DESCRIPCION ,
        PROVEEDOR.NOM_PROVEEDOR ,
        MAQUINA.DESC_MAQ        ,
        PD_OT_DET.OBS           ,
		  PD_OT_DET.IND_ACT_MAQ   ,
		  PD_OT_DET.NRO_ORDEN     ,
		  PD_OT_DET.CENCOS        ,
		  CAMPO.COD_CAMPO        ,
		  CAMPO.DESC_CAMPO       ,
		  LE.COSTO_UNITARIO AS COSTO_UNITARIO,
		  (select und_hr from prod_param where reckey = '1') as und_hrs	
FROM PD_OT_DET ,
     LABOR       ,
	  LABOR_EJECUTOR LE,
     EJECUTOR    ,
     PROVEEDOR   ,
     MAQUINA     ,
	  CAMPO_CICLO ,
	  CAMPO
WHERE PD_OT_DET.PROVEEDOR 		= PROVEEDOR.PROVEEDOR (+)
  and PD_OT_DET.COD_MAQUINA 	= MAQUINA.COD_MAQUINA (+)
  and PD_OT_DET.COD_LABOR 		= LABOR.COD_LABOR 
  and PD_OT_DET.COD_EJECUTOR 	= EJECUTOR.COD_EJECUTOR 
  and PD_OT_DET.NRO_ORDEN    	= CAMPO_CICLO.NRO_ORDEN  (+)
  and CAMPO_CICLO.COD_CAMPO 	= CAMPO.COD_CAMPO (+)
  AND PD_OT_DET.COD_LABOR 		= LE.COD_LABOR (+)
  AND PD_OT_DET.COD_EJECUTOR	= LE.COD_EJECUTOR (+)
  and PD_OT_DET.NRO_PARTE 		= :as_nro_parte " update="PD_OT_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string))  sort="nro_item A " )
