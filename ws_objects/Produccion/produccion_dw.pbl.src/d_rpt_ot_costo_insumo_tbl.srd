 column=(type=char(400) updatewhereclause=yes name=articulo_desc_art dbname="articulo.desc_art" )
 column=(type=char(3) updatewhereclause=yes name=articulo_und dbname="articulo.und" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 column=(type=decimal(2) updatewhereclause=yes name=pd_ot_insumos_costo dbname="pd_ot_insumos.costo" )
 column=(type=number updatewhereclause=yes name=total_costo dbname="total_costo" )
 retrieve="select pdi.cod_art,
       a.desc_art,
       a.und,
       Sum(pdi.cantidad) as cantidad,
       pdi.costo_insumo as costo,
       Sum(pdi.cantidad * pdi.costo_insumo) as total_costo
  from pd_ot_insumos pdi, articulo a, pd_ot p
 WHERE a.cod_art = pdi.cod_art
   and p.nro_parte = pdi.nro_parte
   and p.ot_adm = :as_ot_adm
   and trunc(p.fecha) BETWEEN TRUNC(:ad_Fecha1) AND TRUNC(:ad_fecha2)
   and substr(p.nro_parte, 1, 2) = :as_origen
   
GROUP BY pdi.cod_art, a.desc_art, a.und, pdi.costo_insumo" arguments=(("as_ot_adm", string),("as_origen", string),("ad_fecha1", date),("ad_fecha2", date)) )
