 retrieve="SELECT 	A.COD_TRABAJADOR,   
			A.FEC_MOVIM,   
			A.TURNO,   
			A.COD_TIPO_MOV,   
			A.FEC_DESDE,   
			A.FEC_HASTA,   
			A.COD_USR,   
			m.nom_trabajador,
			m.nro_doc_ident_rtps,
			TMA.DESC_MOVIMI,   
			A.OT_ADM,   
			OA.DESCRIPCION,   
			T.DESCRIPCION,   
			A.COD_ORIGEN,   
			OT.NRO_ORDEN,
			A.OPER_SEC,
			A.FLAG_REPLICACION,
			A.HOR_DIU_NOR,
			A.HOR_NOC_NOR,
			A.HOR_EXT_DIU_1,
			A.HOR_EXT_DIU_2,
			A.HOR_EXT_NOC_1,
			A.HOR_EXT_NOC_2,
			a.hor_ext_100,
			A.HOR_DIU_NOR + A.HOR_NOC_NOR + A.HOR_EXT_DIU_1 + A.HOR_EXT_DIU_2 + A.HOR_EXT_NOC_1 + A.HOR_EXT_NOC_2 + a.hor_ext_100 as horas_Trab,
			a.IMP_HOR_DIU_NOR,
			a.IMP_HOR_DIU_EXT1,
			a.IMP_HOR_DIU_EXT2,
			a.IMP_HOR_NOC_NOR,
			a.IMP_HOR_NOC_EXT1,
			a.IMP_HOR_NOC_EXT2,
			a.IMP_HOR_EXT_100
 FROM 	ASISTENCIA					A,   
			vw_pr_trabajador			m,   
			TIPO_MOV_ASISTENCIA		TMA,   
			TURNO							T,   
			(Select oa1.* 
				from OT_ADMINISTRACION 	oa1,
					  ot_adm_usuario 		otu
				where oa1.ot_adm = otu.ot_adm
 				  and otu.cod_usr = :as_usuario)	OA,
			OPERACIONES 				OP,
			ORDEN_TRABAJO				OT
WHERE 	A.cod_trabajador 		= m.cod_trabajador 	(+)
  and 	A.COD_TIPO_MOV	  		= TMA.cod_tipo_mov	(+)
  and 	A.turno 			  		= T.turno				(+)
  and 	A.ot_adm 		  		= OA.ot_adm 			(+)
  and 	A.Oper_sec		  		= OP.Oper_sec 			(+)
  and 	op.nro_orden	  		= ot.nro_orden 		(+)
  and 	trunc(A.FEC_MOVIM) between trunc(:adi_fec_ini) and TRUNC(:adi_fec_fin)
  AND 	instr(:asi_cod_origen, A.COD_ORIGEN) > 0 
  and 	((A.OT_ADM like :asi_ot_adm and a.ot_adm in (select ot_adm from ot_adm_usuario where cod_usr = :as_usuario)) or a.ot_adm is null)
  and 	A.COD_TRABAJADOR 	like :asi_cod_tra
order by A.FEC_MOVIM, A.COD_TRABAJADOR, A.FEC_DESDE" update="ASISTENCIA" updatewhere=0 updatekeyinplace=yes arguments=(("as_usuario", string),("adi_fec_ini", date),("adi_fec_fin", date),("asi_cod_origen", string),("asi_ot_adm", string),("asi_cod_tra", string)) )
