 retrieve="SELECT A.COD_CERTIFICADO,
       B.COD_SERVICIO,
       C.DESC_SERVICIO, 
       A.PROVEEDOR,
       P.Nom_Proveedor,
       A.FEC_INICIO,
       B.FEC_VENCIMIENTO,
       ROUND(B.FEC_VENCIMIENTO - (SELECT SYSDATE FROM DUAL) + 1,0) as DIAS,
       USF_PR_DURACION_CERTIFICADO_1(B.FEC_VENCIMIENTO) as mensaje

  FROM CERTIFICADO_PLANTA       A, 
       CERTIFICADO_PLANTA_DET   B,
       CERTIFICADO_SERVICIO     C,
       PROVEEDOR                P

       
 WHERE A.COD_CERTIFICADO        = B.COD_CERTIFICADO
   AND A.PROVEEDOR              = P.PROVEEDOR
   AND B.COD_SERVICIO           = C.COD_SERVICIO
   AND A.COD_CERTIFICADO		  = :AS_COD_CERTIFICADO" arguments=(("as_cod_certificado", string)) )
