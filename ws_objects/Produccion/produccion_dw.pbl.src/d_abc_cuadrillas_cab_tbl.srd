 retrieve="select tc.*, 
		 t.descripcion as desc_turno, 
		 tp.desc_planta,
		 ota.descripcion as desc_ot_adm
from tg_cuadrillas 		tc,
     turno         		t,
     tg_plantas    		tp,
	  ot_administracion 	ota,
	  ot_adm_usuario	otu
where tc.turno = t.turno
  and tc.ot_adm = ota.ot_adm
  and ota.ot_adm = otu.ot_adm
  and otu.cod_usr	= :as_usuario
  and tc.cod_planta = tp.cod_planta   
--  and tc.flag_estado = '1'  " update="tg_cuadrillas" updatewhere=0 updatekeyinplace=yes arguments=(("as_usuario", string)) )
