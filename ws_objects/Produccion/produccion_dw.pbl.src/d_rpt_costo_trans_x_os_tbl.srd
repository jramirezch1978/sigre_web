 retrieve="SELECT DISTINCT ot.nro_orden AS OT, ot.ot_adm, NVL(b.oper_sec,null) as oper_sec, 
       NVL(DECODE(Z.FLAG_GEN_OS_TRANS, '1', (SELECT P.CENCOS||'Y CUENTA'||' '||P.CNTA_PRSP FROM PROD_TRANS_PRSP_FONDO P), '2', b.cencos||'Y CUENTA'||' '||(SELECT A.CNTA_PRSP_EGRESO
         FROM ARTICULO_SUB_CATEG A, SERVICIOS S
        WHERE A.COD_SUB_CAT       = S.COD_SUB_CAT
          AND S.SERVICIO          = (SELECT l.servicio_transp FROM costo_param l)), '3', o.cencos||'Y CUENTA'||' '||(SELECT A.CNTA_PRSP_EGRESO
         FROM ARTICULO_SUB_CATEG A, SERVICIOS S
        WHERE A.COD_SUB_CAT       = S.COD_SUB_CAT
          AND S.SERVICIO          = (SELECT l.servicio_transp FROM costo_param l)), '4', z.cencos||'Y CUENTA'||' '||(SELECT A.CNTA_PRSP_EGRESO
         FROM ARTICULO_SUB_CATEG A, SERVICIOS S
        WHERE A.COD_SUB_CAT       = S.COD_SUB_CAT
          AND S.SERVICIO          = (SELECT l.servicio_transp FROM costo_param l))), NULL) AS CENCOS_BOLSA,
       B.CENCOS AS CENCOS_PARTE, c.desc_cencos, decode(aa.flag_obr_empl, 'O', 'OBREROS', 'E', 'EMPLEADOS') AS TIPO,
       Sum(b.importe) as total

  FROM operaciones                 o,
       prod_parte_transp           aa,
       prod_parte_transp_det       b,
       com_zona_proceso            z,
       centros_costo               c,
       orden_trabajo               ot
       
 Where o.oper_sec(+)                 = b.oper_sec
   And aa.nro_parte                   = b.nro_parte
   AND z.zona_proceso                = b.zona_proceso
   AND c.cencos                      = b.cencos
   AND o.nro_orden                   = ot.nro_orden(+)

   And Trunc(aa.fec_parte) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
   And substr(aa.nro_parte, 1, 2)     = :as_origen
   And aa.proveedor                   = :as_proveedor
   And aa.flag_obr_empl					 like :as_flag_obr_empl
   AND aa.flag_estado                 = '1'
 Group by ot.nro_orden, ot.ot_adm, b.oper_sec, Z.FLAG_GEN_OS_TRANS, B.CENCOS, O.CENCOS, Z.CENCOS, c.desc_cencos, aa.FLAG_OBR_EMPL, o.desc_operacion
 ORDER BY ot.nro_orden" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_origen", string),("as_proveedor", string),("as_flag_obr_empl", string)) )
