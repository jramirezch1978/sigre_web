 retrieve="  SELECT A.NRO_PARTE,   
         A.NRO_ITEM,   
         A.COD_TRABAJADOR,   
         A.LINEA_DESTAJO,   
         A.CANT_DESTAJADA,   
         D.DESCR_LINEA,   
         M.APEL_PATERNO || ' ' || M.APEL_MATERNO || ', ' || M.NOMBRE1 AS NOM_TRABAJADOR,
			D.PRECIO_UNIT,
			D.UNIDAD_DESTAJO
    FROM PD_OT_ASIST_DESTAJO A,   
         MAESTRO M ,   
         DE_LINEAS D  
   WHERE A.COD_TRABAJADOR = M.COD_TRABAJADOR 
     and A.LINEA_DESTAJO = D.LINEA_DESTAJO 
 	  and A.NRO_PARTE = :as_parte 
	  AND A.NRO_ITEM = :an_item 
" update="PD_OT_ASIST_DESTAJO" updatewhere=0 updatekeyinplace=no arguments=(("as_parte", string),("an_item", number)) )
