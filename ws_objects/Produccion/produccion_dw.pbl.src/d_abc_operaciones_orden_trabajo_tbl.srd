 retrieve="  SELECT ~"OPERACIONES~".~"OPER_SEC~",   
         ~"OPERACIONES~".~"CORR_CORTE~",   
         ~"OPERACIONES~".~"NRO_OPERACION~",   
         ~"OPERACIONES~".~"COD_LABOR~",   
         ~"OPERACIONES~".~"COD_EJECUTOR~",   
         ~"OPERACIONES~".~"PROVEEDOR~",   
         ~"OPERACIONES~".~"NRO_OPER_PRECED~",   
         ~"OPERACIONES~".~"FLAG_PRE~",   
         ~"OPERACIONES~".~"FLAG_ESTADO~",   
         ~"OPERACIONES~".~"NRO_PERSONAS~",   
         ~"OPERACIONES~".~"DESC_OPERACION~",   
         ~"OPERACIONES~".~"DIAS_PARA_INICIO~",   
         ~"OPERACIONES~".~"DIAS_DURACION_PROY~",   
         ~"OPERACIONES~".~"FEC_INICIO_EST~",   
         ~"OPERACIONES~".~"FEC_INICIO~",   
         ~"OPERACIONES~".~"FEC_FIN~",   
         ~"OPERACIONES~".~"CANT_PROYECT~",   
         ~"OPERACIONES~".~"CANT_REAL~",   
         ~"OPERACIONES~".~"COSTO_UNIT~",   
         ~"OPERACIONES~".~"TIPO_ORDEN~",   
         ~"OPERACIONES~".~"NRO_ORDEN~", 
         ~"OPERACIONES~".~"OBS~"               ,    
         ~"OPERACIONES~".~"NRO_ITEMS_PD~"      ,   
         ~"OPERACIONES~".~"WORK_LANZ~"         ,   
         ~"OPERACIONES~".~"WORK_OPER~"         ,
			~"LABOR_EJECUTOR~".~"COSTO_UNITARIO~" ,
			~"OPERACIONES~".~"AVANCE_ESPERADO~"   ,
			~"OPERACIONES~".~"AVANCE_UND~"        ,
			~"OPERACIONES~".~"AVANCE_ACTUAL~"		 ,
			~"ORDEN_TRABAJO~".~"COD_MAQUINA~"		 ,
			~"ORDEN_TRABAJO~".~"CLIENTE~"			 ,
			~"ORDEN_TRABAJO~".~"CENCOS_SLC~"		 ,
			~"CAMPO_CICLO~"	.~"CORR_CORTE~"		 ,
			~"CAMPO~".~"COD_CAMPO~"		 			 ,
			~"CAMPO~".~"DESC_CAMPO~"		 			 ,
			USF_OPE_OPERSEC_X_RIEGO(~"OPERACIONES~".~"OPER_SEC~") as nro_riego
		 
    FROM ~"OPERACIONES~"  ,~"LABOR_EJECUTOR~",~"ORDEN_TRABAJO~",~"CAMPO_CICLO~",~"CAMPO~"

   WHERE ((~"OPERACIONES~".~"COD_LABOR~"    = ~"LABOR_EJECUTOR~".~"COD_LABOR~"    )  AND
			 (~"OPERACIONES~".~"COD_EJECUTOR~" = ~"LABOR_EJECUTOR~".~"COD_EJECUTOR~" )) AND
			((~"OPERACIONES~".~"TIPO_ORDEN~"   = (SELECT doc_ot FROM prod_param WHERE reckey = '1') )) AND
			((~"OPERACIONES~".~"NRO_ORDEN~"	 = ~"ORDEN_TRABAJO~".~"NRO_ORDEN~"	  )) AND

			 (~"OPERACIONES~".~"NRO_ORDEN~"	 = ~"CAMPO_CICLO~".~"NRO_ORDEN~"	   (+)) AND
			 (~"CAMPO_CICLO~".~"COD_CAMPO~" 	 = ~"CAMPO~".~"COD_CAMPO~"      	   (+)) AND

			((~"OPERACIONES~".~"TIPO_ORDEN~"   = :as_tipo_doc                    )  AND  
          (~"OPERACIONES~".~"NRO_ORDEN~"    = :as_nro_doc                     )) AND  
			 (~"OPERACIONES~".~"FLAG_ESTADO~"  IN   ('1','3')                    )  AND 
			((~"OPERACIONES~".~"COD_LABOR~"	 like :as_cod_labor					  )  AND
			 (~"OPERACIONES~".~"COD_EJECUTOR~" like :as_cod_ejecutor				  ))



" arguments=(("as_tipo_doc", string),("as_nro_doc", string),("as_cod_labor", string),("as_cod_ejecutor", string))  sort="nro_operacion A " )
