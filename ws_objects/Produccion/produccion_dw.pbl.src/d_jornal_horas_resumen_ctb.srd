 retrieve="select oa.descripcion as ot_adm_desc, 
       a.cod_trabajador as cod_trab, 
       cr.nombre as nom_trab, 
       trunc(a.fec_movim) as fecha, 
       (a.fec_hasta - a.fec_desde) * 24 as horas
   from asistencia a
      left outer join codigo_relacion cr on a.cod_trabajador = cr.cod_relacion
      left outer join ot_administracion oa on a.ot_adm = oa.ot_adm
      inner join maestro m on m.cod_trabajador = a.cod_trabajador
   where a.ot_adm in (select distinct u.ot_adm from ot_adm_usuario u where trim(u.cod_usr) = trim(:asi_user))
      and trunc(a.fec_movim) between trunc(:adi_ini) and trunc(:adi_fin)
      and trim(m.cod_origen) = trim(:asi_cod_origen)
   order by oa.descripcion, cr.nombre, a.fec_movim" arguments=(("asi_user", string),("asi_cod_origen", string),("adi_ini", date),("adi_fin", date))  sort="ot_adm_desc A cod_trab A nom_trab A " )
