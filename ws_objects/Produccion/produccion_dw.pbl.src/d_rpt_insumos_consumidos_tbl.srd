 column=(type=char(40) updatewhereclause=yes name=articulo_nom_articulo dbname="articulo.nom_articulo" )
 column=(type=char(3) updatewhereclause=yes name=articulo_und dbname="articulo.und" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 column=(type=number updatewhereclause=yes name=total_costo dbname="total_costo" )
 retrieve="select pdi.cod_art, 
		 ar.nom_articulo, 
       ar.und,
		 Sum(pdi.cantidad) as cantidad, 
       Sum(pdi.cantidad * pdi.costo_insumo) as total_costo
from pd_ot_insumos pdi,
     articulo      ar,
     pd_ot         pd
where ar.cod_art   = pdi.cod_art
  and pd.nro_parte = pdi.nro_parte
  and to_char(pd.FECHA, 'yyyymmdd') >= to_char(:ad_fecha1, 'yyyymmdd')
  AND to_char(pd.FECHA, 'yyyymmdd') <= to_char(:ad_fecha2, 'yyyymmdd')
  AND pd.ot_adm = :as_ot_adm
  AND SUBSTR(pd.NRO_PARTE,1,2) LIKE :as_origen
  AND pd.TURNO LIKE :as_turno
group by pdi.cod_art, ar.nom_articulo, ar.und
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string),("as_origen", string),("as_turno", string)) )
