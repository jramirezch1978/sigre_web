 retrieve="SELECT F.NRO_ATRIBUTO, INITCAP(TRIM(M.DESC_MAQ)) AS MAQUINA, F.ATRIBUTO, INITCAP(TRIM(A.DESCRIPCION)) AS DESC_ATRIBUTO,TRIM(TO_CHAR(F.MEDICION_MIN, '999,999,990.00')) || ' - ' ||TRIM(TO_CHAR(F.MEDICION_MAX, '999,999,990.00')) AS LIMITES,PPD.NRO_LECTURA,PPD.NRO_CONTROL,PPD.VALOR_LECTURA, PPD.HORA_INICIO
     FROM TG_PARTE_PISO_DET   PPD,
       MAQUINA             M,
       TG_MED_ACT_ATRIBUTO A,
       TG_PARTE_PISO       PP,
       TG_FMT_MED_ACT_DET  F
       
 WHERE PPD.NRO_PARTE       =  PP.NRO_PARTE
   AND PPD.FORMATO         =  F.FORMATO(+)
   AND PPD.NRO_ITEM        =  F.NRO_ITEM
   AND F.COD_MAQUINA       =  M.COD_MAQUINA(+)
   AND F.ATRIBUTO          =  A.ATRIBUTO(+)
   AND PPD.NRO_PARTE       =  :asi_nro_parte
   AND PPD.NRO_CONTROL     >  0
   ORDER BY                F.NRO_ATRIBUTO, M.DESC_MAQ, A.DESCRIPCION" arguments=(("asi_nro_parte", string))  sort="tg_parte_piso_det_nro_lectura A " )
