 retrieve="  SELECT ~"PD_OT~".~"NRO_PARTE~",   
         ~"OT_ADMINISTRACION~".~"DESCRIPCION~",   
         ~"PD_OT~".~"FECHA~"  
    FROM ~"PD_OT~",   
         ~"OT_ADMINISTRACION~"  
   WHERE ( ot_administracion.ot_adm (+) = pd_ot.ot_adm) and  
         ( ( trunc(~"PD_OT~".~"FECHA~") between :adi_ini and :adi_fin ) and
				 pd_ot.ot_adm = :as_ot_adm )   
ORDER BY ~"PD_OT~".~"FECHA~" DESC   
" arguments=(("adi_ini", date),("adi_fin", date),("as_ot_adm", string)) )
