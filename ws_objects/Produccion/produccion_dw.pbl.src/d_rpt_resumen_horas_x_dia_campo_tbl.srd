 column=(type=number updatewhereclause=yes name=hrs_noc_extras_35 dbname="hrs_noc_extras_35" )
 retrieve="SELECT a.fecha, a.cod_trabajador, 
       m.apel_paterno || ' ' || m.apel_materno || ',  ' || m.nombre1 || ' ' || m.nombre2 AS nom_trabajador, 
       SUM(a.hrs_normales) AS hrs_normales,
       SUM(a.hrs_extras_25) AS hrs_extras_25,
       SUM(a.hrs_extras_35) AS hrs_extras_35,
       SUM(a.hrs_extras_100) AS hrs_extras_100,
       SUM(a.hrs_noc_extras_35) AS hrs_noc_extras_35
FROM pd_jornal_campo a,
     maestro              m
WHERE a.cod_trabajador = m.cod_trabajador 
  and a.ot_adm       like :as_ot_adm
  and a.cod_labor      like :as_labor
  and a.cod_trabajador like :as_trabajador
  and trunc(a.fecha) between trunc(:ad_Fecha1) and trunc(:ad_fecha2)
GROUP BY a.fecha, a.cod_trabajador, 
       m.apel_paterno || ' ' || m.apel_materno || ',  ' || m.nombre1 || ' ' || m.nombre2
ORDER BY a.fecha, a.cod_trabajador" arguments=(("as_ot_adm", string),("as_labor", string),("as_lote", string),("as_trabajador", string),("ad_fecha1", date),("ad_fecha2", date)) )
