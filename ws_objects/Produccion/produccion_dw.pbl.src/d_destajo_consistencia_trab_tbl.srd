 retrieve="  SELECT ~"PD_OT_ASIST_DESTAJO~".~"NRO_PARTE~",   
         ~"PD_OT_ASIST_DESTAJO~".~"NRO_ITEM~",   
         ~"PD_OT_ASIST_DESTAJO~".~"COD_TRABAJADOR~",   
         ~"PD_OT_ASIST_DESTAJO~".~"CANT_DESTAJADA~",   
         ~"PD_OT_ASIST_DESTAJO~".~"HORAS_EFECTIVAS~",   
         ~"PD_OT_ASIST_DESTAJO~".~"TARIFA_NORMAL~",   
         ~"PD_OT_ASIST_DESTAJO~".~"FACTOR_CALCULO~",   
         ~"PD_OT_ASIST_DESTAJO~".~"NRO_SUB_ITEM~",   
         ~"CODIGO_RELACION~".~"NOMBRE~",   
         ~"PD_OT_DET~".~"COD_LABOR~",   
         ~"LABOR~".~"DESC_LABOR~",   
         ~"PD_OT_DET~".~"CANT_LABOR~",   
         ~"PD_OT~".~"FECHA~",   
         ~"PD_OT_DET~".~"UND_AVANCE~"  
    FROM ~"PD_OT_ASIST_DESTAJO~",   
         ~"CODIGO_RELACION~",   
         ~"PD_OT_DET~",   
         ~"LABOR~",   
         ~"PD_OT~"  
   WHERE ( pd_ot_asist_destajo.cod_trabajador = codigo_relacion.cod_relacion (+)) and  
         ( pd_ot_asist_destajo.nro_parte = pd_ot_det.nro_parte (+)) and  
         ( pd_ot_asist_destajo.nro_item = pd_ot_det.nro_item (+)) and  
         ( pd_ot_det.cod_labor = labor.cod_labor (+)) and  
         ( pd_ot_asist_destajo.nro_parte = pd_ot.nro_parte (+)) and  
         ( ( trunc(~"PD_OT~".~"FECHA~") between :adi_fecha_1 and :adi_fecha_2 ) and
				PD_OT.ot_adm = :ad_ot_adm ) and
			(  codigo_relacion.cod_relacion like :as_cod_relacion)
ORDER BY ~"CODIGO_RELACION~".~"NOMBRE~" ASC,   
         ~"PD_OT~".~"FECHA~" ASC,   
         ~"PD_OT_ASIST_DESTAJO~".~"NRO_PARTE~" ASC,   
         ~"PD_OT_DET~".~"COD_LABOR~" ASC   
" arguments=(("adi_fecha_1", date),("adi_fecha_2", date),("ad_ot_adm", string),("as_cod_relacion", string)) )
