 retrieve="select m.cod_trabajador,
       trim(m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1 || ' ' || 
      decode(length(m.nombre2), 0, '', substr(m.nombre2,1,1) || '.')) as nom_trabajador,
     '*' || m.cod_trabajador || '*' as cod_barra
from 	maestro m,
		tt_telecredito,
		tt_proveedor tt1
where tt1.proveedor = m.cod_trabajador
  and m.cod_area like :as_area
  and m.cod_seccion like :as_seccion
  and m.cod_trabajador between :as_desde and :as_hasta
  and m.flag_estado = '1'
order by nom_trabajador" arguments=(("as_area", string),("as_seccion", string),("as_desde", string),("as_hasta", string)) )
