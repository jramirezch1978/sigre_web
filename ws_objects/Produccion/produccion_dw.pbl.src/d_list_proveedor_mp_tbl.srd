 column=(type=char(50) updatewhereclause=yes name=nom_proveedor dbname="nom_proveedor" )
 column=(type=number updatewhereclause=yes name=peso_venta dbname="peso_venta" )
 column=(type=char(3) updatewhereclause=yes name=und_peso dbname="und_peso" )
 retrieve="SELECT p.proveedor, p.nom_proveedor, 
       SUM(usf_ap_conv_und(a.und, 
                    (SELECT t.und_tm FROM ap_param t WHERE origen = 'XX'), a.peso_bruto + a.peso_variacion * DECODE(a.flag_variacion, 'C', -1, 1))) AS peso_venta,
       (SELECT t.und_tm FROM ap_param t WHERE origen = 'XX') AS und_peso      
  FROM AP_PD_DESCARGA_DET A,
       (SELECT T.NRO_PARTE, T.ITEM
          FROM AP_PD_DESCARGA_DET T
        MINUS
        SELECT M.AP_NRO_PARTE, M.AP_NRO_ITEM 
          FROM PROD_PARTE_PROD_RECEP_MP M) B,
       proveedor p
 WHERE A.NRO_PARTE = B.NRO_PARTE
   AND a.proveedor = p.proveedor
   AND A.ITEM = B.ITEM
   and trunc(a.inicio_descarga) between :ad_Fecha1 and :ad_fecha2
GROUP BY p.proveedor, p.nom_proveedor
" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
