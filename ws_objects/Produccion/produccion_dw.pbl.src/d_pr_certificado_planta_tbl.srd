 column=(type=char(122) updatewhereclause=yes name=direccion dbname="direccion" )
 retrieve="  SELECT ~"CERTIFICADO_PLANTA~".~"COD_CERTIFICADO~",   
         ~"CERTIFICADO_PLANTA~".~"PROVEEDOR~",   
         ~"UNIDAD~".~"DESC_UNIDAD~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"TG_PLANTAS~".~"DESC_PLANTA~",   
         ~"CERTIFICADO_PLANTA~".~"DIRECCTION_ITEM~",   
         ~"CERTIFICADO_PLANTA~".~"NRO_CERTIF_PROV~",   
         ~"CERTIFICADO_PLANTA~".~"COD_PLANTA~",   
         ~"CERTIFICADO_PLANTA~".~"FEC_REGISTRO~",   
         ~"CERTIFICADO_PLANTA~".~"FEC_INICIO~",   
         ~"CERTIFICADO_PLANTA~".~"AREA_TRATADA~",   
         ~"CERTIFICADO_PLANTA~".~"UND~",   
         ~"CERTIFICADO_PLANTA~".~"COD_USR~",   
         ~"CERTIFICADO_PLANTA~".~"FLAG_ESTADO~",   
         ~"CERTIFICADO_PLANTA~".~"FLAG_REPLICACION~",
			TRIM(DIR_DIRECCION) || ' ' || TRIM(DIR_DEP_ESTADO) || ' ' || TRIM(DIR_DISTRITO) AS DIRECCION
    FROM ~"CERTIFICADO_PLANTA~",   
         ~"PROVEEDOR~",   
         ~"TG_PLANTAS~",   
         ~"UNIDAD~",   
         ~"USUARIO~",   
         ~"DIRECCIONES~"  
   WHERE ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CERTIFICADO_PLANTA~".~"PROVEEDOR~" ) and  
         ( ~"TG_PLANTAS~".~"COD_PLANTA~" = ~"CERTIFICADO_PLANTA~".~"COD_PLANTA~" ) and
			( CERTIFICADO_PLANTA.proveedor = direcciones.codigo (+)) and  
         ( CERTIFICADO_PLANTA.direcction_item = direcciones.item (+)) and  
         ( ~"UNIDAD~".~"UND~" = ~"CERTIFICADO_PLANTA~".~"UND~" ) and  
         ( ~"USUARIO~".~"COD_USR~" = ~"PROVEEDOR~".~"COD_USR~" ) and  
         ( ~"USUARIO~".~"COD_USR~" = ~"CERTIFICADO_PLANTA~".~"COD_USR~" ) and  
         ( ~"USUARIO~".~"FLAG_REPLICACION~" = ~"DIRECCIONES~".~"FLAG_REPLICACION~" )    
" )
