 retrieve="  SELECT ~"PD_OT_ASISTENCIA~".~"NO_PARTE~",   
         ~"PD_OT_ASISTENCIA~".~"NRO_ITEM~",   
         ~"PD_OT_ASISTENCIA~".~"COD_TRABAJADOR~",   
         ~"PD_OT_ASISTENCIA~".~"NRO_HORAS~",   
         ~"PD_OT_ASISTENCIA~".~"COSTO_HORAS~",   
         ~"PD_OT_ASISTENCIA~".~"OBSERVACION~",   
         ~"PD_OT_ASISTENCIA~".~"FACTOR~",   
         ~"CODIGO_RELACION~".~"NOMBRE~" AS NOM_TRABAJADOR  ,
			~"PD_OT_DET~".~"OPER_SEC~"

    FROM ~"PD_OT_ASISTENCIA~",   
			~"PD_OT_DET~"			,
         ~"CODIGO_RELACION~"  
   WHERE ((~"PD_OT_ASISTENCIA~".~"NO_PARTE~"      = ~"PD_OT_DET~".~"NRO_PARTE~"          )  
	  AND (~"PD_OT_ASISTENCIA~".~"NRO_ITEM~"       = ~"PD_OT_DET~".~"NRO_ITEM~"           )) 
	  AND (~"PD_OT_ASISTENCIA~".~"COD_TRABAJADOR~" = ~"CODIGO_RELACION~".~"COD_RELACION~" )  
	  AND ~"PD_OT_ASISTENCIA~".~"NO_PARTE~"      = :as_nro_parte                    
	  AND ~"PD_OT_ASISTENCIA~".~"NRO_ITEM~"       = :an_item
" update="PD_OT_ASISTENCIA" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string),("an_item", number)) )
