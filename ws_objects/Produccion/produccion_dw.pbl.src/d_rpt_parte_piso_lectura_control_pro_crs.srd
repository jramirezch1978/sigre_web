 retrieve="SELECT F.NRO_ATRIBUTO, 
       INITCAP(F.DESC_CABECERA) AS MAQUINA, 
       F.ATRIBUTO, 
       INITCAP(TRIM(A.DESCRIPCION)) AS DESC_ATRIBUTO,
       F.ATRIBUTO_NIV3, INITCAP(B.DESCRIPCION) AS DES_NIVEL3,
       F.ATRIBUTO_NIV4, INITCAP(C.DESCRIPCION) AS DES_NIVEL4,
       TRIM(TO_CHAR(F.MEDICION_MIN, '999,999,990.00')) || '-' ||TRIM(TO_CHAR(F.MEDICION_MAX, '999,999,990.00')) AS LIMITES,
       PPD.NRO_LECTURA,
       DECODE(TO_CHAR(PPD.NRO_CONTROL),1,'A','B') AS tg_parte_piso_det_nro_control,
       TRIM(PPD.VALOR_LECTURA_TEXTO)||TRIM(TO_CHAR(PPD.VALOR_LECT_FEC_HORA,'hh24:mi:ss'))||TRIM(to_char(PPD.VALOR_LECT_NUMERICO, '999,990.0000')) AS VALOR_LECTURA, 
       PPD.HORA_INICIO

  FROM TG_PARTE_PISO_DET   PPD,
       MAQUINA             M,
       TG_MED_ACT_ATRIBUTO A,
       TG_PARTE_PISO       PP,
       TG_FMT_MED_ACT_DET  F,
       TG_MED_ACT_ATRIBUTO B,
       TG_MED_ACT_ATRIBUTO C
       
 WHERE PPD.NRO_PARTE       =  PP.NRO_PARTE
   AND PPD.FORMATO         =  F.FORMATO(+)
   AND PPD.NRO_ITEM        =  F.NRO_ITEM(+)
	AND PPD.NRO_VERSION     =  F.NRO_VERSION(+)
   AND F.COD_MAQUINA       =  M.COD_MAQUINA(+)
   AND F.ATRIBUTO          =  A.ATRIBUTO(+)
   AND PPD.NRO_PARTE       =  :asi_nro_parte
   AND B.ATRIBUTO(+)       =  F.ATRIBUTO_NIV3
   AND C.ATRIBUTO(+)       =  F.ATRIBUTO_NIV4
   AND PPD.NRO_CONTROL     >  0
   ORDER BY                F.NRO_ATRIBUTO, M.DESC_MAQ, A.DESCRIPCION" arguments=(("asi_nro_parte", string))  sort="tg_parte_piso_det_nro_control A tg_parte_piso_det_nro_lectura A " )
