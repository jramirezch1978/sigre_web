 retrieve="  SELECT ~"PROD_COSTOS_DIARIOS~".~"COD_ART~",   
         ~"PROD_COSTOS_DIARIOS~".~"ALMACEN~",   
         ~"ALMACEN~".~"DESC_ALMACEN~",   
         ~"PROD_COSTOS_DIARIOS~".~"FECHA~",   
         ~"PROD_COSTOS_DIARIOS~".~"COD_MONEDA~",   
         ~"MONEDA~".~"DESCRIPCION~",   
         ~"PROD_COSTOS_DIARIOS~".~"COD_PLANTILLA~",   
         ~"PROD_COSTOS_DIARIOS~".~"COSTO_PRODUCCION~",   
         ~"PROD_COSTOS_DIARIOS~".~"COSTO_CONTABLE~",   
         ~"PROD_COSTOS_DIARIOS~".~"VALOR_VENTA~",   
         ~"PROD_COSTOS_DIARIOS~".~"UNIDADES_PRODUCIDAS~",   
         ~"PROD_COSTOS_DIARIOS~".~"UNIDADES_VENDIDAS~",   
         ~"ARTICULO~".~"DESC_ART~",   
         ~"PLANTILLA_COSTO~".~"OBSERVACIONES~"  
    FROM ~"PROD_COSTOS_DIARIOS~",   
         ~"ALMACEN~",   
         ~"MONEDA~",   
         ~"ARTICULO~",   
         ~"PLANTILLA_COSTO~"  
   WHERE ( plantilla_costo.cod_plantilla (+) = prod_costos_diarios.cod_plantilla) and  
         ( moneda.cod_moneda (+) = prod_costos_diarios.cod_moneda) and  
         ( ~"ALMACEN~".~"ALMACEN~" = ~"PROD_COSTOS_DIARIOS~".~"ALMACEN~" ) and  
         ( ~"ARTICULO~".~"COD_ART~" = ~"PROD_COSTOS_DIARIOS~".~"COD_ART~" ) and  
         ( ( ~"PROD_COSTOS_DIARIOS~".~"COD_ART~" like :as_articulo ) AND  
         ( ~"PROD_COSTOS_DIARIOS~".~"ALMACEN~" like :as_almacen ) AND  
         ( trunc(~"PROD_COSTOS_DIARIOS~".~"FECHA~") between trunc(:ad_fecha1) and trunc(:ad_fecha2) ) )   
ORDER BY ~"PROD_COSTOS_DIARIOS~".~"FECHA~" ASC,   
         ~"PROD_COSTOS_DIARIOS~".~"ALMACEN~" ASC,   
         ~"PROD_COSTOS_DIARIOS~".~"COD_ART~" ASC   
" update="PROD_COSTOS_DIARIOS" updatewhere=0 updatekeyinplace=yes arguments=(("as_articulo", string),("as_almacen", string),("ad_fecha1", date),("ad_fecha2", date)) )
