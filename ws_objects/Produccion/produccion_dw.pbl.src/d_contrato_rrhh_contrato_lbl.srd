 retrieve="SELECT  distinct
        m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2 AS ~"TRABAJADOR~",
		  m.cod_trabajador,
        P.NACIONALIDAD,
        m.fec_nacimiento,
        m.fec_ingreso,
        USF_OBTIENE_EDAD(m.fec_nacimiento) as edad,
        M.DIRECCION AS DOMICILIO,
        d.desc_distrito,
        pc.desc_PROV,
        dp.desc_DPTO,
        m.dni,
        TP.DESC_TIPO_TRA,
        A.DESC_AREA,
        S.DESC_SECCION,
		  DECODE(tp.tipo_trabajador, 'EMP','mensuales.',
									  'por día.') as t_diaomes,

		  DECODE(tp.tipo_trabajador, 'EMP',NVL(USF_OBTIENE_GANANCIA_NETA_D(m.cod_trabajador),0),
									  NVL(USF_OBTIENE_GANANCIA_NETA(m.cod_trabajador),0)) as t_gdiaomes,
        USF_OBTIENE_MESES_DE_TRABAJO(:ad_f_cese, :ad_f_ingreso),
        USF_OBTIENE_PERIODO_PRUEBA (:ad_f_ingreso,:ad_f_prueba),
		  ' '
             
                 
  FROM origen              o,
       maestro             m,
       pais                p,
       tipo_trabajador     tp,
       area                a,
       seccion             S,
       gan_desct_fijo      g,
       distrito            d,
       provincia_condado   pc,
       departamento_estado dp,
       ciudad 					c
  
  WHERE         
			m.cod_origen      = o.cod_origen
   and m.cod_origen        = :as_origen
   and m.cod_area          = a.COD_AREA
   and m.cod_area          = :as_area
   and m.cod_seccion       = s.cod_seccion
   and m.cod_seccion       = :as_seccion
   and a.COD_AREA          = s.COD_AREA 
   and m.tipo_trabajador   = tp.tipo_trabajador
   and m.tipo_trabajador   = :as_tipo_trabajador
   and m.cod_trabajador    = g.cod_trabajador (+)
   and d.cod_distr (+)        = m.cod_distr
   and d.cod_dpto  (+)        = m.cod_dpto
   and d.cod_pais  (+)        = m.cod_pais
   and d.cod_prov  (+)        = m.cod_prov
   and pc.COD_PAIS (+)        = d.COD_PAIS
   and pc.COD_DPTO (+)        = d.COD_DPTO
   and pc.COD_PROV (+)        = d.COD_PROV
   and m.flag_estado       = :as_flag_estado
   and dp.COD_PAIS   (+)      = pc.COD_PAIS
   and dp.COD_DPTO   (+)     = pc.COD_DPTO
   and p.COD_PAIS    (+)      = dp.COD_PAIS
   and pc.COD_PAIS         = c.COD_PAIS (+)
   and pc.COD_DPTO         = c.COD_DPTO (+)
   and pc.COD_PROV         = c.COD_PROV (+)
  GROUP BY 
        
        m.apel_paterno||' '||m.apel_materno||' '||m.nombre1||' '||m.nombre2,
		  m.cod_trabajador,
        P.NACIONALIDAD,
        m.fec_nacimiento,
        m.fec_ingreso,
        M.DIRECCION,
        d.desc_distrito,
        pc.desc_PROV,
        dp.desc_DPTO,
        m.dni,
        TP.DESC_TIPO_TRA,
        A.DESC_AREA,
        S.DESC_SECCION,
		  DECODE(tp.tipo_trabajador, 'EMP','mensuales.',
									  'por día.'),
		  DECODE(tp.tipo_trabajador, 'EMP',NVL(USF_OBTIENE_GANANCIA_NETA_D(m.cod_trabajador),0),
									  NVL(USF_OBTIENE_GANANCIA_NETA(m.cod_trabajador),0)),
        g.imp_gan_desc,   
        USF_OBTIENE_MESES_DE_TRABAJO(:ad_f_cese, :ad_f_ingreso),
        USF_OBTIENE_PERIODO_PRUEBA (:ad_f_ingreso,:ad_f_prueba)     
                " arguments=(("as_origen", string),("as_area", string),("as_seccion", string),("as_tipo_trabajador", string),("as_flag_estado", string),("ad_f_ingreso", date),("ad_f_cese", date),("ad_f_prueba", date)) )
