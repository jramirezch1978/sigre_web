 retrieve="SELECT  ~"TG_FMT_MED_ACT_DET~".~"FORMATO~",
        ~"TG_FMT_MED_ACT_DET~".~"NRO_VERSION~", 
        ~"TG_FMT_MED_ACT_DET~".~"COD_MAQUINA~", 
        ~"TG_FMT_MED_ACT_DET~".~"ATRIBUTO~", 
         A1.~"DESCRIPCION~" AS descripcion_nivel2,
         U1.~"DESC_UNIDAD~" AS desc_un2, 
         DECODE(A1.~"FLAG_TIPO_DATO~",'C','TEXTO', 'N', 'NUMERO', 'H', 'HORA') AS TIPO_DATO_2,
        ~"TG_FMT_MED_ACT_DET~".~"ATRIBUTO_NIV3~",
         A2.DESCRIPCION AS description_nivel3,
         U2.~"DESC_UNIDAD~" AS desc_un3, 
         DECODE(A2.~"FLAG_TIPO_DATO~",'C','TEXTO', 'N', 'NUMERO', 'H', 'HORA') AS TIPO_DATO_3,
        ~"TG_FMT_MED_ACT_DET~".~"MEDICION_MIN~", 
        ~"TG_FMT_MED_ACT_DET~".~"MEDICION_MAX~", 
        ~"TG_FMT_MED_ACT_DET~".~"FLAG_REPLICACION~", 
        ~"TG_FMT_MED_ACT_DET~".~"NRO_ATRIBUTO~", 
        ~"TG_FMT_MED_ACT_DET~".~"FLAG_ESTADO~", 
        ~"TG_FMT_MED_ACT_DET~".~"DESC_CABECERA~", 
        ~"TG_FMT_MED_ACT_DET~".~"NRO_ITEM~",
        ~"TG_FMT_MED_ACT_DET~".~"FLAG_VISIBLE~" 
        
FROM  ~"TG_FMT_MED_ACT_DET~", 
      ~"UNIDAD~" U1,
       UNIDAD U2,
      ~"TG_MED_ACT_ATRIBUTO~" A1,
       Maquina,
       TG_MED_ACT_ATRIBUTO A2
       
WHERE ( A1.und               = U1.und (+)) and 
      ( A1.atributo (+)      = tg_fmt_med_act_det.atributo) and
      ( maquina.cod_maquina  = tg_fmt_med_act_det.cod_maquina) and
      ( ~"TG_FMT_MED_ACT_DET~".~"FORMATO~" = :as_formato ) AND
      ( ~"TG_FMT_MED_ACT_DET~".~"NRO_VERSION~" = :ai_nro_version) AND
      ( A2.und               = U2.und(+)) and 
      ( A2.atributo (+)      = tg_fmt_med_act_det.atributo_niv3)
 ORDER BY ~"TG_FMT_MED_ACT_DET~".~"NRO_ATRIBUTO~"" update="TG_FMT_MED_ACT_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_formato", string),("ai_nro_version", number)) )
