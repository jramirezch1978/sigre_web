 retrieve="SELECT  A.NRO_PARTE ,
        A.FECHA ,
        A.COD_SUPERVISOR ,
        A.COD_ADMINISTRADOR ,
		  A.OT_ADM		,
		  B.DESCRIPCION,
        LTRIM(RTRIM(MAESTRO_A.NOMBRE1))||'  '||LTRIM(RTRIM(MAESTRO_A.APEL_PATERNO))||' '||LTRIM(RTRIM(MAESTRO_A.APEL_MATERNO)) as nom_supervisor,
        LTRIM(RTRIM(MAESTRO_B.NOMBRE1))||'  '||LTRIM(RTRIM(MAESTRO_B.APEL_PATERNO))||' '||LTRIM(RTRIM(MAESTRO_B.APEL_MATERNO)) as nom_administrador  ,
		  A.OBS,
		  A.TURNO,
		  T.DESCRIPCION	  
  FROM  PD_OT A ,
        MAESTRO MAESTRO_A ,
        MAESTRO MAESTRO_B ,
		  OT_ADMINISTRACION B,
		  TURNO T    
  WHERE A.COD_SUPERVISOR    = MAESTRO_A.COD_TRABAJADOR (+)
    and A.COD_ADMINISTRADOR = MAESTRO_B.COD_TRABAJADOR (+)
    and A.OT_ADM				 = B.OT_ADM	  
    and A.TURNO				 = T.TURNO (+)	  
    and A.NRO_PARTE         = :as_nro_parte                   " update="PD_OT" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string)) )
