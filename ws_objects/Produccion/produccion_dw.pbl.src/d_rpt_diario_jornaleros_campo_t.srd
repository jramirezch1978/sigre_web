 retrieve="SELECT a.fecha, a.cod_trabajador, 
       m.apel_paterno || ' ' || m.apel_materno || ',  ' || m.nombre1 || ' ' || m.nombre2 AS nom_trabajador, 
       a.cod_labor, l.desc_labor, l.und,
		 a.rendimiento,
       b.nro_lote,
       (SELECT a1.hrs_normales
          FROM pd_jornal_campo a1
         WHERE a1.fecha = a.fecha
           AND a1.cod_trabajador = a.cod_trabajador
           AND a1.nro_item       = a.nro_item) AS hrs_normales,
       (SELECT a1.hrs_extras_25
          FROM pd_jornal_campo a1
         WHERE a1.fecha = a.fecha
           AND a1.cod_trabajador = a.cod_trabajador
           AND a1.nro_item       = a.nro_item) AS hrs_extras_25,
       (SELECT a1.hrs_extras_35
          FROM pd_jornal_campo a1
         WHERE a1.fecha = a.fecha
           AND a1.cod_trabajador = a.cod_trabajador
           AND a1.nro_item       = a.nro_item) AS hrs_extras_35,
       (SELECT a1.hrs_extras_100
          FROM pd_jornal_campo a1
         WHERE a1.fecha = a.fecha
           AND a1.cod_trabajador = a.cod_trabajador
           AND a1.nro_item       = a.nro_item) AS hrs_extras_100,
       (SELECT a1.hrs_noc_extras_35
          FROM pd_jornal_campo a1
         WHERE a1.fecha = a.fecha
           AND a1.cod_trabajador = a.cod_trabajador
           AND a1.nro_item       = a.nro_item) AS hrs_noc_extras_35,
       (SELECT nvl(SUM(lc.total_ha),0)
          FROM pd_jornal_campo_lote a2,
               lote_campo           lc
         WHERE lc.nro_lote = a2.nro_lote
           AND a2.fecha = a.fecha
           AND a2.cod_trabajador = a.cod_trabajador
           AND a2.nro_item       = a.nro_item) AS total_has,
       NVL(lc.total_ha,0) AS area_lote,
		 a.ot_adm
FROM pd_jornal_campo a,
     pd_jornal_campo_lote b,
     maestro              m,
     labor                l,
     lote_campo           lc
WHERE a.fecha          = b.fecha          (+)
  AND a.cod_trabajador = b.cod_trabajador (+)
  AND a.nro_item       = b.nro_item       (+)
  AND a.cod_trabajador = m.cod_trabajador 
  AND a.cod_labor      = l.cod_labor      (+)
  AND b.nro_lote       = lc.nro_lote      (+)
  and a.ot_adm 		  like :as_ot_adm
  and a.cod_labor		  like :as_labor
  and ((:as_lote = '%%') or (:as_lote <> '%%' and b.nro_lote like :as_lote))
  and a.cod_trabajador like :as_trabajador
  and trunc(a.fecha) between trunc(:ad_Fecha1) and trunc(:ad_fecha2)
ORDER BY a.fecha, a.cod_trabajador, a.cod_labor, b.nro_lote " arguments=(("as_ot_adm", string),("as_labor", string),("as_lote", string),("as_trabajador", string),("ad_fecha1", date),("ad_fecha2", date)) )
