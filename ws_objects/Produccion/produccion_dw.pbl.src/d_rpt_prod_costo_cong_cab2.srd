 column=(type=char(40) updatewhereclause=yes name=nom_articulo dbname="articulo.nom_articulo" )
 column=(type=char(3) updatewhereclause=yes name=und dbname="articulo.und" )
 column=(type=number updatewhereclause=yes name=cant_procesada dbname="cant_procesada" )
 column=(type=number updatewhereclause=yes name=consumo_dia dbname="consumo_dia" )
 retrieve="  SELECT a.cod_art,
			a.nom_articulo,
			a.und,
			NVL(SUM(am.cant_procesada), 0) as cant_procesada, 
			NVL(tt.TOTAL_MP,0) as consumo_dia
    FROM vale_mov              vm,
         articulo_mov          am,
         articulo_mov_proy     amp,
         orden_trabajo         ot,
         centro_benef_articulo ca,
			tt_prod_costo_hp_cab  tt,
			plantilla_costo		 pc,
			articulo					 a
   WHERE vm.nro_vale = am.nro_vale
     AND am.origen_mov_proy = amp.cod_origen
     AND am.nro_mov_proy    = amp.nro_mov
     AND ot.nro_orden       = amp.nro_doc
     AND amp.tipo_doc       = (select doc_ot from logparam where reckey = '1')
     AND ca.cod_art         = am.cod_art
     and am.cod_art			 = a.cod_art
     AND vm.flag_estado     <> '0'
     AND am.flag_estado     <> '0'
     AND trunc(vm.fec_registro) BETWEEN trunc(:ad_fecha1) AND trunc(:ad_fecha2)
     AND ot.centro_benef        = ca.centro_benef
     AND ca.centro_benef        = pc.centro_benef
     and pc.cod_plantilla		  = :as_plantilla
     AND vm.tipo_mov            = (select oper_ing_prod from logparam where reckey = '1')
group by a.cod_art,
			a.nom_articulo,
			a.und, NVL(tt.TOTAL_MP,0)" arguments=(("as_plantilla", string),("ad_fecha1", date),("ad_fecha2", date)) )
