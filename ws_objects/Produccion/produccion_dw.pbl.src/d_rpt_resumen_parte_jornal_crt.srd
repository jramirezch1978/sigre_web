table(column=(type=char(20) updatewhereclause=yes name=cod_trabajador dbname="vw_pd_rpt_destajo_jornal_cod_trabajador" )
 column=(type=char(20) updatewhereclause=yes name=nom_trabajador dbname="vw_pd_rpt_destajo_jornal_nom_trabajador" )
 column=(type=number updatewhereclause=yes name=total_horas dbname="total_horas" )
 retrieve="select COD_TRABAJADOR,
       NOM_TRABAJADOR,
       T.desc_tarea,
		 round(case 
		 	when t.precio_unit = 0 then
				t.imp_hora
			else
				t.precio_unit
		 end,4) as tarifa,
       T.und,
		 T.nro_parte,
       OT.NRO_ORDEN,
       OT.TITULO,
		 sum(total_horas) as total_horas,
       SUM(T.horas_diu_nor) AS HORAS_DIU_NOR,
       SUM(T.horas_diu_ext1) AS HORAS_DIU_EXT1,
       SUM(T.horas_diu_ext2) AS HORAS_DIU_EXT2,
       SUM(T.imp_hor_diu_nor) AS IMP_HOR_DIU_NOR,
       SUM(T.imp_hor_diu_ext1) AS IMP_HOR_DIU_EXT1,
       SUM(T.imp_hor_diu_ext2) AS IMP_HOR_DIU_EXT2
       
from VW_PD_RPT_DESTAJO_JORNAL t,
     OPERACIONES              OP,
     ORDEN_TRABAJO            OT
where T.oper_sec = OP.OPER_SEC (+)
  AND OP.NRO_ORDEN = OT.NRO_ORDEN (+)
  AND t.tipo = 'PJ'
  and trunc(t.fec_parte) between trunc(:ad_Fecha1) and trunc(:ad_fecha2)
  and t.cod_trabajador		like :as_trabajador
  and (t.ot_adm				like :as_ot_adm  or t.ot_adm is null)
  and t.cod_cliente			like :as_cliente
  and (t.cod_tarea			like :as_tarea or t.cod_tarea is null)
  and (t.ot_adm	in (select ot_adm from ot_adm_usuario where cod_usr = :as_usuario) or t.ot_adm is null)
group by COD_TRABAJADOR,
       NOM_TRABAJADOR,
       T.desc_tarea,
       T.und,
		 T.nro_parte,
       OT.NRO_ORDEN,
       OT.TITULO,
		 round(case 
		 	when t.precio_unit = 0 then
				t.imp_hora
			else
				t.precio_unit
		 end,4)
order by 3, 4, 7       " arguments=(("as_Trabajador", string),("as_tarea", string),("as_cliente", string),("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string),("as_usuario", string))  sort="nom_trabajador A " )
