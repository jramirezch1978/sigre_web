 retrieve="select t.anio, t.mes, t.tipo_mov, amt.desc_tipo_mov, t.codigo, a.desc_art, t.costo_unitario 
from  valorizacion_pptt   t, 
     	articulo         a,
    	articulo_mov_tipo    amt
where t.codigo   = a.cod_art   (+)
  and t.tipo_mov  = amt.tipo_mov  (+)
  AND t.anio 		= :an_year
" update="valorizacion_pptt" updatewhere=0 updatekeyinplace=yes arguments=(("an_year", number))  sort="año A mes A tipo_mov A codigo A " )
