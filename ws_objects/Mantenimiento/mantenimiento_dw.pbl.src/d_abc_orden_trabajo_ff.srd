 retrieve="SELECT  ~"ORDEN_TRABAJO~".~"COD_ORIGEN~" ,
           ~"ORDEN_TRABAJO~".~"FLAG_ESTADO~" ,
           ~"ORDEN_TRABAJO~".~"FEC_SOLICITUD~" ,
           ~"ORDEN_TRABAJO~".~"FEC_INICIO~" ,
           ~"ORDEN_TRABAJO~".~"CENCOS_RSP~" ,
           ~"ORDEN_TRABAJO~".~"CENCOS_SLC~" ,
           ~"ORDEN_TRABAJO~".~"COD_USR~" ,
           ~"ORDEN_TRABAJO~".~"NRO_SOLICITUD~" ,
           ~"ORDEN_TRABAJO~".~"DESCRIPCION~" ,
           ~"ORDEN_TRABAJO~".~"CLIENTE~" ,
           ~"ORDEN_TRABAJO~".~"FEC_ESTIMADA~" ,
           ~"ORDEN_TRABAJO~".~"PLANT_RATIO~" ,
           ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" ,
           ~"ORDEN_TRABAJO~".~"COD_MAQUINA~" ,
           ~"ORDEN_TRABAJO~".~"PROG_MNT~" ,
           ~"ORDEN_TRABAJO~".~"MNT_UND_ACT_PROY~" ,
           ~"ORDEN_TRABAJO~".~"MNT_UND_ACT_REAL~" ,
           ~"ORDEN_TRABAJO~".~"FLAG_ESTRUCTURA~" ,
			  ~"ORDEN_TRABAJO~".~"TITULO~" ,
			  ~"ORDEN_TRABAJO~".~"FLAG_COSTO_TIPO~" ,
           ~"CENTROS_COSTO_A~".~"DESC_CENCOS~" ,
           ~"MAQUINA~".~"DESC_MAQ~" ,
           ~"PROD_RATIO~".~"DESC_PLANTILLA~" ,
           ~"ORDEN_TRABAJO~".~"OT_ADM~" ,
           ~"ORDEN_TRABAJO~".~"OT_TIPO~" ,
           ~"OT_TIPO~".~"DESCRIPCION~" ,
           ~"OT_ADMINISTRACION~".~"DESCRIPCION~" ,
           ~"ORDEN_TRABAJO~".~"FEC_REGISTRO~" ,
           ~"CENTROS_COSTO_B~".~"DESC_CENCOS~" ,
           ~"ORDEN_TRABAJO~".~"FLAG_REPLICACION~", 
           ~"ORDEN_TRABAJO~".~"RESPONSABLE~", 
           ~"ORDEN_TRABAJO~".~"FLAG_PROGRAMADO~",
           ~"PROVEEDOR_A~".~"NOM_PROVEEDOR~" AS ~"NOM_CLIENTE~",  
           ~"PROVEEDOR_B~".~"NOM_PROVEEDOR~" AS ~"NOM_RESPONSABLE~",
  			  ~"SOLICITUD_OT~".~"CNTA_PRSP~"	
        FROM ~"ORDEN_TRABAJO~" ,
           ~"CENTROS_COSTO~" ~"CENTROS_COSTO_A~" ,
           ~"MAQUINA~" ,
           ~"PROD_RATIO~" ,
           ~"OT_ADMINISTRACION~" ,
           ~"OT_TIPO~" ,
	   	  ~"SOLICITUD_OT~" ,
			  ~"PROVEEDOR~" ~"PROVEEDOR_A~",
			  ~"PROVEEDOR~" ~"PROVEEDOR_B~",
           ~"CENTROS_COSTO~" ~"CENTROS_COSTO_B~" 
        WHERE ( ~"ORDEN_TRABAJO~".~"CENCOS_SLC~" = ~"CENTROS_COSTO_A~".~"CENCOS~" (+)) and
		        ( ~"ORDEN_TRABAJO~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~" (+)) and  
      		  ( ~"ORDEN_TRABAJO~".~"PLANT_RATIO~" = ~"PROD_RATIO~".~"COD_PLANTILLA~" (+)) and  
		        ( ~"ORDEN_TRABAJO~".~"CENCOS_RSP~" = ~"CENTROS_COSTO_B~".~"CENCOS~" (+)) and   
		        ( ~"ORDEN_TRABAJO~".~"NRO_SOLICITUD~" = ~"SOLICITUD_OT~".~"NRO_SOLICITUD~" (+)) and   
      		  ( ~"ORDEN_TRABAJO~".~"CLIENTE~" = ~"PROVEEDOR_A~".~"PROVEEDOR~" (+)) and   
		        ( ~"ORDEN_TRABAJO~".~"RESPONSABLE~" = ~"PROVEEDOR_B~".~"PROVEEDOR~" (+)) and   
      		  ( ~"ORDEN_TRABAJO~".~"OT_ADM~" = ~"OT_ADMINISTRACION~".~"OT_ADM~" ) and
				  ( ~"ORDEN_TRABAJO~".~"OT_TIPO~" = ~"OT_TIPO~".~"OT_TIPO~" ) and      
				  ( ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" = :as_nro_orden )   " update="ORDEN_TRABAJO" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_orden", string)) )
