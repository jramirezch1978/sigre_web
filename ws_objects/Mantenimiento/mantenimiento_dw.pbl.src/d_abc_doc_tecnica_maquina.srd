 retrieve="SELECT  M.COD_ORIGEN,
        M.COD_MAQUINA ,
        M.FLAG_ESTADO ,
        M.FLAG_PROPIEDAD ,
        M.PROVEEDOR ,
        M.DESC_MAQ ,
        M.COD_MARCA ,
        M.MODELO ,
        M.COLOR ,
        M.NRO_PLACA ,
        M.NRO_CHASIS ,
        M.NRO_MOTOR ,
        M.NRO_BOMBA_INYEC ,
        M.NRO_BOMBA_TANS ,
        M.NRO_CILINDROS ,
        M.TIPO_LLANTA_DEL ,
        M.TIPO_LLANTA_TRAS ,
        M.COMBUSTIBLE ,
        M.CAP_NORM_DIARIA ,
        M.CAP_EXTRA_DIARIA ,
        M.CAPACIDAD_CARGA ,
        M.TARA ,
        M.UND ,
        M.TIPO_MAQUINA ,
        M.HP ,
        M.FLAG_MANTENIMIENTO ,
        M.FEC_COMPRA ,
        M.ANO_FAB ,
        M.CENCOS ,
        M.COD_ACTIVO ,
        M.COSTO_X_UND ,
        M.FILE_IMAGEN ,
        M.FLAG_MAQ_EQUIPO ,
        M.INSUMO_ENERGETICO ,
        M.IND_ACTIVIDAD ,
        M.IND_ACT_ACUMULADA ,
		  M.COSTO_UNITARIO ,
		  M.COD_ART ,
		  M.FLAG_IND_ACT_PD,
		  M.ICONO,
		  M.DOC_TECNICA ,
		  M.FLAG_REPLICACION ,
		  ARTICULO_A.DESC_ART ,
		  MT.DESC_TIPO_MAQ ,
		  ARTICULO_B.DESC_ART ,
		  ACTIVO_FIJO.DESCRIPCION ,
		  UNIDAD.DESC_UNIDAD ,
		  CODIGO_RELACION.NOMBRE ,
		  MARCA.NOM_MARCA ,
		  COLOR.DESCRIPCION ,
		  CENTROS_COSTO.DESC_CENCOS     
  FROM MAQUINA M ,
		 ARTICULO ARTICULO_A ,
		 MAQUINA_TIPO MT,
		 ARTICULO ARTICULO_B ,
		 ACTIVO_FIJO ,
		 UNIDAD ,
		 CODIGO_RELACION ,
		 MARCA ,
		 COLOR ,
		 CENTROS_COSTO     
WHERE M.COD_ART  		= ARTICULO_A.COD_ART   			 (+)
  and M.TIPO_MAQUINA	= MT.TIPO_MAQUINA				 	 (+)
  and M.COMBUSTIBLE 	= ARTICULO_B.COD_ART 			 (+)
  and M.COD_ACTIVO 	= ACTIVO_FIJO.NRO_ACTIVO 		 (+)
  and M.UND 			= UNIDAD.UND 						 (+)
  and M.PROVEEDOR 	= CODIGO_RELACION.COD_RELACION (+)
  and M.COD_MARCA 	= MARCA.COD_MARCA 				 (+)
  and M.COLOR 			= COLOR.COLOR 						 (+)
  and M.CENCOS 		= CENTROS_COSTO.CENCOS 			 (+)
  and M.COD_MAQUINA 	= :as_cod_maquina" update="MAQUINA" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_maquina", string)) )
