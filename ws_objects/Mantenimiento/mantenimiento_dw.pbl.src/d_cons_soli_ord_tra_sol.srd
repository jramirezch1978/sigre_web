 retrieve="  SELECT ~"SOLICITUD_OT~".~"NRO_SOLICITUD~" ,   
         ~"SOLICITUD_OT~".~"CENCOS_RSP~"    ,   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~"  ,   
         ~"SOLICITUD_OT~".~"FECHA_ESPERADA~",   
			~"SOLICITUD_OT~".~"FLAG_ESTADO~"   ,
         ~"SOLICITUD_OT~".~"DESCRIPCION~"   ,   
         ~"ORDEN_TRABAJO~".~"NRO_ORDEN~"    ,   
         ~"ORDEN_TRABAJO~".~"FLAG_ESTADO~"  ,
			~"SOLICITUD_OT~".~"COD_MAQUINA~"	 ,
			~"MAQUINA~".~"DESC_MAQ~"
    FROM ~"SOLICITUD_OT~" ,   
         ~"ORDEN_TRABAJO~",   
         ~"CENTROS_COSTO~",
			~"MAQUINA~"		,
			~"TT_OPE_CENCOS~"
   WHERE ( solicitud_ot.nro_solicitud   = orden_trabajo.nro_solicitud (+)) and  
         ( ~"SOLICITUD_OT~".~"CENCOS_RSP~"  = ~"CENTROS_COSTO~".~"CENCOS~"       ) and	
			( ~"SOLICITUD_OT~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~"     (+)) AND
			( ~"SOLICITUD_OT~".~"CENCOS_SLC~"  = ~"TT_OPE_CENCOS~".~"CENCOS~"	    ) AND
         ((~"SOLICITUD_OT~".~"FECHA_ESPERADA~" between :ad_fecha_inicio and :ad_fecha_final )) AND  
			(( ~"SOLICITUD_OT~".~"FLAG_ESTADO~" in (:asa_estado)						 )) 
			

    
" arguments=(("asa_estado", stringlist),("ad_fecha_inicio", date),("ad_fecha_final", date),("as_empresa", string),("as_user", string)) )
