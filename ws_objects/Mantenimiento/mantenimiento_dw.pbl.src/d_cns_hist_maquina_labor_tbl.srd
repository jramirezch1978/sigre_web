 retrieve="SELECT  ~"PD_OT~".~"NRO_PARTE~" ,
           ~"PD_OT~".~"FECHA~" ,
           ~"PD_OT~".~"OT_ADM~" ,
           ~"PD_OT_DET~".~"NRO_ITEM~" ,
           ~"PD_OT_DET~".~"NRO_ORDEN~" ,
           ~"PD_OT_DET~".~"COD_LABOR~" ,
           ~"PD_OT_DET~".~"COD_EJECUTOR~" ,
           ~"PD_OT_DET~".~"CANT_LABOR~" ,
           ~"PD_OT_DET~".~"CANT_AVANCE~" ,
           ~"PD_OT_DET~".~"HORAS_INCIDEN~" ,
           ~"PD_OT_DET~".~"COSTO_LABOR~" ,
           ~"PD_OT_DET~".~"OPER_SEC~" ,
           ~"PD_OT_INSUMOS~".~"COD_ART~" ,
           ~"PD_OT_INSUMOS~".~"CANTIDAD~" ,
           ~"PD_OT_INSUMOS~".~"COSTO_INSUMO~" ,
           ~"ARTICULO~".~"NOM_ARTICULO~" ,
           ~"ARTICULO~".~"UND~" ,
           ~"PD_OT_DET~".~"DESC_LABOR~" ,
           ~"PD_OT_DET~".~"UND_LABOR~"     
        FROM ~"PD_OT~" ,
           ~"PD_OT_DET~" ,
           ~"PD_OT_INSUMOS~" ,
           ~"ARTICULO~"     
        WHERE ( ~"PD_OT_DET~".~"NRO_PARTE~" = ~"PD_OT_INSUMOS~".~"NRO_PARTE~" (+)) and 
				  ( ~"PD_OT_DET~".~"NRO_ITEM~" = ~"PD_OT_INSUMOS~".~"NRO_ITEM~" (+)) and 
				  ( ~"PD_OT~".~"NRO_PARTE~" = ~"PD_OT_DET~".~"NRO_PARTE~" ) and 
				  ( ~"PD_OT_INSUMOS~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" ) and 
				( ( ~"PD_OT_DET~".~"COD_MAQUINA~" = :as_cod_maquina ) and 
				  ( ~"PD_OT~".~"FECHA~" >= :ad_fec_ini ) and ( ~"PD_OT~".~"FECHA~" <= :ad_fec_fin ) ) 
        ORDER BY ~"PD_OT_DET~".~"NRO_ITEM~"          ASC  " arguments=(("as_cod_maquina", string),("ad_fec_ini", date),("ad_fec_fin", date)) )
