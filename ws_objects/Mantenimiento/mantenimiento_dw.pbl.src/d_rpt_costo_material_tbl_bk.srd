 retrieve="  SELECT ~"ARTICULO_MOV~".~"CNTA_PRSP~",   
         ~"TT_MAN_CNTA_PRSP~".~"DESCRIPCION~",   
         ~"OPERACIONES~".~"OPER_SEC~",   
         ~"OPERACIONES~".~"NRO_ORDEN~",   
         ~"OPERACIONES~".~"COD_LABOR~",   
         ~"LABOR~".~"DESC_LABOR~",   
         ~"ARTICULO_MOV~".~"COD_MAQUINA~",   
         ~"MAQUINA~".~"DESC_MAQ~",   
         ~"ARTICULO_MOV~".~"CENCOS~",   
         ~"TT_MAN_CENCOS~".~"DESC_CENCOS~",   
         ~"CENCOS_NIV3~".~"DESCRIPPCION~",   
         ~"OPERACIONES~".~"COD_EJECUTOR~",   
         ~"VALE_MOV~".~"NOM_RECEPTOR~",   
         ~"VALE_MOV~".~"FEC_REGISTRO~",   
         ~"ARTICULO_MOV~".~"COD_ART~",   
         ~"ARTICULO~".~"NOM_ARTICULO~",   
         ~"ARTICULO~".~"UND~",   
         ~"ARTICULO_MOV~".~"CANT_PROCESADA~", 
         ~"ARTICULO_MOV~".~"PRECIO_UNIT~", 
         ~"ARTICULO_MOV~".~"COD_MONEDA~",
          ~"CALENDARIO~".~"VTA_DOL_PROM~"
    FROM ~"ARTICULO_MOV~",   
         ~"TT_MAN_CENCOS~",   
         ~"TT_MAN_CNTA_PRSP~",   
         ~"OPERACIONES~",   
         ~"LABOR~",   
         ~"MAQUINA~",   
         ~"ORDEN_TRABAJO~",   
         ~"VALE_MOV~",   
         ~"CENCOS_NIV3~",   
         ~"CENTROS_COSTO~",   
         ~"ARTICULO~",
         ~"CALENDARIO~"
   WHERE ( ~"VALE_MOV~".~"COD_ORIGEN~" = ~"ARTICULO_MOV~".~"COD_ORIGEN~" ) and  
         ( ~"VALE_MOV~".~"NRO_VALE~" = ~"ARTICULO_MOV~".~"NRO_VALE~" ) and  
         ( ~"ARTICULO_MOV~".~"OPER_SEC~" = ~"OPERACIONES~".~"OPER_SEC~" ) and  
         ( ~"OPERACIONES~".~"NRO_ORDEN~" = ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" ) and  
         ( ~"CENTROS_COSTO~".~"COD_N1~" = ~"CENCOS_NIV3~".~"COD_N1~" ) and  
         ( ~"CENTROS_COSTO~".~"COD_N2~" = ~"CENCOS_NIV3~".~"COD_N2~" ) and  
         ( ~"CENTROS_COSTO~".~"COD_N3~" = ~"CENCOS_NIV3~".~"COD_N3~" ) and  
         ( ~"ARTICULO_MOV~".~"CNTA_PRSP~" = ~"TT_MAN_CNTA_PRSP~".~"CNTA_PRSP~" ) and  
         ( ~"ARTICULO_MOV~".~"CENCOS~" = ~"TT_MAN_CENCOS~".~"CENCOS~" ) and  
         ( ~"ARTICULO_MOV~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" ) and  
         ( ~"ARTICULO_MOV~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~" ) and  
         ( ~"OPERACIONES~".~"COD_LABOR~" = ~"LABOR~".~"COD_LABOR~" ) and  
         ( ~"ORDEN_TRABAJO~".~"CENCOS_SLC~" = ~"CENTROS_COSTO~".~"CENCOS~" ) and  
         ( trunc(~"VALE_MOV~".~"FEC_REGISTRO~" ) >= trunc(~"CALENDARIO~".~"FECHA~" ) ) AND  
         ( ( trunc(~"VALE_MOV~".~"FEC_REGISTRO~" ) >= :ld_fecini ) AND  
         ( trunc(~"VALE_MOV~".~"FEC_REGISTRO~" ) <= :ld_fecfin ) )    
" arguments=(("ld_fecini", date),("ld_fecfin", date)) )
