         ~"ORDEN_TRABAJO~".~"PLANT_RATIO~"  
    FROM ~"ORDEN_TRABAJO~"  
   WHERE ( ~"ORDEN_TRABAJO~".~"COD_ORIGEN~" = :as_cod_origen ) AND  
