 retrieve="  SELECT ~"TT_MAQ_X_OT_VAL~".~"COD_ORIGEN~",   
         ~"TT_MAQ_X_OT_VAL~".~"NRO_ORDEN~",   
         ~"ORDEN_TRABAJO~".~"NRO_MANT_PROG~",   
         SUBSTR(~"TT_MAQ_X_OT_VAL~".~"DESCRIPCION~",1,50) AS DESCRIPCION,   
         ~"TT_MAQ_X_OT_VAL~".~"FEC_INICIO~",   
         SUM(~"TT_MAQ_X_OT_VAL~".~"GTOS_OPERATIVOS~") AS GTOS_OPERATIVOS,   
         SUM(~"TT_MAQ_X_OT_VAL~".~"GTOS_INSUMOS~")	  AS GTOS_INSUMOS   ,   
         SUM(~"TT_MAQ_X_OT_VAL~".~"GTOS_TOTAL~") 	  AS GTOS_TOTAL	 
    FROM ~"TT_MAQ_X_OT_VAL~",   
         ~"ORDEN_TRABAJO~"  
   WHERE ( ~"TT_MAQ_X_OT_VAL~".~"COD_ORIGEN~" = ~"ORDEN_TRABAJO~".~"COD_ORIGEN~" ) and  
         ( ~"TT_MAQ_X_OT_VAL~".~"NRO_ORDEN~" = ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" ) and  
         ( ( ~"TT_MAQ_X_OT_VAL~".~"COD_MAQUINA~" = :as_cod_maquina )         )    
GROUP BY ~"TT_MAQ_X_OT_VAL~".~"COD_ORIGEN~",   
         ~"TT_MAQ_X_OT_VAL~".~"NRO_ORDEN~",   
         ~"ORDEN_TRABAJO~".~"NRO_MANT_PROG~",   
         ~"TT_MAQ_X_OT_VAL~".~"DESCRIPCION~",   
         ~"TT_MAQ_X_OT_VAL~".~"FEC_INICIO~"   " arguments=(("as_cod_maquina", string)) )
