 retrieve="  SELECT ~"TT_OPER_ORDEN_TRABAJO~".~"COD_ORIGEN~",  
			~"TT_OPER_ORDEN_TRABAJO~".~"NRO_ORDEN~",   
         ~"TT_OPER_ORDEN_TRABAJO~".~"COD_MAQUINA~",   
         ~"MAQUINA~".~"DESC_MAQ~",   
         ~"TT_OPER_ORDEN_TRABAJO~".~"TIPO_OT~",   
         SUM(~"TT_OPER_ORDEN_TRABAJO~".~"COSTO_INSUMO~") AS COSTO_INSUMO,   
         SUM(~"TT_OPER_ORDEN_TRABAJO~".~"COSTO_OPER~")   AS COSTO_OPER   
    FROM ~"TT_OPER_ORDEN_TRABAJO~",   
         ~"MAQUINA~"  
   WHERE ( ~"TT_OPER_ORDEN_TRABAJO~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~" ) AND
			
			( ~"TT_OPER_ORDEN_TRABAJO~".~"TIPO_OT~"     = :as_tipo_ot					)  
GROUP BY ~"TT_OPER_ORDEN_TRABAJO~".~"COD_ORIGEN~",  
			~"TT_OPER_ORDEN_TRABAJO~".~"NRO_ORDEN~",   
         ~"TT_OPER_ORDEN_TRABAJO~".~"COD_MAQUINA~",   
         ~"MAQUINA~".~"DESC_MAQ~",   
         ~"TT_OPER_ORDEN_TRABAJO~".~"TIPO_OT~"   
" arguments=(("as_tipo_ot", string)) )
