 retrieve="SELECT op.cod_labor    ,   
			ot.cencos_slc   ,
			fm.cod_maquina	 ,
			m.desc_maq   	 ,
			op.oper_sec     ,   
         ot.nro_orden    ,
		   op.fec_inicio   , 
         op.nro_personas ,   
         op.cant_proyect ,  
		   op.fec_inicio_est
    FROM operaciones         op,   
         orden_trabajo       ot,
			ficha_mantenimiento fm,
			maquina				  m
   WHERE (( op.nro_orden  = ot.nro_orden     )  AND  
          ( op.tipo_orden = (SELECT DOC_OT FROM PROD_PARAM WHERE RECKEY = '1')  )) AND  
			(( ot.cod_origen = fm.cod_origen(+) )  AND
			 ( ot.nro_orden  = fm.nro_orden (+) )) AND  
			 ( fm.cod_maquina= m.cod_maquina(+) )  AND	
          ( ot.cencos_rsp = :as_cc_resp                 ) AND  
		   (( TO_CHAR(op.fec_inicio,'yyyymmdd') >= :as_fecha_inicio  ) AND
		    ( TO_CHAR(op.fec_inicio,'yyyymmdd') <= :as_fecha_final  )) " arguments=(("as_cc_resp", string),("as_fecha_inicio", string),("as_fecha_final", string)) )
