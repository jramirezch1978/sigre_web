 retrieve="SELECT a.cod_art, a.desc_art, a.und,
       am.cant_procesada,
       usf_fl_conv_mon(am.precio_unit, am.cod_moneda, (select cod_dolares from logparam where reckey = '1'), vm.fec_registro) as precio_unit,
       usf_fl_conv_mon(am.cant_procesada * am.precio_unit, am.cod_moneda, (select cod_dolares from logparam where reckey = '1'), vm.fec_registro) AS importe,
       ot.ot_tipo,
       ot.nro_orden,
		 ot.ot_adm,
		 ot.centro_benef,
		 am.cencos,
		 am.cnta_prsp
FROM orden_trabajo ot,
     articulo_mov_proy amp,
     vale_mov          vm,
     articulo_mov      am,
     articulo          a
WHERE ot.nro_orden = amp.nro_doc
  AND amp.tipo_doc = (select doc_ot from logparam where reckey = '1')
  AND vm.nro_vale  = am.nro_vale
  AND am.origen_mov_proy = amp.cod_origen
  AND am.nro_mov_proy    = amp.nro_mov
  AND am.cod_art         = a.cod_art
  AND vm.flag_estado     <> '0'
  AND am.flag_estado     <> '0'
  AND ot.ot_adm          like :as_ot_adm
  AND ot.centro_benef    like :as_cenbef
  and ot.ot_tipo		    like :as_tipo_ot
  and ot.flag_estado in (:as_estado)
  and ( (:as_maquina = '1' and ot.cod_maquina is not null) or (:as_maquina = '0') )
  AND trunc(vm.fec_registro) BETWEEN trunc(:ad_fecha1) and trunc(:ad_fecha2)  " arguments=(("ad_fecha1", date),("ad_Fecha2", date),("as_tipo_ot", string),("as_ot_adm", string),("as_cenbef", string),("as_estado", stringlist),("as_maquina", string)) )
