 retrieve="select m.cod_maquina, m.desc_maq,
       t.prog_mnt, t.cod_plantilla, b.desc_plantilla, 
       c.cod_labor, c.nro_operacion, d.desc_extendida,
       t.frecuencia, 
       t.und_actividad as und, 
       t.ultimo_mnt, t.fecha_ultimo_mnt, t.und_previa_aviso as punto_aviso,
       t.flag_estado
from mt_prog_ciclo_mantto t,
     plant_prod           b,
     plant_prod_oper      c,
     labor                d,
     maquina              m
where t.cod_plantilla = b.cod_plantilla     
  and b.cod_plantilla = c.cod_plantilla     
  and c.cod_labor     = d.cod_labor
  and m.cod_maquina   = t.cod_maquina
  and m.flag_estado   = '1'
  and t.cod_maquina   in (select cod_maquina from tt_mtt_equipos)
order by t.cod_maquina, t.cod_plantilla" )
