 retrieve="  SELECT DISTINCT ~"PLANT_PROD_OPER~".~"NRO_OPERACION~",   
         ~"PLANT_PROD_OPER~".~"FLAG_PRE~"         ,   
		   ~"PLANT_PROD_OPER~".~"NRO_PRECEDENCIA~"  ,         
         ~"PLANT_PROD_OPER~".~"NRO_DIAS_INICIO~"  ,   
         ~"PLANT_PROD_OPER~".~"DIAS_DURACION~"    ,   
         ~"PLANT_PROD_OPER~".~"DESC_OPERACION~"   ,   
         ~"PLANT_PROD_OPER~".~"CANTIDAD~"         ,   
         ~"LABOR~".~"UND~"                        ,   
			~"PLANT_PROD_OPER~".~"COD_PLANTILLA~"    ,   
         ~"PLANT_PROD_OPER~".~"FLAG_DIAS_INICIO~" ,   
         ~"PLANT_PROD_OPER~".~"COD_LABOR~"        ,   
         ~"PLANT_PROD_OPER~".~"COD_EJECUTOR~"     , 
			~"PLANT_PROD_OPER~".~"NRO_PERSONAS~"    
    FROM ~"PLANT_PROD_OPER~",   
         ~"LABOR~"  
   WHERE ( ~"PLANT_PROD_OPER~".~"COD_LABOR~" = ~"LABOR~".~"COD_LABOR~" (+)) and  
         ( ( ~"PLANT_PROD_OPER~".~"COD_PLANTILLA~" = :as_cod_plantilla ) )   
ORDER BY ~"PLANT_PROD_OPER~".~"NRO_OPERACION~" ASC   
" update="PLANT_PROD_OPER" updatewhere=1 updatekeyinplace=yes arguments=(("as_cod_plantilla", string)) )
