 retrieve="  select ot.nro_orden, amp.oper_sec, vm.nro_vale, vm.fec_registro, vm.tipo_mov,
			a.cod_Art, a.desc_art, a.und, 
			am.cant_procesada,
			usf_fl_conv_mon( NVL(am.precio_unit,0), am.cod_moneda, (select cod_dolares from logparam where reckey = '1'), vm.fec_registro) as precio_unit,
			am.cant_procesada * usf_fl_conv_mon(NVL(am.precio_unit,0), am.cod_moneda, (select cod_dolares from logparam where reckey = '1'), vm.fec_registro) as importe
  from orden_Trabajo     ot,
       articulo_mov      am,
       vale_mov          vm,
		 articulo			 a,
       articulo_mov_proy amp,
       tt_mtt_ot_adm     tt1
  where ot.nro_orden       = amp.nro_doc
    and amp.tipo_doc       = (select doc_ot from logparam where reckey = '1')
    and am.nro_vale        = vm.nro_vale
    and am.cod_art			= a.cod_art
    and am.nro_mov_proy    = amp.nro_mov
    and am.origen_mov_proy = amp.cod_origen
    and tt1.ot_adm         = ot.ot_adm
    and ot.cod_maquina     = :as_maquina
    and trunc(vm.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
    and am.flag_estado     <> '0'
    and vm.flag_estado     <> '0'
order by vm.fec_registro" arguments=(("as_maquina", string),("ad_fecha1", date),("ad_Fecha2", date)) )
