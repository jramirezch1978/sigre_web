 retrieve="  SELECT ~"SOLICITUD_OT~".~"NRO_SOLICITUD~",   
         ~"SOLICITUD_OT~".~"CENCOS_SLC~",   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~",   
         ~"SOLICITUD_OT~".~"FECHA_ESPERADA~",   
			~"SOLICITUD_OT~".~"FLAG_ESTADO~",
         ~"SOLICITUD_OT~".~"DESCRIPCION~",   
         ~"ORDEN_TRABAJO~".~"NRO_ORDEN~",   
         ~"ORDEN_TRABAJO~".~"FLAG_ESTADO~"  
    FROM ~"SOLICITUD_OT~",   
         ~"ORDEN_TRABAJO~",   
         ~"CENTROS_COSTO~"  
   WHERE ( solicitud_ot.nro_solicitud = orden_trabajo.nro_solicitud (+)) and  
         ( ~"SOLICITUD_OT~".~"CENCOS_SLC~" = ~"CENTROS_COSTO~".~"CENCOS~" ) and  
         ( ( ~"SOLICITUD_OT~".~"FECHA_ESPERADA~" between :ad_fec_inicio and :ad_fec_fin ) AND  
         ( ~"SOLICITUD_OT~".~"CENCOS_RSP~" = :as_cencos_rsp ) )    
" arguments=(("ad_fec_inicio", date),("ad_fec_fin", date),("as_cencos_rsp", string)) )
