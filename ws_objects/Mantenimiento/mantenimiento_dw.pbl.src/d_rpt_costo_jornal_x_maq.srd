 retrieve="select 	ot.nro_orden, 
			pdd.oper_sec,
			pd.nro_parte,
			pdd.hora_inicio as fec_inicio,
			m.cod_Trabajador,
		 	m.apel_paterno || ' ' || m.apel_materno || ', ' || m.nombre1 || ' ' || m.nombre2 as full_name,
			NVL(pda.nro_horas,0)* usf_fl_conv_mon(NVL(pda.costo_horas,0), (select cod_soles from logparam where reckey = '1'), (select cod_dolares from logparam where reckey = '1'), pdd.hora_inicio) * NVL(pda.factor,0) as importe
  from orden_Trabajo     ot,
		 pd_ot             pd,
		 pd_ot_det         pdd,
		 pd_ot_asistencia  pda,
		 maestro				 m,
		 tt_mtt_ot_adm     tt1
  where ot.nro_orden  = pdd.nro_orden
	 and pd.nro_parte  = pdd.nro_parte
	 and pda.nro_item  = pdd.nro_item
	 and pda.no_parte  = pdd.nro_parte
    and m.cod_trabajador = pda.cod_Trabajador
	 and ot.ot_adm     = tt1.ot_adm
	 and pdd.cod_maquina = :as_maquina
	 and trunc(pd.fecha) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
order by pdd.hora_inicio
" arguments=(("as_maquina", string),("ad_fecha1", date),("ad_Fecha2", date)) )
