 retrieve="  SELECT ~"SOLICITUD_OT~".~"NRO_SOLICITUD~",   
         ~"SOLICITUD_OT~".~"FEC_SOLICITUD~",   
         ~"SOLICITUD_OT~".~"CENCOS_SLC~",   
         ~"SOLICITUD_OT~".~"CENCOS_RSP~",   
         ~"SOLICITUD_OT~".~"DESCRIPCION~",   
         ~"SOLICITUD_OT~".~"FLAG_URGENCIA~",
         ~"SOLICITUD_OT~".~"CNTA_PRSP~",
         ~"PRESUPUESTO_CUENTA~".~"DESCRIPCION~",
         ~"MAQUINA~".~"COD_MAQUINA~",   
         ~"MAQUINA~".~"DESC_MAQ~",   
         ~"CENTROS_COSTO_A~".~"DESC_CENCOS~",   
         ~"CENCOS_NIV3_A~".~"DESCRIPPCION~" as desc_solicitante,   
         ~"CENCOS_NIV3_B~".~"DESCRIPPCION~" as desc_responsable
    FROM ~"SOLICITUD_OT~",   
         ~"MAQUINA~",
         ~"PRESUPUESTO_CUENTA~",
         ~"CENTROS_COSTO~" ~"CENTROS_COSTO_A~",   
         ~"CENTROS_COSTO~" ~"CENTROS_COSTO_B~",   
         ~"CENCOS_NIV3~" ~"CENCOS_NIV3_A~",   
         ~"CENCOS_NIV3~" ~"CENCOS_NIV3_B~"  
   WHERE ( ~"SOLICITUD_OT~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~" (+) ) and  
         ( ~"SOLICITUD_OT~".~"CENCOS_SLC~" = ~"CENTROS_COSTO_A~".~"CENCOS~" ) and  
         ( ~"SOLICITUD_OT~".~"CENCOS_RSP~" = ~"CENTROS_COSTO_B~".~"CENCOS~" ) and  
         ( ~"SOLICITUD_OT~".~"CNTA_PRSP~" = ~"PRESUPUESTO_CUENTA~".~"CNTA_PRSP~" (+) ) and  
         ( ~"CENTROS_COSTO_A~".~"COD_N1~" = ~"CENCOS_NIV3_A~".~"COD_N1~" ) and  
         ( ~"CENTROS_COSTO_A~".~"COD_N2~" = ~"CENCOS_NIV3_A~".~"COD_N2~" ) and  
         ( ~"CENTROS_COSTO_A~".~"COD_N3~" = ~"CENCOS_NIV3_A~".~"COD_N3~" ) and  
         ( ~"CENTROS_COSTO_B~".~"COD_N1~" = ~"CENCOS_NIV3_B~".~"COD_N1~" ) and  
         ( ~"CENTROS_COSTO_B~".~"COD_N2~" = ~"CENCOS_NIV3_B~".~"COD_N2~" ) and  
         ( ~"CENTROS_COSTO_B~".~"COD_N3~" = ~"CENCOS_NIV3_B~".~"COD_N3~" ) and  
         ( ( ~"SOLICITUD_OT~".~"NRO_SOLICITUD~" = :as_nro_solicitud ) )    
" arguments=(("as_nro_solicitud", string)) )
