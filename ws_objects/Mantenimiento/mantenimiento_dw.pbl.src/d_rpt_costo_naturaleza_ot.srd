 column=(type=number updatewhereclause=yes name=costo_terceros dbname="costo_terceros" )
 retrieve="select distinct 
		 ot.nro_orden, 
       ot.cencos_slc, cc1.desc_cencos,
       ot.cencos_rsp, cc2.desc_cencos,
       ot.cod_maquina, m.desc_maq,
       ot.fec_solicitud,
       ot.nro_solicitud,
       ot.fec_inicio,
       ot.flag_estado,
       round(usf_mto_costo_art_x_ot(ot.nro_orden), 2) as costo_material,
       round(usf_ope_costo_jor_x_ot(ot.nro_orden), 2) as costo_jornal,
       round(usf_ope_costo_os_x_ot(ot.nro_orden), 2) as costo_terceros
from orden_trabajo ot,
     centros_costo cc1,
     centros_costo cc2,
     maquina       m,
	  operaciones   op
where ot.cencos_slc = cc1.cencos
  and ot.cencos_rsp = cc2.cencos     
  and op.nro_orden  = ot.nro_orden
  and ot.cod_maquina  = m.cod_maquina (+)
  and ot.ot_adm like :as_ot_adm
  and ( ( :as_fec_inicio = '0' and trunc(ot.fec_inicio) between trunc(:ad_fecha1) and trunc(:ad_fecha2) )
		or ( :as_fec_inicio = '1' and ot.fec_inicio is null ) )
  and ot.ot_tipo like :as_tipo_ot
  and ot.flag_estado in (:as_estado)
  and ot.cencos_rsp like :as_cencos_rsp
  and ot.cencos_slc like :as_cencos_slc
  and op.cod_ejecutor like :as_ejecutor
  and ( (:as_maquina = '1' and ot.cod_maquina is not null) or (:as_maquina = '0') )
  and ( (:as_fec_inicio = '1' and ( round(usf_mto_costo_art_x_ot(ot.nro_orden), 2) +
       									  round(usf_ope_costo_jor_x_ot(ot.nro_orden), 2) +
       									  round(usf_ope_costo_os_x_ot(ot.nro_orden), 2) ) > 0) or (:as_fec_inicio = '0') )
  " arguments=(("ad_fecha1", date),("ad_Fecha2", date),("as_tipo_ot", string),("as_ot_adm", string),("as_cencos_rsp", string),("as_cencos_slc", string),("as_estado", stringlist),("as_maquina", string),("as_fec_inicio", string),("as_ejecutor", string)) )
