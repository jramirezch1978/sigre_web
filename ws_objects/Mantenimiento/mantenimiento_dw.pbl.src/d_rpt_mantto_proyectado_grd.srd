 retrieve="SELECT A.PROX_MANTTO,   
         A.FEC_PROX_MANTTO,   
         B.COD_MAQUINA,   
         M.DESC_MAQ,   
         B.FRECUENCIA,   
         B.FLAG_TIPO_CICLO,   
         B.UND_ACTIVIDAD,   
         B.UND_PROM_DIA,   
         A.FLAG_REPROG,   
         A.REPROG,   
         A.FLAG_GENERAR,   
         A.NRO_OT,   
         M.IND_ACT_ACUMULADA,   
         U.DESC_UNIDAD,   
         P.COD_PLANTILLA || ' ' || P.DESC_PLANTILLA || ' (' ||
				to_char((select count(*) from plant_prod_oper where cod_plantilla = P.COD_plantilla)) || ' labores)'
				as desc_plantilla,   
         A.FEC_ULT_MANTTO,   
         A.ULT_MANTTO,   
         A.PROG_MNT,   
         OT.FEC_ESTIMADA,   
         OT.FEC_INICIO,   
         '0' as imprimir  
    FROM TT_MTT_PROYECTADO      A,   
         MT_PROG_CICLO_MANTTO   B,   
         MAQUINA                M,   
         PLANT_PROD        	  P,   
         UNIDAD                 U,   
         ORDEN_TRABAJO          OT  
   WHERE A.nro_ot          = OT.nro_orden (+)
     and A.PROG_MNT        = B.PROG_MNT 
     and M.COD_MAQUINA     = B.COD_MAQUINA 
     and B.COD_PLANTILLA   = P.COD_PLANTILLA 
     and B.UND_ACTIVIDAD   = U.UND    
ORDER BY B.COD_MAQUINA ASC, B.FRECUENCIA ASC, A.PROG_MNT ASC   " )
