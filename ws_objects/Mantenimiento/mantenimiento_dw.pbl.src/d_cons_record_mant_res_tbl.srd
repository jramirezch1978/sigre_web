 column=(type=number updatewhereclause=yes name=gtos_insumos dbname="gtos_insumos" )
 column=(type=number updatewhereclause=yes name=gtos_operativos dbname="gtos_operativos" )
 column=(type=number updatewhereclause=yes name=gtos_total dbname="gtos_total" )
 retrieve="  SELECT ~"TT_RECORD_MANTEN~".~"FLAG_MANTEN~",   
         SUM(gtos_insumos)  AS gtos_insumos,   
         SUM(gtos_operativos)  AS gtos_operativos,  
			SUM(gtos_insumos + gtos_operativos) AS gtos_total
    FROM ~"TT_RECORD_MANTEN~"  
   WHERE ( ~"TT_RECORD_MANTEN~".~"COD_MAQUINA~" like :as_cod_maquina ) AND  
         ( ~"TT_RECORD_MANTEN~".~"CENCOS_RSP~" like :as_cencos ) AND  
         ( ~"TT_RECORD_MANTEN~".~"FLAG_MANTEN~" like :as_flag_manten )   
GROUP BY ~"TT_RECORD_MANTEN~".~"FLAG_MANTEN~"   
" arguments=(("as_cod_maquina", string),("as_cencos", string),("as_flag_manten", string)) )
