 retrieve="  SELECT ~"LABOR~".~"COD_LABOR~",   
         ~"LABOR~".~"COD_ETAPA~",   
         ~"LABOR~".~"DESC_LABOR~",   
         ~"LABOR~".~"FLAG_ESTADO~",   
         ~"LABOR~".~"FLAG_INCIDENCIA~",   
         ~"LABOR~".~"FLAG_MARCADOR~",   
         ~"LABOR~".~"FLAG_MAQ_MO~",   
         ~"LABOR~".~"FLAG_TIPO_HAS~",   
         ~"LABOR~".~"CNTA_OPER~",   
         ~"LABOR~".~"UND~",   
         ~"LABOR~".~"FLAG_USO~",   
         ~"LABOR~".~"FLAG_TRANSPORTE~",   
         ~"LABOR~".~"CNTA_PRSP~",   
         ~"PRESUPUESTO_CUENTA_A~".~"DESCRIPCION~",   
         ~"CNTBL_CNTA~".~"ABREV_CNTA~",   
         ~"LABOR~".~"CNTA_PRSP_3RO~",   
         ~"PRESUPUESTO_CUENTA_B~".~"DESCRIPCION~",   
         ~"LABOR~".~"CNTA_PRSP_INSM~",   
         presupuesto_cuenta_c.descripcion as desc_cuenta_ins,   
         ~"LABOR~".~"DESC_EXTENDIDA~"  
    FROM ~"LABOR~",   
         ~"PRESUPUESTO_CUENTA~" ~"PRESUPUESTO_CUENTA_A~",   
         ~"CNTBL_CNTA~",   
         ~"PRESUPUESTO_CUENTA~" ~"PRESUPUESTO_CUENTA_B~",   
         ~"PRESUPUESTO_CUENTA~" ~"PRESUPUESTO_CUENTA_C~"  
   WHERE ( labor.cnta_prsp = presupuesto_cuenta_a.cnta_prsp (+)) and  
         ( labor.cnta_oper = cntbl_cnta.cnta_ctbl (+)) and  
         ( labor.cnta_prsp_3ro = presupuesto_cuenta_b.cnta_prsp (+)) and  
         ( labor.cnta_prsp_insm = presupuesto_cuenta_c.cnta_prsp (+)) and  
         ( ( ~"LABOR~".~"FLAG_USO~" in ('F','T','A','S','M') ) )   
ORDER BY ~"LABOR~".~"COD_ETAPA~" ASC   
" update="LABOR" updatewhere=1 updatekeyinplace=yes  sort="cod_etapa A cod_labor A " )
