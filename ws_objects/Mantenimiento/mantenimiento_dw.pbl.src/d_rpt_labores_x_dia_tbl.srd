 retrieve="select a.oper_sec, a.cod_labor, a.desc_labor, a.obs, a.cod_maquina, d.desc_maq,
       a.cant_labor, a.und_labor,
       b.cod_art, c.desc_art, c.und,b.cantidad,
		 a.nro_parte, a.nro_item,
       a.cencos, e.desc_cencos,
		 a.cod_ejecutor, f.descripcion as desc_ejecutor,
		 a.hora_inicio, a.hora_fin
from pd_ot_det         a,
     pd_ot_insumos     b,
     articulo          c,
     maquina           d,
	  centros_costo	  e,
     ejecutor		     f,
	  pd_ot				  g
where g.nro_parte = a.nro_parte
  and a.nro_parte = b.nro_parte (+)
  and a.nro_item  = b.nro_item  (+)
  and b.cod_art   = c.cod_art (+)
  and a.cencos	   = e.cencos    (+)
  and a.cod_ejecutor = f.cod_ejecutor (+)
  and a.cod_maquina = d.cod_maquina (+)
  and to_char(a.hora_inicio, 'yyyymmdd') >= to_char(:ad_fecha1, 'yyyymmdd')
  and to_char(a.hora_inicio, 'yyyymmdd') <= to_char(:ad_fecha2, 'yyyymmdd')
  and g.ot_adm = :as_ot_adm
order by a.nro_parte, a.nro_item, a.hora_inicio" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_ot_adm", string)) )
