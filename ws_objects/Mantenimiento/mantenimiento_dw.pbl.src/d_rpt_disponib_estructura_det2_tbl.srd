 retrieve="  SELECT trunc(mc.fecha) as fecha,
         md.cod_maquina,
         sum(md.und_trabajadas) as und_trabajadas,
         usf_mtt_horas_inc_x_maq(md.cod_maquina, trunc(mc.fecha)) as horas_incidencias,
         usf_mtt_horas_disponibles(trunc(mc.fecha)) as horas_diponibles
    FROM mt_pd_grupo_trab       mc,
         mt_pd_grupo_trab_det   md,
			maquina					  m
   WHERE mc.nro_parte = md.nro_parte 
     and md.cod_maquina = m.cod_maquina
     AND trunc(mc.fecha) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
     and mc.cod_maquina = :as_cod_maquina
GROUP BY trunc(mc.fecha), md.cod_maquina" arguments=(("as_cod_maquina", string),("ad_fecha1", date),("ad_fecha2", date)) )
