global function string f_cod_soles ();String ls_cod_soles

  SELECT "LOGPARAM"."COD_SOLES"  
    INTO :ls_cod_soles  
    FROM "LOGPARAM"  
   WHERE "LOGPARAM"."RECKEY" = '1'   ;

Return ls_cod_soles
end function

