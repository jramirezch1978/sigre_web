public subroutine of_load ();// load message

u_tab_pop3 lu_parent
Integer li_idx, li_max
String ls_type

lu_parent = this.GetParent()

ole_web.of_SetSource("<html><body><p>No HTML View</p></body></html>")

li_max = UpperBound(lu_parent.is_types)
For li_idx = 1 To li_max
	ls_type = Lower(lu_parent.is_types[li_idx])
	If Pos(ls_type, "content-type: text/html;") > 0 Then
		ole_web.of_SetSource(lu_parent.is_parts[li_idx])
		Exit
	End If
Next

end subroutine

on u_tabpg_html.create
int iCurrent
call super::create
this.ole_web=create ole_web
iCurrent=UpperBound(this.Control)
this.Control[iCurrent+1]=this.ole_web
end on

on u_tabpg_html.destroy
call super::destroy
destroy(this.ole_web)
end on

event constructor;call super::constructor;ole_web.of_Navigate("about:blank")

end event

