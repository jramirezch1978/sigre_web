 column=(type=decimal(1) updatewhereclause=yes name=dias_reposicion dbname="articulo.dias_reposicion" dbalias="A.dias_reposicion" )
 column=(type=char(6) updatewhereclause=yes name=almacen dbname="almacen.almacen" dbalias="AL.almacen" )
