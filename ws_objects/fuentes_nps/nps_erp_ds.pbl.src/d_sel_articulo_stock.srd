 column=(type=char(20) updatewhereclause=yes name=codigo_serie dbname="articulo.codigo_serie" dbalias="A.codigo_serie" )
 retrieve="  SELECT A.COD_ART,   
         A.DESC_ART,   
         A.UND,   
         A.COSTO_ULT_COMPRA,   
         A.DIAS_REPOSICION,   
			AL.ALMACEN,
			AL.DESC_ALMACEN,
			(select distinct numero_serie 
			  from articulo_mov am 
			 where am.numero_serie = :as_nro_serie
			   and flag_estado <> '0') as numero_serie,
			CASE WHEN :as_nro_serie is null THEN
				AA.SLDO_TOTAL
			ELSE
				NVL((SELECT SUM(AM.CANT_PROCESADA * AMT.FACTOR_SLDO_TOTAL)
						 FROM ARTICULO_MOV AM,
								VALE_MOV		 VM,
								ARTICULO_MOV_TIPO AMT
						WHERE AM.NRO_VALE = VM.NRO_VALE
 						  AND VM.TIPO_MOV	= AMT.TIPO_MOV
						  AND VM.FLAG_ESTADO <> '0'
						  AND AM.FLAG_ESTADO <> '0'
						  AND AM.NUMERO_SERIE = :as_nro_serie
						  AND AM.COD_ART		 = :as_cod_art
						  and vm.almacen		 = al.almacen
					),0)
			END AS SLDO_TOTAL,
			a.codigo_serie   
    FROM ARTICULO					A,
			ARTICULO_ALMACEN		AA,
			ALMACEN					AL
   WHERE A.COD_ART     = AA.COD_ART
	  and AA.ALMACEN	  = AL.ALMACEN
     and A.FLAG_ESTADO = '1' 
	  AND A.FLAG_INVENTARIABLE = '1' 
     and A.COD_ART = :as_cod_art
     and CASE WHEN :as_nro_serie is null THEN
				AA.SLDO_TOTAL
			ELSE
				NVL((SELECT SUM(AM.CANT_PROCESADA * AMT.FACTOR_SLDO_TOTAL)
						 FROM ARTICULO_MOV AM,
								VALE_MOV		 VM,
								ARTICULO_MOV_TIPO AMT
						WHERE AM.NRO_VALE = VM.NRO_VALE
 						  AND VM.TIPO_MOV	= AMT.TIPO_MOV
						  AND VM.FLAG_ESTADO <> '0'
						  AND AM.FLAG_ESTADO <> '0'
						  AND AM.NUMERO_SERIE = :as_nro_serie
						  AND AM.COD_ART		 = :as_cod_art
						  and vm.almacen		 = al.almacen
					),0)
			END > 0
" arguments=(("as_cod_art", string),("as_nro_serie", string)) )
