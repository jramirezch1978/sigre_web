 retrieve=" SELECT  GD.ITEM,
         GD.ORIGEN_MOV,
         GD.NRO_MOV,
         AM.NRO_VALE,
         GD.ALMACEN_DST,
			TE.COD_ART,
         A.DESC_ART,
         GD.PESO_VENTA,
         PDD.UND,
         (NVL(GD.PRECIO_UNITARIO,0) + NVL(GD.PRECIO_UNITARIO_CASTIGO,0) + 
         NVL(GD.PRECIO_REPRESENTACION,0) + NVL(GD.PRECIO_AJUSTE_CNTBL,0)) AS PRECIO,
			NVL(GD.MONTO_FACTURADO,0) AS MONTO_FACTURADO
 
 FROM    AP_GUIA_RECEPCION      GC,
         AP_GUIA_RECEPCION_DET  GD,
         ARTICULO_MOV           AM,
         AP_PD_DESCARGA_DET     PDD,
			TG_ESPECIES            TE,
         ARTICULO               A
 
 WHERE   GC.COD_GUIA_REC = GD.COD_GUIA_REC
	AND   PDD.ESPECIE     = TE.ESPECIE
   AND   TE.COD_ART      = A.COD_ART
   AND   GD.NRO_PARTE    = PDD.NRO_PARTE
   AND   GD.ITEM_PARTE   = PDD.ITEM
   AND   GD.ORIGEN_MOV   = AM.COD_ORIGEN
   AND   GD.NRO_MOV      = AM.NRO_MOV
   AND   GC.PROVEEDOR    = :AS_PROVEEDOR
   AND   GC.COD_GUIA_REC = :AS_COD_GUIA
   AND   NVL(GD.MONTO_FACTURADO,0) < ROUND( GD.PESO_VENTA * (NVL(GD.PRECIO_UNITARIO,0) +
			NVL(GD.PRECIO_UNITARIO_CASTIGO,0) + NVL(GD.PRECIO_REPRESENTACION,0) + 
			NVL(GD.PRECIO_AJUSTE_CNTBL,0)), 2)" arguments=(("AS_PROVEEDOR", string),("AS_COD_GUIA", string)) )
