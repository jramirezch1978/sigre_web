 retrieve="select osd.cod_origen   ,osd.nro_os      ,osd.nro_item              ,osd.flag_estado  ,   
       osd.fec_registro ,osd.fec_proyect ,Substr(osd.descripcion,1,60) as descripcion ,   
       osd.importe      ,osd.impuesto    ,Nvl(osd.decuento,0) as descuento            ,
       osd.cencos       ,null as vacio	  ,osd.cnta_prsp   ,osd.oper_sec
  from orden_servicio os,orden_servicio_det osd
 where (os.nro_os           = osd.nro_os    ) and
       (os.nro_sol_serv     is not null     ) and
       (osd.conformidad_usr is null         ) and
       (os.flag_estado      IN ('1','3')    ) and
		 (os.cod_origen   	 = :as_cod_origen) and
		 (os.nro_os				 = :as_nro_os    )
union
select osd.cod_origen   ,osd.nro_os      ,osd.nro_item              ,osd.flag_estado  ,   
       osd.fec_registro ,osd.fec_proyect ,Substr(osd.descripcion,1,60) as descripcion ,   
       osd.importe      ,osd.impuesto    ,Nvl(osd.decuento,0) as descuento            ,
       osd.cencos       ,null as vacio   ,osd.cnta_prsp   ,osd.oper_sec
  from orden_servicio os,orden_servicio_det osd
 where (os.nro_os           = osd.nro_os     ) and
       (osd.conformidad_usr is not null      ) and
       (os.nro_sol_serv     is null          ) and              
       (os.flag_estado      IN ('1','3')     ) and
		 (os.cod_origen		 = :as_cod_origen ) and
