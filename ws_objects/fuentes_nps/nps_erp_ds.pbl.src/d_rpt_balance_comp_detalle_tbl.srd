 retrieve="select cd.origen, cd.ano, cd.mes, cd.nro_libro, 
		 cd.nro_asiento, cd.item, cd.fec_cntbl, cd.det_glosa, 
		 cd.cnta_ctbl, c.desc_cnta, cd.cod_relacion, p.nom_proveedor, cd.tipo_docref1, 
		 cd.nro_docref1, 
	    decode(cd.flag_debhab,'D',cd.imp_movsol,0) as debe_sol,
		 decode(cd.flag_debhab,'H',cd.imp_movsol,0) as haber_sol, 
	    decode(cd.flag_debhab,'D',cd.imp_movdol,0) as debe_dol,
		 decode(cd.flag_debhab,'H',cd.imp_movdol,0) as haber_dol
  from cntbl_asiento_det cd, 
		 proveedor p, 
		 cntbl_cnta c
 where cd.cnta_ctbl = c.cnta_ctbl 
   and cd.cod_relacion = p.proveedor(+) 
   and cd.ano=:an_ano 
   and mes between :an_mes_ini and :an_mes_fin
	and substr(cd.cnta_ctbl,1,:an_nivel)= trim(:as_cnta_ctbl)
order by cnta_ctbl, ano, mes, origen, nro_libro, nro_asiento, item" arguments=(("an_ano", number),("an_mes_ini", number),("an_mes_fin", number),("as_cnta_ctbl", string),("an_nivel", number)) )
