 retrieve="SELECT  A.ALMACEN ,
        A.CENCOS ,
        A.DESC_ALMACEN ,
        A.DIRECCION ,
        A.AREA_TOTAL ,
        A.VOL_TOTAL ,
        A.COD_RESPONSABLE ,
        A.FLAG_TIPO_ALMACEN ,
        A.COD_ORIGEN ,
        U.NOMBRE ,
        CC.DESC_CENCOS ,
        O.NOMBRE ,
        A.FLAG_ESTADO ,
        A.FLAG_CNTRL_LOTE,
		  A.DISTRITO,
	     A.PROVINCIA,
		  A.DEPARTAMENTO,
		  A.PROV_ALMACEN,
		  P.NOM_PROVEEDOR,
		  A.COD_EMPRESA, E.NOMBRE AS NOM_EMPRESA,
		  A.FLAG_REPLICACION
FROM ALMACEN   		A,
     ORIGEN 			O,
     CENTROS_COSTO 	CC,
     USUARIO     		U,
	  PROVEEDOR			P,
	  EMPRESA			E
WHERE A.COD_ORIGEN 		= O.COD_ORIGEN (+)
  and A.CENCOS 			= CC.CENCOS (+)
  and A.COD_RESPONSABLE = U.COD_USR (+)  
  and A.PROV_ALMACEN		= P.PROVEEDOR (+)
  AND A.COD_EMPRESA		= E.COD_EMPRESA (+)
  and a.cod_empresa		= :as_empresa
ORDER BY A.ALMACEN          ASC    
" update="ALMACEN" updatewhere=0 updatekeyinplace=yes arguments=(("as_empresa", string)) )
