 column=(type=char(1) update=yes updatewhereclause=yes name=flag_estado dbname="articulo.flag_estado" dbalias="a.flag_estado" values="Activo	1/Inactivo	0/" )
 column=(type=char(1) update=yes updatewhereclause=yes name=flag_numero_serie dbname="articulo.flag_numero_serie" dbalias="a.flag_numero_serie" values="Si	1/No	0/" )
 column=(type=char(8) update=yes updatewhereclause=yes name=cod_empresa dbname="articulo.cod_empresa" dbalias="a.cod_empresa" )
 retrieve="select a.cod_art, a.desc_art, 
       a1.cat_art, a1.desc_categoria,
       a.sub_cat_art, a2.desc_sub_cat,
       a.flag_reposicion, 
		 a.sldo_minimo,
		 a.flag_critico, a.und,
		 a.cod_marca, m.nom_marca,
		 a.flag_inventariable,
		 a.codigo_serie,
       a.precio_venta, a.moneda_vta, 
		 a.garantia_cliente, 
		 a.flag_estado,
		 a.flag_numero_Serie,
		 a.cod_empresa
from articulo a,
     articulo_sub_categ a2,
     articulo_categ     a1,
	  marca					m
where a.sub_cat_art = a2.cod_sub_cat
  and a2.cat_art    = a1.cat_art
  and a.cod_marca	  = m.cod_marca (+)
  and a1.cod_empresa = :as_empresa
" update="articulo" updatewhere=0 updatekeyinplace=yes arguments=(("as_empresa", string)) )
