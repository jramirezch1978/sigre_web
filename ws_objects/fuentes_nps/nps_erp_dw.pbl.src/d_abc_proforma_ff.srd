 retrieve="select pr.*, p.nom_proveedor, fp.desc_forma_pago, u.nombre
from proforma   pr,
     proveedor  p,
     forma_pago fp,
	  usuario    u
where pr.vendedor = u.cod_usr
  and pr.cliente = p.proveedor     
  and pr.forma_pago = fp.forma_pago
  and pr.nro_proforma = :as_nro_proforma
" update="proforma" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_proforma", string)) )
