global function boolean f_nro_libro_cobros (ref long al_nro_libro);Boolean lb_retorno = TRUE

SELECT libro_cobranzas
  INTO :al_nro_libro
  FROM finparam
 WHERE (reckey = '1') ;
 
IF Isnull(al_nro_libro) OR al_nro_libro = 0 THEN
	lb_retorno = FALSE
END IF

Return lb_retorno
end function

