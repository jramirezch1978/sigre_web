 column=(type=char(10) updatewhereclause=yes name=operaciones_oper_sec dbname="operaciones.oper_sec" )
 column=(type=char(150) updatewhereclause=yes name=operaciones_descripcion dbname="operaciones.descripcion" )
 column=(type=datetime updatewhereclause=yes name=fec_inicio dbname="fec_inicio" )
 column=(type=char(12) updatewhereclause=yes name=operaciones_codigo dbname="operaciones.codigo" )
 column=(type=char(60) updatewhereclause=yes name=servicios_desc_codigo dbname="servicios.desc_codigo" )
 column=(type=number updatewhereclause=yes name=operaciones_cant_proyect dbname="operaciones.cant_proyect" )
 column=(type=number updatewhereclause=yes name=costo dbname="costo" )
 retrieve="select ot.nro_orden, o.nro_operacion, o.oper_sec, o.desc_operacion as descripcion, trunc(o.fec_inicio) as fec_inicio, 
       o.servicio as codigo, s.descripcion as desc_codigo, o.cant_proyect, nvl(o.costo_unit,1) as costo
from orden_trabajo ot, operaciones o, servicios s 
where (ot.nro_orden = o.nro_orden and 
       o.servicio = s.servicio and 
       o.flag_estado='1' and 
       o.servicio is not null and 
       ot.ot_adm=:as_ot_adm) 
union all        
select ot.nro_orden, o.nro_operacion, o.oper_sec, a.nom_articulo as dscripcion, trunc(amp.fec_proyect) as fec_inicio, 
       amp.cod_art as codigo, a.nom_articulo as desc_codigo, amp.cant_proyect, NVL(amp.precio_unit,1) as costo
from orden_trabajo ot, operaciones o, articulo_mov_proy amp, articulo a 
where (ot.nro_orden = o.nro_orden and 
       o.flag_estado='1' and 
       ot.ot_adm=:as_ot_adm) and 
     (o.oper_sec = amp.oper_sec and 
      amp.tipo_mov=:as_tipo_mov and 
      amp.tipo_doc=:as_tipo_doc) and 
      amp.flag_estado='1' and 
      amp.cod_art = a.cod_art and 
      a.flag_estado ='1'
      
" arguments=(("as_ot_adm", string),("as_tipo_mov", string),("as_tipo_doc", string)) )
