 retrieve="  SELECT PPO.NRO_OPERACION,   
         PPO.NRO_PRECEDENCIA,   
         PPO.COD_LABOR,   
         PPO.COD_EJECUTOR,   
         PPO.NRO_DIAS_INICIO,   
         PPO.DIAS_DURACION,   
         PPO.CANTIDAD,   
         ppo.desc_operacion,   
         PP.COD_PLANTILLA,   
         PP.DESC_PLANTILLA,   
         LA.UND,   
         LE.COSTO_UNITARIO,   
         PPO.OT_SECCION  
    FROM PLANT_PROD_OPER  PPO,   
         LABOR            LA,   
         PLANT_PROD       PP,   
         LABOR_EJECUTOR   LE
   WHERE PPO.COD_LABOR     = LA.COD_LABOR 
     and PPO.COD_PLANTILLA = PP.COD_PLANTILLA 
     and LE.COD_LABOR      = PPO.COD_LABOR 
     and LE.COD_EJECUTOR   = PPO.COD_EJECUTOR
     and PPO.COD_PLANTILLA = :as_cod_plantilla
ORDER BY PPO.COD_PLANTILLA ASC,   
         PPO.NRO_OPERACION ASC   
" arguments=(("as_cod_plantilla", string))  sort="nro_operacion A " )
