 column=(type=number updatewhereclause=yes name=precio_tercero dbname="precio_tercero" )
 retrieve="select tt.cod_labor, l.desc_labor, 
	sum(decode(tt.flag_propiedad,'P', tt.cant_propio, 0)) as cant_propio, 
	sum(decode(tt.flag_propiedad,'T', tt.cant_tercero, 0)) as cant_tercero, 
	decode(tt.flag_propiedad,'P', tt.precio_propio, 0) as precio_propio, 
	decode(tt.flag_propiedad,'T', tt.precio_tercero, 0) as precio_tercero
from tt_ope_valorizacion_labores tt, labor l
where tt.cod_labor = l.cod_labor and
		tt.cod_art is null 
group by tt.cod_labor, l.desc_labor, tt.flag_propiedad, 
			tt.precio_propio, tt.precio_tercero
order by tt.cod_labor" )
