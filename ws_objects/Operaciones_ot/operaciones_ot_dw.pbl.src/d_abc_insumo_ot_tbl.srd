 retrieve="  SELECT AMP.COD_ORIGEN,   
         AMP.NRO_MOV,   
         AMP.FLAG_ESTADO,   
         AMP.COD_ART,   
         AMP.TIPO_MOV,   
         AMP.TIPO_DOC,   
         AMP.NRO_DOC,   
         AMP.FEC_REGISTRO,   
         AMP.FEC_PROYECT, 
			AMP.FEC_PROY_APROB,   
         AMP.CANT_PROYECT,   
         AMP.CANT_PROCESADA,   
			AMP.CANT_RESERVADO,
         AMP.PRECIO_UNIT,   
         AMP.DECUENTO,   
         AMP.IMPUESTO,   
         AMP.COD_MONEDA,   
         AMP.TIPO_REF,   
         AMP.REFERENCIA,   
         AMP.CENCOS,
         CC.DESC_CENCOS,   
         AMP.CNTA_PRSP,   
			PC.DESCRIPCION AS DESC_CNTA_PRSP,
			AMP.ALMACEN,
			AMP.COD_USR,
         AMP.OPER_SEC,
         AMP.FLAG_MODIFICACION ,
         AMP.FLAG_REPLICACION, 
			AMP.FLAG_URGENCIA, 
			ea.descripcion as desc_urgencia,
         AMP.NRO_APROB, 
			OTA.FECHA_APROB,
   		CASE WHEN AMP.FLAG_MODIFICACION = '0' or AMP.FLAG_ESTADO = '1' THEN
				null
			ELSE
				'S'
			END as MODIFICA,
			DECODE(AMP.CANT_PROCESADA,0,'S',NULL) AS ESTADO,
			DECODE(AMP.FLAG_ESTADO,'2',NULL, 'S') AS flag_cerrado, 
			A.DESC_ART,
         A.UND,
         NVL(AA.DIAS_REPOSICION,0) as dias_reposicion,
         NVL(AA.FLAG_REPOSICION, '0') as flag_reposicion,          
			nvl(usf_saldo_total_x_alm(A.COD_ART, AMP.ALMACEN), 0) AS SALDO_TOTAL,
			usf_cmp_cant_cmp_AMP_OT(AMP.COD_ORIGEN, AMP.NRO_MOV) AS CANT_COMPRADA,
         CASE WHEN usf_cmp_cant_cmp_AMP_OT(AMP.COD_ORIGEN, AMP.NRO_MOV) > 0 THEN 
				null 
			ELSE 
				'1' 
			END as flag_cnt_cmp,
			0 AS NRO_OPERACION,
			(select cod_dolares from logparam where reckey = '1') as cod_dolares
    FROM ARTICULO 				A,   
         ARTICULO_MOV_PROY 	AMP,
			CENTROS_COSTO			CC,
			ARTICULO_ALMACEN		AA,
			OT_APROBACIONES		OTA,
			PRESUPUESTO_CUENTA	PC,
			ESTADO_ATENCION_REQ	EA
   WHERE AMP.COD_ART  	= A.COD_ART 
	  AND AMP.ALMACEN	 	= AA.ALMACEN    (+)
 	  AND AMP.COD_ART	 	= AA.COD_ART    (+)
     AND AMP.NRO_APROB	= OTA.NRO_APROB (+)
     AND AMP.CENCOS		= CC.CENCOS
     AND AMP.CNTA_PRSP	= PC.CNTA_PRSP	 
	  and amp.flag_urgencia = ea.flag_urgencia (+)
	  and AMP.TIPO_MOV  	= (select oper_cons_interno from logparam where reckey = '1')
	  AND AMP.TIPO_DOC  	= (select doc_ot from logparam where reckey = '1')
	  AND AMP.OPER_SEC 	= :as_oper_sec 
ORDER BY AMP.COD_ORIGEN ASC, AMP.NRO_MOV ASC, AMP.COD_ART ASC   
" update="ARTICULO_MOV_PROY" updatewhere=0 updatekeyinplace=yes arguments=(("as_oper_sec", string)) )
