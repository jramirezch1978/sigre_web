 retrieve="SELECT     OT.COD_ORIGEN ,
			  OT.NRO_ORDEN ,
           OT.FLAG_ESTADO ,
           OT.FEC_INICIO ,
           OT.COD_USR ,
           OT.DESCRIPCION ,
           OT.OT_ADM ,
           OT.OT_TIPO ,
           OTT.DESCRIPCION ,
           OTA.DESCRIPCION ,
           OT.FEC_REGISTRO , 
           OT.FLAG_ESTRUCTURA 
      FROM ORDEN_TRABAJO 		OT ,
           OT_ADMINISTRACION 	OTA,
           OT_TIPO 				OTT, 
			  OPERACIONES			OPE
     WHERE OT.OT_ADM 			= OTA.OT_ADM 
		 And OT.OT_TIPO 			= OTT.OT_TIPO 
		 AND OT.NRO_ORDEN			= OPE.NRO_ORDEN 
		 and OPE.OPER_SEC 		= :as_oper_sec" arguments=(("as_oper_sec", string)) )
