 retrieve="  SELECT ~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TRABAJADOR~",  
         ~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TJT~",  
         ~"TT_OPE_TEFECTIVO_X_TRAB~".~"CODPRD~",   
         ~"PRODUCTO_PESAJE~".~"DESCRIPCION~",   
         Nvl(maestro.apel_paterno,' ') ||' '|| Nvl(maestro.apel_materno,' ') ||' '|| Nvl(maestro.nombre1,' ') ||' '|| Nvl(maestro.nombre2,' ') as nombres  ,
			count (~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TRABAJADOR~") as nro_pesadas,
			Sum(tt_ope_tefectivo_x_trab.peso) as peso,
			Sum(tt_ope_tefectivo_x_trab.hora_efectiva) as hefectiva,
			Round(Sum(tt_ope_tefectivo_x_trab.peso) / Sum(tt_ope_tefectivo_x_trab.hora_efectiva),3) as promedio

    FROM ~"TT_OPE_TEFECTIVO_X_TRAB~",   
         ~"MAESTRO~",   
         ~"PRODUCTO_PESAJE~"  
   WHERE ( ~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TRABAJADOR~" = ~"MAESTRO~".~"COD_TRABAJADOR~" ) and  
         ( ~"TT_OPE_TEFECTIVO_X_TRAB~".~"CODPRD~"         = ~"PRODUCTO_PESAJE~".~"CODPRD~" ) and
			( ~"TT_OPE_TEFECTIVO_X_TRAB~".~"CODPRD~"         LIKE :as_producto  ) 
GROUP BY ~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TRABAJADOR~", 
   ~"TT_OPE_TEFECTIVO_X_TRAB~".~"COD_TJT~",    
         ~"TT_OPE_TEFECTIVO_X_TRAB~".~"CODPRD~",   
         ~"PRODUCTO_PESAJE~".~"DESCRIPCION~",   
         Nvl(maestro.apel_paterno,' ') ||' '|| Nvl(maestro.apel_materno,' ') ||' '|| Nvl(maestro.nombre1,' ') ||' '|| Nvl(maestro.nombre2,' ') 
" arguments=(("ad_fecha", date),("as_producto", string))  sort="codprd A peso D " )
