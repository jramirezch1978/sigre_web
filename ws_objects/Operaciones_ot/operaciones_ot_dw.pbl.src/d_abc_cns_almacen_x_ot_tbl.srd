 retrieve="select a.cod_art, 
     a.desc_art,
     vm.nro_vale,
	  nvl(am.precio_unit, 0) as precio_un_sol,  
     nvl(am.cant_procesada, 0) as cant_proc, 
	  nvl(c.vta_dol_prom, 0) as tipo_cambio, 
	  round((nvl(am.precio_unit, 0)) / nvl(c.vta_dol_prom, 0), 2) as precio_dolar, 
	  round((nvl(am.precio_unit, 0) * nvl(am.cant_procesada, 0)) / nvl(c.vta_dol_prom, 0), 2) as total, 
	  am.oper_sec
   from vale_mov vm,
      articulo_mov am,
      calendario c,
      articulo a 
   where vm.cod_origen = am.cod_origen 
      and trunc(vm.fec_registro) = trunc(c.fecha)
      and am.cod_art = a.cod_art 
      and vm.nro_vale = am.nro_vale
      and am.oper_sec is not null 
      and am.flag_estado <> 0
      and am.oper_sec in (select op.oper_sec
         from operaciones op
         where op.nro_orden = :asi_nro_orden
            and op.cod_labor = :asi_cod_labor
            and op.cod_ejecutor = :asi_cod_ejecutor)
   order by a.cod_art" arguments=(("asi_nro_orden", string),("asi_cod_labor", string),("asi_cod_ejecutor", string)) )
