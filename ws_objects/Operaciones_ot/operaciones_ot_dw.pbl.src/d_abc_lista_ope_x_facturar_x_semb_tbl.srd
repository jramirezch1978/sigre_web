 column=(type=char(8) updatewhereclause=yes name=cod_labor dbname="operaciones.cod_labor" )
 column=(type=char(40) updatewhereclause=yes name=desc_operacion dbname="operaciones.desc_operacion" )
 column=(type=char(4) updatewhereclause=yes name=cod_ejecutor dbname="operaciones.cod_ejecutor" )
 column=(type=char(10) updatewhereclause=yes name=oper_sec dbname="operaciones.oper_sec" )
 column=(type=datetime updatewhereclause=yes name=fec_inicio dbname="operaciones.fec_inicio" )
 column=(type=number updatewhereclause=yes name=precio dbname="facturacion_operacion.precio" )
 column=(type=char(0) updatewhereclause=yes name=flag dbname="flag" values=" Facturar	1/ Facturar	0" )
 retrieve="select op.nro_orden     ,
       op.cod_labor     ,
       op.desc_operacion,
       op.cod_ejecutor  ,
       op.oper_sec      ,
       op.fec_inicio    ,
       0.00 as precio   ,
       '' as flag
  from facturacion_operacion fo, operaciones op
 where  (fo.oper_sec    = op.oper_sec      ) and
        (fo.item        = 1                ) and
        (op.flag_estado = '2'              ) and
		  (op.proveedor   = :as_cod_relacion ) and  
		  (op.cod_ejecutor= :as_ejecutor		 ) and        
       ((to_char(op.fec_inicio,'yyyymmdd') >= :as_fecha_inicio )  and
        (to_char(op.fec_inicio,'yyyymmdd') <= :as_fecha_final  )) and
		  (fo.oper_sec		in (select oper_sec from facturacion_operacion where item = 1
									 minus
									 select oper_sec from facturacion_operacion where item = 2))" arguments=(("as_cod_relacion", string),("as_ejecutor", string),("as_fecha_inicio", string),("as_fecha_final", string)) )
