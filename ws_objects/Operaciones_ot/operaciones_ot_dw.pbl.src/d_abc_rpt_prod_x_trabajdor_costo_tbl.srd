 retrieve="  select tt_mm.cod_trabajador ,
         tt_mm.codprd         ,   
         tt_mm.rend_max       ,   
         to_char(tt_mm.fecha_rend_max ,'dd/mm/yyyy hh24') as fecha_rend_max,   
         tt_mm.rend_mind      ,   
         to_char(tt_mm.fecha_rend_min,'dd/mm/yyyy hh24')  as fecha_rend_min ,
         nvl(m.apel_materno,' ') ||' '|| nvl(m.apel_paterno,' ') ||' '||nvl(m.nombre1,' ')||' '||nvl(m.nombre2,' ') as nombres  ,
         (select round(sum(tt.peso) / sum(tt.hora_efectiva ),2) as rendimiento_x_periodo
           from tt_ope_tefectivo_x_trab tt
          where tt.cod_trabajador = tt_mm.cod_trabajador and
                tt.codprd         = tt_mm.codprd         
       group by tt.cod_trabajador,tt.codprd) as rend_X_periodo
         
    from tt_ope_trend_max_min tt_mm ,maestro m
   where ( tt_mm.cod_trabajador = m.cod_trabajador )  and
			   ( tt_mm.cod_trabajador like :as_cod_trabajador      )" arguments=(("ad_fecha_inicio", date),("ad_fecha_final", date),("as_cod_trabajador", string))  sort="cod_trabajador A codprd A " )
