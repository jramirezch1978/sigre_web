 column=(type=number updatewhereclause=yes name=avance dbname="avance" )
 column=(type=number updatewhereclause=yes name=ratio_costo dbname="ratio_costo" )
 column=(type=number updatewhereclause=yes name=ratio_avance dbname="ratio_avance" )
 retrieve="select op.cod_labor      ,
       op.desc_operacion ,
       lb.und            ,
       op.cod_ejecutor   ,
       am.cod_art        ,
       art.nom_articulo  ,
       art.und           ,
       vm.fec_registro   ,
       vm.cod_usr        ,
		 decode(lb.flag_tipo_has,'1','Factor','T','Has Totales','N','Has netas','H','Tonelada','C','Caja') AS und_avance,
       sum(am.cant_procesada) as cantidad  ,     
       Round(Nvl(am.precio_unit,0) / usf_fin_tasa_cambio(sysdate),2) as costo_unit,
       Round(sum(am.cant_procesada) * Round(Nvl(am.precio_unit,0) / usf_fin_tasa_cambio(sysdate),2),2) as   total_us,
       Decode(Nvl(sum(op.avance_esperado),0),0,1,Nvl(sum(op.avance_esperado),0)) as avance,
       Round(Round(sum(am.cant_procesada) * Round(Nvl(am.precio_unit,0) / usf_fin_tasa_cambio(sysdate),2),2) / Decode(sum(am.cant_procesada),0,1,sum(am.cant_procesada)),2) as ratio_costo,
       Round(Round(sum(am.cant_procesada) * Round(Nvl(am.precio_unit,0) / usf_fin_tasa_cambio(sysdate),2),2) / Decode(Nvl(sum(op.avance_esperado),0),0,1,Nvl(sum(op.avance_esperado),0)),2) as ratio_avance
  from operaciones op,labor lb,articulo_mov am,articulo art,vale_mov vm
 where (op.cod_labor   = lb.cod_labor  ) and
       (op.oper_sec    = am.oper_sec   ) and
       (am.cod_art     = art.cod_art   ) and
		 (am.flag_estado <> '0' 		   ) and
		 (op.tipo_orden  = (select doc_ot from prod_param where reckey = '1') ) and
       (am.cod_origen  = vm.cod_origen ) and
       (am.nro_vale    = vm.nro_vale   ) and
		 (op.nro_orden	  = :as_nro_orden )
group by op.cod_labor         ,
         op.desc_operacion    ,
         lb.und               ,
         op.cod_ejecutor      ,
         am.cod_art           ,
         art.nom_articulo     ,
         art.und              ,
         Nvl(am.precio_unit,0),
         vm.fec_registro      ,
         vm.cod_usr        	,
			lb.flag_tipo_has
       " arguments=(("as_nro_orden", string))  sort="operaciones_cod_labor A operaciones_desc_operacion A labor_und A operaciones_cod_ejecutor A " )
