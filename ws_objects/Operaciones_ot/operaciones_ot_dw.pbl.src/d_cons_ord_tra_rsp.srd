 retrieve="SELECT ~"TT_OPE_OT~".~"NRO_ORDEN~"		  ,   
         ~"TT_OPE_OT~".~"CENCOS_SLC~"	  ,   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~",   
         ~"TT_OPE_OT~".~"FEC_SOLICITUD~"  ,   
         ~"TT_OPE_OT~".~"FEC_INICIO~"     ,   
         ~"TT_OPE_OT~".~"DESCRIPCION~"    ,   
         ~"TT_OPE_OT~".~"NRO_SOLICITUD~"  ,   
         ~"TT_OPE_OT~".~"FLAG_ESTADO~"    ,   
         ~"TT_OPE_OT~".~"COD_MAQUINA~"    ,   
         ~"MAQUINA~".~"DESC_MAQ~"         ,   
         ~"TT_OPE_OT~".~"CLIENTE~"        ,   
         ~"CODIGO_RELACION~".~"NOMBRE~"  
    FROM ~"TT_OPE_OT~"      ,   
         ~"CENTROS_COSTO~"  ,   
         ~"MAQUINA~"        ,   
         ~"CODIGO_RELACION~"  
   WHERE ( ~"TT_OPE_OT~".~"COD_MAQUINA~" = ~"MAQUINA~".~"COD_MAQUINA~"          (+)) and  
         ( ~"TT_OPE_OT~".~"CLIENTE~"     = ~"CODIGO_RELACION~".~"COD_RELACION~" (+)) and  
         ( ~"TT_OPE_OT~".~"CENCOS_SLC~"  = ~"CENTROS_COSTO~".~"CENCOS~"            ) and
			( ~"TT_OPE_OT~".~"FLAG_ESTADO~" IN (:asa_estado))  
ORDER BY ~"TT_OPE_OT~".~"NRO_ORDEN~" ASC

