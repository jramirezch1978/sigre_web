 column=(type=number updatewhereclause=yes name=factor dbname="pd_ot_asistencia.factor" )
 column=(type=number updatewhereclause=yes name=costo_total dbname="costo_total" )
 retrieve="select 'JORNAL' as tipo, poa.cod_trabajador, cr.nombre, poa.nro_horas, 'HRS' as und, poa.costo_horas, poa.factor, round(nvl(poa.nro_horas, 0) * nvl(poa.costo_horas, 0) * nvl(poa.factor, 0), 2) as costo_total
   from pd_ot_asistencia poa,
      codigo_relacion cr
   where poa.cod_trabajador = cr.cod_relacion(+)
      and poa.no_parte = :ls_no_parte
      and poa.nro_item = :ls_nro_item

union all

select 'DESTAJO' as tipo, poad.cod_trabajador, cr.nombre, poad.cant_destajada, l.und, poad.tarifa_normal, poad.factor_calculo, round(nvl(poad.cant_destajada, 0) * nvl(poad.tarifa_normal, 0) * nvl(poad.factor_calculo, 0))
   from pd_ot_asist_destajo poad,
		pd_ot_det pod,
      codigo_relacion cr,
      labor l 
   where pod.nro_parte = poad.nro_parte(+)
		and pod.nro_item = poad.nro_item(+)
		and poad.cod_trabajador = cr.cod_relacion(+)
		and pod.cod_labor = l.cod_labor(+)
		and poad.nro_parte = :ls_no_parte
      and poad.nro_item = :ls_nro_item" arguments=(("ls_no_parte", string),("ls_nro_item", number)) )
