 retrieve="select tt.cod_maquina, m.desc_maq, 
       tt.ot_adm, ota.descripcion, 
       tt.cod_incidencia, i.desc_incidencia, 
       sum(tt.nro_horas) as horas
from tt_ope_incidencias_maq_ot_adm tt, maquina m, ot_administracion ota, incidencias_dma i
where tt.cod_maquina=m.cod_maquina(+) and
      tt.ot_adm=ota.ot_adm(+) and
      tt.cod_incidencia=i.cod_incidencia(+)
group by tt.cod_maquina, m.desc_maq, 
      tt.ot_adm, ota.descripcion, 
      tt.cod_incidencia, i.desc_incidencia
order by tt.cod_maquina, tt.ot_adm, tt.cod_incidencia" )
