 retrieve="select distinct 
		 ot.ot_adm,
		 amp.oper_sec, 
		 amp.cod_art,
		 amp.cod_origen,
		 amp.nro_mov, 
		 a.desc_art, 
		 a.und,
		 amp.almacen,
       amp.tipo_doc, amp.nro_doc,
		 amp.flag_estado,
		 amp.cnta_prsp,
		 a.costo_ult_compra,
       usf_cmp_fec_proyect(amp.cod_origen, amp.nro_mov) as fec_proyect,
       usf_cmp_cant_proyect(amp.cod_origen, amp.nro_mov) as cant_proyect,
       usf_cmp_cant_programada(amp.cod_origen, amp.nro_mov) as cant_programada,
       usf_cmp_cant_comp_prog(amp.cod_origen, amp.nro_mov) as cant_comp_prog,
       usf_cmp_cant_comp_sprog(amp.cod_origen, amp.nro_mov) as cant_comp_sprog,
       usf_cmp_cant_ingresada(amp.cod_origen, amp.nro_mov) as cant_ingresada,
       usf_cmp_cant_procesada(amp.cod_origen, amp.nro_mov) as cant_procesada
from articulo_mov_proy amp,
	  articulo a, orden_trabajo ot
where a.cod_art = amp.cod_art
  and amp.nro_doc = ot.nro_orden
  and amp.flag_estado <> '0'
  and amp.tipo_doc = (select doc_ot from logparam where reckey = '1')
  and trunc(amp.fec_proyect) >= :ad_fecha1
  and trunc(amp.fec_proyect) <= :ad_fecha2
  and ot.ot_adm in (select ot_adm from tt_ot_adm)
  and amp.nro_doc like :as_nro_ot
order by tipo_doc, nro_doc
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_nro_ot", string))  sort="ot_adm A tipo_doc A nro_doc A " )
