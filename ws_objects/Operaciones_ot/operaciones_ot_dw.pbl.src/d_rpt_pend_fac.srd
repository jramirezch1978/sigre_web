 column=(type=char(1) updatewhereclause=yes name=operaciones_flag_estado dbname="operaciones.flag_estado" values="Anulado	0/Activo	1/Terminado	2/Planeado	3/Facturado	4/Activo no Modificación	5/Aprobado por Gerencia	6/" )
 retrieve="select ot.ot_adm, oad.descripcion, o.proveedor, p.nom_proveedor, ot.nro_orden, 
		 o.cod_labor, l.desc_labor, o.cod_ejecutor, o.oper_sec, o.cant_real, o.has, 
     l.und, o.fec_inicio, cc.cod_campo, o.flag_facturacion, o.flag_estado
from orden_trabajo ot, operaciones o, labor l, proveedor p, 
     ot_administracion oad, campo_ciclo cc
where ot.nro_orden=o.nro_orden and
      ot.ot_adm = oad.ot_adm and
      ot.ot_adm = :as_ot_adm and
      o.cod_labor = l.cod_labor and
      o.proveedor = p.proveedor and
      o.cant_real > 0 and
		  o.nro_orden = cc.nro_orden(+) and
      o.oper_sec not in (SELECT f.oper_sec FROM facturacion_operacion f WHERE f.oper_sec=o.oper_sec) and
      ( o.fec_inicio between trunc(:ad_fecha_ini) and trunc(:ad_fecha_fin)+0.999) 
order by o.proveedor, o.cod_ejecutor" arguments=(("as_ot_adm", string),("ad_fecha_ini", date),("ad_fecha_fin", date)) )
