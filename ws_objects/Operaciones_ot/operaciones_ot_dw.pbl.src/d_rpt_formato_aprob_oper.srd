 column=(type=char(3) updatewhereclause=yes name=labor_ejecutor_cod_moneda dbname="labor_ejecutor.cod_moneda" dbalias="le.cod_moneda" )
 retrieve="select op.nro_aprob, op.nro_orden, op.cod_labor, op.cod_ejecutor, 
op.desc_operacion, op.obs, op.fec_inicio, op.cant_proyect, op.costo_unit, 
(op.cant_proyect * le.costo_unitario) as importe, cod_moneda 
from operaciones op, labor_ejecutor le
where op.nro_aprob = :as_aprob and
op.flag_estado = '1' AND 
op.cod_labor = le.cod_labor AND 
op.cod_ejecutor = le.cod_ejecutor
order by op.nro_orden" arguments=(("as_aprob", string)) )
