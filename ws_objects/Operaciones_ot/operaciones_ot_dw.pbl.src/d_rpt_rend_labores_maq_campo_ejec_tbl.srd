 retrieve="select pd.cod_labor, l.desc_labor, l.und, pd.cod_ejecutor, 
       cc.corr_corte, c.desc_campo, pd.nro_orden, 
		 sum(nvl(pd.cant_labor,0)-nvl(pd.horas_inciden,0)) as cant, 
		 sum(nvl(pd.cant_avance,0)) as avance
from orden_trabajo ot, operaciones o, pd_ot p, pd_ot_det pd, labor l, campo_ciclo cc, campo c
where ot.nro_orden=o.nro_orden and
      o.oper_sec=pd.oper_sec and
      pd.nro_parte=p.nro_parte and
      ot.nro_orden=cc.nro_orden and
      cc.cod_campo=c.cod_campo and 
      pd.cod_labor=l.cod_labor and
      pd.cod_maquina is not null and
		pd.cod_ejecutor = :as_ejecutor and
      trunc(p.fecha) between :ad_fec_ini and :ad_fec_fin 
group by pd.cod_labor, l.desc_labor, l.und, pd.cod_ejecutor, cc.corr_corte, c.desc_campo, pd.nro_orden
order by pd.cod_labor, pd.cod_ejecutor, cc.corr_corte" arguments=(("as_ejecutor", string),("ad_fec_ini", date),("ad_fec_fin", date)) )
