 column=(type=char(100) updatewhereclause=yes name=operaciones_desc_operacion dbname="operaciones.desc_operacion" )
 column=(type=char(3) updatewhereclause=yes name=und dbname="labor.und" )
 column=(type=char(4) updatewhereclause=yes name=cod_ejecutor dbname="operaciones.cod_ejecutor" )
 column=(type=number updatewhereclause=yes name=cant_proyect dbname="cant_proyect" )
 column=(type=decimal(4) updatewhereclause=yes name=costo_unitario dbname="labor_ejecutor.costo_unitario" )
 column=(type=char(11) updatewhereclause=yes name=und_avance dbname="und_avance" )
 column=(type=number updatewhereclause=yes name=total_us dbname="total_us" )
 column=(type=number updatewhereclause=yes name=avance_esperado dbname="avance_esperado" )
 column=(type=number updatewhereclause=yes name=ratio_costo dbname="ratio_costo" )
 column=(type=number updatewhereclause=yes name=ratio_avance dbname="ratio_avance" )
 retrieve="select o.cod_labor       ,
		 o.desc_operacion  ,
       l.und             ,
		 o.cod_ejecutor    ,
		 SUM(o.cant_proyect) AS cant_proyect ,
       le.costo_unitario ,
		 decode(l.flag_tipo_has,'1','Factor','T','Has Totales','N','Has netas','H','Tonelada','C','Caja') AS und_avance,

       Round(Nvl(sum(o.cant_proyect),0) * Nvl(le.costo_unitario,0),2)     as total_us       ,
       Decode(Nvl(sum(o.avance_esperado),1),0,1,Nvl(sum(o.avance_esperado),1)) as avance_esperado,
       Round ((Nvl(sum(o.cant_proyect),0) * Nvl(le.costo_unitario,0)) / Decode(Nvl(sum(o.cant_proyect),1),0,1,Nvl(sum(o.cant_proyect),1)),2) as ratio_costo,
       (Nvl(sum(o.cant_proyect),0) * Nvl(le.costo_unitario,0)) / Decode (Nvl(sum(o.avance_esperado),1),0,1,Nvl(sum(o.avance_esperado),1)) as ratio_avance
  from operaciones o,labor l,labor_ejecutor le
 where (o.cod_labor    = l.cod_labor     ) and
       (o.cod_labor    = le.cod_labor    ) and
       (o.cod_ejecutor = le.cod_ejecutor ) and
       (o.cod_ejecutor not in (select ejecutor_3ro from prod_param where reckey = '1') ) and
       (o.tipo_orden   = (select doc_ot from prod_param where reckey = '1') ) and
       (o.nro_orden    = :as_nro_orden    ) 
group by o.cod_labor       ,
		   o.desc_operacion  ,
         l.und             ,
		   o.cod_ejecutor    ,
         le.costo_unitario ,
			l.flag_tipo_has" arguments=(("as_nro_orden", string)) )
