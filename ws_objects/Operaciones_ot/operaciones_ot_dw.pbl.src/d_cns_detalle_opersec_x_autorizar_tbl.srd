 retrieve="select ot.nro_orden, ot.descripcion, 
       ot.ot_adm, ot.cencos_slc, 
       op.NRO_OPERACION, op.COD_LABOR,
			op.COD_EJECUTOR, op.PROVEEDOR,
			op.NRO_PERSONAS, op.DESC_OPERACION,
			op.DIAS_DURACION_PROY, op.FEC_INICIO,
			op.CANT_PROYECT, op.COSTO_UNIT,		
			op.OBS, op.DIAS_HOLGURA,
			op.AVANCE_ESPERADO, op.AVANCE_UND,
			op.OPER_SEC, la.desc_labor,
			la.und, ej.descripcion as desc_ejecutor,
			p.nom_proveedor
from 	operaciones	op,
      orden_trabajo ot, 
		  Labor			la,
		  Ejecutor		ej,
		  Proveedor	p
where (op.nro_orden = ot.nro_orden)
  and (la.cod_labor 		= op.cod_labor)
  and (op.cod_ejecutor 	= ej.cod_ejecutor)
  and op.proveedor		= p.proveedor		(+)
  and op.oper_sec = :as_oper_sec
" arguments=(("as_oper_sec", string)) )
