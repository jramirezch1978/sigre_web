 retrieve="select m.cod_maquina,m.desc_maq,vm.nro_vale,am.cencos,vm.fec_registro,am.cod_art,a.desc_art,
       a.und,am.cant_procesada,
       round(((am.cant_procesada * am.precio_unit) / (select nvl(vta_dol_prom,0) from calendario where fecha = trunc(vm.fec_registro))),2)
from maquina m,articulo a, articulo_mov am, vale_mov vm, ot_administracion oa, orden_trabajo ot,
     operaciones o
where m.cod_maquina = am.cod_maquina
  and vm.cod_origen = am.cod_origen and vm.nro_vale = am.nro_vale
  and am.cod_art = a.cod_art and am.flag_estado <> '0' and vm.flag_estado <> '0'
  and am.oper_sec = o.oper_sec 
  and o.nro_orden = ot.nro_orden 
  and ot.ot_adm = oa.ot_adm 
  and vm.fec_registro >= :ld_fec_ini and vm.fec_registro <= :ld_fec_fin
  and oa.ot_adm = :ls_ot and m.cod_maquina in (select cod_maquina from tt_maquina)
" arguments=(("ld_fec_ini", datetime),("ld_fec_fin", datetime),("ls_ot", string),("as_empresa", string),("as_user", string))  sort="maquina_cod_maquina A maquina_desc_maq A " )
