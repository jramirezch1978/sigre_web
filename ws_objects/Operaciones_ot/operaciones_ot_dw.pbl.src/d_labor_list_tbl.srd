 retrieve="SELECT  L.COD_LABOR          ,
        L.DESC_LABOR         ,
        lf.cod_fase          ,
        lf.desc_fase         ,
        L.COD_ETAPA          ,
        le.desc_etapa        ,
        L.FLAG_ESTADO        ,
        L.FLAG_INCIDENCIA    ,
        L.FLAG_MARCADOR      ,
        L.FLAG_MAQ_MO        ,
        L.FLAG_TIPO_HAS      ,
        L.CNTA_OPER          ,
        L.UND                ,
        L.FLAG_USO           ,
        L.FLAG_TRANSPORTE    ,
        L.CNTA_PRSP          , PC1.DESCRIPCION as desc_cnta_prsp,
        CC.Desc_Cnta,
        L.CNTA_PRSP_3RO      , PC2.DESCRIPCION as desc_cnta_prsp_3ro,
        L.CNTA_PRSP_INSM     , PC3.descripcion as desc_cnta_prsp_insm,
        L.DESC_EXTENDIDA     ,
        L.VAL_VENTA_UND_OPER ,
        L.VAL_VENTA_AVANCE   ,
        L.VAL_VENTA_UND_MAQ  , 
        L.FLAG_REPLICACION   ,
        L.CONCEPTO_RRHH,
        L.SUB_CAT_SERV_3RO,
        L.FLAG_JORNAL_DESTAJO,
        c.DESC_CONCEP AS DESC_CONCEPTO,
        a3.desc_sub_Cat as desc_sub_cat_serv_3ro
   FROM LABOR                L,
        PRESUPUESTO_CUENTA   PC1,
        CNTBL_CNTA           CC,
        PRESUPUESTO_CUENTA   PC2 ,
        PRESUPUESTO_CUENTA   PC3,
        CONCEPTO             C,
        labor_etapa          le,
        labor_fase           lf,
        articulo_sub_categ   a3
WHERE   L.CNTA_PRSP      = PC1.CNTA_PRSP (+) 
  and   L.CNTA_OPER      = CC.CNTA_CTBL  (+)
  and   L.CNTA_PRSP_3RO  = PC2.CNTA_PRSP (+)
  and   L.CNTA_PRSP_INSM = PC3.CNTA_PRSP (+)
  and   L.CONCEPTO_RRHH  = C.CONCEP      (+)
  and   L.COD_ETAPA      = le.cod_etapa  (+)
  and   le.cod_fase      = lf.cod_fase   (+)
  and   L.SUB_CAT_SERV_3RO = a3.cod_sub_cat (+)
ORDER BY lf.cod_fase, L.COD_ETAPA ASC,  L.COD_LABOR ASC  " update="LABOR" updatewhere=0 updatekeyinplace=yes  sort="cod_fase A cod_etapa A cod_labor A " )
