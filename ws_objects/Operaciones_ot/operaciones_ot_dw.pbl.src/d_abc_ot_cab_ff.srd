 retrieve="SELECT  ~"PD_OT~".~"NRO_PARTE~" ,
        ~"PD_OT~".~"FECHA~" ,
        ~"PD_OT~".~"COD_SUPERVISOR~" ,
        ~"PD_OT~".~"COD_ADMINISTRADOR~" ,
		  ~"PD_OT~".~"OT_ADM~"		,
		  ~"OT_ADMINISTRACION~".~"DESCRIPCION~",
        LTRIM(RTRIM(~"MAESTRO_A~".~"NOMBRE1~"))||'  '||LTRIM(RTRIM(~"MAESTRO_A~".~"APEL_PATERNO~"))||' '||LTRIM(RTRIM(~"MAESTRO_A~".~"APEL_MATERNO~")) as nom_supervisor,
        LTRIM(RTRIM(~"MAESTRO_B~".~"NOMBRE1~"))||'  '||LTRIM(RTRIM(~"MAESTRO_B~".~"APEL_PATERNO~"))||' '||LTRIM(RTRIM(~"MAESTRO_B~".~"APEL_MATERNO~")) as nom_administrador  ,
		 ~"PD_OT~".~"OBS~" ,
       ~"PD_OT~".~"FLAG_REPLICACION~"			  

  FROM ~"PD_OT~" ,
       ~"MAESTRO~" ~"MAESTRO_A~" ,
       ~"MAESTRO~" ~"MAESTRO_B~" ,
		 ~"OT_ADMINISTRACION~"    
  WHERE (~"PD_OT~".~"COD_SUPERVISOR~"    = ~"MAESTRO_A~".~"COD_TRABAJADOR~" (+)) and 
        (~"PD_OT~".~"COD_ADMINISTRADOR~" = ~"MAESTRO_B~".~"COD_TRABAJADOR~" (+)) and
		  (~"PD_OT~".~"OT_ADM~"				 = ~"OT_ADMINISTRACION~".~"OT_ADM~"	  ) and	
       ((~"PD_OT~".~"NRO_PARTE~"         = :as_nro_parte                   ))  " update="PD_OT" updatewhere=1 updatekeyinplace=yes arguments=(("as_nro_parte", string)) )
