 column=(type=decimal(4) updatewhereclause=yes name=articulo_sldo_total dbname="articulo.sldo_total" )
 column=(type=number updatewhereclause=yes name=cant_requerida dbname="cant_requerida" )
 retrieve="SELECT am.cod_art, a.nom_articulo, a.und, 
       a.costo_ult_compra, a.sldo_total, 
       sum(am.cant_proyect - am.cant_procesada) as cant_requerida
FROM operaciones o, orden_trabajo ot, articulo_mov_proy am,
     articulo a
WHERE o.oper_sec = am.oper_sec and
      am.cod_art = a.cod_art and
      (am.cant_proyect - am.cant_procesada)>0 and
      am.flag_estado <> '0' and
      o.fec_inicio between trunc(:ad_fecha_ini) and
                           trunc(:ad_fecha_fin) + 0.999 
GROUP BY am.cod_art, a.nom_articulo, a.und, a.costo_ult_compra, a.sldo_total
ORDER BY am.cod_art
" arguments=(("ad_fecha_ini", date),("ad_fecha_fin", date)) )
