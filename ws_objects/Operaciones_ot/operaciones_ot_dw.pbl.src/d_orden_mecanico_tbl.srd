         ~"MAESTRO~"  
   WHERE ( orden_mecanico.cod_trabajador = maestro.cod_trabajador (+)) and  
         (( ~"ORDEN_MECANICO~".~"NRO_ORDEN~"  = :as_nro_orden  ))    
