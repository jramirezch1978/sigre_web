 retrieve="SELECT ~"OPERACIONES~".~"NRO_ORDEN~"     ,   
		 ~"OPERACIONES~".~"COD_LABOR~"     ,
		 ~"OPERACIONES~".~"DESC_OPERACION~",   
		'' AS DESCRIPCION,
       ~"OPERACIONES~".~"COD_EJECUTOR~"  ,   
       ~"OPERACIONES~".~"OPER_SEC~"      ,   
       ~"OPERACIONES~".~"CANT_PROYECT~"  ,   
       ~"OPERACIONES~".~"CANT_REAL~"     ,
		 ~"LABOR~".~"UND~"					AS UND,

		 ~"OPERACIONES~".~"FEC_INICIO~"    ,
		 ~"OPERACIONES~".~"FEC_FIN~"       ,
		 '' as flag       				 ,
		 ~"OPERACIONES~".~"CANT_REAL~"  as cantidad					 ,
		 ~"LABOR~".~"VAL_VENTA_UND_OPER~"  as precio						 ,	
		 ~"OPERACIONES~".~"AVANCE_ACTUAL~" ,
		'HAS' AS UND_HAS,
		 ~"LABOR~".~"VAL_VENTA_UND_OPER~"	 ,
		 ~"LABOR~".~"VAL_VENTA_AVANCE~"	 ,
		null as flag_duplicar			 ,
		'F'  as flag_fact			 
  FROM ~"OPERACIONES~" ,
		 ~"LABOR~"  
 WHERE ~"OPERACIONES~".~"OPER_SEC~" in (select oper_sec from operaciones op
												  where (op.flag_estado = '2'              ) and
														  (op.proveedor        = :as_cod_relacion ) and
														  (op.cod_ejecutor     <> :as_ejecutor    ) and
														  (op.flag_facturacion <> 'N'             ) and	
														  (TO_CHAR(op.fec_inicio,'yyyymmdd') >= :as_fecha_inicio ) and
														  (TO_CHAR(op.fec_inicio,'yyyymmdd') <= :as_fecha_final  ) 
												  minus
												  select oper_sec from facturacion_operacion ) AND
      (~"OPERACIONES~".~"COD_LABOR~" = ~"LABOR~".~"COD_LABOR~")
		" arguments=(("as_cod_relacion", string),("as_ejecutor", string),("as_fecha_inicio", string),("as_fecha_final", string))  sort="fec_inicio A " )
