 retrieve="select o.ot_adm, o.cod_usr, u.nombre, o.flag_aprobador, NVL(o.flag_conformidad_os,'0') as flag_conformidad_os,
		 NVL(o.flag_aprob_serv3ro,'0') as flag_aprob_serv3ro, 
       oa.imp_max_aprob_ot, oa.imp_max_aprob_grp, oa.imp_max_aprob_amp, 
       oa.imp_max_mensual, oa.saldo_aprob_mes, 
		 oa.nivel_aprob, oa.flag_saldo_disp 
from ot_adm_usuario o, ot_adm_aprob_req oa, usuario u 
where ( o.ot_adm = oa.ot_adm(+) and o.cod_usr = oa.cod_usr(+) )
  and ( o.cod_usr = u.cod_usr ) 
  and ( u.flag_estado <> '0' ) 
  and   o.ot_adm = :as_ot_adm 
order by o.ot_adm, o.cod_usr" arguments=(("as_ot_adm", string)) )
