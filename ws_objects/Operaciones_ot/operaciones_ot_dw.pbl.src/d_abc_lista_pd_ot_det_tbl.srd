			( ~"PD_OT_DET~".~"NRO_ORDEN~"    = ~"CAMPO_CICLO~".~"NRO_ORDEN~"  (+)) and
         ( ~"CAMPO_CICLO~".~"COD_CAMPO~" = ~"CAMPO~".~"COD_CAMPO~" (+)) and
