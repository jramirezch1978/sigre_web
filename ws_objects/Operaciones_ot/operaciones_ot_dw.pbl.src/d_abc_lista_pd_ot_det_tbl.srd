				~"CAMPO~".~"COD_CAMPO~"        ,
			  ~"CAMPO~".~"DESC_CAMPO~"       ,
			  (select und_hr from prod_param where reckey = '1') as und_hrs	,
           ~"PD_OT_DET~".~"FLAG_REPLICACION~" 
        FROM ~"PD_OT_DET~" ,
           ~"LABOR~"       ,
