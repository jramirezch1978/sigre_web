 column=(type=char(3) updatewhereclause=yes name=articulo_und dbname="articulo.und" )
 column=(type=decimal(4) updatewhereclause=yes name=articulo_costo_ult_compra dbname="articulo.costo_ult_compra" )
 column=(type=decimal(4) updatewhereclause=yes name=articulo_sldo_total dbname="articulo.sldo_total" )
 column=(type=number updatewhereclause=yes name=total_cant_proy dbname="total_cant_proy" )
 retrieve="SELECT am.cod_art, a.nom_articulo, a.und, a.costo_ult_compra, 
		 a.sldo_total, sum(am.cant_proyect) as total_cant_proy
FROM programa_trabajo_det_rh pr, operaciones o, 
     orden_trabajo ot, articulo_mov_proy am,
     centros_costo cc, articulo a
WHERE pr.oper_sec = o.oper_sec and
      o.nro_orden = ot.nro_orden and
      o.oper_sec = am.oper_sec and
      ot.cencos_rsp = cc.cencos and
      am.cod_art = a.cod_art and
      am.flag_estado <> '0' and
      pr.fecha_ini_estimada between trunc(:ad_fecha_ini) and
                           trunc(:ad_fecha_fin) + 0.999 
GROUP BY am.cod_art, a.nom_articulo, a.und, a.costo_ult_compra, a.sldo_total
ORDER BY am.cod_art
" arguments=(("ad_fecha_ini", date),("ad_fecha_fin", date)) )
