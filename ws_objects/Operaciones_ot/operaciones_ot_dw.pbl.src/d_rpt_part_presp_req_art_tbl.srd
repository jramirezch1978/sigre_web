 retrieve="select cencos,cnta_prsp,ano,0 as mes,
       Decode(flag_control,'1','Acumulado Anual') as control,
       monto_real,sum(monto_proyectado)  as total_proy,
		 flag_control,flag_trim,
		 Round(monto_real - sum(monto_proyectado),2) as saldo_proy_o
  from tt_presup_art_aprob 
 where flag_control in ('1')
 group by cencos,cnta_prsp,ano,flag_control,monto_real,flag_trim
union
select cencos,cnta_prsp,ano,mes,
       Decode(flag_control,'3','Mensual','0','No Existe','') ,
       monto_real,sum(monto_proyectado) ,
		 flag_control,flag_trim,
		 Round(monto_real - sum(monto_proyectado),2) as saldo_proy_o
  from tt_presup_art_aprob 
 where flag_control IN ('3','0')
 group by cencos,cnta_prsp,ano,mes,flag_control,monto_real,flag_trim
union
select cencos,cnta_prsp,ano,Max(mes) as mes,
       Decode(flag_control,'2','Acumulado a la Fecha','4','Trimestre Fijo',''),
       monto_real,sum(monto_proyectado) ,
		 flag_control,flag_trim,
		 Round(monto_real - sum(monto_proyectado),2) as saldo_proy_o
  from tt_presup_art_aprob 
 where flag_control in ('2','4')  
 group by cencos,cnta_prsp,ano,flag_control,monto_real,flag_trim
order by cencos,cnta_prsp,ano,mes


