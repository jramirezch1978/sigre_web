 retrieve="select po.fecha, po.turno, t.descripcion, po.nro_parte, po.ot_adm, oa.descripcion, po.cod_administrador, cr.nombre, po.cod_supervisor, cr2.nombre, pod.nro_item, pod.oper_sec, pod.cod_labor, pod.desc_labor, pod.cod_ejecutor, e.descripcion, e.cencos, cc.desc_cencos, pod.cant_labor, pod.und_labor, pod.cant_avance, pod.und_avance, pod.flag_terminado, pod.flag_conformidad, pod.nro_orden
   from pd_ot po,
		pd_ot_det pod, 
      ejecutor e, 
      centros_costo cc, 
      ot_administracion oa, 
      codigo_relacion cr, 
      codigo_relacion cr2, 
      turno t
   where po.nro_parte = pod.nro_parte(+)
		and pod.cod_ejecutor = e.cod_ejecutor(+)
		and e.cencos = cc.cencos(+)
		and po.ot_adm = oa.ot_adm(+)
		and po.cod_administrador = cr.cod_relacion(+)
		and po.cod_supervisor = cr2.cod_relacion(+)
		and po.turno = t.turno(+)
	   and po.nro_parte in (select pod.nro_parte from pd_ot_det pod where pod.oper_sec = :asi_oper_sec)
   order by pod.nro_parte, pod.nro_item" arguments=(("asi_oper_sec", string)) )
