 retrieve=" SELECT  O.OPER_SEC, O.NRO_ORDEN, 
			O.NRO_OPERACION, O.COD_LABOR,   
         O.COD_EJECUTOR, 
		   O.PROVEEDOR, 
			p.nom_proveedor,
         O.NRO_OPER_PRECED, 
		   O.FLAG_ESTADO,   
         O.NRO_PERSONAS, 
		   O.DESC_OPERACION,
         O.FEC_INICIO, 
         O.CANT_PROYECT,
         O.CANT_REAL, 
			O.TIPO_ORDEN,
         O.CENCOS, CC.DESC_CENCOS, 
         O.OBS,
         O.FEC_INICIO_EST, 
			O.FLAG_PRE,
         O.AVANCE_ACTUAL, O.FLAG_FACTURACION,
         O.COSTO_UNIT, 
			O.COD_USR,
         O.FLAG_REPLICACION, 
			O.NRO_APROB, 
			O.CENTRO_BENEF, CB.DESC_CENTRO, 
         L.UND,
			O.SERVICIO, S.DESCRIPCION AS DESC_SERVICIO,
			o.cod_maquina, m.desc_maq,
			le.cod_moneda, 
			le.costo_unitario,
			o.COSTO_PROYECT
    FROM OPERACIONES 		O,   
         LABOR 				L,
			labor_ejecutor		le,
			SERVICIOS			S,
			CENTROS_COSTO 		CC,
			CENTRO_BENEFICIO 	CB,
			Proveedor			p,
			maquina				m
   WHERE O.COD_LABOR    = L.COD_LABOR     (+)
	  AND O.SERVICIO 	   = S.SERVICIO      (+)
     AND O.CENCOS		   = CC.CENCOS	      (+)
     AND O.CENTRO_BENEF = CB.CENTRO_BENEF (+)
	  and o.proveedor	   = p.proveedor     (+)
     and o.cod_maquina	= m.cod_maquina	(+)
	  and o.cod_labor		= le.cod_labor		(+)
	  and o.cod_ejecutor = le.cod_ejecutor	(+)
     and O.TIPO_ORDEN   = (select doc_ot from logparam where reckey = '1') 
	  AND O.NRO_ORDEN    = :as_nro_orden
ORDER BY O.NRO_OPERACION ASC   

" update="OPERACIONES" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_orden", string)) )
