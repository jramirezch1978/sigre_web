 retrieve="select distinct ot.nro_orden, ot.ot_adm, ot.titulo, ot.cod_usr, ot.fec_solicitud, ot.fec_inicio, ot.flag_estado,
		 ot.ot_tipo
from orden_trabajo ot,
     articulo_mov_proy amp,
     reservacion_det   rd,
     ot_adm_usuario    oau
where ot.nro_orden = amp.nro_doc
  and amp.tipo_doc = (select doc_ot from logparam where reckey = '1')
  and rd.org_amp_ref = amp.cod_origen
  and rd.nro_amp_ref = amp.nro_mov
  and oau.ot_adm     = ot.ot_adm
  and oau.cod_usr    = :as_usuario
  and rd.flag_estado = '1'     
  and amp.flag_estado = '1'
     
     " arguments=(("as_usuario", string)) )
