 retrieve="  SELECT ~"PD_OT_DET~".~"NRO_PARTE~",   
         ~"PD_OT_DET~".~"NRO_ITEM~",   
         ~"PD_OT_DET~".~"OPER_SEC~",   
         ~"PD_OT_DET~".~"COD_LABOR~",   
         ~"PD_OT_DET~".~"COD_EJECUTOR~",   
         ~"LABOR~".~"UND~",   
         ~"PD_OT_DET~".~"CANT_LABOR~",   
         ~"PD_OT_DET~".~"HORA_INICIO~",   
         ~"PD_OT~".~"FECHA~",   
         ~"LABOR~".~"DESC_LABOR~"  
    FROM ~"PD_OT_DET~",   
         ~"LABOR~",   
         ~"PD_OT~"  
   WHERE ( ~"PD_OT_DET~".~"COD_LABOR~" = ~"LABOR~".~"COD_LABOR~" ) and  
         ( ~"PD_OT_DET~".~"NRO_PARTE~" = ~"PD_OT~".~"NRO_PARTE~" ) and  
         ( ( nvl(~"PD_OT_DET~".~"CANT_LABOR~",0) <= 0 ) AND  
         ( ~"LABOR~".~"UND~" = (select und_hr from prod_param where reckey = '1') ) AND  
         ( TO_CHAR(~"PD_OT~".~"FECHA~",'YYYYMMDD') >= :as_fecha_inicio ) AND  
         ( TO_CHAR(~"PD_OT~".~"FECHA~",'YYYYMMDD') <= :as_fecha_final ) )   
ORDER BY ~"PD_OT_DET~".~"NRO_PARTE~" ASC,   
         ~"PD_OT_DET~".~"NRO_ITEM~" ASC   
" arguments=(("as_empresa", string),("as_user", string),("as_fecha_inicio", string),("as_fecha_final", string))  sort="fecha A " )
