 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 column=(type=number updatewhereclause=yes name=costo_unit dbname="costo_unit" )
 column=(type=number updatewhereclause=yes name=total_us dbname="total_us" )
 column=(type=number updatewhereclause=yes name=avance dbname="avance" )
 column=(type=number updatewhereclause=yes name=ratio_costo dbname="ratio_costo" )
 column=(type=number updatewhereclause=yes name=ratio_avance dbname="ratio_avance" )
 retrieve="select op.cod_labor      ,
       op.desc_operacion ,
       lb.und            ,
       op.cod_ejecutor   ,
       amp.cod_art       ,
       art.nom_articulo  ,
       art.und           ,
		 decode(lb.flag_tipo_has,'1','Factor','T','Has Totales','N','Has netas','H','Tonelada','C','Caja') AS und_avance,
       sum(op.cant_proyect) as cantidad,
       Nvl(am.costo_ult_compra,0) as costo_unit,
       Round(sum(op.cant_proyect) * Nvl(am.costo_ult_compra,0),2) as total_us,
       Decode(Nvl(sum(op.avance_esperado),0),0,1,Nvl(sum(op.avance_esperado),0)) as avance,
       Round(sum(op.cant_proyect) * Nvl(am.costo_ult_compra,0),2) / decode (sum(op.cant_proyect), 0,1,sum(op.cant_proyect)) as ratio_costo,
       Round(sum(op.cant_proyect) * Nvl(am.costo_ult_compra,0),2) / Decode(Nvl(sum(op.avance_esperado),0),0,1,Nvl(sum(op.avance_esperado),0)) as ratio_avance
  from operaciones op,labor lb,articulo_mov_proy amp,articulo art,articulo_almacen am
 where (op.cod_labor  = lb.cod_labor  ) and
       (op.oper_sec   = amp.oper_sec  ) and
       (amp.cod_art   = art.cod_art   ) and
       (amp.cod_art   = am.cod_art    ) and
       (amp.almacen   = am.almacen    ) and
		 (op.tipo_orden = (select doc_ot from prod_param where reckey = '1') ) and
		 (op.nro_orden	 = :as_nro_orden )
group by op.cod_labor	  ,
			op.desc_operacion,
			lb.und			  ,
			op.cod_ejecutor  ,
			amp.cod_art 	  ,
			art.nom_articulo ,
			art.und 			  ,
			am.costo_ult_compra,
			lb.flag_tipo_has" arguments=(("as_nro_orden", string))  sort="operaciones_cod_labor A operaciones_desc_operacion A labor_und A operaciones_cod_ejecutor A " )
