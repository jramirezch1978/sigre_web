 retrieve="  SELECT ~"ARTICULO_MOV_PROY~".~"COD_ORIGEN~",   
         ~"ARTICULO_MOV_PROY~".~"NRO_MOV~",   
         ~"ARTICULO_MOV_PROY~".~"FLAG_ESTADO~",   
         ~"ARTICULO_MOV_PROY~".~"COD_ART~",   
         ~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~",   
         ~"ARTICULO_MOV_PROY~".~"ALMACEN~"  ,
			usf_saldo_total_x_alm(ARTICULO_MOV_PROY.COD_ART,ARTICULO_MOV_PROY.ALMACEN) AS SALDO_TOTAL,
         ~"ARTICULO_MOV_PROY~".~"PRECIO_UNIT~"  ,
			~"ARTICULO~".~"NOM_ARTICULO~",
			~"ARTICULO~".~"UND~"
    FROM ~"ARTICULO_MOV_PROY~"  ,
			~"ARTICULO~"
   WHERE (~"ARTICULO_MOV_PROY~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" ) AND 
			nvl(usf_cmp_cant_programada(articulo_mov_proy.cod_origen,articulo_mov_proy.nro_mov),0) = 0 AND  
         nvl(usf_cmp_cant_cmp_AMP_OT(articulo_mov_proy.cod_origen,articulo_mov_proy.nro_mov),0) = 0 AND  
         ~"ARTICULO_MOV_PROY~".~"FLAG_ESTADO~" = '1' AND  
         nvl(~"ARTICULO_MOV_PROY~".~"CANT_PROCESADA~",0) = 0 AND  
         nvl(~"ARTICULO_MOV_PROY~".~"CANT_RESERVADO~",0) = 0 AND  
         nvl(~"ARTICULO_MOV_PROY~".~"CANT_TRANSPORTADA~",0) = 0    AND
			~"ARTICULO_MOV_PROY~".~"TIPO_DOC~"					= :as_tipo_doc AND
			~"ARTICULO_MOV_PROY~".~"NRO_DOC~"					= :as_nro_doc " update="ARTICULO_MOV_PROY" updatewhere=1 updatekeyinplace=yes arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
