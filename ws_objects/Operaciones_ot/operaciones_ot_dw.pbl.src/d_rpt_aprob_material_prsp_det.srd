 retrieve="select '2.- Articulos' as grupo,
		 amp.cod_art, a.nom_articulo, amp.nro_doc, amp.fec_proyect, 
		 (Nvl(amp.cant_proyect,0) - Nvl(amp.cant_procesada,0)) * Nvl(amp.precio_unit,0) as costo_art, 
		 amp.oper_sec, 
		 amp.cod_usr, 
		 amp.nro_aprob
  from articulo_mov_proy amp,
       articulo          a
 where amp.flag_estado = '1'
   and amp.nro_aprob is not null        
   and amp.cod_art    = a.cod_art      
   and amp.tipo_doc   = (select doc_ot from logparam where reckey='1')            
   and amp.tipo_mov   = (select oper_cons_interno from logparam where reckey='1') 
   and a.flag_inventariable = '1'                                        
   and Nvl(amp.cant_proyect,0) - Nvl(amp.cant_procesada,0) > 0           
   and trunc(amp.fec_proyect) between :ad_fec_ini and :ad_fec_fin    
   and amp.cencos        = :as_cencos            
   and amp.cnta_prsp     = :as_cnta_prsp         

union

SELECT  '1.- Operaciones' as grupo,
		  op.oper_sec, op.desc_operacion, op.nro_orden, op.fec_inicio_est, 
        usf_fl_conv_mon(le.costo_unitario * op.cant_proyect, le.cod_moneda, 
                      (SELECT cod_dolares FROM logparam WHERE reckey = '1'), op.fec_inicio_est) AS costo_proyect,
        op.oper_sec,
		  op.cod_usr,
		  op.nro_aprob
   FROM operaciones    op,
        labor_ejecutor le,
        labor          l,
        ejecutor       e,
        (SELECT op2.oper_sec
           FROM operaciones    op2,
                ejecutor       e2
          WHERE e2.cod_ejecutor = op2.cod_ejecutor
            AND op2.flag_estado = '1'
            AND e2.flag_externo = '1'
          MINUS
          SELECT oper_sec
            FROM orden_servicio_det osd
           WHERE osd.flag_estado <> '0'
             AND osd.oper_sec IS NOT NULL) s
WHERE op.cod_labor     = le.cod_labor
  AND op.cod_ejecutor  = le.cod_ejecutor
  AND le.cod_labor     = l.cod_labor
  AND le.cod_ejecutor  = e.cod_ejecutor
  AND op.flag_estado   = '1'
  AND e.flag_externo   = '1'
  AND (le.costo_unitario * op.cant_proyect) > 0
  and trunc(op.fec_inicio_est) between :ad_fec_ini and :ad_fec_fin    
  and op.cencos 		  = :as_cencos
  and l.cnta_prsp_3ro  = :as_cnta_prsp" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_cencos", string),("as_cnta_prsp", string),("as_flag_ctrl", string))  sort="grupo A nro_doc A oper_sec A " )
