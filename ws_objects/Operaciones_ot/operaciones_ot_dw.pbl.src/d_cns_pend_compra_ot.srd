table(column=(type=char(2) updatewhereclause=yes name=cod_origen dbname="articulo_mov_proy.cod_origen" dbalias="amp.cod_origen" )
 column=(type=decimal(0) updatewhereclause=yes name=nro_mov dbname="articulo_mov_proy.nro_mov" dbalias="amp.nro_mov" )
 column=(type=char(12) updatewhereclause=yes name=cod_art dbname="articulo_mov_proy.cod_art" dbalias="amp.cod_art" )
 column=(type=char(2000) updatewhereclause=yes name=desc_art dbname="articulo.desc_art" dbalias="art.desc_art" )
 column=(type=datetime updatewhereclause=yes name=fec_proyect dbname="articulo_mov_proy.fec_proyect" dbalias="amp.fec_proyect" )
 column=(type=decimal(4) updatewhereclause=yes name=cant_proyect dbname="articulo_mov_proy.cant_proyect" dbalias="amp.cant_proyect" )
 column=(type=decimal(4) updatewhereclause=yes name=cant_procesada dbname="articulo_mov_proy.cant_procesada" dbalias="amp.cant_procesada" )
 column=(type=number updatewhereclause=yes name=cant_comprada dbname="cant_comprada" )
 column=(type=char(6) updatewhereclause=yes name=almacen dbname="articulo_mov_proy.almacen" dbalias="amp.almacen" )
 column=(type=number updatewhereclause=yes name=sldo_total dbname="sldo_total" )
 column=(type=decimal(7) updatewhereclause=yes name=precio_unit dbname="articulo_mov_proy.precio_unit" dbalias="amp.precio_unit" )
 column=(type=char(10) updatewhereclause=yes name=cencos dbname="articulo_mov_proy.cencos" dbalias="amp.cencos" )
 column=(type=char(10) updatewhereclause=yes name=cnta_prsp dbname="articulo_mov_proy.cnta_prsp" dbalias="amp.cnta_prsp" )
 column=(type=char(10) updatewhereclause=yes name=nro_aprob dbname="articulo_mov_proy.nro_aprob" dbalias="amp.nro_aprob" )
 column=(type=char(6) updatewhereclause=yes name=usr_aprob dbname="ot_aprobaciones.usr_aprob" dbalias="ota.usr_aprob" )
 retrieve="select 	amp.cod_origen,
			amp.nro_mov,
			amp.cod_art,
			art.desc_art,
			amp.fec_proyect,
			amp.cant_proyect,
			amp.cant_procesada,
			(select nvl(sum(decode(amp_oc.flag_estado, '1', amp_oc.cant_proyect, amp_oc.cant_procesada)),0)
 				from articulo_mov_proy amp_oc
			  where amp_oc.tipo_doc = (select doc_oc from logparam where reckey = '1')
			    and amp_oc.flag_estado <> '0'
  				 and amp_oc.org_amp_ref = amp.cod_origen
 				 and amp_oc.nro_amp_ref	= amp.nro_mov) as cant_comprada,
			amp.almacen,
			nvl(aa.SLDO_TOTAL, 0) as SLDO_TOTAL,
			amp.precio_unit,
			amp.cencos,
			amp.cnta_prsp,
			amp.nro_aprob,	
			ota.USR_APROB
from 	articulo_mov_proy amp, 
		articulo 			art,
		ot_aprobaciones	ota,
		articulo_almacen	aa
where amp.cod_art 		= art.cod_art 
  and amp.nro_aprob		= ota.nro_aprob
  and amp.almacen			= aa.almacen     (+)
  and amp.cod_art			= aa.cod_Art	  (+)
  and tipo_doc				= (Select doc_ot from logparam where reckey=1) 
  and NRO_DOC				= :as_nro_ot 
  and amp.flag_estado 	= '1'
  and amp.cant_proyect - amp.cant_reservado - (select nvl(sum(decode(amp_oc.flag_estado, '1', amp_oc.cant_proyect, amp_oc.cant_procesada)),0)
 				from articulo_mov_proy amp_oc
			  where amp_oc.tipo_doc = (select doc_oc from logparam where reckey = '1')
			    and amp_oc.flag_estado <> '0'
  				 and amp_oc.org_amp_ref = amp.cod_origen
 				 and amp_oc.nro_amp_ref	= amp.nro_mov) > 0" arguments=(("as_nro_ot", string)) )
