 retrieve="  SELECT ~"PLANT_OT~".~"COD_PLANTILLA~",   
         ~"PLANT_OT~".~"FLAG_ESTADO~",   
         ~"PLANT_OT~".~"FLAG_PROGRAMADO~",   
         ~"PLANT_OT~".~"FLAG_ESTRUCTURA~",   
         ~"PLANT_OT~".~"DIAS_ESTIM_INI~",   
         ~"PLANT_OT~".~"DIAS_ESTIM_FIN~",   
         ~"PLANT_OT~".~"CENCOS_RSP~",
			CCR.DESC_CENCOS AS DESC_CENCOS_RSP,   
         ~"PLANT_OT~".~"CENCOS_SLC~",   
			CCS.DESC_CENCOS AS DESC_CENCOS_SLC,
         ~"PLANT_OT~".~"COD_MAQUINA~",
			m.desc_maq,   
         ~"PLANT_OT~".~"OT_ADM~",
   		ota.descripcion desc_ot_adm,
         ~"PLANT_OT~".~"OT_TIPO~",
			ott.descripcion as desc_ot_tipo,   
         ~"PLANT_OT~".~"TITULO~",   
         ~"PLANT_OT~".~"DESCRIPCION~",   
         ~"PLANT_OT~".~"COSTO_ESTIMADO~",   
         ~"PLANT_OT~".~"PLANT_PROD~",
			pp.desc_plantilla as desc_plant_prod,   
         ~"PLANT_OT~".~"PLANT_RATIO~",   
         ~"PLANT_OT~".~"FLAG_REPLICACION~"
    FROM ~"PLANT_OT~", CENTROS_COSTO CCS, CENTROS_COSTO CCR, ot_administracion ota, maquina m,
			ot_tipo ott, plant_prod pp
   WHERE ~"PLANT_OT~".~"COD_PLANTILLA~" = :as_plantilla    AND
         PLANT_OT.CENCOS_SLC = CCS.CENCOS(+) AND
			PLANT_OT.CENCOS_RSP = CCR.CENCOS(+) and
			plant_ot.ot_adm = ota.ot_adm(+) and
			plant_ot.cod_maquina = m.cod_maquina(+) and
			plant_ot.ot_tipo = ott.ot_tipo(+) and
			plant_ot.plant_prod = pp.cod_plantilla(+)
" update="PLANT_OT" updatewhere=0 updatekeyinplace=yes arguments=(("as_plantilla", string)) )
