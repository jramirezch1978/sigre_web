 retrieve="  SELECT ~"ORDEN_TRABAJO~".~"CENCOS_SLC~",   
         ~"ARTICULO_MOV_PROY~".~"CNTA_PRSP~",   
         to_char(fec_proyect,'yyyy') as ano,   
         to_char(fec_proyect,'mm') as mes,   
         ~"ARTICULO~".~"COSTO_PROM_DOL~"  
    FROM ~"ARTICULO_MOV_PROY~",   
         ~"ORDEN_TRABAJO~",   
         ~"ARTICULO~"  
   WHERE ( ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" = ~"ARTICULO_MOV_PROY~".~"NRO_DOC~" ) and  
         ( ~"ARTICULO_MOV_PROY~".~"COD_ART~" = ~"ARTICULO~".~"COD_ART~" )   
GROUP BY ~"ORDEN_TRABAJO~".~"CENCOS_SLC~",   
         ~"ARTICULO_MOV_PROY~".~"CNTA_PRSP~",   
         ~"ARTICULO_MOV_PROY~".~"FEC_PROYECT~",   
         ~"ARTICULO~".~"COSTO_PROM_DOL~"   
" )
