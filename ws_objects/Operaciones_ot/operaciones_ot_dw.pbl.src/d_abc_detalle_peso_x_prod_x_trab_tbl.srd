         Nvl(maestro.apel_paterno,' ')||' '||Nvl(maestro.apel_materno,' ') ||' '||Nvl(maestro.nombre1,' ') ||' '|| Nvl(maestro.nombre2,' ') as nombres,   
         Nvl(flag_ing_inf,'A') AS flag_ing_inf  
    FROM ~"PESAJE_BALANZA_DSTJ~",   
         ~"MAESTRO~"  
   WHERE ( ~"MAESTRO~".~"COD_TRABAJADOR~" = ~"PESAJE_BALANZA_DSTJ~".~"COD_TRABAJADOR~" ) and  
         ( ( trunc(~"PESAJE_BALANZA_DSTJ~".~"FECHA~") between :ad_fecha_inicio and :ad_fecha_final ) ) and
			( ~"PESAJE_BALANZA_DSTJ~".~"CODPRD~" = :as_cod_prod									 ) and
			( ~"MAESTRO~".~"COD_TRABAJADOR~"  = :as_cod_trabajador							 ) 

			
