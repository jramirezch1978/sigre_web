 retrieve="  SELECT ~"ARTICULO_MOV_PROY~".~"COD_ORIGEN~",   
         ~"ARTICULO_MOV_PROY~".~"NRO_MOV~",   
         ~"ARTICULO_MOV_PROY~".~"FLAG_ESTADO~",   
         ~"ARTICULO_MOV_PROY~".~"COD_ART~",   
         ~"ARTICULO_MOV_PROY~".~"TIPO_MOV~",   
         ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~",   
         ~"ARTICULO_MOV_PROY~".~"NRO_DOC~",   
         ~"ARTICULO_MOV_PROY~".~"FEC_REGISTRO~",   
         ~"ARTICULO_MOV_PROY~".~"FEC_PROYECT~",   
         ~"ARTICULO_MOV_PROY~".~"CANT_PROYECT~",   
         ~"ARTICULO_MOV_PROY~".~"CANT_PROCESADA~",   
         ~"ARTICULO_MOV_PROY~".~"PRECIO_UNIT~",   
         ~"ARTICULO_MOV_PROY~".~"DECUENTO~",   
         ~"ARTICULO_MOV_PROY~".~"IMPUESTO~",   
         ~"ARTICULO_MOV_PROY~".~"COD_MONEDA~",   
         ~"ARTICULO_MOV_PROY~".~"TIPO_REF~",   
         ~"ARTICULO_MOV_PROY~".~"REFERENCIA~",   
         ~"ARTICULO_MOV_PROY~".~"CENCOS~",   
         ~"ARTICULO_MOV_PROY~".~"CNTA_PRSP~",   
			~"ARTICULO_MOV_PROY~".~"ALMACEN~",
			~"ARTICULO_MOV_PROY~".~"COD_USR~",
         ~"ARTICULO~".~"UND~",
         ~"ARTICULO~".~"DIAS_REPOSICION~",
         usf_saldo_total_x_alm(~"ARTICULO~".~"COD_ART~", ~"ARTICULO_MOV_PROY~".~"ALMACEN~") AS SALDO_TOTAL,
			~"ARTICULO~".~"FLAG_REPOSICION~",
         ~"ARTICULO_MOV_PROY~".~"OPER_SEC~",   
         ~"ARTICULO~".~"NOM_ARTICULO~",
			DECODE(~"ARTICULO_MOV_PROY~".~"CANT_PROCESADA~",0,'S',NULL) AS ESTADO,
			DECODE(~"ARTICULO~".~"FLAG_REPOSICION~",0,'S',NULL) AS REPOSICION,
			DECODE(~"ARTICULO_MOV_PROY~".~"FLAG_ESTADO~",3,'S',NULL) AS FLAG,
			~"OPERACIONES~".~"NRO_OPERACION~",
         ~"ARTICULO_MOV_PROY~".~"FLAG_MODIFICACION~" ,
         ~"ARTICULO_MOV_PROY~".~"FLAG_REPLICACION~"	
    FROM ~"ARTICULO~",   
         ~"ARTICULO_MOV_PROY~"  ,
			~"OPERACIONES~" 
   WHERE ( ~"ARTICULO_MOV_PROY~".~"COD_ART~"   = ~"ARTICULO~".~"COD_ART~" ) AND
			( ~"ARTICULO_MOV_PROY~".~"OPER_SEC~"  = ~"OPERACIONES~".~"OPER_SEC~" ) AND
			( ~"ARTICULO_MOV_PROY~".~"TIPO_MOV~"  = (select oper_cons_interno from logparam where reckey = '1')) AND
			( ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~"  = :as_tipo_doc ) AND
			( ~"ARTICULO_MOV_PROY~".~"NRO_DOC~"   = :as_nro_doc  ) 
ORDER BY ~"ARTICULO_MOV_PROY~".~"COD_ORIGEN~" ASC,   
         ~"ARTICULO_MOV_PROY~".~"NRO_MOV~" ASC,   
         ~"ARTICULO_MOV_PROY~".~"COD_ART~" ASC   
" update="ARTICULO_MOV_PROY" updatewhere=0 updatekeyinplace=no arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
