 retrieve="SELECT a.cod_art, a.nom_articulo, 
       a.und, NVL(am.cant_proyect,0) as cantidad, 
       NVL(a.costo_ult_compra,0) as precio_unit, NVL(am.cant_proyect,0)*NVL(a.costo_ult_compra,0) as monto_dol,
       a.nivel_aprob, oa.imp_max_aprob_amp, 
		 oa.nivel_aprob, o.oper_sec
 from orden_trabajo ot, operaciones o, articulo_mov_proy am, articulo a, ot_adm_aprob_req oa
where ot.nro_orden=o.nro_orden 
  and o.oper_sec = am.oper_sec 
  and am.cod_art = a.cod_art 
  and ot.ot_adm = oa.ot_adm 
  and oa.cod_usr = :as_user 
  and ot.ot_adm = :as_ot_adm
  and ( a.costo_ult_compra > oa.imp_max_aprob_amp 
   or a.nivel_aprob > oa.nivel_aprob) 
  and am.flag_estado = '3' 
  and TRUNC(am.fec_proyect) between :ad_fec_ini and :ad_fec_fin" arguments=(("as_user", string),("as_ot_adm", string),("ad_fec_ini", date),("ad_fec_fin", date)) )
