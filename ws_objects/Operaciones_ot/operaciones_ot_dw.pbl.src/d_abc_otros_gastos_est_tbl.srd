 column=(type=number updatewhereclause=yes name=total_us dbname="total_us" )
 retrieve="select oog.cod_relacion ,
		 cr.nombre        ,
		 oog.tipo_doc 		,
		 oog.nro_doc 		,
       cp.cod_moneda    ,
		 cp.importe_doc 	,
       decode(cp.cod_moneda,(select cod_dolares from logparam where reckey = '1'),nvl(cp.importe_doc,0.00) ,(nvl(cp.importe_doc,0.00) / usf_fin_tasa_cambio (sysdate)) ) as total_us
  from ot_otros_gastos oog,
       codigo_relacion cr ,
       cntas_pagar cp
 where  (oog.cod_relacion  = cr.cod_relacion )  and
       ((oog.cod_relacion = cp.cod_relacion  )  and
        (oog.tipo_doc     = cp.tipo_doc      )  and
        (oog.nro_doc      = cp.nro_doc       )) and
        (oog.nro_orden    = :as_nro_orden     ) " arguments=(("as_nro_orden", string)) )
