 retrieve="select p.cod_plantilla, p.desc_plantilla, 
		 p.ot_adm, po.nro_operacion, 
		 po.cod_labor, po.cod_ejecutor, 
		 po.cantidad, po.nro_personas,
       ppm.cod_art, ppm.cantidad, 
		 le.costo_unitario, l.desc_labor, 
		 l.und, a.nom_articulo, a.und, 
		 a.costo_ult_compra
from plant_prod p, plant_prod_oper po, plant_prod_mov ppm, labor l, labor_ejecutor le, articulo a
where ( p.cod_plantilla=po.cod_plantilla AND
        p.cod_plantilla=:as_cod_plantilla ) AND
      ( po.cod_plantilla=ppm.cod_plantilla(+) AND
        po.nro_operacion=ppm.nro_operacion(+) ) AND
       ( po.cod_labor = l.cod_labor (+)) AND
       ( po.cod_labor = le.cod_labor (+)AND
         po.cod_ejecutor = le.cod_ejecutor(+) ) AND
       ( ppm.cod_art = a.cod_art(+) )
" arguments=(("as_cod_plantilla", string)) )
