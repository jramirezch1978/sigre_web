 retrieve="SELECT     OT.NRO_ORDEN ,
			  OT.COD_ORIGEN ,
           OT.FLAG_ESTADO ,
           OT.FEC_SOLICITUD ,
           OT.FEC_INICIO ,
           OT.CENCOS_RSP ,
           OT.CENCOS_SLC ,
           OT.COD_USR ,
           OT.NRO_SOLICITUD ,
           OT.DESCRIPCION ,
           OT.CLIENTE ,
           OT.FEC_ESTIMADA ,
           OT.COD_MAQUINA ,
           OT.PROG_MNT ,
           OT.MNT_UND_ACT_PROY ,
           OT.MNT_UND_ACT_REAL ,
           OT.FLAG_ESTRUCTURA ,
			  OT.TITULO ,
			  OT.FLAG_COSTO_TIPO ,
           CCA.DESC_CENCOS ,
           M.DESC_MAQ ,
           OT.OT_ADM ,
           OT.OT_TIPO ,
           OTT.DESCRIPCION ,
           OTA.DESCRIPCION ,
           OT.FEC_REGISTRO ,
           CCB.DESC_CENCOS ,
           OT.FLAG_REPLICACION, 
           OT.RESPONSABLE, 
           OT.FLAG_PROGRAMADO,
           PA.NOM_PROVEEDOR AS NOM_CLIENTE,  
           PB.NOM_PROVEEDOR AS NOM_RESPONSABLE,
  			  SOT.CNTA_PRSP,
			  OT.CENTRO_BENEF,
			  CB.DESC_CENTRO,
			  OT.LOTE_CAMPO,	
			  LC.DESCRIPCION,
			  OT.VARIEDAD,
			  A.DESC_ART,
			  OT.NRO_OV,
			  OV.OBS
      FROM ORDEN_TRABAJO 		OT ,
           CENTROS_COSTO 		CCA,
			  CENTROS_COSTO 		CCB,
           MAQUINA 				M,
           OT_ADMINISTRACION 	OTA,
           OT_TIPO 				OTT,
	   	  SOLICITUD_OT 		SOT,
			  PROVEEDOR 			PA,
			  PROVEEDOR 			PB,
           CENTRO_BENEFICIO 	CB,
			  LOTE_CAMPO			LC,
			  ARTICULO				A,
			  ORDEN_VENTA			OV
     WHERE OT.CENCOS_SLC  		= CCA.CENCOS 			(+)
		 And OT.CENCOS_RSP 		= CCB.CENCOS 			(+)
 		 and OT.COD_MAQUINA 		= M.COD_MAQUINA 		(+)
		 and OT.NRO_SOLICITUD 	= SOT.NRO_SOLICITUD 	(+)
		 and OT.CLIENTE 			= PA.PROVEEDOR 		(+)
		 and OT.RESPONSABLE 		= PB.PROVEEDOR 		(+)
		 and OT.OT_ADM 			= OTA.OT_ADM 
		 And OT.OT_TIPO 			= OTT.OT_TIPO 
		 AND OT.CENTRO_BENEF		= CB.CENTRO_BENEF 	(+)
		 and OT.LOTE_CAMPO		= LC.NRO_LOTE 			(+)
       and OT.VARIEDAD			= A.COD_ART				(+)
 		 AND OT.NRO_OV				= OV.NRO_OV				(+)
		 and OT.NRO_ORDEN = :as_nro_orden" update="ORDEN_TRABAJO" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_orden", string)) )
