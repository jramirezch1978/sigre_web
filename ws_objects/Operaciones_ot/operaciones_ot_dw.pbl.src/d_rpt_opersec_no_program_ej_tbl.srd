 retrieve="select o.fec_inicio, ot.nro_orden, o.oper_sec, o.cod_labor, o.cod_ejecutor, 
       o.desc_operacion, o.cant_proyect, o.cant_real, o.flag_estado, 
       ot.cod_maquina, ot.cencos_rsp, ot.cencos_slc, ej.descripcion
from orden_trabajo ot, operaciones o, ejecutor ej
where ot.nro_orden = o.nro_orden and
      ot.ot_adm = :as_ot_adm and
      o.oper_sec not in (select distinct(pd.oper_sec) 
                        from programa_trabajo_det_rh pd 
                        where o.oper_sec=pd.oper_sec) and
      o.flag_estado in ('1','3') and
		o.cod_ejecutor = ej.cod_ejecutor and
      o.cod_ejecutor = :as_ejecutor and
      o.fec_inicio between trunc(:ad_fecha_ini) and
                           trunc(:ad_fecha_fin)+0.999" arguments=(("as_ot_adm", string),("as_ejecutor", string),("ad_fecha_ini", date),("ad_fecha_fin", date)) )
