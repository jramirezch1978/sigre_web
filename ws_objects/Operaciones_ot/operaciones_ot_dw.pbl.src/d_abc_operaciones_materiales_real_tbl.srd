 column=(type=number updatewhereclause=yes name=total_us dbname="total_us" )
 retrieve="select op.cod_labor      ,
		 op.desc_operacion ,
		 lb.und				 ,
		 op.cod_ejecutor	 ,
		 podm.cod_maquina  ,
       m.desc_maq        ,
		 sum(podm.cant_trabajada) as cantidad,
       usf_ope_costo_x_maq(podm.cod_maquina,m.tipo_maquina,op.cod_labor) as costo_unit,
       sum(podm.cant_trabajada) * usf_ope_costo_x_maq(podm.cod_maquina,m.tipo_maquina,op.cod_labor) as total_us 
  from operaciones op,labor lb,pd_ot_det pod,pd_ot_det_maq podm,maquina m
 where (op.cod_labor    = lb.cod_labor  ) and
       (op.oper_sec     = pod.oper_sec  ) and
       (pod.nro_parte   = podm.nro_parte) and
       (pod.nro_item    = podm.nro_item ) and
       (pod.cod_maquina = m.cod_maquina ) and
       (op.tipo_orden   = (select doc_ot from prod_param where reckey = '1') ) and
		 (op.nro_orden		= :as_nro_orden ) 	
group by op.cod_labor  ,op.desc_operacion ,lb.und,op.cod_ejecutor,podm.cod_maquina,
       m.desc_maq      ,m.tipo_maquina     
       
       " arguments=(("as_nro_orden", string)) )
