 retrieve="  SELECT ~"OPERACIONES~".~"NRO_OPERACION~",   
         ~"OPERACIONES~".~"COD_LABOR~",   
         ~"OPERACIONES~".~"COD_EJECUTOR~",   
         ~"LABOR~".~"DESC_LABOR~",   
         ~"OPERACIONES~".~"CANT_PROYECT~",   
         ~"OPERACIONES~".~"CANT_REAL~",   
         ~"OPERACIONES~".~"NRO_ORDEN~",   
         ~"OPERACIONES~".~"OPER_SEC~",   
			~"OPERACIONES~".~"CENCOS~",   
         0 as porc  ,
			'0' as flag_cierre
    FROM ~"OPERACIONES~",   

         ~"LABOR~"  			 ,
			~"ARTICULO_MOV_PROY~"
   WHERE  
          ( ~"OPERACIONES~".~"COD_LABOR~"      = ~"LABOR~".~"COD_LABOR~"                )  and  
			(( ~"OPERACIONES~".~"TIPO_ORDEN~"	    = ~"ARTICULO_MOV_PROY~".~"TIPO_DOC~"	  )  and
			 ( ~"OPERACIONES~".~"NRO_ORDEN~"	    = ~"ARTICULO_MOV_PROY~".~"NRO_DOC~"		  )) and
         (( ~"OPERACIONES~".~"FLAG_ESTADO~" in ('1')                            )) and
			 ( ~"OPERACIONES~".~"COD_LABOR~"      = :as_cod_labor							  )  and
          ( ~"OPERACIONES~".~"COD_EJECUTOR~"   = :as_cod_ejecutor						  )  and
			 ( ~"ARTICULO_MOV_PROY~".~"TIPO_MOV~" =	:as_tipo_mov							  )  and
			 ( ~"ARTICULO_MOV_PROY~".~"COD_ART~"  like	:as_cod_art							  )  


group by ~"OPERACIONES~".~"NRO_OPERACION~",   
         ~"OPERACIONES~".~"COD_LABOR~",   
         ~"OPERACIONES~".~"COD_EJECUTOR~",   
         ~"LABOR~".~"DESC_LABOR~",   
         ~"OPERACIONES~".~"CANT_PROYECT~",   
         ~"OPERACIONES~".~"CANT_REAL~",   
         ~"OPERACIONES~".~"NRO_ORDEN~",   
         ~"OPERACIONES~".~"OPER_SEC~", 
         ~"OPERACIONES~".~"CENCOS~"   " arguments=(("as_cod_labor", string),("as_cod_ejecutor", string),("as_tipo_mov", string),("as_cod_art", string)) )
