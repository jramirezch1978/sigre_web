 retrieve="select pd.cod_maquina, m.desc_maq, pd.nro_orden, pd.nro_parte, pd.nro_item, pd.cod_labor, 
       USF_OPE_OPERADOR_MAQUINA(pd.nro_parte, pd.nro_item) as operador, 
		 sum(nvl(pd.cant_labor,0)-nvl(pd.horas_inciden,0)) as cant, 
       sum(nvl(pd.cant_avance,0)) as avance
 from orden_trabajo ot, operaciones o, 
      pd_ot p, pd_ot_det pd, 
      labor l, campo_ciclo cc, 
      campo c, maquina m
where (ot.nro_orden=o.nro_orden) and
      (o.oper_sec=pd.oper_sec) and
      (pd.nro_parte=p.nro_parte) and
      (ot.nro_orden=cc.nro_orden) and
      (cc.cod_campo=c.cod_campo) and 
      pd.cod_labor=l.cod_labor and
      pd.cod_maquina = m.cod_maquina and
      pd.nro_orden = :as_nro_orden and
      pd.cod_labor = :as_cod_labor  and
      pd.cod_ejecutor = :as_ejecutor  and
      p.fecha between :ad_fec_ini and :ad_fec_fin
group by pd.cod_maquina, m.desc_maq, pd.nro_orden, pd.nro_parte, pd.nro_item, pd.cod_labor, 
			USF_OPE_OPERADOR_MAQUINA(pd.nro_parte, pd.nro_item)
order by pd.cod_maquina, pd.nro_orden, pd.nro_parte, pd.nro_item" arguments=(("as_cod_labor", string),("as_ejecutor", string),("as_nro_orden", string),("ad_fec_ini", date),("ad_fec_fin", date)) )
