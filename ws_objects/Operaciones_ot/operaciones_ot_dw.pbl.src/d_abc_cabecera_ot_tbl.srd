 retrieve="  SELECT ~"ORDEN_TRABAJO~".~"NRO_ORDEN~",   
         ~"ORDEN_TRABAJO~".~"CENCOS_SLC~",   
         ~"ORDEN_TRABAJO~".~"CENCOS_RSP~",   
         ~"ORDEN_TRABAJO~".~"OT_ADM~",   
         ~"ORDEN_TRABAJO~".~"OT_TIPO~",   
         ~"ORDEN_TRABAJO~".~"FEC_ESTIMADA~",   
         ~"ORDEN_TRABAJO~".~"FEC_INICIO~",   
         centros_costo_a.desc_cencos as desc_cencos_resp,   
         centros_costo_b.desc_cencos as desc_cencos_slc  
    FROM ~"ORDEN_TRABAJO~",   
         ~"CENTROS_COSTO~" ~"CENTROS_COSTO_A~",   
         ~"CENTROS_COSTO~" ~"CENTROS_COSTO_B~"  
   WHERE ( ~"CENTROS_COSTO_A~".~"CENCOS~" (+) = ~"ORDEN_TRABAJO~".~"CENCOS_RSP~" ) and  
         ( ~"ORDEN_TRABAJO~".~"CENCOS_SLC~" = ~"CENTROS_COSTO_B~".~"CENCOS~" (+) ) and  
         ( ( ~"ORDEN_TRABAJO~".~"NRO_ORDEN~" = :as_nro_orden ) )    
" arguments=(("as_nro_orden", string)) )
