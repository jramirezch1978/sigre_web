 retrieve="SELECT COD_ART, nro_doc, tipo_doc, fec_registro, cod_moneda, precio_unit,
        decuento, almacen, proveedor, nom_proveedor, cant_proyect, cant_procesada,
        fec_proyect, impuesto, forma_pago
    FROM ( select amp.COD_ART, amp.nro_doc, amp.tipo_doc, amp.fec_registro,
                  amp.cod_moneda, amp.precio_unit, NVL(amp.decuento,0) as decuento,
                  amp.almacen, oc.proveedor, p.nom_proveedor, amp.cant_proyect, 
						amp.cant_procesada, amp.fec_proyect, NVL(amp.impuesto,0) as impuesto, 
						oc.forma_pago
             from articulo_mov_proy amp,
                  articulo          a,
                  orden_compra      oc, 
						proveedor			p
             WHERE a.cod_art = amp.cod_art
               and amp.nro_doc = oc.nro_oc
               and amp.tipo_doc = (Select doc_oc from logparam where reckey = '1')
               and amp.flag_estado <> '0'
               and amp.cod_art = :as_cod_art
					and (oc.proveedor = p.proveedor)
           ORDER BY amp.fec_registro desc)
    where rownum <= :an_registros" arguments=(("as_cod_art", string),("an_registros", number)) )
