 retrieve="  SELECT OT.NRO_ORDEN,
         OT.OT_ADM,
         OTA.DESCRIPCION AS DESC_OT_ADM,
         OT.OT_TIPO,
         'ARTICULOS PENDIENTES' AS DESC_PENDIENTE,
         AMP.FEC_PROYECT,
         AMP.COD_ART,
         A.DESC_ART,
			a.und,
         AMP.CANT_PROYECT
    FROM ORDEN_TRABAJO      OT,
         ARTICULO_MOV_PROY  AMP,   
         ARTICULO           A,
         OT_ADMINISTRACION  OTA
   WHERE AMP.NRO_DOC = OT.NRO_ORDEN
     AND OT.OT_ADM   = OTA.OT_ADM
     AND AMP.TIPO_DOC = (SELECT DOC_OT FROM LOGPARAM WHERE RECKEY = '1')
     AND AMP.COD_ART  = A.COD_ART
     AND AMP.FLAG_ESTADO = '3'
     AND trunc(AMP.FEC_PROYECT ) BETWEEN TRUNC(:ad_fec_ini) AND TRUNC(:ad_fec_fin )
UNION
  SELECT OT.NRO_ORDEN,
         OT.OT_ADM,
         OTA.DESCRIPCION AS DESC_OT_ADM,
         OT.OT_TIPO,
         'OPERACIONES PENDIENTES' AS DESC_PENDIENTE,
         OP.FEC_INICIO,
         OP.OPER_SEC,
         OP.DESC_OPERACION,
			null as und,
         OP.CANT_PROYECT * OP.COSTO_PROYECT AS IMPORTE
    FROM ORDEN_TRABAJO      OT,
         OPERACIONES        OP,   
         OT_ADMINISTRACION  OTA
   WHERE OP.NRO_ORDEN = OT.NRO_ORDEN
     AND OT.OT_ADM   = OTA.OT_ADM
     AND OP.FLAG_ESTADO = '3'
     AND trunc(OP.FEC_INICIO ) BETWEEN TRUNC(:ad_fec_ini) AND TRUNC(:ad_fec_fin )
     
" arguments=(("ad_fec_ini", date),("ad_fec_fin", date))  sort="desc_pendiente A ot_adm A nro_orden A " )
