 retrieve="
  SELECT PPM.COD_PLANTILLA,   
         PPM.NRO_OPERACION,   
			ac.cod_clase,
			ac.desc_clase,
         PPM.COD_ART,   
         A.DESC_ART,   
         PPM.CANTIDAD,   
         A.UND,   
         PPM.FLAG_REPLICACION,   
         PPM.CNTA_PRSP,   
         PPO.DESC_OPERACION  
    FROM PLANT_PROD_MOV_ingreso	PPM,   
         ARTICULO						A,   
			articulo_clase				ac,
         PLANT_PROD_OPER			PPO  
   WHERE PPM.COD_ART 		= A.COD_ART 
 	  and PPM.COD_PLANTILLA = PPO.COD_PLANTILLA 
 	  and PPM.NRO_OPERACION = PPO.NRO_OPERACION 
     and ac.cod_clase		= a.cod_clase
	  and PPM.COD_PLANTILLA = :as_cod_plantilla
ORDER BY PPM.COD_PLANTILLA ASC,   
         PPM.NRO_OPERACION ASC,   
         PPM.COD_ART ASC   

" update="PLANT_PROD_MOV_ingreso" updatewhere=0 updatekeyinplace=yes arguments=(("as_cod_plantilla", string))  sort="nro_operacion A cod_clase A desc_art A " )
