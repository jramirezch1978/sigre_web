 retrieve="select ot.ot_adm, oad.descripcion, o.proveedor, p.nom_proveedor, ot.nro_orden, 
		 o.cod_labor, o.cod_ejecutor, o.oper_sec, f.item, f.cantidad, o.has, f.descripcion, f.und, f.precio
from orden_trabajo ot, operaciones o, facturacion_operacion f, 
     proveedor p, ot_administracion oad
where ot.nro_orden=o.nro_orden and
      ot.ot_adm = oad.ot_adm and
      ot.ot_adm = :as_ot_adm and
      o.oper_sec = f.oper_sec and
      o.proveedor = p.proveedor and
		f.tipo_doc is null 
order by ot.ot_adm, o.proveedor, ot.nro_orden, o.oper_sec, f.item" arguments=(("as_ot_adm", string)) )
