 retrieve="SELECT cp.cronograma_pago_id,   
		   cp.empresa_id,
		   cp.nro_cuota,
		   cp.fec_vencimiento,
		   cp.dias_tolerancia,
            cp.flag_pagado,   
            cp.nro_factura,   
            cp.fec_pago,   
            cp.nro_voucher,   
            cp.banco_id,   
            cp.modified_by,   
            cp.fec_modified,
		   b.cod_banco,
		   b.desc_banco 
FROM cronograma_pagos cp,
		bancos b
WHERE cp.banco_id	= b.banco_id (+)
	and  cronograma_pago_id = :ai_cronograma_pago_id" update="cronograma_pagos" updatewhere=0 updatekeyinplace=no arguments=(("ai_cronograma_pago_id", number)) )
