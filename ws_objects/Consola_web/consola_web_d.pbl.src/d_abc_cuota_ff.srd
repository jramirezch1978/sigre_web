 retrieve="SELECT  t.EMPRESA_ID,   
			t.NRO_CUOTA,   
			t.cod_servicio,
			t.FEC_VENCIMIENTO,   
			t.DIAS_TOLERANCIA,   
			t.FLAG_PAGADO,   
			t.CRONOGRAMA_PAGO_ID,   
			t.CREATED_BY,   
			t.FEC_REGISTRO,   
			t.MODIFIED_BY,   
			t.FEC_MODIFIED ,
			s.DESC_SERVICIO 
FROM CRONOGRAMA_PAGOS t,
		SERVICIOS s
WHERE t.CRONOGRAMA_PAGO_ID		= :ai_cronograma_pago_id    
	AND t.COD_SERVICIO					= s.COD_SERVICIO
" update="CRONOGRAMA_PAGOS" updatewhere=0 updatekeyinplace=yes arguments=(("ai_cronograma_pago_id", number)) )
