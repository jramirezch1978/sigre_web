table(column=(type=char(20) updatewhereclause=yes name=descr_especie dbname="descr_especie" )
 column=(type=char(20) updatewhereclause=yes name=desc_categoria dbname="desc_categoria" )
 column=(type=char(20) updatewhereclause=yes name=desc_sub_cat dbname="desc_sub_cat" )
 column=(type=char(20) updatewhereclause=yes name=cod_art dbname="cod_art" )
 column=(type=char(20) updatewhereclause=yes name=desc_art dbname="desc_art" )
 column=(type=char(20) updatewhereclause=yes name=und dbname="und" )
 column=(type=number updatewhereclause=yes name=saldo_total dbname="saldo_total" )
 column=(type=number updatewhereclause=yes name=total_pendiente dbname="total_pendiente" )
 column=(type=number updatewhereclause=yes name=cant_por_producir dbname="cant_por_producir" )
 column=(type=number updatewhereclause=yes name=saldo_disponible dbname="saldo_disponible" )
 column=(type=number updatewhereclause=yes name=cant_pendiente dbname="cant_pendiente" )
 retrieve="select te.especie,
       te.descr_especie,
       a1.cat_art,
       a1.desc_categoria,
       a2.cod_sub_cat,
       a2.desc_sub_cat,
       a.cod_art,
       a.desc_art,
       a.und,
       ov.cliente,
       ov.nro_ov,
       p.nom_proveedor as nom_cliente,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
		 nvl(sum(usf_ap_conv_und(amp.und, a.und, amp.cant_proyect) - amp.cant_procesada),0) as cant_pendiente,
       s1.cant_pendiente as total_pendiente,
       nvl(s2.saldo_total,0) as saldo_total,
       case
		    when s1.cant_pendiente > nvl(s2.saldo_total,0) then
			    s1.cant_pendiente - nvl(s2.saldo_total,0)	
			 else
			    0
		 end as cant_por_producir,
		 nvl(s2.saldo_total,0) - s1.cant_pendiente as saldo_disponible
  from orden_venta ov,
       articulo_mov_proy amp,
       proveedor         p,
       articulo          a,
       tg_especies       te,
       tg_especies_articulo tea,
       articulo_categ       a1,
       articulo_sub_categ   a2,
       (select amp.cod_art,
               nvl(sum(usf_ap_conv_und(amp.und, an.und, amp.cant_proyect)),0) as cant_pedida,
               nvl(sum(usf_ap_conv_und(amp.und, an.und, amp.cant_proyect) - amp.cant_procesada),0) as cant_pendiente
          from articulo_mov_proy  amp,
               articulo           an,
               articulo_sub_categ an2,
               articulo_Categ     an1
         where amp.flag_estado  = '1'
           and amp.cod_Art      = an.cod_Art
           and an.sub_Cat_art   = an2.cod_sub_Cat
           and an2.cat_Art      = an1.cat_art
 			  and an1.cat_art in (select tt2.cat_art from tt_articulo_almacen tt2)
           and usf_ap_conv_und(amp.und, an.und, amp.cant_proyect) - amp.cant_procesada > 0
           and amp.tipo_doc     = (select l.doc_ov from logparam l where reckey = '1')
         group by amp.cod_art) s1,
       (select am1.cod_art,
               nvl(sum(am1.cant_procesada * amt1.factor_sldo_total), 0) as saldo_total
          from vale_mov             vm1,
               articulo_mov         am1,
               articulo_mov_tipo    amt1,
               almacen              al1
         where vm1.nro_vale     = am1.Nro_Vale
           and vm1.tipo_mov     = amt1.tipo_mov
           and vm1.almacen      = al1.almacen
           and am1.flag_estado  <> '0'
           and vm1.flag_estado  <> '0'
           and al1.flag_virtual = '0'
			  and al1.almacen in (select almacen from tt_almacenes)
           and al1.almacen in (select * from table(split(pkg_config.USF_GET_PARAMETER('ALMACENES_PPTT', 'ALCG01'))))
        group by am1.cod_art) s2
 where ov.nro_ov   		= amp.nro_doc
   and amp.tipo_doc 		= (select doc_ov from logparam where reckey = '1')
   and amp.cod_art  		= s2.cod_art     (+)
   and amp.cod_art  		= s1.cod_art 
   and amp.cod_art  		= a.cod_art
   and a.cod_art    		= tea.cod_art    (+)
   and tea.especie  		= te.especie     (+)
   and ov.cliente   		= p.proveedor
   and a.sub_cat_art  	= a2.cod_sub_cat
   and a2.cat_art     	= a1.cat_art
   and usf_ap_conv_und(amp.und, a.und, amp.cant_proyect) - amp.cant_procesada > 0
   and (te.especie in (select tt.especie from tt_especies tt) or te.especie is null)
   and a1.cat_art in (select tt2.cat_art from tt_articulo_almacen tt2)
	and a2.cod_sub_cat in (select sub_cat from TT_CNT_SUB_CAT)
	and ov.cliente in (select tt3.proveedor from TT_PROVEEDOR tt3)
   and amp.almacen in (select almacen from tt_almacenes)
   and ov.flag_estado = '1'
group by te.especie,
       te.descr_especie,
       a1.cat_art,
       a1.desc_categoria,
       a2.cod_sub_cat,
       a2.desc_sub_cat,
       a.cod_art,
       a.desc_art,
       a.und,
       ov.cliente,
       ov.nro_ov,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
       s1.cant_pendiente,
       s2.saldo_total
union
select te.especie,
       te.descr_especie,
       a1.cat_art,
       a1.desc_categoria,
       a2.cod_sub_cat,
       a2.desc_sub_cat,
       a.cod_art,
       a.desc_art,
       a.und,
       null as cliente,
       null as nro_ov,
       null as nom_cliente,
       null as ruc_dni,
       0 as cant_pendiente,
       0 as total_pendiente,
       nvl(s2.saldo_total,0) as saldo_total,
       0 as cant_por_producir,
		 nvl(s2.saldo_total,0) as saldo_disponible
  from articulo          a,
       tg_especies       te,
       tg_especies_articulo tea,
       articulo_categ       a1,
       articulo_sub_categ   a2,
       (select cod_art
          from articulo a
        minus
        select amp.cod_art
          from articulo_mov_proy  amp,
               articulo           an,
               articulo_sub_categ an2,
               articulo_Categ     an1
         where amp.flag_estado  = '1'
           and amp.cod_Art      = an.cod_Art
           and an.sub_Cat_art   = an2.cod_sub_Cat
           and an2.cat_Art      = an1.cat_art
           and usf_ap_conv_und(amp.und, an.und, amp.cant_proyect) - amp.cant_procesada > 0
           and amp.tipo_doc     = (select l.doc_ov from logparam l where reckey = '1')
         group by amp.cod_art) s1,
       (select am1.cod_art,
               nvl(sum(am1.cant_procesada * amt1.factor_sldo_total), 0) as saldo_total
          from vale_mov             vm1,
               articulo_mov         am1,
               articulo_mov_tipo    amt1,
               almacen              al1
         where vm1.nro_vale     = am1.Nro_Vale
           and vm1.tipo_mov     = amt1.tipo_mov
           and vm1.almacen      = al1.almacen
           and am1.flag_estado  <> '0'
           and vm1.flag_estado  <> '0'
           and al1.flag_virtual = '0'
			  and al1.almacen in (select almacen from tt_almacenes)
           and al1.almacen in (select * from table(split(pkg_config.USF_GET_PARAMETER('ALMACENES_PPTT', 'ALCG01'))))
        group by am1.cod_art) s2
 where a.cod_art        = tea.cod_art    (+)
   and tea.especie      = te.especie     (+)
   and a.sub_cat_art    = a2.cod_sub_cat
   and a2.cat_art       = a1.cat_art
   and a.cod_art        = s2.cod_art     
   and a.cod_art        = s1.cod_art
	and s2.saldo_total > 0
   and (te.especie in (select tt.especie from tt_especies tt) or te.especie is null)
   and a1.cat_art in (select tt2.cat_art from tt_articulo_almacen tt2)
   and a2.cod_sub_cat in (select sub_cat from TT_CNT_SUB_CAT)
group by te.especie,
         te.descr_especie,
         a1.cat_art,
         a1.desc_categoria,
         a2.cod_sub_cat,
         a2.desc_sub_cat,
         a.cod_art,
         a.desc_art,
         a.und,
         s2.saldo_total"  sort="descr_especie A desc_categoria A desc_sub_cat A cod_art A desc_art A und A saldo_total A total_pendiente A cant_por_producir A saldo_disponible A " )
