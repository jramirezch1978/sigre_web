table(column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="nro_doc" )
 column=(type=char(4000) updatewhereclause=yes name=full_nro_doc dbname="full_nro_doc" )
 column=(type=datetime updatewhereclause=yes name=fecha_documento dbname="fecha_documento" )
 column=(type=char(2000) updatewhereclause=yes name=observacion dbname="observacion" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cod_moneda" )
 column=(type=decimal(2) updatewhereclause=yes name=importe_doc dbname="importe_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_detraccion dbname="nro_detraccion" )
 column=(type=decimal(2) updatewhereclause=yes name=imp_detraccion dbname="imp_detraccion" )
 column=(type=char(6) updatewhereclause=yes name=cod_usr dbname="cod_usr" )
 column=(type=char(6) updatewhereclause=yes name=vendedor dbname="vendedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_vendedor dbname="nom_vendedor" )
 retrieve="select  distinct
		  vw.tipo_doc,
        vw.nro_doc,
        pkg_fact_electronica.of_get_full_nro(vw.nro_doc) as full_nro_doc,
        vw.fecha_documento,
        vw.observacion,
        vw.cod_moneda,
        vw.importe_doc,
        vw.nro_detraccion,
        vw.imp_detraccion,
        vw.cod_usr,
        vw.vendedor,
        vw.nom_vendedor
  from (SELECT  CCD.TIPO_DOC,   
                CCD.NRO_DOC,   
                cc.fecha_documento,
                cc.cod_relacion,
                cc.cod_usr,
                cc.nro_detraccion,
                cc.imp_detraccion,
                cc.importe_doc,
					 cc.flag_estado,
                CCD.ITEM,   
                CCD.CONFIN,   
                CCD.DESCRIPCION,   
                CCD.COD_ART,   
                CC.COD_MONEDA,   
                CCD.PRECIO_UNITARIO,   
                CF.MATRIZ_CNTBL,
                CCD.TIPO_CRED_FISCAL,
                CFI.DESCRIPCION AS DESC_CRED_FISCAL,
                cc.observacion,
                ccd.und,
                ccd.und2,
                s1.cantidad_und1,
                s1.cantidad_und2,
                ccd.cencos,
                ccd.centro_benef,
                cc.vendedor,
                v.nom_vendedor,
                s1.nro_vale,
                s1.org_am,
                s1.nro_am,
                s1.flag_und2,
                s1.factor_conv_und,
                ccd.org_amp_ref,
                ccd.nro_amp_ref
        FROM  CNTAS_COBRAR_DET      CCD,   
              CNTAS_COBRAR          CC,   
              CONCEPTO_FINANCIERO   CF,
              credito_fiscal        CFI,
              vendedor              v,
              (select distinct 
                      vw.tipo_doc,
                      vw.nro_doc,
                      vw.item,
                      vw.org_amp_ref,
                      vw.nro_amp_ref,
                      vm.nro_Vale,
                      vm.tipo_mov,
                      am.cod_art,
                      am.cant_procesada as cantidad_und1,
                      am.cant_proc_und2 as cantidad_und2,
                      am.cod_origen as org_am,
                      am.nro_mov    as nro_am,
                      a.factor_conv_und,
                      a.flag_und2
                 from vw_vta_reg_ventas_det vw,
                      articulo_mov          am,
                      vale_mov              vm,
                      articulo              a
                where vw.org_amp_ref        = am.origen_mov_proy
                  and vw.nro_amp_ref        = am.nro_mov_proy
                  and am.nro_Vale           = vm.nro_vale
                  and am.cod_art            = a.cod_art
                  and vm.flag_estado        <> '0'
                  and am.flag_estado        <> '0'
                  and vm.tipo_mov           like 'I%'
                  and vw.tipo_doc           in ('FAC', 'BVC')) s1,
              (select ccd.tipo_doc, ccd.nro_doc, ccd.item, 
                      (ccd.precio_unitario * ccd.cantidad) as importe,
                      (select nvl(sum(ccd2.precio_unitario * ccd2.cantidad),0)
                         from cntas_cobrar_det ccd2,
                              cntas_cobrar     cc2
                        where ccd2.tipo_doc    = cc2.tipo_doc
                          and ccd2.nro_doc     = cc2.nro_doc
                          and cc2.flag_estado  <> '0'
                          and ccd2.tipo_ref    = ccd.tipo_doc
                          and ccd2.nro_ref     = ccd.nro_doc
                          and ccd2.item_ref    = ccd.item
                          and cc2.tipo_Doc     = 'NCC') as referencia
                 from cntas_cobrar_det ccd,
                      cntas_cobrar     cc
                where cc.tipo_Doc      = ccd.tipo_doc
                  and cc.nro_doc       = ccd.nro_doc
                  and cc.flag_estado   <> '0'
                  and cc.tipo_doc      in ('FAC', 'BVC')
                and (ccd.precio_unitario * ccd.cantidad) - (select nvl(sum(ccd2.precio_unitario * ccd2.cantidad),0)
                                                              from cntas_cobrar_det ccd2,
                                                                   cntas_cobrar     cc2
                                                             where ccd2.tipo_doc    = cc2.tipo_doc
                                                               and ccd2.nro_doc     = cc2.nro_doc
                                                               and cc2.flag_estado  <> '0'
                                                               and ccd2.tipo_ref    = ccd.tipo_doc
                                                               and ccd2.nro_ref     = ccd.nro_doc
                                                               and ccd2.item_ref    = ccd.item
                                                               and cc2.tipo_Doc     = 'NCC') > 0
              ) s
        WHERE CCD.CONFIN            = CF.CONFIN 
          and CCD.TIPO_DOC          = CC.TIPO_DOC
          and CCD.NRO_DOC           = CC.NRO_DOC 
          and cc.vendedor           = v.vendedor
          and ccd.tipo_doc          = s.tipo_doc
          and ccd.nro_doc           = s.nro_doc
          and ccd.item              = s.item
          and ccd.tipo_cred_fiscal  = cfi.tipo_cred_fiscal
          and ccd.tipo_doc          = s1.tipo_doc
          and ccd.nro_doc           = s1.nro_doc 
          and ccd.item              = s1.item    
          and ccd.org_amp_ref       is not null
          and ccd.nro_amp_ref       is not null
        union
        SELECT  CCD.TIPO_DOC,   
                CCD.NRO_DOC,   
                cc.fecha_documento,
                cc.cod_relacion,
                cc.cod_usr,
                cc.nro_detraccion,
                cc.imp_detraccion,
                cc.importe_doc,
					 cc.flag_estado,
                CCD.ITEM,   
                CCD.CONFIN,   
                CCD.DESCRIPCION,   
                CCD.COD_ART,   
                CC.COD_MONEDA,   
                CCD.PRECIO_UNITARIO,   
                CF.MATRIZ_CNTBL,
                CCD.TIPO_CRED_FISCAL,
                CFI.DESCRIPCION AS DESC_CRED_FISCAL,
                cc.observacion,
                ccd.und,
                ccd.und2,
                ccd.cantidad,
                ccd.cantidad_und2,
                ccd.cencos,
                ccd.centro_benef,
                cc.vendedor,
                v.nom_vendedor,
                null as nro_vale,
                null as org_am,
                null as nro_am,
                a.flag_und2,
                a.factor_conv_und,
                ccd.org_amp_ref,
                ccd.nro_amp_ref
        FROM  CNTAS_COBRAR_DET      CCD,   
              CNTAS_COBRAR          CC,   
              CONCEPTO_FINANCIERO   CF,
              credito_fiscal        CFI,
              vendedor              v,
            articulo           a,
              (select ccd.tipo_doc, ccd.nro_doc, ccd.item, 
                      (ccd.precio_unitario * ccd.cantidad) as importe,
                      (select nvl(sum(ccd2.precio_unitario * ccd2.cantidad),0)
                         from cntas_cobrar_det ccd2,
                              cntas_cobrar     cc2
                        where ccd2.tipo_doc    = cc2.tipo_doc
                          and ccd2.nro_doc     = cc2.nro_doc
                          and cc2.flag_estado  <> '0'
                          and ccd2.tipo_ref    = ccd.tipo_doc
                          and ccd2.nro_ref     = ccd.nro_doc
                          and ccd2.item_ref    = ccd.item
                          and cc2.tipo_Doc     = 'NCC') as referencia
                 from cntas_cobrar_det ccd,
                      cntas_cobrar     cc
                where cc.tipo_Doc      = ccd.tipo_doc
                  and cc.nro_doc       = ccd.nro_doc
                  and cc.flag_estado   <> '0'
                  and cc.tipo_doc      in ('FAC', 'BVC')
                and (ccd.precio_unitario * ccd.cantidad) - (select nvl(sum(ccd2.precio_unitario * ccd2.cantidad),0)
                                                              from cntas_cobrar_det ccd2,
                                                                   cntas_cobrar     cc2
                                                             where ccd2.tipo_doc    = cc2.tipo_doc
                                                               and ccd2.nro_doc     = cc2.nro_doc
                                                               and cc2.flag_estado  <> '0'
                                                               and ccd2.tipo_ref    = ccd.tipo_doc
                                                               and ccd2.nro_ref     = ccd.nro_doc
                                                               and ccd2.item_ref    = ccd.item
                                                               and cc2.tipo_Doc     = 'NCC') > 0
              ) s
        WHERE CCD.CONFIN            = CF.CONFIN 
          and CCD.TIPO_DOC          = CC.TIPO_DOC
          and CCD.NRO_DOC           = CC.NRO_DOC 
          and cc.vendedor           = v.vendedor
          and ccd.tipo_cred_fiscal  = cfi.tipo_cred_fiscal
          and ccd.cod_art        = a.cod_art        (+)
          and ccd.org_amp_ref       is null
          and ccd.nro_amp_ref       is null
          and ccd.tipo_doc          = s.tipo_doc
          and ccd.nro_doc           = s.nro_doc
          and ccd.item              = s.item 
     ) vw
where vw.cod_relacion		 = :as_cod_relacion
  and vw.FLAG_ESTADO  in ('1','2','3')
  and substr(pkg_fact_electronica.of_Get_serie(vw.nro_doc),1,1) = 'B'

