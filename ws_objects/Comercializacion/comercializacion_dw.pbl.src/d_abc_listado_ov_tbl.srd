 column=(type=datetime updatewhereclause=yes name=fec_registro dbname="fec_registro" )
 column=(type=char(8) updatewhereclause=yes name=cliente dbname="orden_venta.cliente" dbalias="ov.cliente" )
 column=(type=char(200) updatewhereclause=yes name=nom_cliente dbname="proveedor.nom_cliente" dbalias="p.nom_cliente" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=char(6) updatewhereclause=yes name=almacen dbname="articulo_mov_proy.almacen" dbalias="amp.almacen" )
 column=(type=char(6) updatewhereclause=yes name=vendedor dbname="orden_venta.vendedor" dbalias="ov.vendedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_vendedor dbname="vendedor.nom_vendedor" dbalias="v.nom_vendedor" )
 column=(type=number updatewhereclause=yes name=cant_pendiente dbname="cant_pendiente" )
 column=(type=number updatewhereclause=yes name=stock_actual dbname="stock_actual" )
 column=(type=number updatewhereclause=yes name=cant_bonif dbname="cant_bonif" )
 column=(type=number updatewhereclause=yes name=stock_bonif dbname="stock_bonif" )
 column=(type=number updatewhereclause=yes name=cant_composicion dbname="cant_composicion" )
 column=(type=number updatewhereclause=yes name=stock_composicion dbname="stock_composicion" )
 retrieve="select '0' as checked,
		 amp.tipo_doc,
       amp.nro_doc,
       trunc(ov.fec_registro) as fec_registro,
       ov.cliente,
       p.nom_proveedor as nom_cliente,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       amp.almacen,
       ov.vendedor,
       v.nom_vendedor,
       sum(case
              when trim(a.cod_clase) <> PKG_CONFIG.USF_GET_PARAMETER('CLASE_BONIFICACION', '20') then
                 amp.cant_proyect - amp.cant_procesada
              else
                 0
           end ) as cant_pendiente,
       sum(case
              when trim(a.cod_clase) <> PKG_CONFIG.USF_GET_PARAMETER('CLASE_BONIFICACION', '20') then
                 (select nvl(sum(aa.sldo_total),0)
                    from articulo_almacen aa
                   where aa.cod_art = amp.cod_art
                     and aa.almacen = amp.almacen)
              else
                 0
           end ) as stock_actual,
       sum(case
              when trim(a.cod_clase) = PKG_CONFIG.USF_GET_PARAMETER('CLASE_BONIFICACION', '20') then
                 amp.cant_proyect - amp.cant_procesada
              else
                 0
           end ) as cant_bonif,
		 sum(case
              when trim(a.cod_clase) = PKG_CONFIG.USF_GET_PARAMETER('CLASE_BONIFICACION', '20') then
                 (select nvl(sum(aa.sldo_total),0)
                    from articulo_almacen aa
                   where aa.cod_art = amp.cod_art
                     and aa.almacen = amp.almacen)
              else
                 0
           end ) as stock_bonif,
       sum(case
              when a.cod_art in (select cod_art from articulo_composicion) then
                 amp.cant_proyect - amp.cant_procesada
              else
                 0
           end ) as cant_composicion,
       sum(case
              when a.cod_art in (select cod_art from articulo_composicion) then
                 (select nvl(sum(aa.sldo_total),0)
                    from articulo_almacen aa
                   where aa.cod_art = amp.cod_art
                     and aa.almacen = amp.almacen)
              else
                 0
           end ) as stock_composicion
 from articulo_mov_proy amp,
      orden_Venta       ov,
      articulo          a,
      proveedor         p,
      vendedor          v
where amp.nro_doc       = ov.nro_ov
  and amp.tipo_doc      = (select doc_ov from logparam where reckey = '1')
  and amp.cod_art       = a.cod_art
  and ov.cliente        = p.proveedor
  and ov.vendedor       = v.vendedor
  --and amp.cod_art       in (select cod_art from articulo_composicion ac)
  and amp.cant_proyect - amp.cant_procesada > 0
  and amp.flag_estado  = '1'
  and ov.flag_estado   = '1'
  and trunc(ov.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
group by amp.tipo_doc,
         amp.nro_doc,
         trunc(ov.fec_registro),
         ov.cliente,
         p.nom_proveedor,
         decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
         amp.almacen,
         ov.vendedor,
         v.nom_vendedor
         " arguments=(("ad_fecha1", date),("ad_fecha2", date))  sort="fec_registro A tipo_doc A nro_doc A " )
