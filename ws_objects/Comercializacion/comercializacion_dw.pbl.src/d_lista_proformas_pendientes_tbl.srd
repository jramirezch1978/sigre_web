 column=(type=char(1) updatewhereclause=yes name=flag_factura_boleta dbname="proforma.flag_factura_boleta" )
 column=(type=char(602) updatewhereclause=yes name=desc_ubigeo dbname="desc_ubigeo" )
 column=(type=char(15) updatewhereclause=yes name=flag_bol_fact dbname="flag_bol_fact" )
 column=(type=char(2000) updatewhereclause=yes name=desc_zona_despacho dbname="vta_zona_despacho.desc_zona_despacho" )
 column=(type=number updatewhereclause=yes name=nro_registros dbname="nro_registros" )
 column=(type=number updatewhereclause=yes name=base_imponible dbname="importe" )
 column=(type=number updatewhereclause=yes name=igv dbname="igv" )
 retrieve="select '0' as checked,
       pf.nro_proforma,
       pf.fec_registro,
       pf.cod_usr,
       v.nom_vendedor as nom_vendedor,
       p.nom_proveedor as nom_cliente,
       pf.flag_estado,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       pf.flag_factura_boleta,
       su.departamento || '-' || su.provincia || '-' || su.distrito as desc_ubigeo,
       case 
          when pf.flag_factura_boleta = 'N' then 'NOTA DE VENTA'
          WHEN pf.flag_factura_boleta = 'B' then 'BOLETA DE VENTA'
          WHEN pf.flag_factura_boleta = 'F' then 'FACTURA'
       end as flag_bol_fact,
       zp.desc_zona_despacho,
       count(pfd.cod_art) as nro_registros,
       nvl(sum(pfd.cantidad * pfd.precio_vta),0) as importe,
       nvl(sum(pfd.cantidad * pfd.igv),0) as igv
from proforma          pf,
     proforma_det      pfd,
     vendedor          v,
     proveedor         p,
     direcciones       d,
     vta_zona_despacho zp,
     sunat_ubigeo      su
where pf.nro_proforma    = pfd.nro_proforma     
  and pf.vendedor        = v.vendedor       (+)
  and pf.cliente         = p.proveedor      (+)
  and pf.cliente         = d.codigo         (+)
  and pf.item_direccion  = d.item           (+) 
  and d.zona_despacho    = zp.zona_despacho (+)
  and d.ubigeo           = su.ubigeo        (+)
  and pf.flag_estado  = '1'
  and pfd.cantidad - (select nvl(sum(fd.cant_proyect), 0)
                        from fs_factura_simpl f,
                             fs_factura_simpl_det fd
                       where f.nro_registro = fd.nro_registro
                         and fd.nro_proforma = pfd.nro_proforma
                         and fd.item_proforma = pfd.nro_item
                         and f.flag_estado    <> '0') > 0
              
group by pf.nro_proforma,
         pf.fec_registro,
         pf.cod_usr,
         v.nom_vendedor,
         p.nom_proveedor,
         pf.flag_estado,
         decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
         pf.flag_factura_boleta,
         case 
            when pf.flag_factura_boleta = 'N' then 'NOTA DE VENTA'
            WHEN pf.flag_factura_boleta = 'B' then 'BOLETA DE VENTA'
            WHEN pf.flag_factura_boleta = 'F' then 'FACTURA'
         end,
         su.departamento || '-' || su.provincia || '-' || su.distrito,
         zp.desc_zona_despacho
"  sort="fec_registro D " )
