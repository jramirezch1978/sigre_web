 retrieve=" SELECT ART.COD_ART,ART.NOM_ARTICULO,ART.UND,
        Round(Sum(CCD.PRECIO_UNITARIO) / COUNT(*),6) as precio_promedio,
		    TRUNC(FECHA_DOCUMENTO) as FECHA_DOCUMENTO
  FROM ARTICULO ART,CNTAS_COBRAR CC,CNTAS_COBRAR_DET CCD
 WHERE (CC.TIPO_DOC = CCD.TIPO_DOC ) AND
       (CC.NRO_DOC  = CCD.NRO_DOC  ) AND
       (CC.FLAG_ESTADO <> '0'      ) AND
       (ART.COD_ART    = CCD.COD_ART ) AND
       (CC.FORMA_PAGO  IN (SELECT FP.FORMA_PAGO FROM FORMA_PAGO FP WHERE NVL(FLAG_NO_COBRABLE,'0') = '0') ) AND
       (CC.TIPO_DOC    IN (SELECT TIPO_DOC FROM DOC_GRUPO_RELACION WHERE GRUPO IN (SELECT DOC_VENTAS FROM FINPARAM))) AND  
       (CCD.PRECIO_UNITARIO <> 0   ) AND
       (CCD.COD_ART         = :as_cod_art ) AND
       (TO_CHAR(CC.FECHA_DOCUMENTO,'YYYYMMDD') >= :as_fecha_inicio and
		  TO_CHAR(CC.FECHA_DOCUMENTO,'YYYYMMDD') <= :as_fecha_final  ) 
GROUP BY ART.COD_ART  ,ART.NOM_ARTICULO,ART.UND,TRUNC(FECHA_DOCUMENTO)	" arguments=(("as_fecha_inicio", string),("as_fecha_final", string),("as_cod_art", string))  sort="fecha_documento A " )
