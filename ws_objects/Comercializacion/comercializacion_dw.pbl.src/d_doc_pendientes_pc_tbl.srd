 retrieve="  SELECT ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"            ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"     ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"  ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"   ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"FACTOR~"      ,
			~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~"  ,
			~"CNTAS_COBRAR~".~"FLAG_PROVISIONADO~"		,
			~"DETRACCION~".~"NRO_DEPOSITO~"				,
			~"DETRACCION~".~"FECHA_DEPOSITO~"				,
			~"CNTAS_COBRAR~".~"NRO_DETRACCION~" AS REFERENCIA	
    FROM ~"CNTAS_COBRAR~"          ,   
         ~"DOC_PENDIENTES_CTA_CTE~",   
         ~"PROVEEDOR~"  				,
			~"DETRACCION~"
   WHERE ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = ~"CNTAS_COBRAR~".~"COD_RELACION~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" = ~"CNTAS_COBRAR~".~"TIPO_DOC~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~" = ~"CNTAS_COBRAR~".~"NRO_DOC~" ) and  
		   ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CNTAS_COBRAR~".~"COD_RELACION~" ) and  
			( ~"CNTAS_COBRAR~".~"NRO_DETRACCION~" = ~"DETRACCION~".~"NRO_DETRACCION~" (+)) AND
         (( ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~" =  (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1')       ) AND  
           ( ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~" >  0                                                                                                                 ) ) OR  
         ( ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~" =  (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1')  ) AND  
           ( ~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~" >  0                                                                                                                 ) ))    AND
			( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = :as_cod_relacion ) 
UNION

  SELECT ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"            ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~"     ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~"  ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~"    ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~"   ,   
         ~"DOC_PENDIENTES_CTA_CTE~".~"FACTOR~"      ,
			~"DOC_PENDIENTES_CTA_CTE~".~"FLAG_TABLA~"  ,
			~"CNTAS_PAGAR~".~"FLAG_PROVISIONADO~"		,
			NULL AS NRO_DEPOSITO ,
			NULL AS FECHA_DEPOSITO,
			(SELECT TIPO_DOC||' '||NRO_DOC FROM CNTAS_COBRAR WHERE NRO_DETRACCION                  =  DOC_PENDIENTES_CTA_CTE.NRO_DOC AND
                                          							 DOC_PENDIENTES_CTA_CTE.TIPO_DOC = (SELECT DOC_DETRAC_CP FROM FINPARAM WHERE RECKEY = '1') ) AS REFERENCIA
    FROM ~"CNTAS_PAGAR~",   
         ~"DOC_PENDIENTES_CTA_CTE~",   
         ~"PROVEEDOR~"  ,
			~"DETRACCION~"
   WHERE ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"TIPO_DOC~" = ~"CNTAS_PAGAR~".~"TIPO_DOC~" ) and  
         ( ~"DOC_PENDIENTES_CTA_CTE~".~"NRO_DOC~" = ~"CNTAS_PAGAR~".~"NRO_DOC~" ) and  
         ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CNTAS_PAGAR~".~"COD_RELACION~" ) and  
			( ~"CNTAS_PAGAR~".~"NRO_DETRACCION~" = ~"DETRACCION~".~"NRO_DETRACCION~" (+) ) AND
         (( ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~" =  (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY = '1')       ) AND  
           ( ~"DOC_PENDIENTES_CTA_CTE~".~"SLDO_SOL~" >  0                                                                                                                 ) ) OR  
         ( ( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_MONEDA~" =  (SELECT COD_DOLARES FROM LOGPARAM WHERE RECKEY = '1')  ) AND  
           ( ~"DOC_PENDIENTES_CTA_CTE~".~"SALDO_DOL~" >  0                                                                                                                 ) ))    and
			( ~"DOC_PENDIENTES_CTA_CTE~".~"COD_RELACION~" = :as_cod_relacion ) 
" arguments=(("as_cod_relacion", string)) )
