 retrieve="
 SELECT ART.COD_ART,ART.NOM_ARTICULO,ART.UND,
        Round(Sum(CCD.PRECIO_UNITARIO) / COUNT(*),6) as precio_promedio,
		TO_NUMBER(TO_CHAR(CC.FECHA_DOCUMENTO,'YYYY')) AS ANO,		
		 TO_NUMBER(TO_CHAR(CC.FECHA_DOCUMENTO,'MM')),		
       DECODE(TO_NUMBER(TO_CHAR(CC.FECHA_DOCUMENTO,'MM')),1,'Enero'  ,2,'Febrero'  ,3,'Marzo'    ,
                                            4,'Abril'  ,5,'Mayo'     ,6,'Junio'    ,
                                            7,'Julio'  ,8,'Agosto'   ,9,'Setiembre', 
                                            10,'Octubre',11,'Noviembre',12,'Diciembre') AS MES
  FROM ARTICULO ART,CNTAS_COBRAR CC,CNTAS_COBRAR_DET CCD
 WHERE  
        (CC.TIPO_DOC = CCD.TIPO_DOC ) AND
        (CC.NRO_DOC  = CCD.NRO_DOC  ) AND
        (CC.FLAG_ESTADO <> '0'      ) AND
        (ART.COD_ART    = CCD.COD_ART ) AND
        (CC.FORMA_PAGO  IN (SELECT FP.FORMA_PAGO FROM FORMA_PAGO FP WHERE NVL(FLAG_NO_COBRABLE,'0') = '0') ) AND
        (CC.TIPO_DOC    IN (SELECT TIPO_DOC FROM DOC_GRUPO_RELACION WHERE GRUPO IN (SELECT DOC_VENTAS FROM FINPARAM))) AND  
        (CCD.PRECIO_UNITARIO <> 0   ) AND
        (CCD.COD_ART         = :as_cod_art) AND
        (TO_CHAR(CC.FECHA_DOCUMENTO,'YYYY') = :al_ano ) 
GROUP BY ART.COD_ART  ,ART.NOM_ARTICULO,ART.UND,		TO_NUMBER(TO_CHAR(CC.FECHA_DOCUMENTO,'YYYY')) ,		TO_NUMBER(TO_CHAR(CC.FECHA_DOCUMENTO,'MM')   )" arguments=(("as_cod_art", string),("al_ano", number)) )
