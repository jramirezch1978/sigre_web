 retrieve="  SELECT ~"VENDEDOR_CUOTA~".~"CANAL~",   
         ~"CANAL_DISTRIBUCION~".~"DESCRIPCION~",   
         ~"VENDEDOR_CUOTA~".~"VENDEDOR~",   
         ~"USUARIO~".~"NOMBRE~",   
         ~"VENDEDOR_CUOTA~".~"UND~",   
         ~"VENDEDOR_CUOTA~".~"CANT_CUOTA_MEN~",   
         ~"VENDEDOR_CUOTA~".~"FECHA_INI~",   
         ~"VENDEDOR_CUOTA~".~"FECHA_FIN~"  
    FROM ~"CANAL_DISTRIBUCION~",   
         ~"USUARIO~",   
         ~"VENDEDOR_CUOTA~"  
   WHERE ( ~"VENDEDOR_CUOTA~".~"CANAL~" = ~"CANAL_DISTRIBUCION~".~"CANAL~" ) and  
         ( ~"VENDEDOR_CUOTA~".~"VENDEDOR~" = ~"USUARIO~".~"COD_USR~" ) and  
         ( ( ~"VENDEDOR_CUOTA~".~"FECHA_INI~" = to_date(Sysdate+1,'dd/mm/yy') ) ) 
Union
Select vz.canal, cd.descripcion, vz.vendedor, u.nombre, 'TON' as und, 0 As cuota,
       USF_FECHA_INICIO_COMISION As fecha_ini, last_day(USF_FECHA_INICIO_COMISION) As fecha_fin
  From vendedor_zona vz, canal_distribucion cd, usuario u
 Where vz.canal    = cd.canal
   And vz.vendedor = u.cod_usr
 Group By vz.canal, cd.descripcion, vz.vendedor, u.nombre
 Order By 2,4"  sort="descripcion A nombre A " )
