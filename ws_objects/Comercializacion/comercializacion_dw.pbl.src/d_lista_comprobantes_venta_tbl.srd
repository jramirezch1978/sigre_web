 retrieve="select '0' as seleccion,
		 v.tipo_doc,
       v.serie,
       v.numero,
		 v.nro_doc,
       v.nom_proveedor,
       v.tipo_doc_ident,
       v.ruc_dni,
       v.fecha_emision,
		 v.flag_estado,
		 v.cod_usr,
	    v.origen,
		 v.ano,
		 v.mes,
		 v.nro_libro,
		 v.nro_asiento,
       sum(v.base_imponible) as base_imponible,
       sum(v.importe_igv) as importe_igv,
       sum(v.base_imponible + v.importe_igv) as total_venta
from vw_vta_ventas v
where trunc(v.fecha_emision) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
group by v.tipo_doc,
       v.serie,
       v.numero,
		 v.nro_doc,
       v.nom_proveedor,
       v.tipo_doc_ident,
       v.ruc_dni,
       v.fecha_emision,
		 v.flag_estado,
		 v.cod_usr,
	    v.origen,
		 v.ano,
		 v.mes,
		 v.nro_libro,
		 v.nro_asiento" arguments=(("ad_fecha1", date),("ad_Fecha2", date))  sort="tipo_doc A serie A numero A " )
