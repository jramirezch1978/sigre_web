 retrieve="  SELECT ~"CNTAS_COBRAR~".~"TIPO_DOC~",   
         ~"CNTAS_COBRAR~".~"NRO_DOC~",   
         ~"CNTAS_COBRAR~".~"COD_RELACION~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~",   
         ~"CNTAS_COBRAR~".~"ORIGEN~",   
         ~"CNTAS_COBRAR~".~"ANO~",   
         ~"CNTAS_COBRAR~".~"MES~",   
         ~"CNTAS_COBRAR~".~"NRO_LIBRO~",   
         ~"CNTAS_COBRAR~".~"NRO_ASIENTO~",   
         ~"CNTAS_COBRAR~".~"COD_MONEDA~",   
         ~"CNTAS_COBRAR~".~"OBSERVACION~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_DOC~",   
         ~"ARTICULO~".~"SUB_CAT_ART~",   
         ~"ARTICULO_SUB_CATEG~".~"DESC_SUB_CAT~",   
         Sum(~"CNTAS_COBRAR_DET~".~"PRECIO_UNITARIO~") as precio_unitario,   
         Sum(~"CNTAS_COBRAR_DET~".~"CANTIDAD~") as cantidad  
    FROM ~"CNTAS_COBRAR_DET~",   
         ~"ORDEN_VENTA~",   
         ~"CNTAS_COBRAR~",   
         ~"PROVEEDOR~",   
         ~"ARTICULO~",   
         ~"ARTICULO_SUB_CATEG~"  
   WHERE ( ~"CNTAS_COBRAR~".~"TIPO_DOC~" = ~"CNTAS_COBRAR_DET~".~"TIPO_DOC~" ) and  
         ( ~"CNTAS_COBRAR~".~"NRO_DOC~" = ~"CNTAS_COBRAR_DET~".~"NRO_DOC~" ) and  
         ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CNTAS_COBRAR~".~"COD_RELACION~" ) and  
         ( ~"CNTAS_COBRAR_DET~".~"NRO_REF~" = ~"ORDEN_VENTA~".~"NRO_OV~" ) and  
         ( ~"ARTICULO~".~"COD_ART~" = ~"CNTAS_COBRAR_DET~".~"COD_ART~" ) and  
         ( ~"ARTICULO_SUB_CATEG~".~"COD_SUB_CAT~" = ~"ARTICULO~".~"SUB_CAT_ART~" ) and  
         ( ( ~"CNTAS_COBRAR_DET~".~"TIPO_REF~" = (select doc_ov from logparam where reckey = '1') ) AND  
         ( ~"ORDEN_VENTA~".~"FLAG_MERCADO~" = 'E' ) )    and
			( cntas_cobrar.flag_estado <> '0' )
group by ~"CNTAS_COBRAR~".~"TIPO_DOC~",   
         ~"CNTAS_COBRAR~".~"NRO_DOC~",   
         ~"CNTAS_COBRAR~".~"COD_RELACION~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CNTAS_COBRAR~".~"FECHA_DOCUMENTO~",   
         ~"CNTAS_COBRAR~".~"ORIGEN~",   
         ~"CNTAS_COBRAR~".~"ANO~",   
         ~"CNTAS_COBRAR~".~"MES~",   
         ~"CNTAS_COBRAR~".~"NRO_LIBRO~",   
         ~"CNTAS_COBRAR~".~"NRO_ASIENTO~",   
         ~"CNTAS_COBRAR~".~"COD_MONEDA~",   
         ~"CNTAS_COBRAR~".~"OBSERVACION~",   
         ~"CNTAS_COBRAR~".~"IMPORTE_DOC~",   
         ~"ARTICULO~".~"SUB_CAT_ART~",   
         ~"ARTICULO_SUB_CATEG~".~"DESC_SUB_CAT~"   
