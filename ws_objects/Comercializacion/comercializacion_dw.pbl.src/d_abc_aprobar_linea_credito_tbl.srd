 retrieve="select lc.proveedor,
       p.nom_proveedor as nom_cliente,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       lc.nro_item,
       lc.importe,
       lc.cod_moneda,
       lc.fec_registro,
       lc.fec_ini_vigencia,
       lc.fec_fin_vigencia,
       lc.cod_usr,
       u.nombre as nom_usuario,
       lc.flag_estado,
       lc.plazo_credito,
       lc.usr_aprob_rech,
       lc.fec_aprob_rech,
       lc.observaciones,
       lc.motivo_rechazo,
		 (select sum(vw.saldo_sol)
			 from vw_fin_pendiente_cobrar vw,
     				doc_tipo     dt
		   where vw.tipo_doc		 = dt.tipo_doc
  			  and dt.flag_signo 	 = '+'
  			  and vw.cod_relacion = p.proveedor
   		  and ((vw.cod_moneda = PKG_LOGISTICA.of_soles(null) and vw.saldo_sol > 0)
    			or (vw.cod_moneda = PKG_LOGISTICA.of_dolares(null) and vw.saldo_dol > 0))) as saldo_sol,
       (select sum(vw.saldo_dol)
			 from vw_fin_pendiente_cobrar vw,
     				doc_tipo     dt
		   where vw.tipo_doc		 = dt.tipo_doc
  			  and dt.flag_signo 	 = '+'
  			  and vw.cod_relacion = p.proveedor
   		  and ((vw.cod_moneda = PKG_LOGISTICA.of_soles(null) and vw.saldo_sol > 0)
    			or (vw.cod_moneda = PKG_LOGISTICA.of_dolares(null) and vw.saldo_dol > 0))) as saldo_dol
from proveedor_linea_credito lc,
     proveedor               p,
     usuario                 u
where lc.proveedor = p.proveedor     
  and lc.cod_usr   = u.cod_usr
  and lc.flag_estado = '3'" )
