 retrieve="  SELECT ~"CLIENTE~".~"CLIENTE~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CANAL_DISTRIBUCION~".~"DESCRIPCION~",   
         ~"CANAL_DISTRIBUCION_DET~".~"DESCRIPCION~",
         (Select Count(*) From direcciones Where codigo = ~"CLIENTE~".~"CLIENTE~"
         And flag_uso = '1' And flag_dir_comercial = '1') As dir
    FROM ~"CLIENTE~",   
         ~"PROVEEDOR~",   
         ~"CANAL_DISTRIBUCION~",   
         ~"CANAL_DISTRIBUCION_DET~"  
   WHERE ( ~"PROVEEDOR~".~"PROVEEDOR~" = ~"CLIENTE~".~"CLIENTE~" ) and  
         ( ~"CANAL_DISTRIBUCION_DET~".~"CANAL~" = ~"CANAL_DISTRIBUCION~".~"CANAL~" ) and  
         ( ~"CLIENTE~".~"CANAL~" = ~"CANAL_DISTRIBUCION_DET~".~"CANAL~" ) and  
         ( ~"CLIENTE~".~"SUB_CANAL~" = ~"CANAL_DISTRIBUCION_DET~".~"SUB_CANAL~" ) 
   Group By ~"CLIENTE~".~"CLIENTE~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~",   
         ~"CANAL_DISTRIBUCION~".~"DESCRIPCION~",   
         ~"CANAL_DISTRIBUCION_DET~".~"DESCRIPCION~"" )
