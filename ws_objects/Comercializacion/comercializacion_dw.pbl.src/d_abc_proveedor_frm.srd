table(column=(type=char(8) updatewhereclause=yes key=yes name=proveedor dbname="proveedor.proveedor" dbalias="p.proveedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=char(1) update=yes updatewhereclause=yes name=flag_linea_credito dbname="proveedor.flag_linea_credito" dbalias="p.flag_linea_credito" values="SI	1/NO	0/" )
 column=(type=char(4000) updatewhereclause=yes name=direccion dbname="direccion" )
 column=(type=number updatewhereclause=yes name=saldo_sol dbname="saldo_sol" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="select p.proveedor,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       p.flag_linea_credito,
       PKG_LOGISTICA.of_get_dir_comercial(p.proveedor) as direccion,
       (select nvl(sum(vw.saldo_sol),0)
			 from vw_fin_pendiente_cobrar vw,
     				doc_tipo     dt
		   where vw.tipo_doc		 = dt.tipo_doc
  			  and dt.flag_signo 	 = '+'
  			  and vw.cod_relacion = p.proveedor
   		  and ((vw.cod_moneda = PKG_LOGISTICA.of_soles(null) and vw.saldo_sol > 0)
    			or (vw.cod_moneda = PKG_LOGISTICA.of_dolares(null) and vw.saldo_dol > 0))) as saldo_sol,
       (select nvl(sum(vw.saldo_dol),0)
			 from vw_fin_pendiente_cobrar vw,
     				doc_tipo     dt
		   where vw.tipo_doc		 = dt.tipo_doc
  			  and dt.flag_signo 	 = '+'
  			  and vw.cod_relacion = p.proveedor
   		  and ((vw.cod_moneda = PKG_LOGISTICA.of_soles(null) and vw.saldo_sol > 0)
    			or (vw.cod_moneda = PKG_LOGISTICA.of_dolares(null) and vw.saldo_dol > 0))) as saldo_dol

from proveedor p
where p.proveedor = :as_proveedor
" update="proveedor" updatewhere=0 updatekeyinplace=yes arguments=(("as_proveedor", string)) )
