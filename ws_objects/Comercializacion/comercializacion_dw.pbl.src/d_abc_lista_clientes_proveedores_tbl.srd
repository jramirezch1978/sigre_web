 retrieve="  SELECT proveedor.proveedor as codigo,   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"  
    FROM ~"PROVEEDOR~",   
         ~"CNTAS_COBRAR~"  
   WHERE ( ~"CNTAS_COBRAR~".~"COD_RELACION~" = ~"PROVEEDOR~".~"PROVEEDOR~" ) and  
          ( ~"PROVEEDOR~".~"FLAG_ESTADO~" = '1' ) AND  
         (to_char(cntas_cobrar.fecha_documento,'yyyymmdd')  >= :as_fecha_inicio ) and
		   (to_char(cntas_cobrar.fecha_documento,'yyyymmdd')  <= :as_fecha_final  )
GROUP BY ~"PROVEEDOR~".~"PROVEEDOR~",   
         ~"PROVEEDOR~".~"NOM_PROVEEDOR~"  
ORDER BY ~"PROVEEDOR~".~"NOM_PROVEEDOR~" ASC   
" arguments=(("as_fecha_inicio", string),("as_fecha_final", string)) )
