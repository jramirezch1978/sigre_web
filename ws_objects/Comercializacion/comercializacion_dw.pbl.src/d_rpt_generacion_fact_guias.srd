 retrieve="Select g.nro_guia, g.cliente, p.Nom_Proveedor, p.ruc, 
       vm.cod_origen As origen_vale, am.cod_art, a.desc_art, 
		 sum(nvl(am.cant_procesada,0)) as cant_procesada,
       nvl(av.precio_minorista,0) as precio_minorista, nvl(av.confin,'Sin Confin') as confin, nvl(av.cod_moneda,'S/M') as cod_moneda,
       USF_FIN_ARTICULO_SINIGV(am.cod_art) As flag_igv
  From guia g, guia_vale gv, vale_mov vm, articulo_mov am,
       proveedor p, articulo a, articulo_venta av
 Where g.cod_origen = gv.origen_guia
   And g.nro_guia = gv.nro_guia
   And gv.origen_vale = vm.cod_origen
   And gv.nro_vale = vm.nro_vale
   And vm.cod_origen = am.cod_origen
   And vm.nro_vale = am.nro_vale
   And g.cliente = p.proveedor (+)
   And am.cod_art = a.cod_art
   And am.cod_art = av.cod_art (+)
   And g.cod_origen = :as_origen
   And trunc(g.fec_registro) >= :ad_fec_ini
   And trunc(g.fec_registro) <= :ad_fec_fin
   And g.flag_estado = '1'
   And vm.tipo_mov = :as_tipo_mov
 group by g.nro_guia, g.cliente, p.Nom_Proveedor, p.ruc,
			 vm.cod_origen, am.cod_art, a.desc_art, 
			 nvl(av.precio_minorista,0), nvl(av.confin,'Sin Confin'), nvl(av.cod_moneda,'S/M'),
			 USF_FIN_ARTICULO_SINIGV(am.cod_art)
 Order By g.nro_guia, g.cliente, am.cod_art
