 column=(type=number updatewhereclause=yes name=monto_contrato dbname="monto_contrato" )
 column=(type=number updatewhereclause=yes name=monto_despachado dbname="monto_despachado" )
 column=(type=number updatewhereclause=yes name=monto_facturado dbname="monto_facturado" )
 retrieve="select ag.grupo_art, ag.desc_grupo_art, 
       sum(amp.cant_proyect * usf_fl_conv_mon(amp.precio_unit, ov.cod_moneda, (select cod_dolares from logparam where reckey = '1'), sysdate)) as monto_contrato,
       sum(amp.cant_procesada * usf_fl_conv_mon(amp.precio_unit, ov.cod_moneda, (select cod_dolares from logparam where reckey = '1'), sysdate)) as monto_despachado,
       sum(amp.cant_facturada * usf_fl_conv_mon(amp.precio_unit, ov.cod_moneda, (select cod_dolares from logparam where reckey = '1'), sysdate)) as monto_facturado
from articulo_mov_proy amp,
     orden_Venta       ov,
     articulo          a,
	   tt_exp_Cencos	  tt,
     tt_exp_grupo_art tt2,
     rel_articulo_grupo rag,
     articulo_grupo     ag
where ov.nro_ov = amp.nro_doc
  and a.cod_Art = amp.cod_Art
  and amp.tipo_doc = (select doc_ov from logparam where reckey = '1')
  and amp.flag_estado <> '0'
  and ov.flag_estado <> '0'
  and amp.cencos = tt.cencos
  and amp.cencos is not null
  and rag.cod_art= amp.cod_art
  and rag.grupo_art = tt2.grupo_art
  and ag.grupo_art  = rag.grupo_art
  and trunc(ov.fec_registro) between trunc(:ad_Fecha1) and trunc(:ad_fecha2)
group by ag.grupo_art, ag.desc_grupo_art" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
