 column=(type=number updatewhereclause=yes name=incial dbname="incial" )
 column=(type=char(4) updatewhereclause=yes name=nro_letra dbname="cntas_cobrar_det.nro_letra" )
 column=(type=number updatewhereclause=yes name=importe_doc dbname="importe_doc" )
 column=(type=number updatewhereclause=yes name=dias dbname="dias" )
 column=(type=number updatewhereclause=yes name=recargo dbname="recargo" )
 retrieve="select cc.tipo_doc,
       pkg_fact_electronica.of_get_full_nro(cc.nro_doc) as nro_doc,
       cc.fecha_vencimiento,
       cc.estado,
		 cc.estado || '-' || case
			when cc.estado = '1' then
				'Letras - Pendiente de Facturar'
			when cc.estado = '2' then
				'Facturado - Pendiente de cobrar'
		   when cc.estado = '6' then
				'Bloqueado'
		 end as desc_estado,
       cc.cod_relacion,
       p.nom_proveedor,
       cb.desc_centro,
       trim(a.mnz) || '-' || trim(to_Char(a.lote, '00')) as lote,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) || a.mnz || trim(to_char(a.lote, '00')) as cod_proveedor,
       case
         	when cc.nro_letra like 'IN%' then
            cc.importe_doc
          else
            0
       end as incial,
       cc.nro_letra,
       case
          when cc.nro_letra like 'IN%' then
            0
          else
            cc.importe_doc
       end as importe_doc,
       trunc(sysdate - cc.fecha_vencimiento) as dias,
       case
			when trunc(sysdate - cc.fecha_vencimiento) > 3 then
				to_number(pkg_config.USF_GET_PARAMETER('RECARGO_COSTO_DIAS', 15)) 
			else 
				0
		 end as RECARGO
              
       
from (select cc1.*, pkg_comercializacion.of_estado_letra(cc1.tipo_doc, cc1.nro_doc) as estado 
	     from cntas_Cobrar cc1
		 where cc1.tipo_doc = 'LTC'
		   and cc1.flag_estado <> '0'
         and to_char(cc1.fecha_vencimiento, 'yyyy') >= 2018) cc,
     cntas_cobrar_det ccd,
     proveedor        p,
     centro_beneficio cb,
     articulo         a
where cc.tipo_doc = ccd.tipo_doc
  and cc.nro_doc  = ccd.nro_doc
  and ccd.centro_benef = cb.centro_benef
  and cc.cod_relacion = p.proveedor
  and ccd.cod_art     = a.cod_art
  and cc.tipo_doc = 'LTC'
  and trunc(cc.fecha_vencimiento) < trunc(:ad_fecha)
  and cc.cod_relacion		like :as_cliente
  and ccd.cod_art				like :as_cod_art
  and cc.estado in ('1', '2', '6')

" arguments=(("as_cliente", string),("ad_fecha", date),("as_cod_art", string))  sort="desc_centro A desc_estado A nom_cliente A nro_doc A " )
