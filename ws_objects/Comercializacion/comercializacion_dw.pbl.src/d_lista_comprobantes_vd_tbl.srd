table(column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc dbname="nro_doc" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="cod_relacion" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=decimal(2) updatewhereclause=yes name=importe_doc dbname="importe_doc" )
 column=(type=number updatewhereclause=yes name=imp_registro dbname="imp_registro" )
 retrieve="select cc.tipo_doc, cc.nro_doc, 
       cc.cod_relacion,
       p.nom_proveedor,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       cc.importe_doc,
       (select nvl(sum(vd.importe), 0)
          from zc_vales_descuento vd
         where vd.tipo_doc = cc.tipo_doc
           and vd.nro_doc  = cc.nro_doc) as imp_registro
from cntas_cobrar cc,
     (
		select distinct tipo_doc, nro_doc
		  from cntas_cobrar_det ccd
		where trim(ccd.confin) in (trim(PKG_SIGRE_FINANZAS.of_confin_vta_vd_sol(null)), trim(PKG_SIGRE_FINANZAS.of_confin_vta_vd_dol(null)))
	  ) s,
     proveedor    p
where cc.cod_relacion = p.proveedor
  and cc.tipo_doc		 = s.tipo_doc
  and cc.nro_doc		 = s.nro_doc
  and cc.flag_estado in ('1', '2', '3')
     " )
