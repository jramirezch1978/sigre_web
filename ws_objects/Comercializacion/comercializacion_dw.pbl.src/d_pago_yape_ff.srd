 column=(type=char(40) updatewhereclause=yes name=nom_banco_cred dbname="banco.nom_banco_cred" )
 retrieve="select 	a.*, 
       	fp.desc_forma_pago,
       	p.nom_proveedor,
       	decode(p.tipo_doc_ident, 6, p.ruc, p.nro_doc_ident) as ruc_dni,
     		zc.cod_relacion,
     		t.nom_proveedor 	as nom_trabajador,
     		zc.IMPORTE 			as imp_dscto,
			bc.cod_banco,
     		b.nom_banco,
			b2.nom_banco as nom_banco_cred
from 	fs_factura_simpl_pagos 	a,
     	proveedor              	p,
     	forma_pago             	fp,
    	zc_vales_descuento     	zc,
    	proveedor          		t,
    	banco_cnta       			bc,
    	banco            			b,
		banco							b2
where a.consignatario 	= p.proveedor 		(+)
  and a.forma_pago    	= fp.forma_pago 	(+)     
  and a.cod_banco_cred	= b2.cod_banco		(+)
  and a.NRO_VALE_VD   	= zc.NRO_VALE_VD 	(+)
  and zc.cod_relacion 	= t.proveedor    	(+)
  and a.cod_ctabco    	= bc.cod_ctabco  	(+)
  and bc.cod_banco    	= b.cod_banco    	(+)
" )
