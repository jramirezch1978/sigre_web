 retrieve="SELECT AV.COD_ART,   
		A.DESC_ART,   
		A.UND,
		m.nom_marca,
		AV.PRECIO_MAYORISTA,   
		AV.PRECIO_MINORISTA,   
		AV.PRECIO_SUPERMERCADO,   
		AV.PRECIO_DISTRIBUIDOR  
 FROM ARTICULO_VENTA	AV,   
		ARTICULO  		A,
		MARCA				M
WHERE A.COD_ART = AV.COD_ART
  and a.cod_marca = m.cod_marca (+)
  and (a.desc_art like :as_filtro or a.und like :as_filtro or av.cod_art like :as_filtro or m.nom_marca like :as_filtro)
" update="ARTICULO_VENTA" updatewhere=0 updatekeyinplace=yes arguments=(("as_filtro", string)) )
