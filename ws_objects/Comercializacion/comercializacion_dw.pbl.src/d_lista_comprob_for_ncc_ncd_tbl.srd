table(column=(type=char(10) updatewhereclause=yes key=yes name=nro_registro dbname="fs_factura_simpl.nro_registro" dbalias="fs.nro_registro" )
 column=(type=char(4) updatewhereclause=yes name=tipo_doc_cxc dbname="fs_factura_simpl.tipo_doc_cxc" dbalias="fs.tipo_doc_cxc" )
 column=(type=char(4) updatewhereclause=yes name=serie_cxc dbname="fs_factura_simpl.serie_cxc" dbalias="fs.serie_cxc" )
 column=(type=char(10) updatewhereclause=yes name=nro_cxc dbname="fs_factura_simpl.nro_cxc" dbalias="fs.nro_cxc" )
 column=(type=char(1000) updatewhereclause=yes name=observacion dbname="fs_factura_simpl.observacion" dbalias="fs.observacion" )
 column=(type=number updatewhereclause=yes name=total dbname="total" )
 retrieve="select distinct
		 fs.nro_registro,
       fs.tipo_doc_cxc,
       fs.serie_cxc,
       fs.nro_cxc,
		 fs.observacion,
       (select nvl(sum(fsd.cant_proyect * (fsd.precio_unit - fsd.descuento + fsd.importe_igv)), 0)
          from fs_factura_simpl_det fsd
         where fsd.nro_registro = fs.nro_registro) as total

from fs_factura_simpl fs,
	  (select distinct s1.nro_registro 
  		  from (select fd1.nro_registro, fd1.nro_item
					 from fs_factura_simpl_det fd1
				  minus
				  select fd2.nro_registro_ref, fd2.item_ref
					 from fs_factura_simpl_det fd2,
							fs_factura_simpl		f2
					where f2.nro_registro = fd2.nro_registro
					  and f2.flag_estado 	<> '0'
					  and fd2.nro_registro_ref is not null
					  and fd2.item_ref         is not null) s1
		) s
where fs.cliente = :as_cliente
  and fs.nro_registro = s.nro_registro
  and substr(fs.tipo_doc_cxc,1,1) = substr(:as_serie, 1, 1)
  and fs.flag_estado <> '0'
  --and (select nvl(sum(fsd.cant_proyect * (fsd.precio_unit - fsd.descuento + fsd.importe_igv)), 0)
  --        from fs_factura_simpl_det fsd
  --       where fsd.nro_registro = fs.nro_registro) > 0" update="fs_factura_simpl" updatewhere=1 updatekeyinplace=no arguments=(("as_cliente", string),("as_serie", string)) )
