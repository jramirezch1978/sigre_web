table(column=(type=char(6) updatewhereclause=yes name=vendedor dbname="vendedor.vendedor" dbalias="v.vendedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_vendedor dbname="vendedor.nom_vendedor" dbalias="v.nom_vendedor" )
 retrieve="select distinct
		 v.vendedor, 
		 v.nom_vendedor
  from orden_venta            ov,
       articulo_mov_proy      amp,
       articulo               a,
       vendedor               v,
       vw_pr_trabajador       m,
       almacen                al,
       articulo_sub_categ     a2
 where ov.nro_ov        = amp.nro_doc
   and amp.tipo_doc     = (select doc_ov from logparam where reckey = '1')
   and ov.vendedor      = v.vendedor
   and amp.cod_art      = a.cod_art
   and amp.almacen      = al.almacen
   and a.sub_cat_art    = a2.cod_sub_cat
   and v.cod_trabajador = m.COD_TRABAJADOR (+) 
   and ov.flag_estado   <> '0'
   and trunc(amp.fec_proyect) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
   and amp.flag_estado <> '0'" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
