 retrieve="select amp.tipo_doc,
       amp.nro_doc,
       amp.cod_art,
       a.desc_art,
       a.und,
       amp.almacen,
		 a.cod_clase,
       ac.desc_clase,
       amp.cant_proyect,
       amp.cant_procesada,
       (select sum(aa.sldo_total)
          from articulo_almacen aa
         where aa.cod_art = amp.cod_art
           and aa.almacen = amp.almacen) as stock_actual
from articulo_mov_proy amp,
     articulo          a,
     articulo_clase    ac
where amp.cod_art      	= a.cod_art
  and a.cod_clase      	= ac.cod_clase
  and amp.tipo_doc		= :as_tipo_doc
  and amp.nro_doc			= :as_nro_doc" arguments=(("as_tipo_doc", string),("as_nro_doc", string))  sort="cod_clase A cod_art A " )
