 column=(type=number updatewhereclause=yes name=importe_articulo dbname="importe_articulo" )
 retrieve="Select cc.cod_relacion, p.nom_proveedor, 
       cc.tipo_doc, cc.nro_doc, cc.fecha_registro, nvl(cddi.tipo_impuesto,'S/IGV'), (nvl(it.tasa_impuesto,100) / 100 ),
       Sum( decode(cc.cod_moneda, :as_moneda, cc.importe_doc, 
                                         		 cc.importe_doc * cc.tasa_cambio ) ) As importe_total_igv,
       Sum( decode(cc.cod_moneda, :as_moneda, cc.saldo_sol,
                                              cc.saldo_dol) ) As saldo_igv, 
       ccd.cod_art, a.nom_articulo, ccd.cantidad, ccd.precio_unitario, 
       Sum( decode(cc.cod_moneda, :as_moneda, ccd.cantidad * ccd.precio_unitario, 
                                              (ccd.cantidad * ccd.precio_unitario)* cc.tasa_cambio ) ) As importe_articulo
  From cntas_cobrar cc, cntas_cobrar_det ccd, proveedor p, articulo a, cc_doc_det_imp cddi, impuestos_tipo it
 Where cc.tipo_doc     = ccd.tipo_doc
   And cc.nro_doc      = ccd.nro_doc
   And ccd.cod_art     = a.cod_art
   And cc.cod_relacion = p.proveedor
   And ccd.tipo_doc    = cddi.tipo_doc (+)
   And ccd.nro_doc     = cddi.nro_doc (+)
   And ccd.item        = cddi.item (+)
   And cddi.tipo_impuesto = it.tipo_impuesto (+)
   And trunc(cc.fecha_registro) >= :ad_fec_ini
   And trunc(cc.fecha_registro) <= :ad_fec_fin
And cc.tipo_doc <> (Select nota_credito From finparam Where reckey = '1')
   And cc.flag_estado  <> '0' 
   And a.cod_clase     Not In (:as_clase)
 Group By cc.cod_relacion, p.nom_proveedor, cc.tipo_doc, cc.nro_doc, cc.fecha_registro, cddi.tipo_impuesto, 
       it.tasa_impuesto, cc.importe_doc, ccd.cod_art, a.nom_articulo, ccd.cantidad, ccd.precio_unitario
 Order By p.nom_proveedor Asc, cc.importe_doc Desc" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_moneda", string),("as_clase", stringlist),("as_empresa", string),("as_user", string))  sort="proveedor_nom_proveedor A cntas_cobrar_tipo_doc A cntas_cobrar_nro_doc A " )
