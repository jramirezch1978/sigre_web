 retrieve="select to_number(to_char(cc.fecha_documento, 'yyyy')) as ano,
       to_number(to_char(cc.fecha_documento, 'mm')) as mes,
       fe.incoterm, 
       ag.desc_grupo_art,
       (Select cod_dolares from logparam where reckey = '1') as moneda,
       sum(ccd.cantidad) as cantidad,
		 sum(ccd.cantidad * usf_fl_conv_mon(ccd.precio_unitario, cc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), cc.fecha_documento)) as monto,
       sum(ccd.cantidad * usf_fl_conv_mon(ccd.precio_unitario, cc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), cc.fecha_documento)) / sum(ccd.cantidad) as precio_real,
       sum(ccd.cantidad * usf_exp_precio_ref(cc.fecha_documento, fe.incoterm, ccd.cod_art, (select cod_dolares from logparam where reckey = '1'))) / sum(ccd.cantidad) as precio_ref
from cntas_cobrar     cc,
     cntas_cobrar_det ccd,
     doc_referencias  df,
     factura_exportacion fe,
     rel_articulo_grupo  rag,
	  articulo_grupo      ag,
     tt_exp_grupo_art    tt2
where cc.tipo_doc = ccd.tipo_doc
  and cc.nro_doc = ccd.nro_doc
  and df.tipo_doc = cc.tipo_doc
  and df.nro_doc  = cc.nro_doc
  and rag.cod_art = ccd.cod_Art
  and rag.grupo_art = ag.grupo_art
  and tt2.grupo_art = ag.grupo_art
  and fe.tipo_doc (+) = cc.tipo_doc
  and fe.nro_doc (+) = cc.nro_doc
  and df.tipo_ref = (select doc_ov from logparam where reckey = '1')
  and cc.flag_estado <> '0'
  and to_number(to_char(cc.fecha_documento, 'yyyy')) = :ani_year
group by to_number(to_char(cc.fecha_documento, 'yyyy')) ,
       to_number(to_char(cc.fecha_documento, 'mm')) ,
       ag.desc_grupo_art,
       fe.incoterm" arguments=(("ani_year", number))  sort="articulo_grupo_desc_grupo_art A mes A " )
