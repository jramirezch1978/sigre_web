table(column=(type=char(8) updatewhereclause=yes name=cliente dbname="fs_factura_simpl.cliente" dbalias="fs.cliente" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(1) updatewhereclause=yes name=tipo_doc_ident dbname="proveedor.tipo_doc_ident" dbalias="p.tipo_doc_ident" values="DNI	1/Otros	0/RUC	6/" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 retrieve="select distinct 
       fs.cliente,
       p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni         
from fs_factura_simpl fs, 
     proveedor        p,
	  (select distinct s1.nro_registro 
  		  from (select fd1.nro_registro, fd1.nro_item
					 from fs_factura_simpl_det fd1
				  minus
				  select fd2.nro_registro_ref, fd2.item_ref
					 from fs_factura_simpl_det fd2,
							fs_factura_simpl		f2
					where f2.nro_Registro = fd2. nro_registro
				     and f2.flag_estado <> '0'
 					  and fd2.nro_registro_ref is not null
					  and fd2.item_ref         is not null) s1
		) s

where fs.cliente      	= p.proveedor     
  and fs.flag_estado	 	<> '0'
  and fs.nro_registro	= s.nro_Registro
  and substr(fs.tipo_doc_cxc,1,1) = substr(:as_serie, 1, 1)
--  and (select nvl(sum(fsd.cant_proyect * (fsd.precio_unit - fsd.descuento + fsd.importe_igv)), 0)
--          from fs_factura_simpl_det fsd
--         where fsd.nro_registro = fs.nro_registro) > 0" arguments=(("as_Serie", string))  sort="nom_proveedor A " )
