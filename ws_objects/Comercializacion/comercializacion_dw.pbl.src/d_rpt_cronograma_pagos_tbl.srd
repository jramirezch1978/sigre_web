 retrieve="select f.nro_registro,
       f.tipo_doc_cxc,
       f.serie_cxc || '-' || f.nro_cxc as nro_doc,
       trunc(f.fec_registro) as fec_emision,
       f.cod_origen, o.nombre as nom_origen,
       -- Datos del cliente
       f.cliente,
       p.nom_proveedor as nom_cliente,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       pkg_logistica.of_get_direccion(f.cliente, f.item_direccion) as direccion,
       (select t.telefono
          from telefonos t
         where t.codigo = f.cliente
           and t.item   = 1) as telefono1,
       (select t.telefono
          from telefonos t
         where t.codigo = f.cliente
           and t.item   = 1) as telefono2,
       v.nom_vendedor,
       f.flag_estado,
       fp.flag_forma_pago,
       f.cod_moneda,
       m.descripcion as desc_moneda,
       (select sum(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv))
          from fs_factura_simpl_det fd
         where fd.nro_registro = f.nro_registro) as total_doc,
       -- Datos del bien
       (select a.desc_color
          from fs_factura_simpl_det fd,
               vw_articulo          a
         where fd.cod_art = a.cod_art
           and a.nro_Serie is not null
           and fd.nro_registro = f.nro_registro) as color,
       (select a.desc_modelo
          from fs_factura_simpl_det fd,
               vw_articulo          a
         where fd.cod_art = a.cod_art
           and a.nro_Serie is not null
           and fd.nro_registro = f.nro_registro) as modelo,
       (select a.nro_Serie
          from fs_factura_simpl_det fd,
               vw_articulo          a
         where fd.cod_art = a.cod_art
           and a.nro_Serie is not null
           and fd.nro_registro = f.nro_registro) as nro_Serie,
       (select a.nro_motor
          from fs_factura_simpl_det fd,
               vw_articulo          a
         where fd.cod_art = a.cod_art
           and a.nro_Serie is not null
           and fd.nro_registro = f.nro_registro) as nro_motor,
       (select a.desc_art
          from fs_factura_simpl_det fd,
               vw_articulo          a
         where fd.cod_art = a.cod_art
           and a.nro_Serie is not null
           and fd.nro_registro = f.nro_registro) as desc_art,
       -- Datos del prestamo
       (select nvl(sum(abs(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv))),0)
          from fs_factura_simpl_det fd
         where fd.nro_registro = f.nro_registro
           and fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv) < 0
           and fd.nro_doc_cxc is not null) as inicial,
       fp.monto_pago,
       fp.nro_cuotas,
       fp.porc_interes,
       fp.flag_tipo_credito,
       fp.tipo_doc_interes,
       pkg_fact_electronica.of_get_full_nro(fp.nro_doc_interes) as nro_doc_interes,
       fp.total_interes,
       fpr.nro_cuota,
       fpr.tipo_doc_cxc as tipo_doc_letra, 
       pkg_fact_electronica.of_get_full_nro(fpr.nro_doc_cxc) as nro_doc_letra,
       cc.fecha_documento,
       cc.fecha_vencimiento,
       fpr.capital,
       fpr.interes,
       fpr.capital + fpr.interes as valor_cuota,
       cc.cod_moneda,
       cc.tasa_cambio,
       cc.importe_doc,
       cc.saldo_sol,
       cc.saldo_dol,
       cc.flag_estado,
       case 
         when cc.cod_moneda = pkg_logistica.of_soles(null) then
           cc.saldo_sol
         else
           cc.saldo_dol
       end as saldo,
       --Datos del pago
       (select max(cb.fecha_emision)
          from caja_bancos cb,
               caja_bancos_det cbd
         where cb.origen = cbd.origen
           and cb.nro_registro = cbd.nro_registro
           and cbd.tipo_doc    = cc.tipo_doc
           and cbd.nro_doc     = cc.nro_doc
           and cb.flag_estado <> '0') as fec_pago,
       (select max(cb.origen || trim(to_char(cb.nro_registro, '00000000')))
          from caja_bancos cb,
               caja_bancos_det cbd
         where cb.origen = cbd.origen
           and cb.nro_registro = cbd.nro_registro
           and cbd.tipo_doc    = cc.tipo_doc
           and cbd.nro_doc     = cc.nro_doc
           and cb.flag_estado <> '0') as nro_recibo,
       (select cb.obs
          from caja_bancos cb,
               caja_bancos_det cbd
         where cb.origen = cbd.origen
           and cb.nro_registro = cbd.nro_registro
           and cbd.tipo_doc    = cc.tipo_doc
           and cbd.nro_doc     = cc.nro_doc
           and cb.flag_estado <> '0'
           and rownum = 1) as observaciones,
       (select nvl(sum(
                 case 
                   when cc.cod_moneda = cbd.cod_moneda then
                     cbd.importe
                   when cbd.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                     cbd.importe / cb.tasa_cambio
                   else
                     cbd.importe * cb.tasa_cambio
                 end),0)
          from caja_bancos cb,
               caja_bancos_det cbd
         where cb.origen = cbd.origen
           and cb.nro_registro = cbd.nro_registro
           and cbd.tipo_doc    = cc.tipo_doc
           and cbd.nro_doc     = cc.nro_doc
           and cb.flag_estado <> '0') as imp_pagado
          
from fs_factura_simpl_pagos_ref fpr,
     fs_factura_simpl_pagos     fp,
     fs_factura_simpl           f,
     cntas_cobrar               cc,
     proveedor                  p,
     vendedor                   v,
     moneda                     m,
     origen                     o
where fp.nro_registro  = fpr.nro_registro
  and fp.flag_forma_pago = fpr.flag_forma_pago
  and fp.nro_item        = fpr.nro_item
  and f.nro_registro     = fp.nro_registro
  and f.cliente          = p.proveedor
  and f.vendedor         = v.vendedor
  and f.cod_moneda       = m.cod_moneda
  and f.cod_origen       = o.cod_origen
  and fpr.tipo_doc_cxc   = cc.tipo_doc
  and fpr.nro_doc_cxc    = cc.nro_doc
  and f.nro_registro		 = :as_nro_Registro
order by f.tipo_doc_cxc, nro_doc, fpr.nro_cuota  
" arguments=(("as_nro_registro", string)) )
