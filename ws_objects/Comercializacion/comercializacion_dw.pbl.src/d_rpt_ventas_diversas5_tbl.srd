 retrieve="select distinct s.cod_origen, (select o.nombre from origen o where o.cod_origen = s.cod_origen) as origen,
       mes,
       dia,
		 'PERIODO ' || to_char(to_number(to_char(:ad_fecha1, 'yyyy')) - 1, '0000') as periodo_anterior,
		 'PERIODO ' || to_char(to_number(to_char(:ad_fecha1, 'yyyy')), '0000') as periodo_actual,
       case 
         when mes = 1 then 'ENERO'
         when mes = 2 then 'FEBRERO'
         when mes = 3 then 'MARZO'
         when mes = 4 then 'ABRIL'
         when mes = 5 then 'MAYO'
         when mes = 6 then 'JUNIO'
         when mes = 7 then 'JULIO'
         when mes = 8 then 'AGOSTO'
         when mes = 9 then 'SETIEMBRE'
         when mes = 10 then 'OCTUBRE'
         when mes = 11 then 'NOVIEMBRE'
         when mes = 12 then 'DICIMBRE'
       end as desc_mes,
       nvl((select sum(fd.cant_proyect * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               articulo_sub_categ   a2,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and a.sub_cat_art  = a2.cod_sub_cat
           and f.tipo_doc_cxc = dt.tipo_doc
           and a2.cat_art     in ('01A', '01B', '01C', '01D')
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy')) - 1),0) calzado_anterior,
       nvl((select sum(fd.cant_proyect * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               articulo_sub_categ   a2,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and a.sub_cat_art  = a2.cod_sub_cat
           and f.tipo_doc_cxc = dt.tipo_doc
           and a2.cat_art     not in ('01A', '01B', '01C', '01D')
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy')) - 1),0) otros_anterior,
       nvl((select sum(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv) * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and f.tipo_doc_cxc = dt.tipo_doc
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy')) - 1),0) total_venta_anterior,
       nvl((select sum(am.cant_procesada * am.precio_unit * amt.factor_sldo_total * -1)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               vale_mov             vm,
               articulo_mov         am,
               articulo_mov_tipo    amt
         where f.nro_registro = fd.nro_registro
           and vm.nro_Vale    = am.nro_vale
           and vm.tipo_mov    = amt.tipo_mov
           and am.cod_origen  = fd.org_am
           and am.nro_mov     = fd.nro_am
           and f.flag_estado  <> '0'
           and am.flag_estado <> '0'
           and vm.flag_estado <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy')) - 1),0) costo_anterior,
       nvl((select sum(fd.cant_proyect * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               articulo_sub_categ   a2,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and a.sub_cat_art  = a2.cod_sub_cat
           and f.tipo_doc_cxc = dt.tipo_doc
           and a2.cat_art     in ('01A', '01B', '01C', '01D')
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy'))),0) calzado_actual,
       nvl((select sum(fd.cant_proyect * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               articulo_sub_categ   a2,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and a.sub_cat_art  = a2.cod_sub_cat
           and f.tipo_doc_cxc = dt.tipo_doc
           and a2.cat_art     not in ('01A', '01B', '01C', '01D')
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy'))),0) otros_actual,
       nvl((select sum(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv) * dt.factor)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               articulo             a,
               doc_tipo             dt
         where f.nro_registro = fd.nro_registro
           and fd.cod_art     = a.cod_art
           and f.tipo_doc_cxc = dt.tipo_doc
           and f.flag_estado  <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy'))),0) total_venta_actual,
       nvl((select sum(am.cant_procesada * am.precio_unit * amt.factor_sldo_total * -1)
          from fs_factura_simpl_det fd,
               fs_factura_simpl     f,
               vale_mov             vm,
               articulo_mov         am,
               articulo_mov_tipo    amt
         where f.nro_registro = fd.nro_registro
           and vm.nro_Vale    = am.nro_vale
           and vm.tipo_mov    = amt.tipo_mov
           and am.cod_origen  = fd.org_am
           and am.nro_mov     = fd.nro_am
           and f.flag_estado  <> '0'
           and am.flag_estado <> '0'
           and vm.flag_estado <> '0'
	      and f.cod_origen = s.cod_origen
		  and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
           and to_number(to_char(f.fec_registro, 'dd')) = s.dia
           and to_number(to_char(f.fec_registro, 'mm')) = s.mes
           and to_number(to_char(f.fec_registro, 'yyyy')) = to_number(to_char(:ad_fecha1, 'yyyy'))),0) costo_actual
from(
    select distinct f.cod_origen,
           to_number(to_char(f.fec_registro, 'mm')) as mes,
           to_number(to_char(f.fec_registro, 'dd')) as dia
      from fs_factura_simpl f,
           fs_factura_simpl_det fd
     where f.nro_registro = fd.nro_registro
       --and to_char(f.fec_registro, 'yyyy') = '2017'
	    and trunc(f.fec_registro) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
	and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))   
       and f.flag_estado  <> '0'      
	   and f.cod_origen like :cod_origen || '%' 
    union
    select distinct f.cod_origen,
           to_number(to_char(f.fec_registro, 'mm')) as mes,
           to_number(to_char(f.fec_registro, 'dd')) as dia
      from fs_factura_simpl f,
           fs_factura_simpl_det fd
     where f.nro_registro = fd.nro_registro
		and f.cliente not in (select * from table(split(PKG_CONFIG.USF_GET_PARAMETER('GRUPO_EMPRESAS_RELACIONADAS', 'E0010444, E0009360'))))
       --and to_char(f.fec_registro, 'yyyy') = '2016'
		 and trunc(f.fec_registro) between to_date(to_char(:ad_fecha1, 'dd/mm/') || trim(to_char(to_number(to_char(:ad_fecha1, 'yyyy')) - 1, '0000')), 'dd/mm/yyyy') and  to_date(to_char(:ad_fecha2, 'dd/mm/') || trim(to_char(to_number(to_char(:ad_fecha2, 'yyyy')) - 1, '0000')), 'dd/mm/yyyy')
       and f.flag_estado  <> '0'
	   and f.cod_origen like :cod_origen || '%') s
order by s.mes, s.dia       
