 retrieve="SELECT  LE.NRO_LOTE,
        LE.NRO_EMBARQUE,
        LE.FEC_CARGA_BARCO,
        LE.FEC_ENTREG_DOC,
        LE.ASEGURADOR,
        P_A.NOM_PROVEEDOR,
        LE.POLIZA_NRO,
        LE.POLIZA_FECHA,
        LE.POLIZA_VENCIMIENTO,
        LE.CONSIGNATARIO,
        P_B.NOM_PROVEEDOR,
        LE.COD_ART,
        A.NOM_ARTICULO,
        LE.DOC_COBR_BANCO,
        B.NOM_BANCO,
        LE.DOC_COBR_TIPO,
        DT.DESC_TIPO_DOC,
        LE.DOC_COBR_NRO,
        LE.DOC_COBR_FECHA,
        LE.DOC_COBR_VENC,
        LE.FLAG_ESTADO,
        LE.COD_USR,
        LE.FLAG_REPLICACION
        
FROM    LOTE_EMBARQUE LE,
        PROVEEDOR     P_A,
        PROVEEDOR     P_B,
        ARTICULO      A,
        BANCO         B,
        DOC_TIPO      DT

WHERE   LE.ASEGURADOR     = P_A.PROVEEDOR (+)
    AND LE.CONSIGNATARIO  = P_B.PROVEEDOR (+)
    AND LE.COD_ART        = A.COD_ART     (+)
    AND LE.DOC_COBR_BANCO = B.COD_BANCO   (+)
    AND LE.DOC_COBR_TIPO  = DT.TIPO_DOC   (+)
    AND LE.NRO_LOTE       = :AS_NRO_LOTE  " update="LOTE_EMBARQUE" updatewhere=0 updatekeyinplace=yes arguments=(("AS_NRO_LOTE", string)) )
