table(column=(type=char(10) updatewhereclause=yes name=nro_vale_vd dbname="zc_vales_descuento.nro_vale_vd" dbalias="a.nro_vale_vd" )
 column=(type=datetime updatewhereclause=yes name=fec_vale dbname="zc_vales_descuento.fec_vale" dbalias="a.fec_vale" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="zc_vales_descuento.cod_relacion" dbalias="a.cod_relacion" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=decimal(4) updatewhereclause=yes name=importe dbname="zc_vales_descuento.importe" dbalias="a.importe" )
 column=(type=number updatewhereclause=yes name=saldo dbname="saldo" )
 retrieve="select a.nro_vale_vd,
       a.fec_vale,
       a.cod_relacion,
       p.nom_proveedor,
		 decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       a.importe,
       a.importe - (select nvl(sum(abs(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv) * dt.factor)),0)
                      from fs_factura_simpl_det fd,
                           fs_factura_simpl     f,
									doc_tipo					dt
                     where fd.nro_registro 	= f.nro_registro
                       and fd.nro_vale_vd 	= a.nro_vale_vd
							  and f.tipo_doc_cxc		= dt.tipo_doc
                       and f.flag_estado <> '0') as saldo
from zc_vales_descuento a,
     proveedor          p
where a.cod_relacion = p.proveedor     
  and a.importe - (select nvl(sum(abs(fd.cant_proyect * (fd.precio_unit - fd.descuento + fd.importe_igv) * dt.factor)),0)
                      from fs_factura_simpl_det fd,
                           fs_factura_simpl     f,
									doc_tipo					dt
                     where fd.nro_registro 	= f.nro_registro
                       and fd.nro_vale_vd 	= a.nro_vale_vd
							  and f.tipo_doc_cxc		= dt.tipo_doc
                       and f.flag_estado <> '0') > 0
" )
