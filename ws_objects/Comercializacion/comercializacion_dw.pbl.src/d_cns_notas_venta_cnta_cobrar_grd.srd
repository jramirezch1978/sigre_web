 retrieve="select DISTINCT(cc.tipo_doc), cc.nro_doc, cc.fecha_registro, cc.cod_moneda, cc.importe_doc, cc.saldo_sol, cc.saldo_dol, 
       trim(cc.origen)||to_char(cc.ano,'9999')||to_char(cc.mes,'00')||to_char(cc.nro_libro)||to_char(cc.nro_asiento,'000000') as voucher, 
       cc.observacion
from cntas_cobrar cc, cntas_cobrar_det ccd 
where cc.tipo_doc = ccd.tipo_doc and 
      cc.nro_doc = ccd.nro_doc and 
      cc.flag_estado <> '0' and 
      ccd.tipo_ref = :as_tipo_doc and 
      ccd.nro_ref = :as_nro_doc " arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
