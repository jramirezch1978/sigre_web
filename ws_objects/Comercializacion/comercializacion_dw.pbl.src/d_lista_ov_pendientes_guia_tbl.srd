 column=(type=char(3) update=yes updatewhereclause=yes name=cod_moneda dbname="orden_venta.cod_moneda" dbalias="OV.cod_moneda" )
 column=(type=char(200) updatewhereclause=yes name=obs dbname="orden_venta.obs" dbalias="OV.obs" )
 column=(type=number updatewhereclause=yes name=importe_total dbname="importe_total" )
 column=(type=number updatewhereclause=yes name=impuesto1 dbname="impuesto1" )
 retrieve="  SELECT distinct OV.COD_ORIGEN,   
         OV.NRO_OV,   
         OV.FLAG_ESTADO,   
         OV.FEC_REGISTRO,   
         OV.COD_MONEDA,
			ov.obs,
         (select nvl(sum(amp.cant_proyect * amp.precio_unit), 0)
            from articulo_mov_proy amp
           where amp.tipo_doc = (select doc_ov from logparam where reckey = '1')
             and amp.nro_doc  = ov.nro_ov) as importe_total,
         (select nvl(sum(amp.impuesto), 0)
            from articulo_mov_proy amp
           where amp.tipo_doc = (select doc_ov from logparam where reckey = '1')
             and amp.nro_doc  = ov.nro_ov) as impuesto1
 
    FROM ORDEN_VENTA 		OV,
			VALE_MOV  			VM,
       	GUIA_VALE 			GV,
       	GUIA      			GR,	
		 	MOTIVO_TRASLADO	MT
  
   WHERE GV.ORIGEN_VALE             = VM.COD_ORIGEN            
     and GV.NRO_VALE                = VM.NRO_VALE              
	  and GV.ORIGEN_GUIA             = GR.COD_ORIGEN                
	  and GV.NRO_GUIA                = GR.NRO_GUIA                  
	  and GR.MOTIVO_TRASLADO         = MT.MOTIVO_TRASLADO
	  and VM.TIPO_REFER              = (select doc_ov from logparam where reckey = '1')
	  and MT.FLAG_FACTURACION 			= '1'                                
	  and VM.ORIGEN_REFER            = OV.COD_ORIGEN 
     and VM.NRO_REFER               = OV.NRO_OV 
	  and GR.FLAG_ESTADO           	<> '0'                                 
     and PKG_ALMACEN.of_get_cant_despachada(gr.nro_guia) > 0
     and PKG_ALMACEN.of_get_cant_despachada(gr.nro_guia) > PKG_ALMACEN.of_get_cant_facturada(gr.nro_guia)
     and OV.CLIENTE 						= :as_cod_relacion    

" arguments=(("as_cod_relacion", string)) )
