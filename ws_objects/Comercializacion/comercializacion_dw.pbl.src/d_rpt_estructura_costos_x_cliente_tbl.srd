 column=(type=number updatewhereclause=yes name=costo_unitario dbname="costo_unitario" )
 retrieve="Select nvl(c.canal,'S/Cnl') as canal, nvl(cd.descripcion,'S/Desc Canal') as desc_canal, 
       nvl(cdt.sub_canal,'S/SCnl') as sub_canal, nvl(cdt.descripcion,'S/Desc Sub - Canal') as desc_sub_canal, 
       ov.comprador_final, p.nom_proveedor, oct.nro_ov,  ov.fec_registro, 
       amp.cod_art, a.nom_articulo, amp.cant_proyect, amp.precio_unit, 
       oct.servicio, s.descripcion, sum( oct.costo_unitario ) As costo_unitario
  From ov_costo_teorico oct, Orden_Venta ov, articulo_mov_proy amp, 
       servicios s, articulo a, proveedor p, cliente c, 
       canal_distribucion cd, canal_distribucion_det cdt
 Where oct.Org_Amp     = ov.cod_origen
   And oct.nro_ov      = ov.nro_ov
   And ov.cod_origen   = amp.cod_origen
   And ov.nro_ov       = amp.nro_doc
   And amp.tipo_doc    = ( Select doc_ov From logparam Where reckey = '1' )
   And ov.comprador_final = p.proveedor
   And ov.comprador_final = c.cliente (+)
   And c.canal         = cdt.canal (+) 
   And c.sub_canal     = cdt.sub_canal (+)
   And cdt.canal       = cd.canal (+)
   And oct.servicio    = s.servicio
   And amp.cod_art     = a.cod_art
   And ov.comprador_final In ( Select proveedor from tt_proveedor )
	And ov.flag_estado  in (:as_estados)
 Group By nvl(c.canal,'S/Cnl'), nvl(cd.descripcion,'S/Desc Canal'), 
       nvl(cdt.sub_canal,'S/SCnl'), nvl(cdt.descripcion,'S/Desc Sub - Canal'),
       ov.comprador_final, p.nom_proveedor, oct.nro_ov,  ov.fec_registro, 
       amp.cod_art, a.nom_articulo, amp.cant_proyect, amp.precio_unit, 
       oct.servicio, s.descripcion
 Order By nvl(c.canal,'S/Cnl'), nvl(cdt.sub_canal,'S/SCnl'), p.nom_proveedor, oct.nro_ov, amp.cod_art" arguments=(("as_empresa", string),("as_user", string),("as_estados", stringlist))  sort="canal A sub_canal A nom_proveedor A ov_costo_teorico_nro_ov A articulo_mov_proy_cod_art A " )
