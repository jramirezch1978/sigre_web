table(column=(type=char(10) updatewhereclause=yes name=nro_proforma dbname="proforma.nro_proforma" dbalias="pf.nro_proforma" )
 column=(type=datetime updatewhereclause=yes name=fec_registro dbname="proforma.fec_registro" dbalias="pf.fec_registro" )
 column=(type=char(6) updatewhereclause=yes name=cod_usr dbname="proforma.cod_usr" dbalias="pf.cod_usr" )
 column=(type=char(200) updatewhereclause=yes name=nom_vendedor dbname="vendedor.nom_vendedor" dbalias="v.nom_vendedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_cliente dbname="proveedor.nom_cliente" dbalias="p.nom_cliente" )
 column=(type=char(1) updatewhereclause=yes name=lag_estado dbname="proforma.flag_estado" dbalias="pf.flag_estado" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=char(15) updatewhereclause=yes name=flag_bol_fact dbname="flag_bol_fact" )
 column=(type=number updatewhereclause=yes name=nro_registros dbname="nro_registros" )
 column=(type=number updatewhereclause=yes name=base_imponible dbname="base_imponible" )
 column=(type=number updatewhereclause=yes name=igv dbname="igv" )
 retrieve="select pf.nro_proforma,
       pf.fec_registro,
		 pf.cod_usr,
       v.nom_vendedor as nom_vendedor,
       p.nom_proveedor as nom_cliente,
		 pf.flag_estado,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       case 
			when pf.flag_factura_boleta = 'N' then 'NOTA DE VENTA'
			WHEN pf.flag_factura_boleta = 'B' then 'BOLETA DE VENTA'
			WHEN pf.flag_factura_boleta = 'F' then 'FACTURA'
       end as flag_bol_fact,
       count(pfd.cod_art) as nro_registros,
       nvl(sum(pfd.cantidad * pfd.precio_vta),0) as base_imponible,
       nvl(sum(pfd.cantidad * pfd.igv),0) as igv
from proforma     pf,
     proforma_det pfd,
     vendedor     v,
     proveedor   p
where pf.nro_proforma = pfd.nro_proforma     
  and pf.vendedor     = v.vendedor    (+)
  and pf.cliente     = p.proveedor    (+)
  and pf.flag_estado = '1'
group by pf.nro_proforma,
        	pf.fec_registro,
			pf.cod_usr,
        	v.nom_vendedor,
        	p.nom_proveedor,
			pf.flag_estado,
       	decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
       case 
			when pf.flag_factura_boleta = 'N' then 'NOTA DE VENTA'
			WHEN pf.flag_factura_boleta = 'B' then 'BOLETA DE VENTA'
			WHEN pf.flag_factura_boleta = 'F' then 'FACTURA'
       end
"  sort="fec_registro D " )
