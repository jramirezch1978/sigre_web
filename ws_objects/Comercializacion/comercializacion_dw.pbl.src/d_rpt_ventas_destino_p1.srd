 column=(type=number updatewhereclause=yes name=monto dbname="monto" )
 retrieve="select DECODE(ov.flag_mercado, 'L', 'LOCAL', 'E', 'EXPORTACION') as flag_mercado,
       ag.desc_grupo_art,
       ccd.cantidad * usf_fl_conv_mon(ccd.precio_unitario, cc.cod_moneda, (select cod_dolares from logparam where reckey = '1'), cc.fecha_documento) as monto
from cntas_cobrar     cc,
     cntas_cobrar_det ccd,
     doc_referencias  df,
     orden_venta      ov,
     rel_articulo_grupo  rag,
     articulo_grupo      ag,
	  tt_exp_grupo_art    tt1
where cc.tipo_doc   = ccd.tipo_doc
  and cc.nro_doc    = ccd.nro_doc
  and df.tipo_doc   = cc.tipo_doc
  and df.nro_doc    = cc.nro_doc
  and rag.cod_art   = ccd.cod_Art
  and rag.grupo_art = ag.grupo_art
  and tt1.grupo_art = ag.grupo_art
  and df.tipo_ref   = (select doc_ov from logparam where reckey = '1')
  and ov.nro_ov     = df.nro_ref 
  and trunc(cc.fecha_documento) between trunc(:ad_fecha1) and trunc(:ad_fecha2)
  and cc.flag_estado <> '0'" arguments=(("ad_fecha1", date),("ad_fecha2", date))  sort="flag_mercado A " )
