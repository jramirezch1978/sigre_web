 retrieve="SELECT CC.TIPO_DOC          ,
       CC.NRO_DOC           ,
       PKG_FACT_ELECTRONICA.of_get_serie(cc.nro_doc) as serie,
       PKG_FACT_ELECTRONICA.of_get_nro(cc.nro_doc) as numero,
       CC.COD_RELACION      ,
       CC.ITEM_DIRECCION    ,
       pkg_logistica.of_get_direccion(cc.cod_relacion, cc.ITEM_DIRECCION) as direccion ,
       CC.FLAG_ESTADO       ,
       CC.FECHA_REGISTRO    ,
       CC.PUNTO_VENTA       ,
       CC.FECHA_DOCUMENTO   ,
       CC.FECHA_VENCIMIENTO ,
       CC.COD_MONEDA        ,
       CC.TASA_CAMBIO       ,
       CC.SALDO_SOL         ,
       CC.SALDO_DOL         ,
       CC.COD_USR           ,
       CC.ORIGEN            ,
       CC.ANO               ,
       CC.MES               ,
       CC.NRO_LIBRO         ,
       CC.NRO_ASIENTO       ,
       CC.MOTIVO            , 
       mn.descripcion as desc_motivo_nota, 
       mn.flag_cab_det     ,
       mn.tipo_nc        ,
       mn.tipo_nd        ,
       CC.OBSERVACION       ,
       P.NOM_PROVEEDOR      ,    
       DECODE(P.Tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       CC.IMPORTE_DOC       ,
       CC.FLAG_PROVISIONADO ,
       CC.FLAG_REPLICACION  ,
       cc.flag_enviar_efact ,
       cc.flag_mercado      ,
       cc.vendedor          ,
       v.nom_vendedor       ,
       cc.forma_pago        ,
       fp.desc_forma_pago   ,
       case when cc.data_xml is null then '0' else '1' end as flag_data_xml,
       case when cc.data_cdr is null then '0' else '1' end as flag_data_cdr,
       case when cc.data_pdf is null then '0' else '1' end as flag_data_pdf

  FROM CNTAS_COBRAR     CC,
       PROVEEDOR        P,
       motivo_nota      mn,
       vendedor         v,
       forma_pago       fp
WHERE CC.COD_RELACION   = P.PROVEEDOR  
  and cc.motivo         = mn.motivo       (+)    
  and cc.vendedor       = v.vendedor      (+)
  and cc.forma_pago     = fp.forma_pago   (+)
  and CC.TIPO_DOC       = :as_tipo_doc                 
  and CC.NRO_DOC        = :as_nro_doc    
" update="CNTAS_COBRAR" updatewhere=0 updatekeyinplace=yes arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
