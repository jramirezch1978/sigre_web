 retrieve="  SELECT DISTINCT CC.CENCOS,   
         CC.DESC_CENCOS  
    FROM ARTICULO_MOV_PROY AMP,   
         CENTROS_COSTO	   CC ,
		   ORDEN_VENTA			OV 
   WHERE OV.NRO_OV = AMP.NRO_DOC
     AND AMP.TIPO_DOC = (SELECT DOC_OV FROM LOGPARAM WHERE RECKEY = '1')
     AND OV.FLAG_ESTADO <> '0'
	  AND AMP.FLAG_ESTADO <> '0'
     and CC.CENCOS = AMP.CENCOS
	  AND To_number(to_char(ov.fec_registro, 'yyyy')) = :an_year" arguments=(("an_year", number)) )
