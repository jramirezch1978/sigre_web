 retrieve="Select sp.nro_pesada, sp.cod_origen, sp.nro_guia, g.almacen,
		 nvl(g.cliente,sp.proveedor) As proveedor , nvl(p.nom_proveedor,p2.Nom_Proveedor) As nom_proveedor, 
		 sp.fecha_tara As fec_ini_carga, 
       sp.fecha_pesada As fec_fin_carga,
       round((sp.fecha_pesada - sp.fecha_tara)*24,3) As tiempo_carga, 
       nvl(g.fec_impresion,g.fec_registro) As fec_guia,
       round((nvl(g.fec_impresion,g.fec_registro) - sp.fecha_tara)*24,3) As tiempo_planta
  From salida_pesada sp, guia g, proveedor p, proveedor p2
 Where sp.cod_origen   = g.cod_origen (+)
   And sp.nro_guia     = g.nro_guia (+)
   And g.cliente       = p.proveedor (+)
   And sp.proveedor    = p2.proveedor (+)
   And nvl(g.fec_registro,sp.fecha_pesada) >= :ad_fec_ini
   And nvl(g.fec_registro,sp.fecha_pesada) <= :ad_fec_fin
   And nvl(g.flag_estado,'1') <> '0'
   And nvl(g.cliente,sp.proveedor) Like :as_cliente
 Order By sp.nro_pesada" arguments=(("ad_fec_ini", datetime),("ad_fec_fin", datetime),("as_cliente", string),("as_empresa", string),("as_user", string))  sort="nro_pesada A " )
