 column=(type=number updatewhereclause=yes name=cant_facturada dbname="cant_facturada" )
 column=(type=number updatewhereclause=yes name=cant_despachada dbname="cant_despachada" )
 retrieve="select ag.desc_grupo_art,
       to_char(ov.fec_registro, 'mm') as mes,
       sum(amp.cant_facturada * usf_fl_conv_mon(amp.precio_unit, ov.cod_moneda, (select cod_dolares from logparam where reckey = '1'), sysdate)) as cant_facturada,
       sum(amp.cant_procesada * usf_fl_conv_mon(amp.precio_unit, ov.cod_moneda, (select cod_dolares from logparam where reckey = '1'), sysdate)) as cant_despachada
from orden_venta        ov,
     articulo_mov_proy  amp,
     articulo_grupo     ag,
     rel_articulo_grupo rag,
	  tt_exp_cencos		tt1,
	  tt_exp_grupo_art	tt2
where ov.nro_ov = amp.nro_doc
  and amp.tipo_doc = (select doc_ov from logparam where reckey = '1')
  and amp.flag_estado <> '0'
  and ov.flag_estado <> '0'
  and rag.cod_art = amp.cod_art
  and ag.grupo_art = rag.grupo_art
  and tt1.cencos = amp.cencos
  and tt2.grupo_Art = ag.grupo_art
  and to_number(to_char(ov.fec_registro, 'yyyy')) = :an_year
group by ag.desc_grupo_art,
       to_char(ov.fec_registro, 'mm')" arguments=(("an_year", number))  sort="mes A " )
