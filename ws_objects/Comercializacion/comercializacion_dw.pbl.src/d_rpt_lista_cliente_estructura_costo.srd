 retrieve="Select ov.comprador_final, p.nom_proveedor
  From ov_costo_teorico oct, Orden_Venta ov, proveedor p
 Where oct.Org_Amp     = ov.cod_origen
   And oct.nro_ov      = ov.nro_ov
   And ov.comprador_final = p.proveedor
   And trunc(ov.fec_registro) >= :ad_fec_ini
   And trunc(ov.fec_registro) <= :ad_fec_fin
   And ov.flag_estado  in (:as_estados)
 Group By ov.comprador_final, p.nom_proveedor
 Order By p.nom_proveedor" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_estados", stringlist)) )
