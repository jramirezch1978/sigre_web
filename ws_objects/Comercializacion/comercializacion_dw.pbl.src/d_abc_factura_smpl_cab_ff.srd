 retrieve="select 	a.NRO_REGISTRO,
     		a.COD_ORIGEN,
     		a.FEC_REGISTRO,
		  	a.FEC_MOVIMIENTO,
		  	a.CLIENTE,
		  	a.COD_MONEDA,
		  	a.TASA_CAMBIO,
		  	a.PUNTO_VENTA,
		  	a.COD_USR,
		  	a.SERIE_GUIA,
		  	a.SERIE_CXC,
		  	a.VENDEDOR,
		  	a.OBSERVACION,
		  	a.FLAG_ESTADO,
		  	a.TIPO_DOC_CXC,
		  	a.NRO_DOC_CXC,
		  	a.NRO_CXC,
		  	a.ITEM_DIRECCION,
		  	PKG_LOGISTICA.of_get_direccion(a.cliente, a.ITEM_DIRECCION) AS DIRECCION,
      	p.nom_proveedor,
       	decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       	m.descripcion as desc_moneda,
       	pv.desc_pto_vta,
       	v.nom_vendedor,
     		a.motivo_nota,
     		mn.descripcion as desc_motivo_nota,
     		a.tipo_doc_Ref,
     		a.nro_doc_ref,
     		a.NRO_REGISTRO_REF
from fs_factura_simpl a,
     proveedor        p,
     moneda           m,
     puntos_venta     pv,
     vendedor         v,
    MOTIVO_NOTA    MN
where a.cliente       = p.proveedor
  and a.cod_moneda    = m.cod_moneda
  and a.punto_venta   = pv.punto_venta
  and a.motivo_nota  = mn.motivo    (+)
  and a.vendedor      = v.vendedor
  and a.nro_registro	 = :as_nro_registro" update="fs_factura_simpl" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_registro", string)) )
