 retrieve="SELECT 	CCI.TIPO_DOC,   
			CCI.NRO_DOC,   
			CCI.ITEM,   
			CCI.TIPO_IMPUESTO,  
			it.desc_impuesto, 
			CCI.IMPORTE,   
			IT.TASA_IMPUESTO,   
			IT.DESC_IMPUESTO,   
			IT.SIGNO,   
			IT.CNTA_CTBL,   
			CC.DESC_CNTA,   
			CCI.FLAG_REPLICACION,   
			IT.FLAG_IGV,   
			IT.FLAG_DH_CXP  
 FROM 	CC_DOC_DET_IMP	CCI,   
			IMPUESTOS_TIPO	IT,   
			CNTBL_CNTA		CC  
WHERE 	CCI.TIPO_IMPUESTO = IT.TIPO_IMPUESTO 
  and 	IT.CNTA_CTBL 		= CC.CNTA_CTBL 
  and 	CCI.TIPO_DOC 		= :as_tipo_doc 
  AND 	CCI.NRO_DOC 		= :as_nro_doc 
" update="CC_DOC_DET_IMP" updatewhere=0 updatekeyinplace=yes arguments=(("as_tipo_doc", string),("as_nro_doc", string)) )
