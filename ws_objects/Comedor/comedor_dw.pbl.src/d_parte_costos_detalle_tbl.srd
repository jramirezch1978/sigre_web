 retrieve="  SELECT A.PARTE_COSTO,   
         A.COD_RELACION,   
         CR.NOMBRE,   
         A.TIPO_DOC,   
         DT.DESC_TIPO_DOC,   
         A.NRO_DOC,   
         usf_fl_conv_mon(USF_COM_MONTO_SIN_IGV(cp.cod_relacion, cp.tipo_doc, cp.nro_doc), CP.cod_moneda, B.COD_MONEDA, CP.Fecha_Emision) 
				AS TOTAL_PAGAR,
         USF_COM_MONTO_SIN_IGV(cp.cod_relacion, cp.tipo_doc, cp.nro_doc) as imp_original,
         cp.cod_moneda  as mon_original,
         trunc(cp.fecha_emision) as fecha_emision,
			A.FLAG_REPLICACION
    FROM COM_COMPR_PAGO_DET A,   
         CODIGO_RELACION    CR,   
         DOC_TIPO           DT,   
         CNTAS_PAGAR        CP,
         COM_COMPR_PAGO     B 
   WHERE A.COD_RELACION 	= CR.COD_RELACION 
	  and A.TIPO_DOC 			= DT.TIPO_DOC 
     AND B.PARTE_COSTO     = A.PARTE_COSTO
	  and CP.COD_RELACION	= A.COD_RELACION 
	  and CP.TIPO_DOC 		= A.TIPO_DOC 
	  and CP.NRO_DOC 			= A.NRO_DOC 
	  and A.PARTE_COSTO 		= :as_nro_parte
" update="COM_COMPR_PAGO_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_nro_parte", string)) )
