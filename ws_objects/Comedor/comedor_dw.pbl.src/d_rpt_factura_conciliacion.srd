 retrieve="SELECT A.PARTE_COSTO,
		 B.FECHA_PARTE,
       A.COD_RELACION,
       CR.NOMBRE,
       A.TIPO_DOC,
       DT.DESC_TIPO_DOC,
       A.NRO_DOC,
		 usf_fl_conv_mon(USF_COM_MONTO_SIN_IGV(cp.cod_relacion, cp.tipo_doc, cp.nro_doc), CP.cod_moneda, B.COD_MONEDA, CP.Fecha_Emision) 
				AS TOTAL_PAGAR,
       DECODE(A.TIPO_DOC, 'FAP ', ROUND(cp.Importe_Doc/1.19,2), ROUND(CP.IMPORTE_DOC,2)) AS Base_Imponible,
		 CP.IMPORTE_DOC,
       cp.cod_moneda as mon_original

  FROM COM_COMPR_PAGO_DET A,
       CODIGO_RELACION    CR,
       DOC_TIPO           DT,
       CNTAS_PAGAR        CP,
       COM_COMPR_PAGO     B
 WHERE A.COD_RELACION = CR.COD_RELACION
   and A.TIPO_DOC = DT.TIPO_DOC
   AND B.PARTE_COSTO = A.PARTE_COSTO
   and CP.COD_RELACION = A.COD_RELACION
   and CP.TIPO_DOC = A.TIPO_DOC
   and CP.NRO_DOC = A.NRO_DOC
   and substr(b.origen, 1, 2) = :as_origen
   and Trunc(b.fecha_parte) BETWEEN trunc(:ad_fecha1) AND trunc(:ad_fecha2)" arguments=(("as_origen", string),("ad_fecha1", date),("ad_fecha2", date)) )
