 retrieve="  SELECT A.FECHA_REGISTRO,   
         A.COD_RELACION,   
         A.NOM_PROVEEDOR,   
         A.TIPO_DOC,   
         A.DESC_TIPO_DOC,   
         A.NRO_DOC,   
         USF_COM_MONTO_SIN_IGV(A.COD_RELACION, A.TIPO_DOC, A.NRO_DOC) as total_pagar ,
			A.FECHA_EMISION ,
			A.COD_MONEDA
    FROM VW_COM_CNTAS_PAGAR A  
   WHERE Trunc(A.FECHA_REGISTRO) >= :ad_fecha1 
	  AND Trunc(A.FECHA_REGISTRO) <= :ad_fecha2 
	  AND A.COD_RELACION || A.TIPO_DOC || A.NRO_DOC not in (:aas_doc_ing)
ORDER BY A.FECHA_REGISTRO ASC,   
         A.NOM_PROVEEDOR ASC,   
         A.TIPO_DOC ASC   
" arguments=(("ad_fecha1", date),("ad_fecha2", date),("aas_doc_ing", stringlist)) )
