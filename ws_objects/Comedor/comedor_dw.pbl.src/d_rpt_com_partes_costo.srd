 retrieve="SELECT 			 a.parte_racion,
                b.flag_tipo_racion,
                r.ratio,
                b.nro_raciones,
                round(r.ratio * b.nro_raciones, 2) as Importe,
                b.cencos,
                c.desc_cencos,
					 a.proveedor, 
                p.nom_proveedor

  FROM com_parte_rac       a,
       com_parte_rac_det   b,
       centros_costo       c,
       com_parte_rac_ratio r,
		 proveedor				p
      
 Where a.parte_racion                = b.parte_racion
   And b.parte_racion                = r.parte_racion
   And b.flag_tipo_racion            = r.flag_tipo_racion
   And a.proveedor                   = p.proveedor(+)
   and b.cencos                      = c.cencos
	and NVL(a.flag_estado, '0') = '1'
	and substr(a.parte_racion, 1, 2)  = :as_origen
   and Trunc(a.fecha_parte_rac) BETWEEN trunc(:ad_fecha1) AND trunc(:ad_fecha2)

Order by a.parte_racion" arguments=(("as_origen", string),("ad_fecha1", date),("ad_fecha2", date))  sort="com_parte_rac_parte_racion A com_parte_rac_det_cencos A centros_costo_desc_cencos A com_parte_rac_proveedor A proveedor_nom_proveedor A " )
