 column=(type=number updatewhereclause=yes name=haber dbname="haber" )
 retrieve="SELECT P.PARTE_COSTO,
       A.COD_RELACION,
       CR.NOMBRE,
       A.TIPO_DOC,
       A.NRO_DOC,
       ROUND((select sum(importe)
               from cntas_pagar_det l
              where l.cod_relacion = a.cod_relacion
                and l.tipo_doc = a.tipo_doc
                and l.nro_doc = a.nro_doc),
             2) AS BASE_IMPONIBLE,
       SUM(C.IMP_MOVSOL) AS CNTA_EN_SOLES_ASIENTO,
       SUM(B.TOT_SOLHAB) AS DEBE,
       SUM(B.TOT_SOLDEB) AS HABER

  FROM CNTAS_PAGAR        A,
       CNTBL_ASIENTO      B,
       CNTBL_ASIENTO_DET  C,
       COM_COMPR_PAGO_DET P,
       CODIGO_RELACION    CR

 WHERE A.ORIGEN 				= B.ORIGEN
   AND A.ANO 					= B.ANO
   AND A.MES 					= B.MES
   AND A.NRO_LIBRO 			= B.NRO_LIBRO
   AND A.NRO_ASIENTO 		= B.NRO_ASIENTO
   AND B.ORIGEN 				= C.ORIGEN
   AND B.ANO 					= C.ANO
   AND B.MES 					= C.MES
   AND B.NRO_LIBRO 			= C.NRO_LIBRO
   AND B.NRO_ASIENTO 		= C.NRO_ASIENTO
   AND A.COD_RELACION 		= P.COD_RELACION
   AND A.TIPO_DOC 			= P.TIPO_DOC
   AND A.NRO_DOC 				= P.NRO_DOC
   AND P.COD_RELACION 		= CR.COD_RELACION
   AND C.CNTA_CTBL 			= :as_cuenta
   AND A.ANO					= :as_year
 GROUP BY A.COD_RELACION, A.TIPO_DOC, CR.NOMBRE, A.NRO_DOC, P.PARTE_COSTO
HAVING (select sum(importe)
          from cntas_pagar_det l
         where l.cod_relacion = a.cod_relacion
           and l.tipo_doc = a.tipo_doc
           and l.nro_doc = a.nro_doc) <> SUM(C.IMP_MOVSOL)" arguments=(("as_cuenta", string),("as_year", string)) )
