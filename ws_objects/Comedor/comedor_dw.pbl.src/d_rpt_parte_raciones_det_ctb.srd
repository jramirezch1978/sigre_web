 retrieve="select a.cencos, 
       c.desc_cencos as desc_cencos,
       a.zona_proceso, 
       b.descr_zona_proc as desc_zona_proceso,
       DECODE(a.Flag_Tipo_Racion, 'D', '1.DESAYUNO', 'C', '4.CENA', 'A', '2.ALMUERZO', 
                                  'R', '3.REFRIGERIO', 'INDEFINIDO') as tipo_racion,
       a.nro_raciones,
		 r.ratio,
		 (a.nro_raciones * r.ratio) as costo
from com_parte_rac_det a,
     com_zona_proceso  b,
     centros_costo     c,
	  COM_PARTE_RAC	  d,
	  com_parte_rac_ratio r


where d.parte_racion = a.parte_racion
  and	a.zona_proceso = b.zona_proceso     
  and c.cencos       = a.cencos
  AND r.parte_racion = a.parte_racion
  AND r.flag_tipo_racion = a.flag_tipo_racion
  and a.parte_racion = :as_nro_parte
  and SUBSTR(d.PARTE_RACION,1,2) = :as_origen
  " arguments=(("as_origen", string),("as_nro_parte", string))  sort="com_parte_rac_det_cencos A centros_costo_desc_cencos A " )
