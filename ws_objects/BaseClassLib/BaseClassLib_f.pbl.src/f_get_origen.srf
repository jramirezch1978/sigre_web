global type f_get_origen from function_object
end type

forward prototypes
global function string f_get_origen ()
end prototypes

global function string f_get_origen ();String ls_origen

Select cod_origen
	Into :ls_origen
	From genparam
		Where reckey = '1' ;
		
RETURN ls_origen
end function

