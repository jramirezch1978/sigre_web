 column=(type=number updatewhereclause=yes name=saldo_acreedor dbname="saldo_acreedor" )
 retrieve="select decode(p.proveedor, null, 'XXX', p.proveedor) as proveedor,
       decode(p.nom_proveedor, null, 'INDETERMINADO', p.nom_proveedor) as razon_social,
       decode(p.ruc, null, p.nro_doc_ident, p.ruc) as ruc_dni,
       sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) as total_debe,
       sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) as total_haber,
       cad.tipo_docref1,
       cad.nro_docref1,
       doc.fecha_emision,
       doc.fec_vencimiento,
       cad.cnta_ctbl,
       cc.desc_cnta,
       case 
         when sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) > sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) then
            round(sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) - sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)),2)
         else
            0
       end as saldo_deudor,
       case 
         when sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) > sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) then
            round(sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) - sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)),2)
         else
            0
       end as saldo_acreedor
from Cntbl_Asiento ca,
     cntbl_asiento_Det cad,
     proveedor         p,
     cntbl_cnta        cc,
     (select cp.cod_relacion, cp.tipo_doc, cp.nro_doc, cp.fecha_emision, cp.vencimiento as fec_vencimiento
        from cntas_pagar cp
       where cp.flag_estado <> '0'
      union all
      select cc.cod_relacion, cc.tipo_doc, cc.nro_doc, cc.fecha_documento, cc.fecha_vencimiento
        from cntas_cobrar cc 
       where cc.flag_estado <> '0'
      union all
      select sg.cod_relacion, 'OG  ', sg.nro_solicitud, sg.fecha_emision, null
        from solicitud_giro sg
       where sg.flag_estado <> '0'
      union all
      select oc.proveedor, 'OC  ', oc.nro_oc, oc.fec_registro, null
        from orden_compra oc
      union all
      select os.proveedor, 'OS  ', os.nro_os, os.fec_registro, null
        from orden_servicio os
      union all
      select vm.proveedor, 'AL  ', vm.nro_vale, vm.fec_registro, null
        from vale_mov vm
       where vm.flag_estado <> '0') doc
where ca.origen        = cad.origen
  and ca.ano           = cad.ano
  and ca.mes           = cad.mes
  and ca.nro_libro     = cad.nro_libro
  and ca.nro_asiento   = cad.nro_asiento
  and cad.cod_relacion = p.proveedor (+)
  and cad.cnta_ctbl    = cc.cnta_ctbl
  and cad.cod_relacion = doc.cod_relacion (+)
  and cad.tipo_docref1 = doc.tipo_doc     (+)
  and cad.nro_docref1  = doc.nro_doc      (+)
  and ca.flag_estado   <> '0'
  and ca.ano           = :an_year
  and ca.mes           between :an_mes1 and :an_mes2
  and substr(cad.cnta_ctbl,1,8) like :as_cntbl_cnta
group by p.proveedor,
         p.nom_proveedor,
         decode(p.ruc, null, p.nro_doc_ident, p.ruc),
         cad.tipo_docref1,
         cad.nro_docref1,
         doc.fecha_emision,
         doc.fec_vencimiento,
         cad.cnta_ctbl,
         cc.desc_cnta
having case 
         when sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) > sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) then
            round(sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) - sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)),2)
         else
            0 
       end <> 0 or
       case 
         when sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) > sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)) then
            round(sum(decode(cad.flag_debhab, 'H', cad.imp_movdol, 0)) - sum(decode(cad.flag_debhab, 'D', cad.imp_movdol, 0)),2)
         else
            0
       end <> 0       
order by nom_proveedor, tipo_docref1, nro_docref1, cad.cnta_ctbl     
       
     
" arguments=(("as_cntbl_cnta", string),("an_year", number),("an_mes1", number),("an_mes2", number)) )
