 column=(type=number updatewhereclause=yes name=total_haber dbname="total_haber" )
 column=(type=char(4) updatewhereclause=yes name=tipo_docref1 dbname="tipo_docref1" )
 column=(type=char(15) updatewhereclause=yes name=nro_docref1 dbname="nro_docref1" )
 column=(type=datetime updatewhereclause=yes name=fecha_emision dbname="fecha_emision" )
 column=(type=datetime updatewhereclause=yes name=fec_vencimiento dbname="fec_vencimiento" )
 column=(type=char(10) updatewhereclause=yes name=cnta_ctbl dbname="cnta_ctbl" )
 column=(type=char(150) updatewhereclause=yes name=desc_cnta dbname="desc_cnta" )
