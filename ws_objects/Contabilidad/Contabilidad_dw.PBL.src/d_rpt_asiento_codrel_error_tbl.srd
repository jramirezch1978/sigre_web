 retrieve="  SELECT ~"CNTBL_ASIENTO_DET~".~"ORIGEN~",   
         ~"CNTBL_ASIENTO_DET~".~"ANO~",   
         ~"CNTBL_ASIENTO_DET~".~"MES~",   
         ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~",   
         ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~",   
         ~"CNTBL_ASIENTO_DET~".~"ITEM~",   
         ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~",   
         ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~",   
         ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~",   
         ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"  
    FROM ~"CNTBL_ASIENTO~",   
         ~"CNTBL_ASIENTO_DET~",   
         ~"CNTBL_CNTA~"  
   WHERE ( ~"CNTBL_ASIENTO~".~"ORIGEN~" = ~"CNTBL_ASIENTO_DET~".~"ORIGEN~" ) and  
         ( ~"CNTBL_ASIENTO~".~"ANO~" = ~"CNTBL_ASIENTO_DET~".~"ANO~" ) and  
         ( ~"CNTBL_ASIENTO~".~"MES~" = ~"CNTBL_ASIENTO_DET~".~"MES~" ) and  
         ( ~"CNTBL_ASIENTO~".~"NRO_LIBRO~" = ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~" ) and  
         ( ~"CNTBL_ASIENTO~".~"NRO_ASIENTO~" = ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~" ) and  
         ( ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~" = ~"CNTBL_CNTA~".~"CNTA_CTBL~" ) and  
         ( ( ~"CNTBL_ASIENTO~".~"ORIGEN~" = :as_origen ) AND  
         ( ~"CNTBL_ASIENTO~".~"ANO~" = :an_ano ) AND  
         ( ~"CNTBL_ASIENTO~".~"MES~" = :an_mes ) AND  
         ( ~"CNTBL_ASIENTO~".~"FLAG_ESTADO~" <> '0' ) AND  
         ( ~"CNTBL_CNTA~".~"FLAG_ESTADO~" <> '0' ) AND  
         ( ~"CNTBL_CNTA~".~"FLAG_CODREL~" = '1' ) AND  
         ((~"CNTBL_ASIENTO_DET~".~"COD_RELACION~" is null) OR
			 (~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~" is null) OR
			 (~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~" is null) ) )   
ORDER BY ~"CNTBL_ASIENTO_DET~".~"ORIGEN~" ASC,   
         ~"CNTBL_ASIENTO_DET~".~"ANO~" ASC,   
         ~"CNTBL_ASIENTO_DET~".~"MES~" ASC,   
         ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~" ASC,   
         ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~" ASC,   
         ~"CNTBL_ASIENTO_DET~".~"ITEM~" ASC   
" arguments=(("as_origen", string),("an_ano", number),("an_mes", number)) )
