 retrieve="SELECT trunc(cp.fecha_emision) as fecha, cp.tipo_doc, cp.nro_doc, cp.cod_relacion, p.nom_proveedor, cpd.item, cpd.descripcion, cp.cod_moneda, cpd.importe
  FROM solicitud_giro s, solicitud_giro_liq_det sd, cntas_pagar cp, cntas_pagar_det cpd, proveedor p
 WHERE (s.origen = sd.origen and 
        s.nro_solicitud = sd.nro_solicitud and 
        s.flag_estado = :as_estado and 
		  s.origen LIKE :as_origen) and 
       (sd.proveedor = cp.cod_relacion and 
        sd.tipo_doc = cp.tipo_doc and 
        sd.nro_doc = cp.nro_doc) and 
       (cp.cod_relacion = cpd.cod_relacion and 
        cp.tipo_doc = cpd.tipo_doc and 
        cp.nro_doc = cpd.nro_doc) and 
       (cp.cod_relacion = p.proveedor ) and 
       (sd.tipo_doc = :as_tipo_doc_ce) and 
       (sd.cnta_prsp = :as_cnta_prsp_mov) and 
       (trunc(cp.fecha_emision) between :ad_fec_ini and :ad_fec_fin) 
order by cp.cod_relacion, cp.tipo_doc, cp.nro_doc, cpd.item       " update="cntas_pagar" updatewhere=1 updatekeyinplace=no arguments=(("as_estado", string),("as_tipo_doc_ce", string),("as_cnta_prsp_mov", string),("as_origen", string),("ad_fec_ini", date),("ad_fec_fin", date)) )
