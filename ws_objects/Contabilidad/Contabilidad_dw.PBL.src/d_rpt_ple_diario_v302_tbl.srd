table(column=(type=char(384) updatewhereclause=yes name=exp_row dbname="exp_row" )
 retrieve="select  periodo         || '|' || -- 1. DPERIODO
        voucher         || '|' || -- 2. DNUMSIOPE
        '01'            || '|' || -- 3. DCODCUE
        cnta_ctbl       || '|' || -- 4. DNUMCTACON
        fec_contable    || '|' || -- 5. DFECOPE
        glosa           || '|' || -- 6. DGLOSA
        trim(to_char(debe,  '9999999990.00'))  || '|' ||  -- 7. DDEBE
        trim(to_char(haber, '9999999990.00'))  || '|' ||  -- 8. DHABER
        estado          || '|' || -- 9. DESTOPE
        case
				when cencos is null then
					'0000000000'
				else
					cencos          
		  end || '|' || -- 10. DCENCOS
        voucher         || '|' || -- 11. DINTKARDEX
        voucher         || '|' || -- 12. DINTVTACOM
        voucher         || '|'  -- 13. DINTREG
        as exp_row
from (       
    select 	trim(to_char(ca.ano,'0000')) || trim(to_char(ca.mes,'00')) || '00' as periodo,						-- 01. Periodo
           	trim(ca.origen) || trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || 
			  	trim(to_char(ca.nro_libro, '00')) || trim(to_char(ca.nro_asiento, '00000')) || 
			  	trim(to_char(cad.item, '00000')) as voucher,																		-- 02. Voucher
			  	case 
				  when ca.nro_libro = 0 then
					'A'
				  when ca.nro_libro = 90 then
					'C'
				  else 
					'M'
			  	end  || trim(to_char(ca.nro_asiento, '000000')) as nro_asiento,											-- 03. Nro asiento
           	trim(cad.cnta_ctbl) as cnta_ctbl,																					-- 04. Cuenta contable
			  	o.nombre as desc_origen,																								-- 05. Unidad Operativa																		
			  	trim(REPLACE(cad.cencos, 'Ñ', 'N')) as cencos,																							-- 06. Cencos
           	decode(ca.cod_moneda, PKG_LOGISTICA.of_soles(null), 'PEN', 'USD') as moneda,    						-- 07. Moneda
			  	p.tipo_doc_ident,																											-- 08. Tipo Documento Emisor
			  	decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as nro_doc_ident,									-- 09. Numero Documento Identidad
			  	decode(dt.cod_sunat, null, '00', dt.cod_sunat) as tipo_docref1,																							-- 10. Tipo de comprobante asociado
				case
					when instr(cad.nro_docref1, '-') > 0 then
						case
							when substr(cad.nro_docref1, 1, 1) in ('E', 'F', 'B') then
								substr(cad.nro_docref1, 1, 1) || lpad(substr(cad.nro_docref1, 2, instr(cad.nro_docref1, '-') - 2), 3, '0')
							else
								lpad(substr(cad.nro_docref1, 1, instr(cad.nro_docref1, '-') - 1),4,'0')
						end
					else
						'0000'
				end as serie,																												-- 11. serie del comprobante
				case
					when instr(cad.nro_docref1, '-') > 0 then
						trim(substr(cad.nro_docref1, instr(cad.nro_docref1, '-') + 1))
					else
						trim(decode(cad.nro_docref1, null, '00000000', cad.nro_docref1))
				end as numero,																												-- 12. Numero del comprobante
           	trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')) as fec_contable, 												-- 13. Fecha contable
				'' as fecha_vencimiento,																								-- 14. Fecha vencimiento
				trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')) as fec_operacion, 												-- 15. Fecha de la operacion
				-- 16. Glosa principal
           	trim(substr(
					replace(
						replace(
							replace(
								replace(
									replace(
										replace(
											upper(decode(cad.det_glosa, null, decode(ca.desc_glosa, null, cc.desc_cnta, ca.desc_glosa), cad.det_glosa)),
										chr(9), ' '),	
									'Ó', 'O'),
								'|', ''), 
							chr(13), ' '),
						chr(10),' '), 
					'/', '-'), 1, 200)
				) as glosa,	
				-- 17. Glosa referencial
				'' as glosa_referencial,																								
       	  	nvl(sum(decode(cad.flag_debhab, 'D', ROUND(cad.imp_movsol,2), 0.00)),0) as debe,						-- 18. Movimientos del debe
       	  	nvl(sum(decode(cad.flag_debhab, 'H', ROUND(cad.imp_movsol,2), 0.00)),0) as haber,					-- 19. Movimientos del haber
				/*case
					when ca.nro_libro = 4 then '140100'
					when ca.nro_libro = 3 then
						case 
							when (select p.flag_personeria
									  from cntas_pagar 	cp,
											 proveedor		p
									where cp.cod_relacion 	= p.proveedor
									  and cp.origen 			= ca.origen
 									  and cp.ano				= ca.ano
									  and cp.mes				= ca.mes
									  and cp.nro_libro		= ca.nro_libro
									  and cp.nro_asiento		= ca.nro_Asiento) = 'E' then '080200'
							else
								'080100'
						end
					else	
						''
				end*/
				'' as dato_codigo,																									-- 20. dato estructurado
			  '1' as estado 																											-- 21. Estado de la operacion

    from cntbl_asiento 		ca,
         cntbl_asiento_Det cad,
			cntbl_cnta			cc,
			origen				o,
			proveedor			p,
			doc_tipo				dt
    where ca.origen 			= cad.origen
      and ca.ano    			= cad.ano
      and ca.mes    			= cad.mes
      and ca.nro_libro 		= cad.nro_libro
      and ca.nro_asiento 	= cad.nro_asiento     
		and cad.cnta_ctbl	 	= cc.cnta_ctbl
		and ca.origen			= o.cod_origen
		and cad.cod_relacion = p.proveedor			(+)
		and cad.tipo_docref1	= dt.tipo_doc			(+)
      and ca.ano = :an_year
      and ca.mes = :an_mes
      and ca.flag_estado <> '0'
  group by 	trim(to_char(ca.ano,'0000')) || trim(to_char(ca.mes,'00')) || '00',
           	trim(ca.origen) || trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || 
			  	trim(to_char(ca.nro_libro, '00')) || trim(to_char(ca.nro_asiento, '00000')) || 
			  	trim(to_char(cad.item, '00000')),
			  	case 
				  when ca.nro_libro = 0 then
					'A'
				  when ca.nro_libro = 90 then
					'C'
				  else 
					'M'
			  	end  || trim(to_char(ca.nro_asiento, '000000')),
           	trim(cad.cnta_ctbl),
			  	o.nombre,
			  	trim(REPLACE(cad.cencos, 'Ñ', 'N')),
           	decode(ca.cod_moneda, PKG_LOGISTICA.of_soles(null), 'PEN', 'USD'),
			  	p.tipo_doc_ident,																											
			  	decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
			  	decode(dt.cod_sunat, null, '00', dt.cod_sunat),
				case
					when instr(cad.nro_docref1, '-') > 0 then
						case
							when substr(cad.nro_docref1, 1, 1) in ('E', 'F', 'B') then
								substr(cad.nro_docref1, 1, 1) || lpad(substr(cad.nro_docref1, 2, instr(cad.nro_docref1, '-') - 2), 3, '0')
							else
								lpad(substr(cad.nro_docref1, 1, instr(cad.nro_docref1, '-') - 1),4,'0')
						end
					else
						'0000'
				end ,
				case
					when instr(cad.nro_docref1, '-') > 0 then
						trim(substr(cad.nro_docref1, instr(cad.nro_docref1, '-') + 1))
					else
						trim(decode(cad.nro_docref1, null, '00000000', cad.nro_docref1))
				end,
           	trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')),
           	trim(substr(
					replace(
						replace(
							replace(
								replace(
									replace(
										replace(
											upper(decode(cad.det_glosa, null, decode(ca.desc_glosa, null, cc.desc_cnta, ca.desc_glosa), cad.det_glosa)),
										chr(9), ' '),	
									'Ó', 'O'),
								'|', ''), 
							chr(13), ' '),
						chr(10),' '), 
					'/', '-'), 1, 200)
				)
  order by voucher
)
WHERE debe > 0 or haber > 0

" arguments=(("an_year", number),("an_mes", number)) )
