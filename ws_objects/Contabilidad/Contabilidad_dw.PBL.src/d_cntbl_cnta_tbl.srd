         CC.DESC_CNTA,   
         CC.ABREV_CNTA,   
         CC.NIV_CNTA,   
         CC.FECHA_CREACION,   
         CC.COD_MONEDA,   
         CC.FLAG_ESTADO,   
         CC.FLAG_PERMITE_MOV,   
         CC.FLAG_CTABCO,   
         CC.FLAG_CENCOS,   
         CC.FLAG_COD_SEGUI,   
         CC.FLAG_LABOR,   
         CC.FLAG_DOC_REF,   
         CC.FLAG_DOC_REF2,   
         CC.FLAG_CODREL,   
         CC.CLASE_SEGUI,   
         CC.TIPO_SDOCONTAB,   
         CC.FLAG_AJUSTE,   
         CC.CNTA_CTBL_AJUSTE,   
         CC.CLASE_CNTBL,   
         CC.FLAG_REPLICACION,   
         CC.FLAG_TIPO_CAM_ADC,   
         CC.FLAG_CENTRO_BENEF,
         ca.cnta_debe, ca.cnta_haber
    FROM CNTBL_CNTA  CC,
         cntbl_ctas_aut ca
where cc.cnta_ctbl = ca.cnta_cntbl (+)          
ORDER BY CC.CNTA_CTBL ASC   
" update="CNTBL_CNTA" updatewhere=0 updatekeyinplace=yes )
