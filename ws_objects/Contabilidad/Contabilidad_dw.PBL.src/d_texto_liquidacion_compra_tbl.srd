table(column=(type=char(142) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="select trim(p.tipo_doc_ident) || '|' ||                                       -- 1. Tipo de Documento
       trim(substr(p.nro_doc_ident, 1, 15))  || '|' ||                                       -- 2. Numero de Documento
       trim(substr(p.apellido_pat, 1, 20)) || '|' ||                          -- 3. Apellidos Paternos
       trim(substr(p.apellido_mat, 1, 20)) || '|' ||                          -- 4. Apellidos Maternos
       trim(substr(trim(p.nombre1 || ' ' || p.nombre2), 1, 20)) || '|' ||     -- 5. Nombres Completos
       case 
         when instr(cp.nro_doc, '-') > 0 then
           substr(cp.nro_doc, 1, instr(cp.nro_doc, '-') - 1)
         else
           ''
       end || '|' ||                                                    -- 6. Serie del documento
       case 
         when instr(cp.nro_doc, '-') > 0 then
           substr(cp.nro_doc, instr(cp.nro_doc, '-') + 1)
         else
           cp.nro_doc
       end || '|' ||                                                    -- 7. Numero del documento
       to_char(cp.fecha_emision, 'dd/mm/yyyy') || '|' ||                -- 8. Fecha de emisión
       to_char(cp.fecha_emision, 'dd/mm/yyyy') || '|' ||                -- 9. Fecha de retención
       trim(to_char(sum(cpd.importe), '999999990.00')) || '|' ||        -- 10. Total de la operacion
       '06' || '|'   as texto                                           -- 11. Tipo de operacion
       
from cntas_pagar cp,
     cntas_pagar_det cpd,
     proveedor       p
where cp.cod_relacion = cpd.cod_relacion
  and cp.tipo_doc     = cpd.tipo_doc
  and cp.nro_doc      = cpd.nro_doc
  and cp.cod_relacion = p.proveedor  
  and cp.tipo_doc     = 'LC'  
  and cp.ano			 = :an_year
  and cp.mes			 = :an_mes 
group by p.tipo_doc_ident,
       p.nro_doc_ident,
       substr(p.apellido_pat, 1, 20), 
       substr(p.apellido_mat, 1, 20),
       substr(trim(p.nombre1 || ' ' || p.nombre2), 1, 20),
       case 
         when instr(cp.nro_doc, '-') > 0 then
           substr(cp.nro_doc, 1, instr(cp.nro_doc, '-') - 1)
         else
           ''
       end,
       case 
         when instr(cp.nro_doc, '-') > 0 then
           substr(cp.nro_doc, instr(cp.nro_doc, '-') + 1)
         else
           cp.nro_doc
       end,
       to_char(cp.fecha_emision, 'dd/mm/yyyy')
" arguments=(("an_year", number),("an_mes", number)) )
