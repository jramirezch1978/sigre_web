detail(height=68 color="536870912" transparency="0" gradient.color="8421504" gradient.transparency="0" gradient.angle="0" brushmode="0" gradient.repetition.mode="0" gradient.repetition.count="0" gradient.repetition.length="100" gradient.focus="0" gradient.scale="100" gradient.spread="100" )
table(column=(type=char(4000) updatewhereclause=yes name=datos dbname="datos" )
 retrieve="  select trim(to_char(cp.ano, '0000')) || trim(to_char(cp.mes, '00')) ||  '00' ||       --  01.Periodo
         '|' || trim(cp.origen) || trim(to_char(cp.ano, '0000')) || trim(to_char(cp.mes, '00')) || trim(to_char(cp.nro_libro, '00')) || trim(to_char(cp.nro_asiento, '000000')) || -- 02. Voucher
         '|' || 'M' || trim(to_char(nro_asiento, '000000')) ||                          -- 03. Nro Asiento
         '|' || to_char(cp.fecha_emision, 'dd/mm/yyyy')     ||                          -- 04. Fecha Emision Documento
         '|' || 
			case 
				when dc.cod_sunat = '14' then
					case 	
						when trunc(cp.vencimiento) > pkg_utility.of_last_day(decode(:ani_mes, 12, :ani_year + 1, :ani_year), decode(:ani_mes, 12, 1, :ani_mes + 1)) then
							to_char(pkg_utility.of_last_day(decode(:ani_mes, 12, :ani_year + 1, :ani_year), decode(:ani_mes, 12, 1, :ani_mes + 1)), 'dd/mm/yyyy')
						else
							to_char(cp.vencimiento, 'dd/mm/yyyy')
					end 
				else
					''     													
			end																||                          -- 05. Fecha de Pago o vencimiento
         '|' || dc.cod_sunat                              	||                          -- 06. Tipo Documento
			'|' || 
				case 
					when dc.cod_sunat  = '05' then -- Boletos de Avion
						'3'
					when dc.cod_sunat in (46, 50, 51, 52, 53, 54) then
						case
							when cp.serie_cp is not null then
								substr(cp.serie_cp, 2)
							else
								substr(PKG_FACT_ELECTRONICA.of_get_serie(cp.nro_doc), 2)
						end
					when cp.serie_cp is null or cp.tipo_doc = 'LC' then
						case
							when instr(cp.nro_doc, '-') > 0 then
								PKG_FACT_ELECTRONICA.of_get_serie(cp.nro_doc)
							else '0000'
						end
					else
						CP.SERIE_CP											
				end															||									 -- 07. Serie del documento
			'|' ||  
				case 
					when dc.cod_sunat in (50, 52) then
				  		to_char(cp.fecha_emision, 'yyyy')
					else
				  		''
				end 															||                          -- 08. Año de Emision de la DUA
			'|' || 
          	case 
					when dc.cod_sunat in (46, 50, 51, 52, 53, 54) then
						case 
							when CP.NUMERO_CP is not null and cp.serie_cp is not null then
								PKG_UTILITY.of_trim(CP.NUMERO_CP, '0')
                   	when instr(cp.nro_doc, '-') > 0 then
                     	trim(substr(cp.nro_doc, instr(cp.nro_doc, '-') + 1))
                   	else 
								trim(cp.nro_doc)
						end
            	when CP.NUMERO_CP is null or cp.tipo_doc = 'LC' then
                 case 
                   when instr(cp.nro_doc, '-') > 0 then
                     trim(substr(cp.nro_doc, instr(cp.nro_doc, '-') + 1))
                   else 
							trim(cp.nro_doc)
                 end
            	else
              		PKG_UTILITY.of_trim(CP.NUMERO_CP, '0')
          	end ||									 														 -- 09. Nro Documento
         '|' || '' ||                                                                   -- 10. Total de Operaciones
         '|' || p.tipo_doc_ident ||                                                     -- 11. Tipo Documento Identidad
         '|' || DECODE(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) ||                -- 12. Nro Documento Identidad
         '|' || trim(substr(replace(p.nom_proveedor, '|', ''), 1, 100)) ||              -- 13. Razon Social o nombre completo

			-- 14. Base Imponible Credito Fiscal 01
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpd.importe, cpd.importe * cp.tasa_cambio) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpd.tipo_cred_fiscal = '01') ||                                        

         -- 15. IGV Credi Fiscal 01
			'|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion 	= cp.cod_relacion
             and cpd.tipo_doc     		= cp.tipo_doc
             and cpd.nro_doc      		= cp.nro_doc
             and cpi.cod_relacion  		= cpd.cod_relacion
             and cpi.tipo_doc      		= cpd.tipo_doc
             and cpi.nro_doc       		= cpd.nro_doc
             and cpi.item          		= cpd.item
			and cpi.tipo_impuesto 	= i.tipo_impuesto
		  	and i.flag_igv				= '1'
             and cpd.tipo_cred_fiscal = '01') ||
                                        
			-- 16. Base Imponible Credito Fiscal 02
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpd.importe, cpd.importe * cp.tasa_cambio) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpd.tipo_cred_fiscal = '02') ||  

			-- 17. IGV Credi Fiscal 02                                      
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpi.cod_relacion  = cpd.cod_relacion
             and cpi.tipo_doc      = cpd.tipo_doc
             and cpi.nro_doc       = cpd.nro_doc
             and cpi.item          = cpd.item
			and cpi.tipo_impuesto = i.tipo_impuesto
			and i.flag_igv				= '1'
             and cpd.tipo_cred_fiscal = '02') ||                                        

			-- 18. Base Imponible Credito Fiscal 03
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpd.importe, cpd.importe * cp.tasa_cambio) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpd.tipo_cred_fiscal = '03') ||                                        

			-- 19. IGV Credi Fiscal 03
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpi.cod_relacion  = cpd.cod_relacion
             and cpi.tipo_doc      = cpd.tipo_doc
             and cpi.nro_doc       = cpd.nro_doc
             and cpi.item          = cpd.item
			and cpi.tipo_impuesto = i.tipo_impuesto
			and i.flag_igv				= '1'
             and cpd.tipo_cred_fiscal = '03') ||        

			-- 20. Compras no gravadas                                
         '|' ||
         trim(to_char((select nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpd.importe, cpd.importe*cp.tasa_cambio) * DECODE(dc.flag_signo, '+', 1, -1)), 0)
                         from cntas_pagar_det cpd
                        where cpd.cod_relacion = cp.cod_relacion
                          and cpd.tipo_doc     = cp.tipo_doc
                          and cpd.nro_doc      = cp.nro_doc 
								  and cpd.tipo_cred_fiscal = '04') +
                      (select nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0)
                         from cntas_pagar_det cpd,
										cp_doc_det_imp  cpi,
										impuestos_tipo	 i
                        where cpd.cod_relacion 	= cp.cod_relacion
								  and cpd.tipo_doc		= cp.tipo_doc
								  and cpd.nro_doc			= cp.nro_doc
							     and cpi.cod_relacion  = cpd.cod_relacion
                          and cpi.tipo_doc      = cpd.tipo_doc
                          and cpi.nro_doc       = cpd.nro_doc 
								  and cpi.item       	= cpd.item
								  and cpi.tipo_impuesto = i.tipo_impuesto
								  and cpd.tipo_cred_fiscal = '04'
								  and i.flag_igv = '1'), '9999990.00')) ||   
			
			-- 21. Impuesto Selectivo al consumo
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpi.cod_relacion  = cpd.cod_relacion
             and cpi.tipo_doc      = cpd.tipo_doc
             and cpi.nro_doc       = cpd.nro_doc
             and cpi.item          = cpd.item
             and cpi.tipo_impuesto = i.tipo_impuesto
				 and cpi.tipo_impuesto = 'ISC') ||  
			
         -- 22. Otros cargos o Impuestos                             	
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpi.cod_relacion  = cpd.cod_relacion
             and cpi.tipo_doc      = cpd.tipo_doc
             and cpi.nro_doc       = cpd.nro_doc
             and cpi.item          = cpd.item
             and cpi.tipo_impuesto = i.tipo_impuesto
				 and cpi.tipo_impuesto = 'ICBPER') ||                                    	

         -- 23. Otros cargos o Impuestos                             	
         '|' ||
         (select trim(to_char(nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpi.importe, cpi.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0), '9999990.00'))
            from cntas_pagar_det cpd,
                 cp_doc_det_imp  cpi,
					  impuestos_tipo	i
           where cpd.cod_relacion = cp.cod_relacion
             and cpd.tipo_doc     = cp.tipo_doc
             and cpd.nro_doc      = cp.nro_doc
             and cpi.cod_relacion  = cpd.cod_relacion
             and cpi.tipo_doc      = cpd.tipo_doc
             and cpi.nro_doc       = cpd.nro_doc
             and cpi.item          = cpd.item
 				 and cpi.tipo_impuesto = i.tipo_impuesto
				 and cpi.tipo_impuesto not in ('ISC', 'ICBPER')
				 and i.flag_igv		  = '0') ||                                        	

			-- 24. Importe Total del documento               
         '|' ||
         trim(to_char((select nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , cpd.importe, cpd.importe*cp.tasa_cambio) * DECODE(dc.flag_signo, '+', 1, -1)), 0)
                         from cntas_pagar_det cpd
                        where cpd.cod_relacion = cp.cod_relacion
                          and cpd.tipo_doc     = cp.tipo_doc
                          and cpd.nro_doc      = cp.nro_doc ) +
                      (select nvl(sum(DECODE(cp.cod_moneda , PKG_LOGISTICA.of_soles(null) , ci.importe, ci.importe*cp.tasa_cambio) * DECODE(i.signo, '-', -1, 1) * DECODE(dc.flag_signo, '+', 1, -1)), 0)
                         from cp_doc_det_imp  ci,
										impuestos_tipo	 i
                        where ci.cod_relacion  = cp.cod_relacion
                          and ci.tipo_doc      = cp.tipo_doc
                          and ci.nro_doc       = cp.nro_doc 
								  and ci.tipo_impuesto = i.tipo_impuesto), '9999990.00')) ||   
     
         '|' || 
			case 
				when cp.cod_moneda = 'US$' then
					'USD'
				else
					'PEN'
			end ||    -- 25. Moneda
         '|' || trim(to_char(cp.tasa_cambio, '9990.000')) ||                            -- 26. Tasa Cambio
         '|' ||
         case 
           when cp.tipo_doc in ('NCP', 'NDP', 'CNC', 'CDB') then
            (select to_char(cp2.fecha_emision, 'dd/mm/yyyy')
               from doc_referencias dr,
                    cntas_pagar     cp2
              where dr.proveedor_ref = cp2.cod_relacion
                and dr.tipo_ref      = cp2.tipo_doc
                and dr.nro_ref       = cp2.nro_doc
                and dr.cod_relacion  = cp.cod_relacion
                and dr.tipo_doc      = cp.tipo_doc
                and dr.nro_doc       = cp.nro_doc
                and rownum           = 1)
           else
             ''
         end ||                                                                         -- 27. Fecha de emision del documento de referencia
         '|' ||
         case 
           when cp.tipo_doc in ('NCP', 'NDP', 'CNC', 'CDB') then
            (select dt2.cod_sunat
               from doc_referencias dr,
                    cntas_pagar     cp2,
                    doc_tipo        dt2
              where dr.proveedor_ref = cp2.cod_relacion
                and dr.tipo_ref      = cp2.tipo_doc
                and dr.nro_ref       = cp2.nro_doc
                and cp2.tipo_doc     = dt2.tipo_doc
                and dr.cod_relacion  = cp.cod_relacion
                and dr.tipo_doc      = cp.tipo_doc
                and dr.nro_doc       = cp.nro_doc
					 and rownum				 = 1)
           else
             ''
         end ||                                                                         -- 28. Tipo Documento Referencia
         '|' ||
         case 
           when cp.tipo_doc in ('NCP', 'NDP', 'CNC', 'CDB') then
            (select case
                      when instr(cp2.nro_doc, '-' ) > 0 then
                        PKG_FACT_ELECTRONICA.of_get_serie(cp2.nro_doc)
                      else
                        '0000'
                    end
               from doc_referencias dr,
                    cntas_pagar     cp2,
                    doc_tipo        dt2
              where dr.proveedor_ref = cp2.cod_relacion
                and dr.tipo_ref      = cp2.tipo_doc
                and dr.nro_ref       = cp2.nro_doc
                and cp2.tipo_doc     = dt2.tipo_doc
                and dr.cod_relacion  = cp.cod_relacion
                and dr.tipo_doc      = cp.tipo_doc
                and dr.nro_doc       = cp.nro_doc
					 and rownum				 = 1 )
           else
             ''
         end ||                                                                         -- 29. Serie Documento Referencia
         '|' || '' ||                                                                   -- 30. Dependencia Aduanera
         '|' ||
         case 
           when cp.tipo_doc in ('NCP', 'NDP', 'CNC', 'CDB') then
            (select case
                      when instr(cp2.nro_doc, '-' ) > 0 then
								PKG_FACT_ELECTRONICA.of_get_nro(cp2.nro_doc)
                      else
                        '000000'
                    end
               from doc_referencias dr,
                    cntas_pagar     cp2,
                    doc_tipo        dt2
              where dr.proveedor_ref = cp2.cod_relacion
                and dr.tipo_ref      = cp2.tipo_doc
                and dr.nro_ref       = cp2.nro_doc
                and cp2.tipo_doc     = dt2.tipo_doc
                and dr.cod_relacion  = cp.cod_relacion
                and dr.tipo_doc      = cp.tipo_doc
                and dr.nro_doc       = cp.nro_doc
				    and rownum				 = 1)
           else
             ''
         end ||                                                                         -- 31. Numero Documento Referencia
         '|' || to_char(d.fecha_deposito, 'dd/mm/yyyy')	||                             -- 32. Decha emision constancia deposito detraccion
         '|' || d.nro_deposito                          	||                             -- 33. Nro de constancia deposito Detraccion
         '|' || 
			case
				when (select count(*)
							from caja_bancos cb,
								  caja_bancos_det cbd
							where cb.origen = cbd.origen
							  and cb.nro_registro = cbd.nro_registro
							  and cbd.cod_relacion = cp.cod_relacion
							  and cbd.tipo_doc	  = cp.tipo_doc
							  and cbd.nro_doc		  = cp.nro_doc
							  and cbd.flag_ret_igv = '1'   
						) > 0 then '1'	
				else ''
			end
			||                             																 -- 34. Marca de comprobante sujeto a retencion
         '|' || trim(NVL(CP.CLASE_BIEN_SERV,'0'))       	||                             -- 35. Clasificación de bienes y servicios adquiridos (Tabla 30 SUNAT)
         '|' || ''                                      	||                             -- 36. Identificacion del Contrato o del proyecto en el caso de los Operadores de Sociedades irregulares
         '|' || ''                                     	||                             -- 37. Inconsistencia en el Tipo de cambio
         '|' || ''                                     	||                             -- 38. Inconsistencia de Proveedores No habido         
         '|' || ''                                     	||                             -- 39. Inconsistencia de proveedores que renunciaron al IGV
         '|' || ''                                     	||                             -- 40. Inconsistencia de DNI para liquidaciones de compra
         '|' || '1'                                     	||                             -- 41. Indicador de comprobantes pagados con algun medio de pago
         '|' || 
			case 
				when dc.cod_sunat = '03' then
					'0'
				when to_char(cp.fecha_emision, 'yyyymm') < trim(to_char(:ani_year, '0000')) || trim(to_char(:ani_mes, '00')) then
					'6'
				else
					'1'
			end                                   			 ||                              -- 42. Oportunidad de anotación o indicación de ajuste de credito fiscal
         '|' as datos
  from   cntas_pagar cp,
         proveedor   p,
         doc_tipo    dc,
         detraccion  d
  where  cp.cod_relacion            	= p.proveedor
    AND  cp.tipo_doc                	= dc.tipo_doc
    and  cp.nro_detraccion          	= d.nro_detraccion   (+)
    AND  cp.flag_estado             	<> '0'
    AND  cp.ano            				= :ani_year
    AND  cp.mes								= :ani_mes
    AND  cp.nro_libro               	= '03'
    and  cp.tipo_doc                	not in ('RE')
    and  NVL(p.flag_personeria, 'N')	<> 'E'
