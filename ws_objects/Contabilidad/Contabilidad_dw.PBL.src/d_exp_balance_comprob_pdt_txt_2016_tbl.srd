table(column=(type=char(183) updatewhereclause=yes name=datos dbname="datos" )
 retrieve="select -- 01. cuenta contable SUNAT
		 cc.cnta_cntbl_sunat || '|' ||
		 -- 02. Saldos iniciales DEBE
       case 
         when substr(cc.cnta_cntbl_sunat,1,2) <= '59' then
           trim(to_char(round(sum(case 
                 when ca.nro_libro = 0 and ca.mes = 0 and cad.flag_debhab = 'D' then
                   cad.imp_movsol
                 else
                   0
               end),0)))
          else
            ''
        end || '|' ||
		  -- 03. Saldos Iniciables Haber
        case 
         when substr(cc.cnta_cntbl_sunat,1,2) <= '59' then
           trim(to_char(round(sum(case 
                 when ca.nro_libro = 0 and ca.mes = 0 and cad.flag_debhab = 'H' then
                   cad.imp_movsol
                 else
                   0
               end),0)))
          else
            ''
        end || '|' ||
		  -- 04. Movimientos del ejercicio DEBE
        trim(to_char(round(sum(case 
           when ca.nro_libro > 0 and ca.mes > 0 and cad.flag_debhab = 'D' then
             cad.imp_movsol
           else
             0
        end),0))) || '|' ||
		  -- 05. Movimientos del ejercicio HABER
        trim(to_char(round(sum(
		  case 
           when ca.nro_libro > 0 and ca.mes > 0 and cad.flag_debhab = 'H' then
             cad.imp_movsol
           else
             0
        end),0))) || '|' ||
		  -- 06. Transferencias y Cancelaciones DEBE
        '0' || '|' ||
		  -- 07. Transferencias y Cancelaciones HABER
        '0' || '|' ||
		  -- 08. Ajuste por adecuacion o Implicancia DEBE
        '0' || '|' ||
		  -- 09. Ajuste por adecuacion o Implicancia HABER
        '0' || '|' 
		  as datos
from cntbl_Asiento ca,
     cntbl_asiento_Det cad,
     cntbl_cnta        cc,
     cnta_cntbl_sunat  ccs
where ca.origen        		= cad.origen
  and ca.ano           		= cad.ano
  and ca.mes           		= cad.mes
  and ca.nro_libro     		= cad.nro_libro
  and ca.nro_asiento   		= cad.nro_asiento
  and cad.cnta_ctbl    		= cc.cnta_ctbl
  and cc.cnta_cntbl_sunat 	= ccs.cnta_sunat
  and ca.ano           		= :ani_year
  and ca.nro_libro			<> 90
  and ca.flag_estado   		<> '0'
group by cc.cnta_cntbl_sunat,
       ccs.desc_cnta
order by 1

     " arguments=(("ani_year", number)) )
