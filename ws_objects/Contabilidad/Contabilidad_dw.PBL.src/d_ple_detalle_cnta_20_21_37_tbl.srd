table(column=(type=char(198) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="select  to_char(ca.fecha_cntbl, 'yyyymm') || '31' || '|'                             -- 01. Periodo
        || '9'                                    || '|'                             -- 02. Codigo del catalogo utilizado
        || ac.cod_sunat                           || '|'                             -- 03. Tipo de Exitencia
        || replace(a.cod_art, '.', '')            || '|'                             -- 04. Código del articulo
        || ''                                     || '|'                             -- 05. codigo del catalogo utilizado
        || a.cod_cubso                            || '|'                             -- 06. Código del articulo segun el CUBSO
        || case
           when a.desc_art is not null then
             trim(substr(PKG_UTILITY.of_trim_SUNAT(a.desc_art), 1, 80))
          else
             trim(substr(PKG_UTILITY.of_trim_SUNAT(cad.det_glosa), 1, 80))
        end   || '|'                                                      					-- 07. Descripcion del articulo
        || case
           when u.cod_sunat is not null then
             u.cod_sunat 
          else
             'D97'
        end                     || '|'                                    					 -- 08. Código de la unidad
        || '1'                                || '|'                                    -- 09. Código del método de valuación utilizado
        || trim(to_char(abs(sum(am.cant_procesada * amt.factor_sldo_total)) , '999999999990.00000000'))   || '|'          -- 10. Cantidad de la existencia
        || case
			     when abs(sum(am.cant_procesada * amt.factor_sldo_total * am.precio_unit)) = 0.00 then
				     '0.00'
				  when abs(sum(am.cant_procesada * amt.factor_sldo_total)) = 0.00 then
				     '0.00'
				  else
				     trim(to_char(abs(sum(am.cant_procesada * amt.factor_sldo_total * am.precio_unit)) / abs(sum(am.cant_procesada * amt.factor_sldo_total)), '999999999990.00000000'))
			  end   						|| '|'             												-- 11. Costo unitario de la existencia
        || case
			     when abs(sum(am.cant_procesada * amt.factor_sldo_total * am.precio_unit)) = 0.00 then
				     '0.00'
				  else
				     trim(to_char(abs(sum(am.cant_procesada * amt.factor_sldo_total * am.precio_unit)), '999999999990.00'))
			 end  || '|'  																						-- 12. Costo total
        || '1'                                                   || '|'               	-- 13. Indica el estado de la operación
        as texto     

   from cntbl_asiento     ca,
        cntbl_asiento_Det cad,
        articulo_mov      am,
        articulo          a,
        articulo_clase    ac,
        unidad            u,
        vale_mov          vm,
        articulo_mov_tipo amt
  where ca.origen       = cad.origen
    and ca.ano          = cad.ano
    and ca.mes          = cad.mes
    and ca.nro_libro    = cad.nro_libro
    and ca.nro_asiento  = cad.nro_asiento     
    and cad.org_am      = am.cod_origen     (+)
    and cad.nro_am      = am.nro_mov        (+)
    and am.nro_vale     = vm.nro_vale       (+)
    and vm.tipo_mov     = amt.tipo_mov      (+)
    and am.cod_art      = a.cod_art         (+)
    and a.cod_clase     = ac.cod_clase      (+)
    and a.und           = u.und             (+)
    and ca.ano      = :an_year
    and ca.mes      = :an_mes
    and ca.flag_estado <> '0'
    and substr(cad.cnta_ctbl, 1, 2) in ('20', '21')
group by  to_char(ca.fecha_cntbl, 'yyyymm'),
          ac.cod_sunat,
           replace(a.cod_art, '.', ''),
           a.cod_cubso,
           case
           when a.desc_art is not null then
             trim(substr(PKG_UTILITY.of_trim_SUNAT(a.desc_art), 1, 80))
          else
             trim(substr(PKG_UTILITY.of_trim_SUNAT(cad.det_glosa), 1, 80))
       end,
          case
           when u.cod_sunat is not null then
             u.cod_sunat 
          else
             'D97'
       end
having abs(sum(am.cant_procesada * amt.factor_sldo_total)) > 0
order by 1


" arguments=(("an_year", number),("an_mes", number)) )
