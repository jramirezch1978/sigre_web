table(column=(type=char(100) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="select to_char(PKG_UTILITY.of_last_day(:an_year, :an_mes), 'yyyymmdd')   || '|'    	-- 1. periodo
    || trim(cad.cnta_ctbl)                   || '|'    										-- 2. Cuenta Contable
    || case
			when bc.flag_flujo_caja = '1' then
				nvl(b.cod_banco_rtps, '99')
			else
				'99'
		 end     || '|'    																				-- 3. Entidad Financiera
    || case
			when bc.flag_flujo_caja = '1' then
				trim(bc.cod_ctabco)
			else
				'-'
		 end                   						|| '|'    										-- 4. Nro Cuenta
    || case 
       	when bc.cod_moneda = 'US$' or bc.cod_moneda is null then 'USD'
        else 'PEN'
       end                                   || '|'    										-- 5. Tipo de Moneda
    || case
         when sum(DECODE(cad.flag_debhab, 'D', cad.imp_movsol, 0)) > sum(DECODE(cad.flag_debhab, 'H', cad.imp_movsol, 0)) then
           trim(to_char(sum(DECODE(cad.flag_debhab, 'D', cad.imp_movsol, 0)) - sum(DECODE(cad.flag_debhab, 'H', cad.imp_movsol, 0)), '99999999990.00'))
         else
           '0.00'
       end                                   || '|'    										-- 6. saldo deudor
    || case
         when sum(DECODE(cad.flag_debhab, 'D', cad.imp_movsol, 0)) < sum(DECODE(cad.flag_debhab, 'H', cad.imp_movsol, 0)) then
           trim(to_char(sum(DECODE(cad.flag_debhab, 'H', cad.imp_movsol, 0)) - sum(DECODE(cad.flag_debhab, 'D', cad.imp_movsol, 0)), '99999999990.00'))
         else
           '0.00'
       end                                   || '|'    										-- 7. saldo acreedor
    || '1'                                   || '|'    										-- 8. Estado de la operacion
	 || '||||||||'																							-- 9-16 campos de libre utilizacion
	 as texto
from cntbl_asiento ca,
     cntbl_asiento_det cad,
     banco_cnta        bc,
     banco             b
where ca.origen = cad.origen
  and ca.ano    = cad.ano
  and ca.mes    = cad.mes
  and ca.nro_libro = cad.nro_libro
  and ca.nro_asiento = cad.nro_asiento     
  and cad.cod_ctabco = bc.cod_ctabco         (+)
  and bc.cod_banco   = b.cod_banco           (+)
  and ca.ano         = :an_year
  and ca.mes         <= :an_mes
  and cad.cnta_ctbl  like '10%'
  and ca.flag_estado <> '0'
group by cad.cnta_ctbl,
         case
				when bc.flag_flujo_caja = '1' then
					nvl(b.cod_banco_rtps, '99')
				else
					'99'
			 end,
         case
				when bc.flag_flujo_caja = '1' then
					trim(bc.cod_ctabco)
				else
					'-'
			 end ,
         case 
          when bc.cod_moneda = 'US$' or bc.cod_moneda is null then 'USD'
          else 'PEN'
         end  
having round(sum(DECODE(cad.flag_debhab, 'D', cad.imp_movsol, 0)) - sum(DECODE(cad.flag_debhab, 'H', cad.imp_movsol, 0)),2) <> 0
order by cad.cnta_ctbl, 
			case
				when bc.flag_flujo_caja = '1' then
					nvl(b.cod_banco_rtps, '99')
				else
					'99'
			 end
" arguments=(("an_year", number),("an_mes", number)) )
