 column=(type=decimal(2) updatewhereclause=yes name=cntbl_asiento_det_imp_movsol dbname="cntbl_asiento_det.imp_movsol" )
 retrieve="select cp.cod_relacion, 
		 cp.tipo_doc, 
		 cp.nro_doc, 
		 trunc(cp.fecha_emision) as fecha, 
       cp.cod_moneda, 
		 cp.origen, 
		 cp.ano, 
		 cp.mes, 
		 cp.nro_libro, 
		 cp.nro_asiento, 
       cp.tasa_cambio as tc_prov, 
		 c.vta_dol_prom as tc_tabl, 
       cad.cnta_ctbl, 
		 cad.flag_debhab, 
		 cad.imp_movsol
  from cntas_pagar cp, calendario c, cntbl_asiento ca, cntbl_asiento_det cad
 where ( trunc(cp.fecha_emision)=trunc(c.fecha) ) and 
       ( cp.flag_estado<>'0' and 
         cp.ano=:an_ano and 
         cp.mes>=:an_mes_ini and 
         cp.mes<=:an_mes_fin and 
         cp.cod_moneda=:as_moneda and 
			cp.nro_libro = :an_nro_libro ) and 
      (round(cp.tasa_cambio,3) <> round(c.vta_dol_prom,3)) and 
      (ca.origen=cad.origen and 
       ca.ano=cad.ano and 
       ca.mes=cad.mes and 
       ca.nro_libro=cad.nro_libro and 
       ca.nro_asiento=cad.nro_asiento and 
       ca.flag_estado<>'0' and 
       cad.flag_gen_aut is null) and 
      (cp.origen=ca.origen and 
       cp.ano=ca.ano and 
       cp.mes=ca.mes and 
       cp.nro_libro=ca.nro_libro and 
       cp.nro_asiento=ca.nro_asiento ) 
order by cp.ano, cp.mes, cp.nro_libro, cp.nro_asiento, cp.origen, cp.cod_relacion, 
		 cp.tipo_doc, cp.nro_doc" arguments=(("an_ano", number),("an_mes_ini", number),("an_mes_fin", number),("as_moneda", string),("an_nro_libro", number)) )
