table(column=(type=char(4000) updatewhereclause=yes name=exp_row dbname="exp_row" )
 retrieve="select   periodo         		|| '|' ||
         voucher         		|| '|' ||
       	nro_asiento     		|| '|' || -- Numero del asiento
         cnta_ctbl       		|| '|' ||
       	desc_origen     		|| '|' ||
      	cencos        			|| '|' ||
      	moneda        			|| '|' ||
      	tipo_doc_ident    	|| '|' ||
      	nro_doc_ident    		|| '|' ||
      	tipo_docref1    		|| '|' ||
      	serie          		|| '|' ||
      	numero        			|| '|' ||
      	fec_contable    		|| '|' ||
      	fecha_vencimiento  	|| '|' ||
      	fec_operacion    		|| '|' ||
      	glosa          		|| '|' ||
      	glosa_referencial  	|| '|' ||
         trim(to_char(saldo_deudor, '9999999990.00'))   		|| '|' ||
         trim(to_char(saldo_acreedor, '9999999990.00'))   	|| '|' ||
      	dato_codigo  			|| '|' ||
         estado 					|| '|' ||
			cod_banco_sunat		|| '|' ||
			nro_cuenta				|| '|' ||
		   medio_pago				|| '|' ||
       	nro_transaccion		|| '|' as exp_row
from (       
    select -- 01. Periodo
           trim(to_char(ca.ano,'0000')) || trim(to_char(ca.mes,'00')) || '00' as periodo, 
           -- 02. Voucher           
           trim(ca.origen) || trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || trim(to_char(ca.nro_libro, '00')) || 
           trim(to_char(ca.nro_asiento, '00000')) || trim(to_char(cad.item, '00000')) as voucher,                                    
           -- 03. Nro asiento
           case 
              when ca.nro_libro = 0 then
                   'A'
              when ca.nro_libro = 90 then
                   'C'
              else 
                   'M'
           end  || trim(to_char(ca.nro_asiento, '000000')) as nro_asiento,                      
           -- 04. Cuenta contable
           trim(cad.cnta_ctbl) as cnta_ctbl,                                          
           -- 05. Unidad Operativa
           o.nombre as desc_origen,                                                 
           -- 06. Cencos                                   
           trim(replace(REPLACE(cad.cencos, 'Ñ', 'N'), '_', '-')) as cencos,                                              
           -- 07. Moneda
           decode(ca.cod_moneda, PKG_LOGISTICA.of_soles(null), 'PEN', 'USD') as moneda,                
           -- 08. Tipo Documento Emisor
           p.tipo_doc_ident,                                                      
           -- 09. Numero Documento Identidad
           decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as nro_doc_ident,    
           -- 10. Tipo de comprobante asociado              
           decode(dt.cod_sunat, null, '00', dt.cod_sunat) as tipo_docref1,                                     
           -- 11. serie del comprobante         
           case
					when cad.nro_docref1 is null or trim(cad.nro_docref1) = '' then
						'0000'
					when decode(dt.cod_sunat, null, '00', dt.cod_sunat) = '05' then
						'4'
					when instr(replace(cad.nro_docref1, '--', '-'), '-') > 0 then
						case
							when decode(dt.cod_sunat, null, '00', dt.cod_sunat) in ('50', '52') then
								lpad(substr(replace(cad.nro_docref1, '--', '-'), 1, instr(replace(cad.nro_docref1, '--', '-'), '-') - 1),3,'0')
							when substr(replace(cad.nro_docref1, '--', '-'), 1, 1) in ('E', 'F', 'B') then
								substr(replace(cad.nro_docref1, '--', '-'), 1, 1) || lpad(substr(replace(cad.nro_docref1, '--', '-'), 2, instr(replace(cad.nro_docref1, '--', '-'), '-') - 2), 3, '0')
							else
								lpad(substr(replace(cad.nro_docref1, '--', '-'), 1, instr(replace(cad.nro_docref1, '--', '-'), '-') - 1),4,'0')
						end
					else
						'0000'
			  end  as serie,                                                      
           -- 12. Numero del comprobante  
           replace(
					replace(
						replace(
							case
								when decode(dt.cod_sunat, null, '00', dt.cod_sunat) = '00' then
									'00000000'
								when cad.nro_docref1 is null or trim(cad.nro_docref1) = '' then
									'00000000'
								when instr(replace(cad.nro_docref1, '--', '-'), '-') > 0 then
									replace(trim(substr(replace(cad.nro_docref1, '--', '-'), instr(cad.nro_docref1, '-') + 1)), '-', '')
								when cp.numero_cp is not null then
									PKG_UTILITY.of_trim(cp.numero_cp, '0')
								else
									replace(trim(replace(cad.nro_docref1, '--', '-')), '-', '')
							end
						,'**** ****', '00000000')
					,'*', '')
			  ,'L_', '') as numero,                                                        
           -- 13. Fecha contable
           trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')) as fec_contable,            
           -- 14. Fecha vencimiento             
           '' as fecha_vencimiento,                                                
           -- 15. Fecha de la operacion
           trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')) as fec_operacion,                         
           -- 16. Glosa principal
           trim(substr(
			  		replace(
							replace(
								replace(
									replace(
										replace(
											replace(
												decode(cad.det_glosa, null, decode(ca.desc_glosa, null, cc.desc_cnta, ca.desc_glosa), cad.det_glosa)
											,'|', ' ')
										, chr(13) || chr(10), ' ')
									, chr(9), ' ')
								, chr(13), ' ')
							, chr(10), '')
						, '/', ' ')	
					, 1, 200)
			  ) as glosa,
			  -- 17. Glosa referencial
           '' as glosa_referencial,                                                
           -- 18. Movimiento del Debe
           case 
              when nvl(sum(decode(cad.flag_debhab, 'D', round(cad.imp_movsol,2), 0.00)),0) > nvl(sum(decode(cad.flag_debhab, 'H', round(cad.imp_movsol,2), 0.00)),0) then
                   nvl(sum(decode(cad.flag_debhab, 'D', round(cad.imp_movsol,2), 0.00)),0) - nvl(sum(decode(cad.flag_debhab, 'H', round(cad.imp_movsol,2), 0.00)),0)
              else
                 0
           end as saldo_deudor,
           -- 19. Movimiento del Haber
           case 
              when nvl(sum(decode(cad.flag_debhab, 'H', round(cad.imp_movsol,2), 0.00)),0) > nvl(sum(decode(cad.flag_debhab, 'D', round(cad.imp_movsol, 2), 0.00)),0) then
                   nvl(sum(decode(cad.flag_debhab, 'H', round(cad.imp_movsol,2), 0.00)),0) - nvl(sum(decode(cad.flag_debhab, 'D', round(cad.imp_movsol,2), 0.00)),0)
              else
                 0
           end as saldo_acreedor,
           -- 20. dato estructurado
           /*case
              when ca.nro_libro = 4 then '140100'
              when ca.nro_libro = 3 then
                   case 
                     when (select p.flag_personeria
                             from cntas_pagar   cp,
                                  proveedor    p
                            where cp.cod_relacion   = p.proveedor
                              and cp.origen         = ca.origen
                              and cp.ano            = ca.ano
                              and cp.mes            = ca.mes
                              and cp.nro_libro      = ca.nro_libro
                              and cp.nro_asiento    = ca.nro_Asiento) = 'E' then '080200'
                     else
                          '080100'
                   end
              else  
                   ''
           end
			  	*/
			  	'' as dato_codigo,                                              
           	-- 21. Estado de la operacion    
           	'1' as estado,
				-- 22. Entidad bancaria
			  	b.cod_banco_sunat,
				-- 23. Cta Banco																									
			  	bc.nro_cuenta,																											
				-- 24. Medio de pago
				case
					when trim(cad.cnta_ctbl) like '10%' then
						dt.cod_sunat_tabla1  
					else
						null
				end as medio_pago,
				-- 25. Numero de Transaccion	  																									
				case
					when trim(cad.cnta_ctbl) like '10%' then
						cad.nro_docref1
					else
						null
				end as nro_transaccion																								                                                   

    from cntbl_asiento     ca,
         cntbl_asiento_Det cad,
      	cntbl_cnta      	cc,
      	origen        		o,
      	proveedor      	p,
			cntas_pagar			cp,
      	doc_tipo        	dt,
			banco_cnta        bc,
       	banco             b
    where ca.origen       		= cad.origen
      and ca.ano          		= cad.ano
      and ca.mes          		= cad.mes
      and ca.nro_libro     	= cad.nro_libro
      and ca.nro_asiento   	= cad.nro_asiento     
      and cad.cnta_ctbl     	= cc.cnta_ctbl
      and ca.origen      		= o.cod_origen
      and cad.cod_relacion 	= p.proveedor      	(+)
      and cad.tipo_docref1  	= dt.tipo_doc      	(+)
		and cad.cod_relacion		= cp.cod_relacion		(+)
		and cad.tipo_docref1		= cp.tipo_doc			(+)
		and cad.nro_docref1		= cp.nro_doc			(+)
		and cad.cod_ctabco   	= bc.cod_ctabco 		(+)
   	and bc.cod_banco     	= b.cod_banco   		(+)
      and ca.ano 					= :an_year
      and ca.mes 					= :an_mes
      and ca.flag_estado <> '0'
  group by  trim(to_char(ca.ano,'0000')) || trim(to_char(ca.mes,'00')) || '00',
            trim(ca.origen) || trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || 
          	trim(to_char(ca.nro_libro, '00')) || trim(to_char(ca.nro_asiento, '00000')) || 
          	trim(to_char(cad.item, '00000')),
          	case 
          		when ca.nro_libro = 0 then
          			'A'
          		when ca.nro_libro = 90 then
          			'C'
          		else 
          			'M'
          	end  || trim(to_char(ca.nro_asiento, '000000')),
            trim(cad.cnta_ctbl),
          	o.nombre,
          	trim(replace(REPLACE(cad.cencos, 'Ñ', 'N'), '_', '-')),
            decode(ca.cod_moneda, PKG_LOGISTICA.of_soles(null), 'PEN', 'USD'),
          	p.tipo_doc_ident,                                                      
          	decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
          	decode(dt.cod_sunat, null, '00', dt.cod_sunat),
			  	case
					when cad.nro_docref1 is null or trim(cad.nro_docref1) = '' then
						'0000'
					when decode(dt.cod_sunat, null, '00', dt.cod_sunat) = '05' then
						'4'
					when instr(replace(cad.nro_docref1, '--', '-'), '-') > 0 then
						case
							when decode(dt.cod_sunat, null, '00', dt.cod_sunat) in ('50', '52') then
								lpad(substr(replace(cad.nro_docref1, '--', '-'), 1, instr(replace(cad.nro_docref1, '--', '-'), '-') - 1),3,'0')
							when substr(replace(cad.nro_docref1, '--', '-'), 1, 1) in ('E', 'F', 'B') then
								substr(replace(cad.nro_docref1, '--', '-'), 1, 1) || lpad(substr(replace(cad.nro_docref1, '--', '-'), 2, instr(replace(cad.nro_docref1, '--', '-'), '-') - 2), 3, '0')
							else
								lpad(substr(replace(cad.nro_docref1, '--', '-'), 1, instr(replace(cad.nro_docref1, '--', '-'), '-') - 1),4,'0')
						end
					else
						'0000'
				end,
        		replace(
					replace(
						replace(
							case
								when decode(dt.cod_sunat, null, '00', dt.cod_sunat) = '00' then
									'00000000'
								when cad.nro_docref1 is null or trim(cad.nro_docref1) = '' then
									'00000000'
								when instr(replace(cad.nro_docref1, '--', '-'), '-') > 0 then
									replace(trim(substr(replace(cad.nro_docref1, '--', '-'), instr(cad.nro_docref1, '-') + 1)), '-', '')
								when cp.numero_cp is not null then
									PKG_UTILITY.of_trim(cp.numero_cp, '0')
								else
									replace(trim(replace(cad.nro_docref1, '--', '-')), '-', '')
							end
						,'**** ****', '00000000')
					,'*', '')
				,'L_', ''),
            trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')),
            trim(substr(
			  		replace(
							replace(
								replace(
									replace(
										replace(
											replace(
												decode(cad.det_glosa, null, decode(ca.desc_glosa, null, cc.desc_cnta, ca.desc_glosa), cad.det_glosa)
											,'|', ' ')
										, chr(13) || chr(10), ' ')
									, chr(9), ' ')
								, chr(13), ' ')
							, chr(10), '')
						, '/', ' ')	
					, 1, 200)
			  	),
			  	b.cod_banco_sunat,
			  	bc.nro_cuenta,																											
				case
					when trim(cad.cnta_ctbl) like '10%' then
						dt.cod_sunat_tabla1  
					else
						null
				end,
				case
					when trim(cad.cnta_ctbl) like '10%' then
						cad.nro_docref1
					else
						null
				end
  order by voucher
)
WHERE saldo_deudor <> 0 or saldo_acreedor <> 0

