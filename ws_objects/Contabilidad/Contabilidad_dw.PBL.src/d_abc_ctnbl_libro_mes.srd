 retrieve="  SELECT CLM.ORIGEN,   
         CLM.NRO_LIBRO,   
         CLM.ANO,   
         CLM.MES,   
         CLM.NRO_ASIENTO,   
         cl.DESC_LIBRO,   
         CLM.FLAG_REPLICACION,
      (select max(ca.nro_asiento)
         from   cntbl_asiento     ca
        where ca.origen       = clm.origen
          and ca.ano          = clm.ano
          and ca.mes          = clm.mes
          and ca.nro_libro    = clm.nro_libro) as ult_asiento

    FROM CNTBL_LIBRO_MES  CLM,   
         CNTBL_LIBRO      CL
   WHERE CLM.NRO_LIBRO  = CL.NRO_LIBRO 
    and CLM.ANO       	= :an_year
ORDER BY CLM.ORIGEN ASC,   
         CLM.ANO ASC,   
         CLM.MES ASC,   
         CLM.NRO_LIBRO ASC   
" update="CNTBL_LIBRO_MES" updatewhere=0 updatekeyinplace=yes arguments=(("an_year", number)) )
