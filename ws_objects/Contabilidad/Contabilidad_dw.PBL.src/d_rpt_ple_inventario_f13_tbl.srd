table(column=(type=char(4000) updatewhereclause=yes name=exp_row dbname="exp_row" )
 retrieve="select to_char(vm.fec_registro, 'yyyymm') || '00' 	|| '|' || 	-- 1.Periodo
       vm.nro_vale 											|| '|' ||   -- 2.CUO
       'M' || substr(vm.nro_Vale, 3) 					|| '|' ||   -- 3.Nro Correlativo
       al.cod_sunat 											|| '|' ||   -- 4.Codigo Establecimiento SUNAT
       '9' 														|| '|' ||   -- 5.Codigo del catalogo utilizado
       t5.codigo 												|| '|' ||   -- 6.Tipo de Existencia
       trim(am.cod_art) 									|| '|' ||   -- 7.Codigo de Existencia
		 '' 														|| '|' ||   -- 8.Codigo del catalogo utilizado
       a.cod_cubso 											|| '|' ||   -- 9.Codigo OSCE de la existencia
       to_char(vm.fec_registro, 'dd/mm/yyyy')		|| '|' ||   -- 10.Fecha del movimiento
       dt.cod_sunat 											|| '|' ||   -- 11.Tipo de Documento
       case
			when vm.nro_doc_ext is not null then
				PKG_FACT_ELECTRONICA.of_get_serie(vm.nro_doc_int) 
         else
           '9999'
       end 														|| '|' ||	-- 12.Nro de Serie
       case 
         when vm.nro_doc_ext is not null then
           PKG_UTILITY.of_trim(PKG_FACT_ELECTRONICA.of_get_nro(vm.nro_doc_ext), '0')
         else
           ''
       end 														|| '|' ||	-- 13.Nro de Documento
       amt.cod_sunat 										|| '|' ||   -- 14.Tipo de Operacion Efectuada
       substr(a.desc_art, 1, 80) 						|| '|' ||   -- 15.Descripcion de la existencia
       u.cod_sunat 											|| '|' ||   -- 16.Unidad
       '1' 														|| '|' ||   -- 14.Metodo de evaluacion (TABLA 14 SUNAT)
		 case 
			 when amt.factor_sldo_total = 1 then
				trim(to_char(am.cant_procesada, '999999999990.00'))
			 else
				''
		 end || '|'  as exp_row -- cantidad ingresada
       
 from vale_mov 			vm,
     	articulo_mov 		am,
     	almacen      		al,
     	articulo_mov_tipo amt,
     	articulo          a,
    	DOC_TIPO          dt,
     	ARTICULO_CLASE    AC,
     	unidad            u,
    	SUNAT_TABLA5      T5,  --Sunat Tabla 05. Tipo de Existencia
    	SUNAT_TABLA6      T6
where vm.almacen = al.almacen
  and vm.nro_vale = am.nro_vale
  and vm.tipo_mov = amt.tipo_mov
  and am.cod_art  = a.cod_art
  and a.und       = u.und
  AND A.COD_CLASE = AC.COD_CLASE
  AND AC.COD_SUNAT  = T5.CODIGO   (+)
  AND U.COD_SUNAT    = T6.CODIGO    (+)
  and vm.tipo_doc_ext = dt.tipo_doc (+)
  and vm.flag_estado <> '0'
  and am.flag_estado <> '0'
  and to_number(to_char(vm.fec_registro, 'yyyy')) = :an_year
  and to_number(to_char(vm.fec_registro, 'mm')) = :an_mes
  and am.cant_procesada > 0
       " arguments=(("an_year", number),("an_mes", number)) )
