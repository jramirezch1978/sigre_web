table(column=(type=char(109) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="select  to_char(pkg_utility.of_last_day(:an_year, :an_mes), 'yyyymmdd') || '|'      -- 01. Periodo
        || max(trim(ca.origen) || trim(to_char(ca.ano, '0000')) || 
           trim(to_char(ca.mes, '00')) || trim(to_char(ca.nro_libro, '00')) || 
           trim(to_char(ca.nro_asiento, '00000'))) || '|'                       		-- 02. Voucher
        || max(
				  case 
						when ca.nro_libro = 0 then
						'A'
						when ca.nro_libro = 90 then
						'C'
						else 
						'M'
				  end  || trim(to_char(ca.nro_asiento, '000000')))   || '|'             -- 03. Nro asiento
        || trim(cad.cnta_ctbl)                                   || '|'        		-- 04. Cuenta Contable
        || '0'                                 						  || '|'        		-- 05. Tipo de Documento de Identidad
        || '0000000000'                                  		  || '|'        		-- 06. Numero de Documento de Identidad
        || '00000000'                                      		  || '|'        		-- 07. Código del Trabajador
        || 'GENERICO'                                      		  || '|'        		-- 08. Nombre del Trabajador
        || trim(to_char(abs(sum(cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1))), '999999999990.00')) || '|' -- 09. saldo Final
        || '1'                                                   || '|'        -- 10. Indica el estado de la operación
		  || '||||||||||'																							-- 11 - 20 para otras utilidades
        as texto     

   from cntbl_asiento     ca,
        cntbl_asiento_Det cad,
        vw_pr_trabajador  m,
        doc_tipo          dt
  where ca.origen       	= cad.origen
    and ca.ano          	= cad.ano
    and ca.mes          	= cad.mes
    and ca.nro_libro    	= cad.nro_libro
    and ca.nro_asiento  	= cad.nro_asiento     
    and cad.cod_relacion 	= m.COD_TRABAJADOR      (+)
    and cad.tipo_docref1  	= dt.tipo_doc      (+)
    and ca.ano 				= :an_year
    and ca.mes 				<= :an_mes
    and ca.flag_estado 		<> '0'
    and substr(cad.cnta_ctbl, 1, 2) in ('41')
group by cad.cnta_ctbl     
having round(abs(sum(cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1))), 2) > 0
  
  order by 1


" arguments=(("an_year", number),("an_mes", number)) )
