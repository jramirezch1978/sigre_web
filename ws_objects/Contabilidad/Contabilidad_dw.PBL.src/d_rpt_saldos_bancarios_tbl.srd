 column=(type=number updatewhereclause=yes name=saldo_sol_hab dbname="saldo_sol_hab" )
 column=(type=number updatewhereclause=yes name=saldo_dol_deb dbname="saldo_dol_deb" )
 column=(type=number updatewhereclause=yes name=saldo_dol_hab dbname="saldo_dol_hab" )
 retrieve="    select bc.cod_ctabco, bc.cnta_ctbl, 
           bc.descripcion as desc_cnta,
           bc.cod_moneda,
           (select nvl(cal.cmp_dol_sbs,0)
              from calendario cal
             where cal.fecha = pkg_utility.of_last_day(:an_year, :an_mes)) as tipo_cambio,
           sum(decode(cad.flag_debhab,'D',cad.imp_movsol,cad.imp_movsol*-1)) as saldo_sol,
           sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) as saldo_dol,
           sum(decode(cad.flag_debhab,'D',cad.imp_movsol,0)) as saldo_sol_deb,
           sum(decode(cad.flag_debhab,'H',cad.imp_movsol,0)) as saldo_sol_hab,
           sum(decode(cad.flag_debhab,'D',cad.imp_movdol,0)) as saldo_dol_deb,
           sum(decode(cad.flag_debhab,'H',cad.imp_movdol,0)) as saldo_dol_hab

      from banco_cnta bc, 
           cntbl_asiento ca, 
           cntbl_asiento_det cad
     where ca.origen      = cad.origen 
       and ca.ano         = cad.ano 
       and ca.mes         = cad.mes 
       and ca.nro_libro   = cad.nro_libro 
       and ca.nro_asiento =cad.nro_asiento 
       and ca.flag_estado <>'0' 
       and ca.ano = :an_year
       and ca.mes <= :an_mes
       and cad.cod_ctabco = bc.cod_ctabco 
    group by bc.cod_ctabco, 
             bc.cnta_ctbl, 
             bc.descripcion,
             bc.cod_moneda
    order by bc.cnta_ctbl
" arguments=(("an_year", number),("an_mes", number)) )
