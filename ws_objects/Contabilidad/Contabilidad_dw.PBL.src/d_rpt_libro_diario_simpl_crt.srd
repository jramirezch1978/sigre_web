table(column=(type=datetime updatewhereclause=yes name=fecha_cntbl dbname="fecha_cntbl" )
 column=(type=char(20) updatewhereclause=yes name=desc_glosa dbname="desc_glosa" )
 column=(type=number updatewhereclause=yes name=importe dbname="importe" )
 retrieve="SELECT trunc(ca.fecha_cntbl) as fecha_cntbl,
       ca.desc_glosa,
       substr(cad.cnta_ctbl,1,:ani_nivel) as cnta_ctbl,
       cc.desc_cnta,
       case
         when substr(cad.cnta_ctbl,1,2) between '10' and '39' then
              '1.ACTIVOS'
         when substr(cad.cnta_ctbl,1,2) between '40' and '49' then
              '2.PASIVOS'
         when substr(cad.cnta_ctbl,1,2) between '50' and '59' then
              '3.PATRIMONIO'
         when substr(cad.cnta_ctbl,1,2) between '70' and '79' then
              '4.INGRESOS'
         when substr(cad.cnta_ctbl,1,2) between '60' and '69' or substr(cad.cnta_ctbl,1,2) between '90' and '99' then
              '5.GASTOS'
       end as titulo,       
       sum(cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1)) as importe 
from cntbl_asiento ca,
     cntbl_asiento_det cad,
     (select trim(s.cnta_ctbl) as cnta_ctbl, s.desc_cnta from cntbl_cnta s)  cc
where ca.origen        = cad.origen
  and ca.ano           = cad.ano
  and ca.mes           = cad.mes
  and ca.nro_libro     = cad.nro_libro
  and ca.nro_asiento   = cad.nro_asiento
  and substr(cad.cnta_ctbl,1,2)= cc.cnta_ctbl (+)
  and ca.flag_estado   <> '0'
  and ca.ano           = :ani_year
  and ca.mes           = :ani_mes
group by trunc(ca.fecha_cntbl),
         ca.desc_glosa,
         substr(cad.cnta_ctbl,1,:ani_nivel),
			substr(cad.cnta_ctbl,1,2),
         cc.desc_cnta  
order by fecha_cntbl, titulo, cnta_ctbl
         " arguments=(("ani_year", number),("ani_mes", number),("ani_nivel", number))  sort="fecha_cntbl A desc_glosa A " )
