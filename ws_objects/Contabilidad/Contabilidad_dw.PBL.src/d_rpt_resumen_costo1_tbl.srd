table(column=(type=decimal(0) updatewhereclause=yes name=mes dbname="cntbl_asiento.mes" values="Enero	1/Febrero	2/Marzo	3/Abril	4/Junio	6/Julio	7/Agosto	8/Setiembre	9/Octubre	10/Noviembre	11/Diciembre	12/" )
 column=(type=char(10) updatewhereclause=yes name=cnta_ctbl dbname="cntbl_cnta.cnta_ctbl" )
 column=(type=char(150) updatewhereclause=yes name=desc_cnta dbname="cntbl_cnta.desc_cnta" )
 column=(type=number updatewhereclause=yes name=saldo_deb dbname="saldo_deb" )
 column=(type=number updatewhereclause=yes name=saldo_hab dbname="saldo_hab" )
 retrieve="select ca.mes,
       cc.cnta_ctbl,
       cc.desc_cnta,
       case
         when sum(decode(cad.flag_debhab, 'D', cad.imp_movsol, 0)) > sum(decode(cad.flag_debhab, 'H', cad.imp_movsol, 0)) then
           sum(decode(cad.flag_debhab, 'D', cad.imp_movsol, 0)) - sum(decode(cad.flag_debhab, 'H', cad.imp_movsol, 0))
         else
           0
       end as saldo_deb,
       case
         when sum(decode(cad.flag_debhab, 'H', cad.imp_movsol, 0)) > sum(decode(cad.flag_debhab, 'D', cad.imp_movsol, 0)) then
           sum(decode(cad.flag_debhab, 'H', cad.imp_movsol, 0)) - sum(decode(cad.flag_debhab, 'D', cad.imp_movsol, 0))
         else
           0
       end as saldo_hab
from cntbl_asiento_det cad,
     cntbl_asiento     ca,
     cntbl_cnta        cc
where ca.origen = cad.origen
  and ca.ano    = cad.ano
  and ca.mes    = cad.mes
  and ca.nro_libro = cad.nro_libro
  and ca.nro_asiento = cad.nro_asiento
  and substr(cad.cnta_ctbl, 1, 2) = trim(cc.cnta_ctbl)
  and ca.flag_estado <> '0'
  and ca.ano         = :an_year
  and substr(cad.cnta_ctbl,1,2) in (select trim(p.valor_string)
                                         from CNTBL_COSTO_PARAM p
                                        where p.opcion = :as_grp_res01) 
group by ca.mes,
       cc.cnta_ctbl,
       cc.desc_cnta
order by mes, cnta_ctbl
" arguments=(("an_year", number),("as_grp_res01", string))  sort="mes A cnta_ctbl A " )
