 retrieve="  SELECT CD.CNTA_CTBL,   
         C.ABREV_CNTA,   
         CD.COD_RELACION,
         P.NOM_PROVEEDOR,   
         SUM( DECODE(CD.FLAG_DEBHAB,'D',
              CD.IMP_MOVSOL,0 )) as soldeb,
         SUM( DECODE(CD.FLAG_DEBHAB,'H',
              CD.IMP_MOVSOL,0 )) as solhab, 
         SUM( DECODE(CD.FLAG_DEBHAB,'D',
              CD.IMP_MOVDOL,0 )) as doldeb,
         SUM( DECODE(CD.FLAG_DEBHAB,'H',
              CD.IMP_MOVDOL,0 )) as dolhab
    FROM CNTBL_ASIENTO_DET cd,   
         CNTBL_CNTA c,   
         PROVEEDOR p
   WHERE ( C.CNTA_CTBL = CD.CNTA_CTBL ) and  
         ( P.PROVEEDOR = CD.COD_RELACION ) and  
         ( ( CD.ANO = :an_ano ) AND  
         ( CD.MES >= :an_mes_ini ) AND  
         ( CD.MES <= :an_mes_fin ) AND  
         ( C.FLAG_CODREL = '1' ) )   
GROUP BY CD.CNTA_CTBL,   
         C.ABREV_CNTA,   
         CD.COD_RELACION,   
         P.NOM_PROVEEDOR
ORDER BY CD.CNTA_CTBL ASC,   
         CD.COD_RELACION ASC   
" arguments=(("an_ano", number),("an_mes_ini", number),("an_mes_fin", number)) )
