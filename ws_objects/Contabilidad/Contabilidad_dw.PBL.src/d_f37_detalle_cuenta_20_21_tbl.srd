table(column=(type=char(12) updatewhereclause=yes name=cod_art dbname="articulo_mov.cod_art" dbalias="am.cod_art" )
 column=(type=char(2) updatewhereclause=yes name=cod_sunat dbname="articulo_clase.cod_sunat" dbalias="ac.cod_sunat" )
 column=(type=char(3000) updatewhereclause=yes name=desc_art dbname="articulo.desc_art" dbalias="a.desc_art" )
 column=(type=char(4) updatewhereclause=yes name=unidad_und dbname="unidad.und" dbalias="u.und" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 column=(type=number updatewhereclause=yes name=costo_unitario dbname="costo_unitario" )
 column=(type=number updatewhereclause=yes name=costo_total dbname="costo_total" )
 retrieve="select am.cod_art,
       ac.cod_sunat,
       a.desc_art,
       u.cod_sunat as und,
       sum(am.cant_procesada * amt.factor_sldo_total) as cantidad,
       case 
         when sum(am.cant_procesada * amt.factor_sldo_total) > 0 then
           sum(am.cant_procesada * am.precio_unit * amt.factor_sldo_total) / sum(am.cant_procesada * amt.factor_sldo_total)
         else
           0
       end as costo_unitario,
       sum(am.cant_procesada * am.precio_unit * amt.factor_sldo_total) as costo_total
from vale_mov vm,
     articulo_mov am,
     articulo     a,
     articulo_clase ac,
     articulo_sub_categ a2,
     unidad             u,
     articulo_mov_tipo  amt
where vm.nro_vale = am.nro_vale
  and am.cod_art  = a.cod_art
  and a.cod_clase = ac.cod_clase
  and a2.cod_sub_cat = a.sub_cat_art
  and vm.tipo_mov    = amt.tipo_mov
  and a.und          = u.und
  and vm.flag_estado <> '0'
  and am.flag_estado <> '0'     
  and to_char(vm.fec_registro, 'yyyymm') = trim(to_char(:an_year , '0000')) || trim(to_char(:an_mes, '00'))
group by am.cod_art,
       ac.cod_sunat,
       a.desc_art,
       u.cod_sunat
having round(sum(am.cant_procesada * amt.factor_sldo_total),8) > 0 or
       round(sum(am.cant_procesada * am.precio_unit * amt.factor_sldo_total),8) > 0" arguments=(("an_year", number),("an_mes", number))  sort="cod_sunat A desc_art A " )
