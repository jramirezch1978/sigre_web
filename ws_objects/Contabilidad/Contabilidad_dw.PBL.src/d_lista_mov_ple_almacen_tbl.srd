 retrieve="select al.almacen,
       to_char(vm.fec_registro,'yyyymm') || '00' as periodo,
       case
			when s.voucher is null then
				vm.cod_origen || to_char(vm.fec_registro, 'yyyymmddHHmiss') || trim(to_char(am.nro_mov, '00000000'))
			else
				s.voucher || 'S' || trim(to_char(am.nro_mov, '00000000') )
	    end as nro_voucher,
       case
			when s.nro_asiento is null then
				'M999999'
			else
		 		'M' || trim(to_char(s.nro_asiento)) 
		 end as nro_asiento,
       al.cod_sunat as cod_establecimiento,
       '9' as catalogo,
       AC.COD_SUNAT AS TIPO_EXISTENCIA,
       a.cod_art,
       trim(substr(REPLACE(REPLACE(REPLACE(a.desc_art, '|', ' '),chr(10),'-'), chr(13), '-'),1,80)) as desc_art,
       a.cod_cubso,
       u.cod_sunat as und, 
       vm.fec_registro as fecha_emision,
       vm.nro_vale,
		 --decode(dt.cod_sunat, null, '00', dt.cod_sunat) 
       CASE 
			when dt2.cod_sunat is null and dt1.cod_sunat is null then
				'09'
         WHEN dt2.cod_sunat is not null then
				-- Documento externo
				dt2.cod_sunat
         else
           dt1.cod_sunat
       end as tipo_doc, 
       CASE 
      	when dt2.cod_sunat is null and dt1.cod_sunat is null then
        		'0000'
         WHEN dt2.cod_sunat is not null and vm.nro_doc_ext is not null and instr(replace(vm.nro_doc_ext, '.', '-'), '-') > 0 then
           pkg_fact_electronica.of_get_serie(vm.nro_doc_ext)
         WHEN dt1.cod_sunat is not null and vm.nro_doc_int is not null and instr(replace(vm.nro_doc_int, '.', '-'), '-') > 0 then
           pkg_fact_electronica.of_get_serie(vm.nro_doc_int)
         else
           ''
       end as serie,
       CASE 
      	when dt2.cod_sunat is null and dt1.cod_sunat is null then
        		substr(vm.nro_vale,1,2) || substr(vm.nro_vale,5,6)
      	WHEN dt2.cod_sunat is not null and vm.nro_doc_ext is not null then
        		pkg_fact_electronica.of_get_nro(vm.nro_doc_ext)
      	WHEN dt1.cod_sunat is not null and vm.nro_doc_int is not null then
        		pkg_fact_electronica.of_get_nro(vm.nro_doc_int)
       end as numero,       
		 amt.cod_sunat as tipo_operacion,
       '1' as metodo_evaluacion,
       '1' as estado_operacion,
       decode(amt.factor_sldo_total, 1, am.cant_procesada, null) cantidad_entrada,
       decode(amt.factor_sldo_total, 1, decode(am.cant_procesada, 0, 1, am.cant_procesada) * am.precio_unit, null) costo_entrada,
       decode(amt.factor_sldo_total, -1, am.cant_procesada, null) cantidad_salida,
       decode(amt.factor_sldo_total, -1, decode(am.cant_procesada, 0, 1, am.cant_procesada) * am.precio_unit * -1, null) costo_salida,
		 '0' as flag_saldo_inicial
       
from vale_mov vm,
     articulo_mov am,
     almacen      al,
     articulo_mov_tipo amt,
     articulo          a,
     DOC_TIPO          dt1,
	  doc_tipo			  dt2,
     ARTICULO_CLASE    AC,
     unidad            u,
     SUNAT_TABLA5      T5,
     SUNAT_TABLA6      T6,
     (select distinct
             cad.origen || trim(to_char(cad.ano, '0000')) || trim(to_char(cad.mes, '00')) || trim(to_char(cad.nro_libro, '00')) ||
             trim(to_char(cad.nro_asiento, '000000'))  as voucher,
             trim(to_char(cad.nro_asiento, '000000')) as nro_asiento,
             cad.org_am, cad.nro_am
        from cntbl_asiento_det cad
       where cad.org_am is not null
         and cad.nro_am is not null
      order by 1   ) s
where vm.almacen 			= al.almacen
  and vm.nro_vale 		= am.nro_vale
  and vm.tipo_mov 		= amt.tipo_mov
  and am.cod_art  		= a.cod_art
  and a.und       		= u.und
  AND A.COD_CLASE 		= AC.COD_CLASE
  AND AC.COD_SUNAT  		= T5.CODIGO   (+)
  AND U.COD_SUNAT    	= T6.CODIGO    (+)
  and vm.tipo_doc_ext 	= dt2.tipo_doc (+)
  and vm.tipo_doc_int 	= dt1.tipo_doc (+)
  and am.cod_origen   	= s.org_am    (+)
  and am.nro_mov      	= s.nro_am    (+)
  and vm.flag_estado 	<> '0'
  and am.flag_estado 	<> '0'
  and to_number(to_char(vm.fec_registro, 'yyyy')) = :an_year
  and to_number(to_char(vm.fec_registro, 'mm')) = :an_mes
union
-- Movimiento de Inventario inicial
select al.almacen,
       trim(to_char(:an_year, '0000')) || trim(to_char(:an_mes, '00')) || '00' as periodo,
       al.cod_origen || trim(to_char(:an_year, '0000')) || trim(to_char(:an_mes, '00')) || '0116' 
		      || PKG_UTILITY.of_trim(trim(replace(am.cod_art, '.', '')), ' ') || '01' as voucher,
       'A000001' as nro_asiento,
       al.cod_sunat as cod_establecimiento,
       '9' as catalogo,
       AC.COD_SUNAT AS TIPO_EXISTENCIA,
       a.cod_art,
       trim(substr(REPLACE(REPLACE(REPLACE(a.desc_art, '|', ' '),chr(10),'-'), chr(13), '-'),1,80)) as desc_art,
       a.cod_cubso,
       u.cod_sunat as und, 
       to_date('01/' || trim(to_char(:an_mes, '00')) || '/' || trim(to_char(:an_year, '0000')), 'dd/mm/yyyy') as fecha_emision,
       null as nro_vale,
       '00' as tipo_doc, 
       null as serie,
       '00000000' as numero,
       '16' as tipo_operacion,
       '1' as metodo_evaluacion,
       '1' as estado_operacion,
       sum(am.cant_procesada * amt.factor_sldo_total) cantidad_entrada,
       sum(decode(am.cant_procesada, 0, 1, am.cant_procesada) * amt.factor_sldo_total * am.precio_unit) costo_entrada,
       null cantidad_salida,
       null costo_salida,
		 '1' as flag_saldo_inicial
       
from vale_mov vm,
     articulo_mov am,
     almacen      al,
     articulo_mov_tipo amt,
     articulo          a,
     ARTICULO_CLASE    AC,
     unidad            u
where vm.almacen = al.almacen
  and vm.nro_vale = am.nro_vale
  and vm.tipo_mov = amt.tipo_mov
  and am.cod_art  = a.cod_art
  and a.und       = u.und
  AND A.COD_CLASE = AC.COD_CLASE
  and vm.flag_estado <> '0'
  and am.flag_estado <> '0'
  and trunc(vm.fec_registro) <= (to_date('01/' || trim(to_char(:an_mes, '00')) || '/' || trim(to_char(:an_year, '0000')), 'dd/mm/yyyy') - 1)
group by al.almacen,
         al.cod_origen || trim(to_char(:an_year, '0000')) || trim(to_char(:an_mes, '00')) || '0116' 
		      || PKG_UTILITY.of_trim(trim(replace(am.cod_art, '.', '')), ' ') || '01',
			al.cod_origen,
       	a.cod_art,
       	AC.COD_SUNAT,
       	trim(substr(REPLACE(REPLACE(REPLACE(a.desc_art, '|', ' '),chr(10),'-'), chr(13), '-'),1,80)),
       	u.cod_sunat, 
     		al.cod_sunat,
     		a.cod_cubso
having sum(am.cant_procesada * amt.factor_sldo_total) > 0 
order by almacen, cod_art, fecha_emision" arguments=(("an_year", number),("an_mes", number)) )
