table(column=(type=char(20) updatewhereclause=yes name=cnta_ctbl dbname="cntbl_asiento_det_cnta_ctbl" )
 column=(type=char(20) updatewhereclause=yes name=desc_cnta dbname="cntbl_cnta_desc_cnta" )
 column=(type=number updatewhereclause=yes name=imp_movsol dbname="imp_movsol" )
 retrieve="select cad.cnta_ctbl, c.desc_cnta,
       trim(cad.cencos) || ' - ' || cc.desc_cencos as cencos,
       sum(DECODE(cad.flag_debhab, 'D', 1, -1) * cad.imp_movsol) as imp_movsol
   from cntbl_asiento ca,
        cntbl_asiento_det cad,
        centros_costo     cc,
        cntbl_cnta        c
where ca.origen     = cad.origen
  and ca.ano        = cad.ano
  and ca.mes        = cad.mes
  and ca.nro_libro  = cad.nro_libro
  and ca.nro_asiento = cad.nro_asiento
  and cad.cencos     = cc.cencos (+)
  and cad.cnta_ctbl  = c.cnta_ctbl
  and ca.flag_estado <> '0'  
  and ca.ano = :ln_year
  and ca.mes between :ln_mes1 and :ln_mes2
  and substr(cad.cnta_ctbl,1,length(:as_cnta1)) >= :as_cnta1 
  and substr(cad.cnta_ctbl,1,length(:as_cnta2)) <= :as_cnta2 
group by cad.cnta_ctbl, c.desc_cnta, trim(cad.cencos) || ' - ' || cc.desc_cencos  " arguments=(("ln_year", number),("ln_mes1", number),("ln_mes2", number),("as_cnta1", string),("as_cnta2", string))  sort="cnta_ctbl A " )
