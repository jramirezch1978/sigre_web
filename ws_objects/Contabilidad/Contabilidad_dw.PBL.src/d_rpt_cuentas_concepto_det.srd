 retrieve="Select Trim(ccad.cnta_ctbl)||' - '||(c.abrev_cnta) As cnta,
       lpad(to_char((Select orden From cnt_concepto_anexo 
       Where cnta_ctbl = ccad.cnta_ctbl And concepto_cnt = ccad.concepto_cnt And ano = :an_ano)),2,'00')||' - '||cc.descripcion As concepto,
       lpad(ccad.mes,2,'00')||' - '||decode(ccad.mes,0,'Apertura',   1,'Enero',     2,'Febrero',  3,'Marzo',
                     4,'Abril',  5,'Mayo',      6,'Junio',    7,'Julio',
                     8,'Agosto', 9,'Setiembre', 10,'Octubre', 11,'Noviembre',
                     12,'Diciembre',13,'Mes Trece') As mes, 
       round(sum(cad.imp_movsol),2) As importe
  From cnt_concepto_anexo_det ccad, cntbl_asiento_det cad, cnt_concepto cc,
       cntbl_cnta c
 Where ccad.Origen       = cad.origen
   And ccad.ano          = cad.ano
   And ccad.mes          = cad.mes
   And ccad.nro_libro    = cad.nro_libro
   And ccad.nro_asiento  = cad.nro_asiento
   And ccad.item         = cad.item
   And ccad.cnta_ctbl    = cc.cnta_ctbl
   And ccad.concepto_cnt = cc.concepto_cnt
   And ccad.cnta_ctbl    = c.cnta_ctbl
   And ccad.ano          = :an_ano
   And ccad.cnta_ctbl    Like :as_cnta
 Group By ccad.cnta_ctbl, c.abrev_cnta, ccad.concepto_cnt, cc.descripcion, ccad.mes" arguments=(("an_ano", number),("as_cnta", string),("as_empresa", string),("as_user", string))  sort="cnta A concepto A " )
