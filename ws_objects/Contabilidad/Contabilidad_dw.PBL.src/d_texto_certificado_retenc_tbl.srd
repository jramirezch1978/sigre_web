table(column=(type=char(4000) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="  SELECT p.ruc || '|' ||																						-- Campo 1. RUC del proveedor
         decode(p.flag_personeria, 'J', substr(p.nom_proveedor, 1, 40), '') || '|' ||  	-- Campo 2. Razon Social 
         decode(p.flag_personeria, 'N', substr(p.apellido_pat, 1, 40), '') || '|' ||		-- Campo 3. Apellido Paterno
         decode(p.flag_personeria, 'N', substr(p.apellido_mat, 1, 40), '') || '|' ||		-- Campo 4. Apellido Materno
         decode(p.flag_personeria, 'N', substr(trim(NVL(p.nombre1,'') || ' ' || NVL(p.nombre2,'')), 1, 40), '') || '|' ||	-- Campo 5. Nombres	
			case
				when instr(r.nro_certificado, '-') > 0 then
					case 
						when substr(r.nro_certificado, 1, 1) = 'E' then
							substr(r.nro_certificado, 1, 1) || lpad(substr(r.nro_certificado, 2, instr(r.nro_certificado, '-') - 1), 3, '0')
						else
							lpad(substr(r.nro_certificado, 1, instr(r.nro_certificado, '-') - 1), 4, '0')
					end
				else
					'0000'
			end || '|' ||																							-- Campo 6. Serie del CRI
			case
				when instr(r.nro_certificado, '-') > 0 then
					lpad(substr(r.nro_certificado, instr(r.nro_certificado, '-') + 1), 6, '0')
				else
					lpad(r.nro_certificado, 6, '0')
			end || '|' ||																							-- Campo 7. Numero del CRI
         to_char(r.fecha_emision, 'dd/mm/yyyy') || '|' ||											-- Campo 8. Fecha de emisión
			(select trim(to_char(nvl(sum(CASE 
                                   WHEN CP2.COD_MONEDA = (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY  = '1') THEN
				                             CBD2.IMPORTE
                                   ELSE
				                             CBD2.IMPORTE * CB.TASA_CAMBIO
                                 end), 0), '999999999990.00'))										
            from caja_bancos_det cbd2,	
                 cntas_pagar     cp2
           where cbd2.cod_relacion = cp2.cod_relacion
             and cbd2.tipo_doc     = cp2.tipo_doc
             and cbd2.nro_doc      = cp2.nro_doc
             and cbd2.nro_registro = cb.nro_registro
             and cbd2.flag_ret_igv = '1') || '|' ||													-- Campo 9. Monto de Pago del CRI
         dt.cod_sunat || '|' ||																				-- Campo 10. Tipo de Comprobante de Pago
         cp.serie_cp || '|' ||																				-- Campo 11. Serie del comprobante de Pago
         substr(cp.numero_cp, 5) || '|' ||																-- Campo 12. Numero de comprobante de Pago
         to_char(cp.fecha_emision, 'dd/mm/yyyy') || '|' ||											-- Campo 13. fecha de emision del comprobante de Pago
         trim(to_Char(CASE
           WHEN cbd.cod_moneda = (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY  = '1') THEN
             CP.importe_doc
           ELSE
             cp.IMPORTE_doc * CB.TASA_CAMBIO
         END, '999999999990.00')) || '|' as texto														-- Campo 14. Importe del CRI
      
    FROM CAJA_BANCOS_DET    CBD,   
         RETENCION_IGV_CRT  R,   
         PROVEEDOR          P,   
         CAJA_BANCOS        CB,
         doc_tipo           dt,
         cntas_pagar        cp
   WHERE R.ORIGEN             = CBD.ORIGEN 
     and R.NRO_REG_CAJA_BAN   = CBD.NRO_REGISTRO 
    and R.PROVEEDOR           = P.PROVEEDOR 
    and cbd.tipo_doc          = dt.tipo_doc
    and CBD.FLAG_RET_IGV       = '1'  
    and CB.ORIGEN             = CBD.ORIGEN 
    and CB.NRO_REGISTRO       = CBD.NRO_REGISTRO 
    and CBD.COD_RELACION       = R.PROVEEDOR 
    and cbd.cod_relacion       = cp.cod_relacion
    and cbd.tipo_doc           = cp.tipo_doc
    and cbd.nro_doc            = cp.nro_doc
    and to_number(to_char(r.fecha_emision, 'yyyy')) = :an_year
    and to_number(to_char(r.fecha_emision, 'mm')) = :an_mes

union

  SELECT p.ruc || '|' ||
         decode(p.flag_personeria, 'J', substr(p.nom_proveedor, 1, 40), '') || '|' ||  
         decode(p.flag_personeria, 'N', substr(p.apellido_pat, 1, 40), '') || '|' ||
         decode(p.flag_personeria, 'N', substr(p.apellido_mat, 1, 40), '') || '|' ||
         decode(p.flag_personeria, 'N', substr(trim(NVL(p.nombre1,'') || ' ' || NVL(p.nombre2,'')), 1, 40), '') || '|' ||
			case
				when instr(r.nro_certificado, '-') > 0 then
					lpad(substr(r.nro_certificado, 1, instr(r.nro_certificado, '-') - 1), 4, '0')
				else
					'0000'
			end || '|' ||																							-- Campo 6. Serie del CRI
			case
				when instr(r.nro_certificado, '-') > 0 then
					lpad(substr(r.nro_certificado, instr(r.nro_certificado, '-') + 1), 6, '0')
				else
					lpad(r.nro_certificado, 6, '0')
			end || '|' ||																							-- Campo 7. Numero del CRI
         to_char(r.fecha_emision, 'dd/mm/yyyy') || '|' ||
			(select trim(to_char(nvl(sum(CASE
												    WHEN CP2.COD_MONEDA = (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY  = '1') THEN
														 DR2.IMPORTE
													 ELSE
														 DR2.IMPORTE * cpb.TASA_CAMBIO
												  END), 0), '999999999990.00'))
            from doc_referencias dr2,
					  cntas_pagar		cp2
           where dr2.proveedor_ref	= cp2.cod_relacion
				 and dr2.tipo_ref			= cp2.tipo_doc
				 and dr2.nro_ref			= cp2.nro_doc
				 and dr2.flag_ret_igv  	= '1'
				 and dr2.cod_relacion  	= cpb.cod_relacion
			    and dr2.tipo_doc		  	= cpb.tipo_doc
				 and dr2.nro_doc		  	= cpb.nro_doc 		) || '|' ||
         dt.cod_sunat || '|' ||
         pkg_fact_electronica.of_get_serie(dr.nro_ref) || '|' ||
			pkg_fact_electronica.of_get_nro(dr.nro_ref) || '|' ||
         to_char(cpa.fecha_emision, 'dd/mm/yyyy') || '|' ||
         trim(to_Char(CASE
           WHEN cpa.cod_moneda = (SELECT COD_SOLES FROM LOGPARAM WHERE RECKEY  = '1') THEN
             cpa.importe_doc
           ELSE
             cpa.importe_doc * cpa.TASA_CAMBIO
         END, '999999999990.00')) || '|'

    FROM DOC_REFERENCIAS  	DR,   
         RETENCION_IGV_CRT	R,   
         DOC_TIPO         	DT,   
         PROVEEDOR        	P,   
         CNTAS_PAGAR 		CPA ,   
         CNTAS_PAGAR 		CPB  
   WHERE R.NRO_CERTIFICADO   = CPB.NRO_CERTIFICADO     
	  and dt.TIPO_DOC         = DR.TIPO_REF     
	  and DR.PROVEEDOR_REF    = CPA.COD_RELACION        
	  and DR.TIPO_REF         = CPA.TIPO_DOC            
	  and DR.NRO_REF          = CPA.NRO_DOC             
 	  and DR.FLAG_RET_IGV     = '1'	                             
	  and CPB.COD_RELACION    = DR.COD_RELACION 
	  and CPB.TIPO_DOC        = DR.TIPO_DOC     
	  and CPB.NRO_DOC         = DR.NRO_DOC      
	  and CPB.TIPO_DOC        = (SELECT doc_letra_pagar FROM finparam WHERE reckey = '1') 
	  and CPB.COD_RELACION    = P.PROVEEDOR  
	  and to_number(to_char(r.fecha_emision, 'yyyy')) = :an_year
     and to_number(to_char(r.fecha_emision, 'mm')) = :an_mes




" arguments=(("an_year", number),("an_mes", number)) )
