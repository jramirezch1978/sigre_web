 retrieve="Select trim(ccad.cnta_ctbl)||' - '||trim(c.desc_cnta) As cnta,
       lpad(to_char((Select orden From cnt_concepto_anexo 
       Where cnta_ctbl = ccad.cnta_ctbl And concepto_cnt = ccad.concepto_cnt and ano = :an_ano)),2,'00')||' - '||ccad.concepto_cnt As concepto, 
       round(sum(cad.imp_movsol),2) As importe
  From cnt_concepto_anexo_det ccad, cntbl_asiento_det cad, cntbl_cnta c
 Where ccad.Origen      = cad.origen
   And ccad.ano         = cad.ano
   And ccad.mes         = cad.mes
   And ccad.nro_libro   = cad.nro_libro
   And ccad.nro_asiento = cad.nro_asiento
   And ccad.item        = cad.item
   And ccad.cnta_ctbl   = c.cnta_ctbl
   And ccad.ano         = :an_ano
   And ccad.cnta_ctbl   Like :as_cnta
 Group By ccad.cnta_ctbl, c.desc_cnta, ccad.concepto_cnt, ccad.nro_item
 Order By 1,2" arguments=(("an_ano", number),("as_cnta", string),("AS_EMPRESA", string),("AS_USER", string))  sort="cnta A " )
