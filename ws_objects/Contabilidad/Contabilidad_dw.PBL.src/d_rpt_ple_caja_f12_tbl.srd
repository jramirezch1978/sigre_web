table(column=(type=char(581) updatewhereclause=yes name=exp_row dbname="exp_row" )
 retrieve="select trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || '00' || '|'                     -- 01. Periodo
       || trim(ca.origen) || trim(to_char(ca.ano, '0000')) || trim(to_char(ca.mes, '00')) || trim(to_char(ca.nro_libro, '00')) 
       || trim(to_char(ca.nro_Asiento, '00000')) || '_' || trim(to_char(cad.item, '00000')) || '|'     -- 02. nro_voucher
       || 'M' || trim(to_char(cad.nro_fila, '00000000')) || '|'                                        -- 03. Nro Asiento
       || s.cod_banco_sunat || '|'                                                                     -- 04. Codigo Banco 
       || trim(s.cod_ctabco)  || '|'                                                                   -- 05. Cuenta Bancaria
       || trim(to_char(ca.fecha_cntbl, 'dd/mm/yyyy')) || '|'                                           -- 06. fecha Contable
       || '003' || '|'                                                                                 -- 07. Medio de PAgo - Transferencia
       || case
			    when PKG_UTILITY.of_trim_SUNAT(cad.det_glosa) is null then 'PAGO'
				 else substr(PKG_UTILITY.of_trim_SUNAT(cad.det_glosa),1,200)
			 end  || '|'                               -- 08. Descripcion de la operacion bancaria
       || case
			    when vw.tipo_doc_ident is null then '0'
				 else vw.tipo_doc_ident
			 end || '|'                                                												 -- 09. tipo de documento 
       || case
			    when vw.ruc_dni is null then '00000000'
				 else trim(vw.ruc_dni) 
			 end || '|'                                                                      				 -- 10. numero de documento
       || case
			    when vw.nom_proveedor is null then '-'
				 else trim(vw.nom_proveedor) 
			 end || '|'                                                                						 -- 11. nombre completo o razon social
       || case
			    when s.nro_doc is null then '00000000'
				 else trim(s.nro_doc)
			 end || '|'                                                                             		 -- 12. Numero de transaccion
       || trim(
            to_char(
               decode(cad.flag_debhab, 'D', cad.imp_movsol, 0)
            , '9999999990.00')) || '|'                                                                 -- 13. Movimiento del Debe
       || trim(
            to_char(
               decode(cad.flag_debhab, 'H', cad.imp_movsol, 0)
            , '9999999990.00')) || '|'                                                                -- 14. Movimiento del Haber
       || '1' || '|'                                                                        			   -- 15. Estado de la operacion
		 || '|||||||||||||||' as exp_row																						-- 16. del 16 al 30 para uso futuro
from  cntbl_Asiento     ca,
      (select cad1.*, rownum as nro_fila
         from (select * 
                 from cntbl_Asiento_det 
                order by origen, ano, mes, nro_libro, nro_asiento) cad1 )cad,
      cntbl_cnta        cc,
      (select distinct cad.origen, cad.ano, cad.mes, cad.nro_libro, cad.nro_Asiento, cad.cnta_ctbl, cc.desc_cnta,
              b.cod_banco_sunat,
              cad.cod_ctabco,
              cb.nro_doc
         from cntbl_asiento ca,
              cntbl_Asiento_det cad,
              cntbl_cnta        cc,
              banco_cnta        bc,
              banco             b,
              caja_bancos       cb
      where ca.origen        = cad.origen
        and ca.ano           = cad.ano
        and ca.mes           = cad.mes
        and ca.nro_libro     = cad.nro_libro
        and ca.nro_Asiento   = cad.nro_Asiento
        and cad.cod_ctabco   = bc.cod_ctabco
        and bc.cod_banco     = b.cod_banco
        and cb.origen        = ca.origen
        and cb.ano           = ca.ano
        and cb.mes           = ca.mes
        and cb.nro_libro     = ca.nro_libro
        and cb.nro_asiento   = ca.nro_asiento
        and cad.cnta_ctbl    = cc.cnta_ctbl
        and substr(cad.cnta_ctbl,1,3) in ('104', '108', '107')) s,
      (select cc1.cod_relacion,
              cc1.tipo_doc,
              cc1.nro_doc,
              pkg_fact_electronica.of_get_serie(cc1.nro_doc) as serie,
              pkg_fact_electronica.of_get_nro(cc1.nro_doc) as numero,
              cc1.fecha_vencimiento as fec_vencimiento,
              cc1.fecha_documento as fec_emision,
              cc1.observacion as glosa,
              p.nom_proveedor,
              p.tipo_doc_ident,
              decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni
         from cntas_cobrar cc1,
              proveedor    p
        where cc1.cod_relacion = p.proveedor
       union
       select cp.cod_relacion,
              cp.tipo_doc,
              cp.nro_doc,
              cp.serie_cp,
              cp.numero_cp,
              cp.vencimiento,
              cp.fecha_emision,
              cp.descripcion,
              p.nom_proveedor,
              p.tipo_doc_ident,
              decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni
         from cntas_pagar cp,
              proveedor    p
        where cp.cod_relacion = p.proveedor ) vw,
      doc_tipo            dt
where ca.origen          = cad.origen
  and ca.ano             = cad.ano
  and ca.mes             = cad.mes
  and ca.nro_libro       = cad.nro_libro
  and ca.nro_asiento     = cad.nro_asiento
  and cad.cod_relacion   = vw.cod_relacion    (+)
  and cad.tipo_docref1   = vw.tipo_doc        (+)
  and cad.nro_docref1    = vw.nro_doc         (+)
  and cad.tipo_docref1   = dt.tipo_doc        (+)
  and ca.origen          = s.origen
  and ca.ano             = s.ano
  and ca.mes             = s.mes
  and ca.nro_libro       = s.nro_libro
  and ca.nro_asiento     = s.nro_asiento
  and cad.cnta_ctbl      = cc.cnta_ctbl
  and ca.flag_estado     <> '0'  
  and NVL(cad.flag_gen_aut,'0')  <> '2'
  and ca.ano             = :an_year
  and ca.mes             = :an_mes
  and substr(cad.cnta_ctbl,1,3) not in ('104', '107', '108')
  and cad.imp_movdol    <> 0
order by ca.origen, ca.ano, ca.mes, ca.nro_libro, ca.nro_asiento, cad.item  

" arguments=(("an_year", number),("an_mes", number)) )
