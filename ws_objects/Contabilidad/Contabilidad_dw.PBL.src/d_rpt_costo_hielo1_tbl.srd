table(column=(type=char(161) updatewhereclause=yes name=desc_cnta dbname="vw_cntbl_costos_x_cencos.desc_cnta" dbalias="t.desc_cnta" )
 column=(type=number updatewhereclause=yes name=total_sol dbname="total_sol" )
 retrieve="    select t.desc_cnta, sum(t.total_sol) as total_sol
      from VW_CNTBL_COSTOS_X_CENCOS t
     where substr(t.cnta_ctbl,1,2) in (select trim(p.valor_string)
                                         from CNTBL_COSTO_PARAM p
                                        where p.opcion = :as_grp_costo) 
       and t.ano  = :an_year
       and t.mes  = :an_mes
       and trim(t.cencos) in (select trim(p.valor_string)
                                         from CNTBL_COSTO_PARAM p
                                        where p.opcion = :as_grp_cencos)
group by t.desc_cnta                                        
order by t.desc_cnta                                        " arguments=(("an_year", number),("an_mes", number),("as_grp_costo", string),("as_grp_cencos", string)) )
