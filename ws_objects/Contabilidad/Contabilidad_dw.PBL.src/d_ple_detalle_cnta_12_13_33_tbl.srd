table(column=(type=char(301) updatewhereclause=yes name=texto dbname="texto" )
 retrieve="select  to_char(pkg_utility.of_last_day(:an_year, :an_mes), 'yyyymmdd') || '|'            -- 01. Periodo
        || min(trim(ca.origen) || trim(to_char(ca.ano, '0000')) || 
           trim(to_char(ca.mes, '00')) || trim(to_char(ca.nro_libro, '00')) || 
           trim(to_char(ca.nro_asiento, '00000'))) || '|'                       				-- 02. Voucher
        || 'M' || min(trim(to_char(ca.nro_asiento, '000000'))) || '|'              			-- 03. Nro asiento
        || nvl(p.tipo_doc_ident, '0') || '|'                                             	-- 04. Tipo Documento Identidad
        || nvl(decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident), '00000000') || '|'  -- 05. Numero de documento de identidad
        || p.nom_proveedor || '|'                                              				-- 06. Apellidos y Nombres, Denominación o Razón Social del cliente
        || to_char(nvl(max(cc.fecha_documento), pkg_utility.of_last_day(:an_year, :an_mes)), 'dd/mm/yyyy')             || '|'        -- 07. Fecha de emisión o fecha de referencia del Comprobante de Pago
        || trim(to_char(sum(cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1)) , '999999999990.00'))      || '|'        -- 08. Monto de cada cuenta por cobrar del cliente
        || '1'                                                   || '|'        				-- 09. Indica el estado de la operación
		  || '|||||||||'																							-- 10 - 18 para otras utilidades
        as texto     

   from cntbl_asiento     ca,
        cntbl_asiento_Det cad,
        cntbl_cnta        cc,
        proveedor         p,
        doc_tipo          dt,
        cntas_cobrar      cc
  where ca.origen       = cad.origen
    and ca.ano          = cad.ano
    and ca.mes          = cad.mes
    and ca.nro_libro    = cad.nro_libro
    and ca.nro_asiento  = cad.nro_asiento     
    and cad.cnta_ctbl   = cc.cnta_ctbl
    and cad.cod_relacion = p.proveedor      (+)
    and cad.tipo_docref1  = dt.tipo_doc      (+)
    and cad.tipo_docref1  = cc.tipo_doc      (+)
    and cad.nro_docref1   = cc.nro_doc       (+)
    and ca.ano = :an_year
    and ca.mes <= :an_mes
    and ca.flag_estado <> '0'
    and substr(cad.cnta_ctbl, 1, 2) in ('12', '13')
group by p.tipo_doc_ident,
        decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
        p.nom_proveedor  
having sum(cad.imp_movsol * decode(cad.flag_debhab, 'D', 1, -1)) <> 0           
--  order by to_char(ca.fecha_cntbl, 'yyyymmdd')


