 column=(type=char(150) updatewhereclause=yes name=cntbl_cnta_desc_cnta dbname="cntbl_cnta.desc_cnta" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="cntbl_asiento_det.cod_relacion" )
 column=(type=char(50) updatewhereclause=yes name=proveedor_nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=number updatewhereclause=yes name=tot_sol dbname="tot_sol" )
 retrieve="SELECT cad.cnta_ctbl, c.desc_cnta, cad.cod_relacion, p.nom_proveedor, 
       SUM(decode(cad.flag_debhab,'D',cad.imp_movsol,cad.imp_movsol*(-1))) as tot_sol
  FROM cntbl_asiento ca, cntbl_asiento_det cad, 
       cntbl_cnta c, proveedor p  
 WHERE (ca.origen=cad.origen AND 
        ca.ano=cad.ano AND 
        ca.mes=cad.mes AND 
        ca.nro_libro=cad.nro_libro AND 
        ca.nro_asiento=cad.nro_asiento AND 
        ca.flag_estado<>'0' AND 
        ca.ano = :an_ano and 
        ca.mes >= :an_mes_ini AND ca.mes<=:an_mes_fin) 
   AND (cad.cnta_ctbl = c.cnta_ctbl AND 
        cad.cod_relacion = p.proveedor ) 
   AND (cad.cod_relacion IN (SELECT cd.cod_relacion 
  FROM cod_rel_grupo c, cod_rel_agrupamiento cd 
 WHERE c.grupo = cd.grupo and 
       c.grupo = :as_grupo)) 
GROUP BY cad.cnta_ctbl, c.desc_cnta, cad.cod_relacion, p.nom_proveedor 
HAVING SUM(decode(cad.flag_debhab,'D',cad.imp_movsol,cad.imp_movsol*(-1))) <> 0 
ORDER BY cad.cnta_ctbl, cad.cod_relacion " arguments=(("an_ano", number),("an_mes_ini", number),("an_mes_fin", number),("as_grupo", string)) )
