table(column=(type=char(507) updatewhereclause=yes name=compute_0001 dbname="compute_0001" )
 retrieve="select trim(cnta_ctbl) || '|' ||
       to_char(round(saldo_inicial_debe,0)) || '|' ||
       to_char(round(saldo_inicial_haber,0)) || '|' ||
       to_char(round(mov_ejercicio_debe, 0)) || '|' ||
       to_char(round(mov_ejercicio_haber, 0)) || '|' ||
       to_char(round(saldo_inicial_debe + mov_ejercicio_debe, 0)) || '|' ||
       to_char(round(saldo_inicial_haber + mov_ejercicio_haber, 0)) || '|' ||
       case when saldo_inicial_debe + mov_ejercicio_debe > saldo_inicial_haber + mov_ejercicio_haber 
            then to_char(round(saldo_inicial_debe + mov_ejercicio_debe - (saldo_inicial_haber + mov_ejercicio_haber),0))
       else
            '0'
       end || '|' ||
       case when saldo_inicial_haber + mov_ejercicio_haber > saldo_inicial_debe + mov_ejercicio_debe 
            then to_char(round(saldo_inicial_haber + mov_ejercicio_haber - (saldo_inicial_debe + mov_ejercicio_debe),0))
       else
            '0'
       end || '|' ||
       '0' || '|' ||
       '0' || '|' ||
       case when trim(cnta_ctbl) <= '592' and (saldo_inicial_debe + mov_ejercicio_debe) > (saldo_inicial_haber + mov_ejercicio_haber) then
            to_char(round((saldo_inicial_debe + mov_ejercicio_debe) - (saldo_inicial_haber + mov_ejercicio_haber),0))            
       else
            '0'
       end || '|' ||
       case when trim(cnta_ctbl) <= '592' and (saldo_inicial_haber + mov_ejercicio_haber) > (saldo_inicial_debe + mov_ejercicio_debe) then
            to_char(round((saldo_inicial_haber + mov_ejercicio_haber) - (saldo_inicial_debe + mov_ejercicio_debe),0))
       else
            '0'
       end || '|' ||
       case when trim(cnta_ctbl) > '592' and (saldo_inicial_debe + mov_ejercicio_debe) > (saldo_inicial_haber + mov_ejercicio_haber) then
            to_char(round((saldo_inicial_debe + mov_ejercicio_debe) - (saldo_inicial_haber + mov_ejercicio_haber), 0))
       else
            '0'
       end || '|' ||
       case when trim(cnta_ctbl) > '592' and (saldo_inicial_haber + mov_ejercicio_haber) > (saldo_inicial_debe + mov_ejercicio_debe) then
            to_char(round((saldo_inicial_haber + mov_ejercicio_haber) - (saldo_inicial_debe + mov_ejercicio_debe), 0))
       else
            '0'
       end || '|'
from (select t.cnta_ctbl, cc.desc_cnta,
         (select NVL(sum(cad.imp_movsol),0)
            from cntbl_asiento ca,
                 cntbl_asiento_det cad
           where ca.origen = cad.origen
             and ca.ano    = cad.ano
             and ca.mes    = cad.mes
             and ca.nro_libro = cad.nro_libro
             and ca.nro_asiento = cad.nro_asiento
             and substr(cad.cnta_ctbl, 1, length(trim(t.cnta_ctbl))) = trim(t.cnta_ctbl)
             and cad.ano = :ani_year
             and cad.mes = 0
             and cad.flag_debhab = 'D'
             and ca.flag_estado = '1') as saldo_inicial_debe,
         (select NVL(sum(cad.imp_movsol),0)
            from cntbl_asiento ca,
                 cntbl_asiento_det cad
           where ca.origen = cad.origen
             and ca.ano    = cad.ano
             and ca.mes    = cad.mes
             and ca.nro_libro = cad.nro_libro
             and ca.nro_asiento = cad.nro_asiento
             and substr(cad.cnta_ctbl, 1, length(trim(t.cnta_ctbl))) = trim(t.cnta_ctbl)
             and cad.ano = :ani_year
             and cad.mes = 0
             and cad.flag_debhab = 'H'
             and ca.flag_estado = '1') as saldo_inicial_haber,
         (select NVL(sum(cad.imp_movsol),0)
            from cntbl_asiento ca,
                 cntbl_asiento_det cad
           where ca.origen = cad.origen
             and ca.ano    = cad.ano
             and ca.mes    = cad.mes
             and ca.nro_libro = cad.nro_libro
             and ca.nro_asiento = cad.nro_asiento
             and substr(cad.cnta_ctbl, 1, length(trim(t.cnta_ctbl))) = trim(t.cnta_ctbl)
             and cad.ano = :ani_year
             and cad.mes between 1 and 12
             and cad.flag_debhab = 'D'
             and ca.flag_estado = '1') as mov_ejercicio_debe,
         (select NVL(sum(cad.imp_movsol),0)
            from cntbl_asiento ca,
                 cntbl_asiento_det cad
           where ca.origen = cad.origen
             and ca.ano    = cad.ano
             and ca.mes    = cad.mes
             and ca.nro_libro = cad.nro_libro
             and ca.nro_asiento = cad.nro_asiento
             and substr(cad.cnta_ctbl, 1, length(trim(t.cnta_ctbl))) = trim(t.cnta_ctbl)
             and cad.ano = :ani_year
             and cad.mes between 1 and 12
             and cad.flag_debhab = 'H'
             and ca.flag_estado = '1') as mov_ejercicio_haber
  from cntas_balance t,
       cntbl_cnta    cc
  where t.cnta_ctbl = cc.cnta_ctbl (+)
 )
 " arguments=(("ani_year", number)) )
