 retrieve="  -- cntas x Cobrar
  select 'CC' as flag_cta,             
         cd.cnta_ctbl, 
         c.desc_cnta,   
         c.flag_tipo_cam_adc,
         cd.cod_relacion as codigo, 
         p.nom_proveedor, 
         cd.tipo_docref1 as tipo_doc, 
         cd.nro_docref1 as nro_doc,
         cc.cod_moneda,
			case
            when c.flag_tipo_cam_adc = 'C' then
               (select c.cmp_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
            when c.flag_tipo_cam_adc = 'V' then
               (select c.vnt_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
         end as tipo_cambio,
         nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0) as saldo_sol,
         nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0) as saldo_dol
    from cntbl_asiento        ca, 
         cntbl_asiento_det    cd, 
         proveedor            p, 
         cntbl_cnta           c,
         cntas_cobrar         cc
   where ca.origen       = cd.origen 
     and ca.ano          = cd.ano 
     and ca.mes          = cd.mes 
     and ca.nro_libro    = cd.nro_libro 
     and ca.nro_asiento  = cd.nro_asiento
     and cd.tipo_docref1 = cc.tipo_doc 
     and substr(cd.nro_docref1,1,10) = cc.nro_doc 
     and cd.cod_relacion = cc.cod_relacion 
     and ca.flag_estado  <> '0' 
     and cd.cnta_ctbl   = c.cnta_ctbl 
     and cd.cod_relacion = p.proveedor 
     and ca.ano          = :ani_year 
     and ca.mes          <= :ani_mes
     and cd.cod_relacion is not null 
     and cd.tipo_docref1 is not null 
     and cd.nro_docref1  is not null 
     and c.flag_codrel   = '1'
     and c.flag_doc_ref  = '1'
  group by 'C',             
         cd.cnta_ctbl, 
         c.desc_cnta,   
         c.flag_tipo_cam_adc,
         cd.cod_relacion, 
         p.nom_proveedor, 
         cd.tipo_docref1, 
         cd.nro_docref1,
         cc.cod_moneda
  having round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0)), 4) >0
      or round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0)), 4) >0
          
  union
    -- Cntas x Pagar
    select 'CP',             
           cd.cnta_ctbl, 
           c.desc_cnta,   
           c.flag_tipo_cam_adc,
           cd.cod_relacion, 
           p.nom_proveedor, 
           cd.tipo_docref1, 
           cd.nro_docref1,
           cp.cod_moneda,
           case
            when c.flag_tipo_cam_adc = 'C' then
               (select c.cmp_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
            when c.flag_tipo_cam_adc = 'V' then
               (select c.vnt_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
           end as tipo_cambio,
           nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0) as saldo_sol,
           nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0) as saldo_dol
      from cntbl_asiento        ca, 
           cntbl_asiento_det    cd, 
           proveedor            p, 
           cntbl_cnta           c,
           cntas_pagar          cp
     where ca.origen            = cd.origen 
       and ca.ano               = cd.ano 
       and ca.mes               = cd.mes 
       and ca.nro_libro         = cd.nro_libro 
       and ca.nro_asiento       = cd.nro_asiento
       and cd.tipo_docref1      = cp.tipo_doc 
       and substr(cd.nro_docref1,1,10) = cp.nro_doc 
       and cd.cod_relacion = cp.cod_relacion 
       and ca.flag_estado  <> '0'
       and cd.cnta_ctbl    = c.cnta_ctbl 
       and cd.cod_relacion = p.proveedor 
       and ca.ano          = :ani_year
       and ca.mes          <= :ani_mes
       and cd.cod_relacion is not null 
       and cd.tipo_docref1 is not null 
       and cd.nro_docref1  is not null 
       and c.flag_codrel   = '1'
       and c.flag_doc_ref  = '1'
  group by 'P',             
           cd.cnta_ctbl, 
           c.desc_cnta,   
           c.flag_tipo_cam_adc,
           cd.cod_relacion, 
           p.nom_proveedor, 
           cd.tipo_docref1, 
           cd.nro_docref1,
           cp.cod_moneda
  having round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0)), 4) >0
      or round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0)), 4) >0 
  union
    -- Solicitud de Giros
    select 'OG',             
           cd.cnta_ctbl, 
           c.desc_cnta,   
           c.flag_tipo_cam_adc,
           cd.cod_relacion, 
           p.nom_proveedor, 
           cd.tipo_docref1, 
           cd.nro_docref1,
           sl.cod_moneda,
           case
            when c.flag_tipo_cam_adc = 'C' then
               (select c.cmp_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
            when c.flag_tipo_cam_adc = 'V' then
               (select c.vnt_dol_sbs
                 from calendario c
                where trunc(c.fecha) = pkg_utility.of_last_day(:ani_year, :ani_mes))
           end as tipo_cambio,
           nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0) as saldo_sol,
           nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0) as saldo_dol
      from cntbl_asiento        ca, 
           cntbl_asiento_det    cd, 
           proveedor            p, 
           cntbl_cnta           c,
           solicitud_giro       sl
     where ca.origen            = cd.origen 
       and ca.ano               = cd.ano 
       and ca.mes               = cd.mes 
       and ca.nro_libro         = cd.nro_libro 
       and ca.nro_asiento       = cd.nro_asiento
       and cd.tipo_docref1      = 'OG'
       and substr(cd.nro_docref1,1,10) = sl.nro_solicitud 
       and cd.cod_relacion      = sl.cod_relacion 
       and ca.flag_estado  <> '0'
       and cd.cnta_ctbl    = c.cnta_ctbl 
       and cd.cod_relacion = p.proveedor 
       and ca.ano          = :ani_year
       and ca.mes          <= :ani_mes
       and cd.cod_relacion is not null 
       and cd.tipo_docref1 is not null 
       and cd.nro_docref1  is not null 
       and c.flag_codrel   = '1'
       and c.flag_doc_ref  = '1'
  group by 'P',             
           cd.cnta_ctbl, 
           c.desc_cnta,   
           c.flag_tipo_cam_adc,
           cd.cod_relacion, 
           p.nom_proveedor, 
           cd.tipo_docref1, 
           cd.nro_docref1,
           sl.cod_moneda
  having round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movsol,0) - decode(cd.flag_debhab,'H',cd.imp_movsol,0)),0)), 4) >0
      or round(abs(nvl(sum(decode(cd.flag_debhab,'D',cd.imp_movdol,0) - decode(cd.flag_debhab,'H',cd.imp_movdol,0)),0)), 4) >0 
  
  order by 1, 2, 3, 4, 9, 10" arguments=(("ani_year", number),("ani_mes", number)) )
