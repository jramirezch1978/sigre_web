table(column=(type=char(2) updatewhereclause=yes name=flag_cta dbname="flag_cta" )
 column=(type=char(10) updatewhereclause=yes name=cnta_ctbl dbname="cntbl_asiento_det.cnta_ctbl" dbalias="cd.cnta_ctbl" )
 column=(type=char(150) updatewhereclause=yes name=desc_cnta dbname="cntbl_cnta.desc_cnta" dbalias="c.desc_cnta" )
 column=(type=char(1) updatewhereclause=yes name=flag_tipo_cam_adc dbname="cntbl_cnta.flag_tipo_cam_adc" dbalias="c.flag_tipo_cam_adc" )
 column=(type=char(8) updatewhereclause=yes name=codigo dbname="cntbl_asiento_det.codigo" dbalias="cd.codigo" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(4) updatewhereclause=yes name=tipo_doc dbname="cntbl_asiento_det.tipo_doc" dbalias="cd.tipo_doc" )
 column=(type=char(15) updatewhereclause=yes name=nro_doc dbname="cntbl_asiento_det.nro_doc" dbalias="cd.nro_doc" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_cobrar.cod_moneda" dbalias="cc.cod_moneda" )
 column=(type=number updatewhereclause=yes name=tipo_cambio dbname="tipo_cambio" )
