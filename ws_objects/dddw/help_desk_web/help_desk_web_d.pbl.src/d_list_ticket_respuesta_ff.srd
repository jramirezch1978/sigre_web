table(column=(type=datetime updatewhereclause=yes name=fec_rpta dbname="ticket_respuesta.fec_rpta" )
 column=(type=char(2000) updatewhereclause=yes name=rpta_publica dbname="ticket_respuesta.rpta_publica" )
 column=(type=char(2000) updatewhereclause=yes name=nota_interna dbname="ticket_respuesta.nota_interna" )
 column=(type=char(1) updatewhereclause=yes name=flag_tipo dbname="ticket_respuesta.flag_tipo" values="Solución	1/Nvo Requerimiento	2/Garantia	3/Soporte	4/" )
 column=(type=char(101) updatewhereclause=yes name=nom_asignado dbname="nom_asignado" )
 retrieve="select tr.fec_rpta,
		tr.rpta_publica,
		tr.nota_interna,
		tr.flag_tipo, 
		u.nombre || ' ' || u.apellidos as nom_asignado
from  ticket_respuesta tr,
		usuarios u
where  tr.created_by		= u.usuario_id
	and tr.ticket_id			= :an_ticket_id
order by tr.fec_rpta desc" arguments=(("an_ticket_id", number)) )
