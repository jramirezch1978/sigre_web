 retrieve="select distinct t.*,
		ce.nombres || ' ' || ce.apellidos as nom_solicitante,
		e.cod_empresa,
		u.nombre || ' ' || u.apellidos as nom_asignado,
		p.desc_prioridad
from  ticket t,
		contactos_empresa ce,
		empresa e,
		usuarios u,
		prioridad p,
		tipo_ticket tt,
		ticket_respuesta tr,
		ticket_capt_pantalla tcp
where  t.solicitante		= ce.email_empresa_id
	and ce.empresa_id	= e.empresa_id
	and t.asignado_a		= u.usuario_id
	and t.cod_prioridad	= p.cod_prioridad
	and t.cod_tipo_ticket	= tt.cod_tipo_ticket
	and t.ticket_id			= tr.ticket_id (+)
	and t.ticket_id			= tcp.ticket_id (+)
	and (UPPER(t.nro_ticket)			like '%' || UPPER(:as_valor) || '%'
	or UPPER(t.asunto)				like '%' || UPPER(:as_valor) || '%'
	or UPPER(t.desc_ticket)			like '%' || UPPER(:as_valor) || '%'
	or UPPER(u.nombre)				like '%' || UPPER(:as_valor) || '%'
	or UPPER(u.apellidos)				like '%' || UPPER(:as_valor) || '%'
	or UPPER(ce.nombres)			like '%' || UPPER(:as_valor) || '%'
	or UPPER(ce.apellidos)			like '%' || UPPER(:as_valor) || '%'
	or UPPER(e.cod_empresa)		like '%' || UPPER(:as_valor) || '%'
	or UPPER(p.desc_prioridad)		like '%' || UPPER(:as_valor) || '%'
	or UPPER(tt.desc_tipo_ticket)	like '%' || UPPER(:as_valor) || '%'
	or UPPER(tr.rpta_publica)		like '%' || UPPER(:as_valor) || '%'
	or UPPER(tr.nota_interna)		like '%' || UPPER(:as_valor) || '%'
	or UPPER(tcp.file_nomb_corto)	like '%' || UPPER(:as_valor) || '%'
)" arguments=(("as_valor", string)) )
