 retrieve="  SELECT ~"DIRECCIONES~".~"ITEM~"				,
			~"DIRECCIONES~".~"DESCRIPCION~"	,   
         ~"DIRECCIONES~".~"DIR_COD_POSTAL~",   
         nvl( dir_pais, '') || ' ' || nvl( dir_dep_estado,'') || ' ' ||
			nvl( dir_provincia, '') || ' ' || nvl( dir_ciudad,'') || ' ' || 
			nvl( dir_distrito, '') || ' ' || nvl( dir_urbanizacion,'') || ' ' || 
			nvl( dir_direccion, '') || ' ' || nvl( dir_mnz,'') || ' ' || 
			nvl( dir_lote, '') || ' ' || nvl( dir_numero,'') 
			 as dir_texto  
    FROM ~"DIRECCIONES~"  
   WHERE (~"DIRECCIONES~".~"CODIGO~" = :as_codigo    ) AND
         (~"DIRECCIONES~".~"ITEM~"	= :an_item		 )

