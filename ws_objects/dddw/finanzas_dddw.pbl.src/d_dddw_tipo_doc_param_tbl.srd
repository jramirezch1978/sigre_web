 retrieve="  SELECT ~"FINPARAM~".~"DOC_VENTAS~",   
         ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~",   
         ~"DOC_TIPO_USUARIO~".~"NRO_SERIE~",   
		   ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~"||lpad(to_char(~"DOC_TIPO_USUARIO~".~"NRO_SERIE~") ,(select digitos_serie from finparam where reckey = '1'),'0') as tipser,  
         ~"DOC_TIPO~".~"NRO_LIBRO~",   
         ~"DOC_TIPO~".~"DESC_TIPO_DOC~",   
         ~"DOC_TIPO_USUARIO~".~"COD_USR~"  
    FROM ~"FINPARAM~",   
         ~"DOC_GRUPO_RELACION~",   
         ~"DOC_TIPO_USUARIO~",   
         ~"DOC_TIPO~"  
   WHERE ( ~"DOC_GRUPO_RELACION~".~"TIPO_DOC~" = ~"DOC_TIPO_USUARIO~".~"TIPO_DOC~" ) and  
         ( ~"DOC_TIPO_USUARIO~".~"TIPO_DOC~" = ~"DOC_TIPO~".~"TIPO_DOC~" ) and  
         ( ~"FINPARAM~".~"DOC_VENTAS~" = ~"DOC_GRUPO_RELACION~".~"GRUPO~" ) and  
         ( ( ~"FINPARAM~".~"RECKEY~" = '1' ) )    
" )
