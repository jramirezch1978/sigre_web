$PBExportComments$Valida si es nulo
global type f_validacion_null from function_object
end type

forward prototypes
global function boolean f_validacion_null (any aa_data)
end prototypes

global function boolean f_validacion_null (any aa_data);Boolean  lbo_rc

IF IsNull(aa_data) OR aa_data = "" THEN
	lbo_rc = FALSE
ELSE
	lbo_rc = TRUE
END IF

RETURN lbo_rc
end function

