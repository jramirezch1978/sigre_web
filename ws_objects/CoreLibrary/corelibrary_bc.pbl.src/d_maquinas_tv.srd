 retrieve="select n1.cod_maquina as nivel1,
       n1.desc_maq as desc_nivel1,
       n2.cod_maquina as nivel2,
       n2.desc_maq as desc_nivel2,
       n3.cod_maquina as nivel3,
       n3.desc_maq as desc_nivel3,
       m.cod_maquina,
       m.desc_maq
  from maquina m,
       (select substr(n1.cod_maquina, 1, 2) as niv, n1.* from maquina n1 where n1.cod_maquina like '%000000') n1,
       (select substr(m.cod_maquina, 1, 4) as niv, m.* from maquina m where m.cod_maquina like '%0000' and m.cod_maquina not like '%000000') n2,
       (select substr(m.cod_maquina, 1, 6) as niv, m.* from maquina m where m.cod_maquina like '%00' and m.cod_maquina not like '%0000') n3
 where substr(m.cod_maquina,1,2) = n1.niv (+)
   and substr(m.cod_maquina,1,4) = n2.niv (+)
   and substr(m.cod_maquina,1,6) = n3.niv (+)
   and m.cod_maquina not like '%00'
   and m.flag_estado <> '0'
   and (m.cod_maquina like :as_string or m.desc_maq like :as_string)
" arguments=(("as_string", string))  sort="nivel1 A nivel2 A nivel3 A cod_maquina A " )
