table(column=(type=number updatewhereclause=yes name=nro_item dbname="doc_tipo.nro_row" dbalias=".nro_row" )
 column=(type=char(10) updatewhereclause=yes name=nro_registro dbname="doc_tipo.nro_registro" dbalias=".nro_registro" )
 column=(type=char(3) updatewhereclause=yes name=moneda dbname="doc_tipo.cod_moneda" dbalias=".cod_moneda" )
 column=(type=char(2) updatewhereclause=yes key=yes name=tipo_doc dbname="doc_tipo.tipo_doc" dbalias="dt.tipo_doc" )
 column=(type=char(4000) updatewhereclause=yes name=serie dbname="doc_tipo.serie_cxc" dbalias=".serie_cxc" )
 column=(type=char(4000) updatewhereclause=yes name=nro dbname="doc_tipo.nro_cxc" dbalias=".nro_cxc" )
 column=(type=char(4) updatewhereclause=yes name=tipo_doc_cxc dbname="tipo_doc" )
 column=(type=char(10) updatewhereclause=yes name=nro_doc_cxc dbname="nro_doc" )
 column=(type=number updatewhereclause=yes name=vta_gravada dbname="vta_gravada" )
 column=(type=number updatewhereclause=yes name=vta_exonerada dbname="vta_exonerada" )
 column=(type=number updatewhereclause=yes name=vta_inafecta dbname="vta_inafecta" )
 column=(type=number updatewhereclause=yes name=total_igv dbname="total_igv" )
 retrieve="select 	rownum as nro_row,
     		f.nro_registro,
     		f.cod_moneda,
       	dt.cod_sunat as tipo_doc,
       	f.serie_cxc,
       	f.nro_cxc,
		 	null as tipo_doc,
		 	null as nro_doc,
			(select nvl(sum(fd.cant_proyect * (fd.precio_unit - fd.descuento)),0)
				 from fs_factura_simpl_det fd
				where fd.nro_registro = f.nro_registro) as vta_gravada,
			0.00 as vta_exonerada,
			0.00 as vta_inafecta,
			(select nvl(sum(fd.cant_proyect * fd.importe_igv),0)
				 from fs_factura_simpl_det fd
				where fd.nro_registro = f.nro_registro) as total_igv
from (select t.*
        from fs_factura_simpl t 
     where t.NRO_RC_ID is null 
       and t.nro_ra_id is null
     order by t.serie_cxc, t.nro_cxc) f,
     doc_tipo         dt
where f.tipo_doc_cxc = dt.tipo_doc
  and f.tipo_doc_cxc in ('BVC', 'NCC', 'NDC')
  and f.serie_cxc		like 'B%'
  and trunc(f.fec_registro) = trunc(:ad_fecha)
union all
select rownum as nro_row,
       null as nro_registro,
       cc.cod_moneda,
       dt.cod_sunat as tipo_doc,
       cc.serie_cxc,
       cc.nro_cxc,
		 cc.tipo_doc,
		 cc.nro_doc,
       (select nvl(sum(ccd.cantidad * ccd.precio_unitario),0)
          from cntas_cobrar_det ccd
         where ccd.tipo_doc = cc.tipo_doc
           and ccd.nro_doc  = cc.nro_doc
           and ccd.tipo_cred_fiscal = '09') as vta_gravada,
       (select nvl(sum(ccd.cantidad * ccd.precio_unitario),0)
          from cntas_cobrar_det ccd
         where ccd.tipo_doc = cc.tipo_doc
           and ccd.nro_doc  = cc.nro_doc
           and ccd.tipo_cred_fiscal = '11') as vta_exonerada,
       (select nvl(sum(ccd.cantidad * ccd.precio_unitario),0)
          from cntas_cobrar_det ccd
         where ccd.tipo_doc = cc.tipo_doc
           and ccd.nro_doc  = cc.nro_doc
           and ccd.tipo_cred_fiscal = '10') as vta_inafecta,
       (select nvl(sum(ci.importe),0)
          from cc_doc_det_imp ci
         where ci.tipo_doc = cc.tipo_doc
           and ci.nro_doc  = cc.nro_doc) as total_igv
from (select PKG_FACT_ELECTRONICA.of_get_serie(t.nro_doc) as serie_cxc,
             PKG_FACT_ELECTRONICA.of_get_nro(t.nro_doc)   as nro_cxc,
             t.*
        from cntas_cobrar t 
     where t.NRO_RC_ID is null 
       and t.nro_ra_id is null
     order by 1, 2) cc,
     (select cc1.tipo_doc, cc1.nro_doc
          from cntas_cobrar cc1
        minus
        select f.tipo_doc_cxc, f.nro_doc_cxc
          from fs_factura_simpl f
         where f.nro_doc_cxc is not null) s,
     doc_tipo         dt
where cc.tipo_doc = dt.tipo_doc
  and cc.tipo_doc = s.tipo_doc
  and cc.nro_doc  = s.nro_doc
  and cc.tipo_doc in ('BVC', 'NCC', 'NDC')
  and cc.serie_cxc		like 'B%'
  and trunc(cc.fecha_documento) = trunc(:ad_fecha)
" update="doc_tipo" updatewhere=0 updatekeyinplace=no arguments=(("ad_fecha", date)) )
