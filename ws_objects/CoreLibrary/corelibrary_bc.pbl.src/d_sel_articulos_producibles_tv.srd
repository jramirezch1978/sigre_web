 column=(type=char(10) updatewhereclause=yes name=cnta_prsp_ingreso dbname="articulo_sub_categ.cnta_prsp_ingreso" dbalias="a2.cnta_prsp_ingreso" )
 column=(type=char(8) updatewhereclause=yes name=cod_clase dbname="articulo_clase.cod_clase" dbalias="ac.cod_clase" )
 column=(type=char(40) updatewhereclause=yes name=desc_clase dbname="articulo_clase.desc_clase" dbalias="ac.desc_clase" )
 column=(type=number updatewhereclause=yes name=saldo_total dbname="saldo_total" )
 retrieve="select a1.cat_art, a1.desc_categoria,
       a2.cod_sub_cat, a2.desc_sub_cat,
       a.cod_art, 
       a.desc_art,
       a.und,
		 a2.cnta_prsp_egreso,
       a2.cnta_prsp_ingreso,
		 ac.cod_clase,
		 ac.desc_clase,
       (select nvl(sum(am.cant_procesada * amt.factor_sldo_total), 0)
          from vale_mov     vm,
               articulo_mov am,
               articulo_mov_tipo amt
         where vm.nro_vale = am.nro_Vale
           and am.cod_Art  = a.cod_art
           and vm.tipo_mov = amt.tipo_mov
           and vm.flag_estado <> '0'
           and am.flag_estado <> '0') as saldo_total
from articulo a,
     articulo_sub_categ a2,
     articulo_Categ     a1,
	  articulo_clase		ac
where a.sub_cat_art	= a2.cod_sub_cat
  and a2.cat_art    	= a1.cat_art     
  and a.cod_clase		= ac.cod_clase
  and (upper(a.desc_art)	  				like upper(:as_filtro)
		or	a.cod_art						like :as_filtro
		or a1.cat_art						like :as_filtro
		or upper(a1.desc_categoria)	like upper(:as_filtro)
		or a2.cod_sub_cat					like :as_filtro
		or upper(a2.desc_sub_cat)		like upper(:as_filtro) 
  and a.cod_clase	in (:as_clase))" arguments=(("as_filtro", string),("as_clase", stringlist))  sort="cat_art A cod_sub_cat A cod_art A " )
