 column=(type=char(10) updatewhereclause=yes name=cnta_ctbl dbname="cnta_ctbl" )
 column=(type=char(150) updatewhereclause=yes name=desc_cnta dbname="desc_cnta" )
 retrieve="select cc1.cnta_ctbl as cnta_niv1, cc1.desc_cnta as desc_niv1,
       cc2.cnta_ctbl as cnta_niv2, cc2.desc_cnta as desc_niv2,
       cc3.cnta_ctbl as cnta_niv3, cc3.desc_cnta as desc_niv3,
       cc4.cnta_ctbl as cnta_niv4, cc4.desc_cnta as desc_niv4,
       cc5.cnta_ctbl, cc5.desc_cnta
       
from (select cc.*, 
             substr(cc.cnta_ctbl, 1, 2) as niv1,  
             substr(cc.cnta_ctbl, 1, 3) as niv2, 
             substr(cc.cnta_ctbl, 1, 5) as niv3, 
             substr(cc.cnta_ctbl, 1, 6) as niv4 
        from cntbl_cnta cc ) cc5,
     (select cc.*, trim(cc.cnta_ctbl) as nivel from cntbl_cnta cc) cc1,
     (select cc.*, trim(cc.cnta_ctbl) as nivel from cntbl_cnta cc) cc2,
     (select cc.*, trim(cc.cnta_ctbl) as nivel from cntbl_cnta cc) cc3,
     (select cc.*, trim(cc.cnta_ctbl) as nivel from cntbl_cnta cc) cc4
where cc5.niv1 = cc1.nivel (+)
  and cc5.niv2 = cc2.nivel (+)
  and cc5.niv3 = cc3.nivel (+) 
  and cc5.niv4 = cc4.nivel (+)
  and length(trim(cc5.cnta_ctbl)) = 8
  and cc5.flag_estado = '1'
  and (cc5.cnta_ctbl like :as_string1 or cc5.desc_cnta like :as_string1)
" arguments=(("as_string1", string))  sort="cnta_niv1 A cnta_niv2 A cnta_niv3 A cnta_niv4 A cnta_ctbl A " )
