 retrieve="select distinct
		 p.proveedor,
       p.nom_proveedor,
       p.tipo_doc_ident,
       decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
       PKG_LOGISTICA.of_get_dir_comercial(p.proveedor) as direccion
from proveedor p,
	  cntas_cobrar cc
where p.proveedor = cc.cod_relacion
  and cc.flag_estado <> '0'
  and p.flag_estado = '1'
  and p.flag_clie_prov in ('0', '2')
  and substr(pkg_fact_electronica.of_Get_serie(cc.nro_doc), 1, 1) not in ('B', 'F')
  and ( p.nom_proveedor like :as_filtro
	  or decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) like :as_filtro
     or PKG_LOGISTICA.of_get_dir_comercial(p.proveedor) like :as_filtro)" arguments=(("as_filtro", string)) )
