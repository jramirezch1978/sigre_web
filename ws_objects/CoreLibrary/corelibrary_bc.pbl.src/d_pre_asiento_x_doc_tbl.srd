 retrieve="  SELECT ~"CNTBL_ASIENTO_DET~".~"CNTA_CTBL~"	 ,   
         DECODE(~"CNTBL_ASIENTO_DET~".~"FLAG_DEBHAB~",'D','H','D') AS FLAG_DEBHAB,
			~"VW_FIN_DOC_GENERAL~".~"TIPO_DOC~"		 ,
			~"VW_FIN_DOC_GENERAL~".~"NRO_DOC~" 		 , 
			~"VW_FIN_DOC_GENERAL~".~"COD_RELACION~"  , 
			~"VW_FIN_DOC_GENERAL~".~"TASA_CAMBIO~"	 ,
         ~"VW_FIN_DOC_GENERAL~".~"FLAB_TABOR~"	 ,
			~"CNTBL_ASIENTO_DET~".~"CENCOS~"		 ,
			~"CNTBL_ASIENTO_DET~".~"COD_CTABCO~" ,
			~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF2~",
			~"CNTBL_ASIENTO_DET~".~"DET_GLOSA~"
    FROM ~"CNTBL_ASIENTO_DET~",   
         ~"VW_FIN_DOC_GENERAL~"  
   WHERE ( ~"VW_FIN_DOC_GENERAL~".~"ORIGEN~"          = ~"CNTBL_ASIENTO_DET~".~"ORIGEN~"        ) AND  
			( ~"VW_FIN_DOC_GENERAL~".~"ANO~"             = ~"CNTBL_ASIENTO_DET~".~"ANO~"           ) AND  
			( ~"VW_FIN_DOC_GENERAL~".~"MES~"	           = ~"CNTBL_ASIENTO_DET~".~"MES~"	          ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"NRO_LIBRO~" 		  = ~"CNTBL_ASIENTO_DET~".~"NRO_LIBRO~"     ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"NRO_ASIENTO~"     = ~"CNTBL_ASIENTO_DET~".~"NRO_ASIENTO~"   ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"TIPO_DOC~" 		  = ~"CNTBL_ASIENTO_DET~".~"TIPO_DOCREF1~"  ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"NRO_DOC~" 		  = ~"CNTBL_ASIENTO_DET~".~"NRO_DOCREF1~"   ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"COD_RELACION~" 	  = ~"CNTBL_ASIENTO_DET~".~"COD_RELACION~"  ) AND  
         ( ( ~"VW_FIN_DOC_GENERAL~".~"COD_RELACION~"  = :as_cod_relacion                    ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"TIPO_DOC~"        = :as_tipo_doc                        ) AND  
         ( ~"VW_FIN_DOC_GENERAL~".~"NRO_DOC~"         = :as_nro_doc                         ))    
" arguments=(("as_cod_relacion", string),("as_tipo_doc", string),("as_nro_doc", string)) )
