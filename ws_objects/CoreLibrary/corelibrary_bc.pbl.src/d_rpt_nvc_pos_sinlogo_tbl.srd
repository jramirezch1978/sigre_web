 retrieve="select case 
			when trim(fs.tipo_doc_cxc) = 'NCC' then
				case
				 when substr(fs.serie_cxc,1,1) in ('F', 'B') then
					'NOTA DE CREDITO ELECTRONICA'
				 else
					'NOTA DE CREDITO'
				end
			when trim(fs.tipo_doc_cxc) = 'NDC' then
				case
				 when substr(fs.serie_cxc,1,1) in ('F', 'B') then
					'NOTA DE DEBITO ELECTRONICA'
				 else
					'NOTA DE DEBITO'
				end      
			when trim(fs.tipo_doc_cxc) = 'FAC' then
				case
				 when substr(fs.serie_cxc,1,1) = 'F' then
					'FACTURA DE VENTA ELECTRONICA'
				 else  
					'FACTURA DE VENTA'
				end 
			when trim(fs.tipo_doc_cxc) = 'BVC' then
				case 
				 when substr(fs.serie_cxc,1,1) = 'B' then
					'BOLETA DE VENTA ELECTRONICA'
				 else
					'BOLETA DE VENTA'
				end 
			when trim(fs.tipo_doc_cxc) = 'NVC' then
				'NOTA DE VENTA'
			end as desc_tipo_doc,
			fs.cod_origen,
			fs.nro_registro,
			fs.tipo_doc_cxc,
			fs.tipo_doc_ref, fs.nro_doc_ref,
			dt.cod_sunat as tipo_doc_sunat,
			pkg_logistica.of_get_direccion(fs.cliente, fs.item_direccion) as direccion_cliente,
			lpad(trim(fs.serie_cxc), 4, '0') as serie_cxc,
			fs.nro_cxc,
			PKG_FACT_ELECTRONICA.of_get_full_nro(fs.serie_cxc || '-' || fs.nro_cxc) as nro_doc_cxc,
			fs.cliente,
			fs.fec_registro,
			u.nombre as cajero,
			u2.nombre as vendedor,
			p.nom_proveedor,
			lpad(trim(p.tipo_doc_ident), 2, '0') as tipo_doc_ident,
			decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
			fsd.nro_item,
			fsd.cod_art,
			case 
				when fsd.cod_art is not null then
					case 
					 when a.desc_clase_vehiculo is null then
						a.full_Desc_art
					 else
						a.full_Desc_vehiculo
					end
				else
					fsd.descripcion
			end as DESC_ART,
			a.und,
			fsd.cant_proyect,
			fsd.precio_unit,
			fsd.importe_igv,
			fsd.descuento,
			mo.descripcion as desc_moneda,
			DECODE(fsd.importe_igv, 0, fsd.precio_unit, 0) as op_inafecta,
			DECODE(fsd.importe_igv, 0, 0, fsd.precio_unit) as op_gravada,
			fsd.nro_proforma,
			fs.observacion,
			--Datos del emisor
			PKG_CONFIG.OF_RAZON_SOCIAL_EMPRESA(:as_empresa) as razon_social_empresa,
			PKG_CONFIG.OF_RUC_EMPRESA(:as_empresa) as RUC,
			PKG_CONFIG.OF_DIRECCION_EMPRESA(:as_empresa, fs.cod_origen) as DIRECCION,
			PKG_CONFIG.OF_FONO_FIJO_EMPRESA(:as_empresa) as FONO_FIJO,
			PKG_CONFIG.OF_CELULAR_EMPRESA(:as_empresa) as celular 
from 	fs_factura_simpl     fs,
     	fs_factura_simpl_det fsd,
     	doc_tipo             dt,
     	proveedor            p,
     	vw_articulo          a,
     	MARCA                M,
    	usuario          		u,
    	usuario          		u2,
    	moneda            	mo
where fs.nro_registro  	= fsd.nro_registro     
  and fs.tipo_doc_cxc  	= dt.tipo_doc  (+)
  and fs.cliente       	= p.proveedor   (+)
  and fsd.cod_art      	= a.cod_art    (+)
  AND A.COD_MARCA      	= M.COD_MARCA   (+)
  and fs.cod_usr      	= u.cod_usr     (+)
  and fs.cod_moneda    	= mo.cod_moneda  (+)
  and fs.vendedor      	= u2.cod_usr    (+)
  and fs.nro_registro  	= :as_nro_registro" arguments=(("as_nro_registro", string),("as_empresa", string))  sort="nro_item A " )
