 retrieve="SELECT  AE.COD_ART,   
			AE.COD_EQUIVA,   
			A.DESC_ART,   
			A.UND,   
			A.DIAS_REPOSICION,   
			A.DIAS_REP_IMPORT,
			A2.CNTA_PRSP_INGRESO,
			A.COSTO_ULT_COMPRA
FROM ARTICULO_EQUIVALENCIAS AE,   
		ARTICULO A,
		ARTICULO_SUB_CATEG	A2  
WHERE AE.COD_EQUIVA		= A.COD_ART 
	and A.SUB_CAT_ART		= A2.COD_SUB_CAT 
	and  AE.COD_ART			= :as_cod_art
" arguments=(("as_cod_art", string))  sort="cod_equiva A " )
