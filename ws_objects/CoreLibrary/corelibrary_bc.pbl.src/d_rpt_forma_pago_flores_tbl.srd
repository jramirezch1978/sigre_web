table(column=(type=char(33) updatewhereclause=yes name=forma_pago dbname="forma_pago" )
 column=(type=char(73) updatewhereclause=yes name=texto1 dbname="texto1" )
 column=(type=char(41) updatewhereclause=yes name=texto2 dbname="texto2" )
 column=(type=char(35) updatewhereclause=yes name=texto3 dbname="texto3" )
 column=(type=char(73) updatewhereclause=yes name=texto4 dbname="texto4" )
 column=(type=char(26) updatewhereclause=yes name=texto5 dbname="texto5" )
 retrieve="select case 
        when t.flag_forma_pago = 'E' then
          'PAGO EN EFECTIVO'
        when t.flag_forma_pago = 'T' then
          'PAGO CON TARJETA DEBITO / CREDITO'
        when t.flag_forma_pago = 'N' then
          'APLICACION NC / ND'
        	when t.flag_forma_pago = 'C' then
				case	
					when t.flag_tipo_credito = 'A' then
          			'FINANCIMIENTO CREDITO DIRECTO'
					when t.flag_tipo_credito = 'B' then
						'CREDITO'
					WHEN t.flag_tipo_credito = '1' then
						'FACTURA A 7 DIAS'
					WHEN t.flag_tipo_credito = '2' then
						'FACTURA A 15 DIAS'
					WHEN t.flag_tipo_credito = '3' then
						'FACTURA A 30 DIAS'
					WHEN t.flag_tipo_credito = '4' then
						'FACTURA A 60 DIAS'
					WHEN t.flag_tipo_credito = '5' then
						'FACTURA A 90 DIAS'
					WHEN t.flag_tipo_credito = '6' then
						'FACTURA A 120 DIAS'
					WHEN t.flag_tipo_credito = '7' then
						'FACTURA A 150 DIAS'
					WHEN t.flag_tipo_credito = '8' then
						'FACTURA A 180 DIAS'
					WHEN t.flag_tipo_credito = '9' then
						'FACTURA A 45 DIAS'
					ELSE
						'INDEFINIDO'
				END
        when t.flag_forma_pago = 'D' then
          'DEPOSITO/ABONO BANCARIO'
       end as forma_pago,
		--texto 1
		case 
        when t.flag_forma_pago in ('E', 'N', 'H', 'D') then
          	'IMPORTE TOTAL = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto, '999,990.00')) || '.'
        when t.flag_forma_pago = 'T' then
          'TIPO TARJETA: ' || DECODE(T.TIPO_TARJETA, 'V', 'VISA', 'M', 'MASTERCARD', 'A', 'AMERICAN EXPRESS', 'S', 'ESTILOS', 'D', 'DINNERS CLUB') 
        	when t.flag_forma_pago = 'C' then
				case	
					WHEN t.flag_tipo_credito = '1' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 7, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '2' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 15, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '3' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 30, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '4' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 60, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '5' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 90, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '6' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 120, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '7' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 150, 'dd/mm/yyyy') || '.'
					WHEN t.flag_tipo_credito = '8' then
						'Fecha Venc. = ' || to_char(a.FEC_REGISTRO + 190, 'dd/mm/yyyy') || '.'
					when t.flag_tipo_Credito = 'B' then
			 			'SON ' || TRIM(TO_CHAR(T.NRO_CUOTAS)) || ' LETRA(S) DE ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago / T.NRO_CUOTAS * (1 + T.PORC_INTERES/100), '999,990.00')) || '.'
					ELSE
						null
				END

		end as texto1,
		-- texto 2
		case 
			when t.flag_forma_pago = 'E' then
				'EFECTIVO      = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago, '999,990.00')) || '.'
        	when t.flag_forma_pago = 'D' then
				'DEPOSITO      = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago, '999,990.00')) || '.'
        	when t.flag_forma_pago in ('T', 'N') then
         	'TOTAL MONTO: ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago, '999,990.00'))
        	when t.flag_forma_pago = 'C' then
				CASE
					WHEN t.flag_tipo_credito = 'A' then
			 			'INTERES FINANCIEMIENTO = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago  * T.PORC_INTERES / 100, '999,990.00')) || '.'
					when t.flag_tipo_credito in ('1', '2', '3', '4', '5', '6', '7', '8', '9') then
						'IMPORTE = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago, '999,990.00')) || '.'
				end
					
		end as texto2,
		--Nro de Pago o Tarjeta
		case 
        when t.flag_forma_pago = 'E' then
				case 
					when t.monto_pago > t.monto then 
						'VUELTO        = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago - t.monto, '999,990.00')) || '.'
					else '' 
				end  
        	when t.flag_forma_pago = 'T' then
          	'NUMERO: **** **** **** ' || TRIM(T.NRO_TJ4) || '.'
        	when t.flag_forma_pago = 'N' then
          	'APLICACION  ' || T.TIPO_REF || ' ' || T.NRO_REF
			when t.forma_pago = 'C' then
				case 
					when t.flag_tipo_Credito = 'A' then
						'MONTO FINANCIADO = ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago  * (1 + T.PORC_INTERES / 100), '999,990.00')) || '.'
				end		
       end as texto3,
		--Texto 4
		case 
        	when t.flag_forma_pago = 'C' then
				case 
					when t.flag_tipo_Credito = 'A' then
			 			'SON ' || TRIM(TO_CHAR(T.NRO_CUOTAS)) || ' LETRA(S) DE ' || trim(a.cod_moneda) || ' ' || trim(to_char(t.monto_pago / T.NRO_CUOTAS * (1 + T.PORC_INTERES/100), '999,990.00')) || '.'
				end
			else
				null
				
       end as texto4,
		--Texto 5
		case 
        	when t.flag_forma_pago = 'C' then
				case
					when t.flag_tipo_Credito = 'A' then
			 			'Primer Venc. = ' || to_char(t.primer_vencimiento, 'dd/mm/yyyy') || '.'
				end
			else
				null	
       end as texto5
from 	fs_factura_simpl_pagos t,
		fs_factura_simpl		  a
where a.nro_registro = t.nro_registro
  and t.nro_registro = :as_nro_registro" arguments=(("as_nro_registro", string)) )
