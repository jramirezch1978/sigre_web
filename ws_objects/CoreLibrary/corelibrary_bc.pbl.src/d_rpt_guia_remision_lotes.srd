 retrieve="  SELECT GUIA.COD_ORIGEN,   
         GUIA.NRO_GUIA,   
         GUIA.ALMACEN,   
         GUIA.FEC_REGISTRO,   
         GUIA.FEC_INICIO_TRASLADO,   
         GUIA.FEC_IMPRESION,   
         GUIA.FLAG_ESTADO,   
         GUIA.MOTIVO_TRASLADO,   
         GUIA.CLIENTE,   
         GUIA.CENCOS,   
         GUIA.DESTINATARIO,   
         GUIA.NOM_CHOFER,   
         GUIA.NRO_BREVETE,   
         GUIA.CERT_INSC_MTC,   
         GUIA.MARCA_VEHICULO,   
         GUIA.NRO_PLACA,   
         GUIA.NRO_PLACA_CARRETA,   
         GUIA.DESTINO,   
         GUIA.TIPO_DOC,   
         GUIA.NRO_DOC,   
         GUIA.COD_USR,   
         GUIA.PROV_TRANSP,   
         GUIA.OBS,
         USF_ALM_GUIA_REFER_VALE(GUIA.COD_ORIGEN, GUIA.NRO_GUIA)  AS REF_VALE,
         ALMACEN.DESC_ALMACEN,  
			ALMACEN.DISTRITO,
		   ALMACEN.PROVINCIA,
			ALMACEN.DEPARTAMENTO,
         PROVEEDOR_A.NOM_PROVEEDOR,   
         PROVEEDOR_A.DOC_IDENT_TRANSP,   
         ARTICULO.DESC_ART,   
         ARTICULO.UND,   
         PROVEEDOR_B.NOM_PROVEEDOR,   
         PROVEEDOR_B.RUC,   
         PROVEEDOR_A.RUC,   
         ARTICULO.COD_ART,   
         CC.FECHA_REGISTRO,   
         ALMACEN.DIRECCION,   
         CC.FORMA_PAGO,   
         FP.DESC_FORMA_PAGO,   
         VW.CANT_PROCESADA,
			VW.CANT_PROC_UND2,   
         VW.TIPO_REFER,   
         VW.NRO_REFER,
         VW.COD_MONEDA,
         VW.COSTO_MIN_TRASLADO,
			VW.NRO_LOTE
    FROM GUIA,   
         ALMACEN,   
         PROVEEDOR 					PROVEEDOR_A,   
         ARTICULO,   
         PROVEEDOR 					PROVEEDOR_B,   
         CNTAS_COBRAR  				CC,   
         FORMA_PAGO					FP,   
         vw_alm_art_x_guia_det 	VW 
   WHERE guia.prov_transp 	= proveedor_b.proveedor (+)
	  and guia.tipo_doc 		= CC.tipo_doc (+)
	  and guia.nro_doc 		= CC.nro_doc (+)
	  and CC.forma_pago 		= fP.forma_pago (+)
	  and GUIA.ALMACEN 		= ALMACEN.ALMACEN 
	  and GUIA.CLIENTE 		= PROVEEDOR_A.PROVEEDOR 
	  and VW.COD_ART 			= ARTICULO.COD_ART 
	  and VW.COD_ORIGEN 		= GUIA.COD_ORIGEN 
	  and VW.NRO_GUIA 		= GUIA.NRO_GUIA 
	  and GUIA.NRO_GUIA 		= :nro_guia 
	  and GUIA.COD_ORIGEN 	= :as_origen  " arguments=(("as_origen", string),("nro_guia", string)) )
