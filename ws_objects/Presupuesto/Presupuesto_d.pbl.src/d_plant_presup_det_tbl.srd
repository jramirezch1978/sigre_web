 retrieve="  SELECT FPPD.COD_FL_PLANTILLA,   
         FPPD.CNTA_PRSP,   
			FPPD.CENCOS,
			FPPD.ITEM,
			FPPD.DESCRIPCION,
			CC.DESC_CENCOS,
         FPPD.RATIO,   
         PC.DESCRIPCION,
			FPPD.tipo_prtda_prsp,
			t.desc_tipo_prsp,
			fppd.centro_benef,
			cb.desc_centro,
			fppd.flag_ingreso_egreso,
			fppd.flag_tipo_compromiso,
			FPPD.FLAG_REPLICACION  
    FROM FL_PLANT_PRESUP_DET 	FPPD,   
         PRESUPUESTO_CUENTA 	PC,
			CENTROS_COSTO 			CC,
		   tipo_prtda_prsp_det  t, 
			centro_beneficio		cb 
   WHERE FPPD.CNTA_PRSP = PC.CNTA_PRSP
	  AND CC.CENCOS 		= FPPD.CENCOS
	  and FPPD.tipo_prtda_prsp = t.tipo_prtda_prsp (+)
	  and fppd.centro_benef		= cb.centro_benef (+)
	  and FPPD.Cod_fl_plantilla = :as_plantilla
order by item
	
" update="FL_PLANT_PRESUP_DET" updatewhere=0 updatekeyinplace=yes arguments=(("as_plantilla", string)) )
