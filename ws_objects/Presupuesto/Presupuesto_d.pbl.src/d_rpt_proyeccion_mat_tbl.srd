 retrieve="SELECT OT.CENCOS_SLC, 
		 cc.desc_cencos as desc_cencos_slc,
		 OT.CENCOS_RSP, OT.NRO_ORDEN,
       AMP.FEC_PROYECT, 
       A.COD_ART,
       A.DESC_ART,
       A.UND,
       AMP.CANT_PROYECT,
       AMP.PRECIO_UNIT * amp.cant_proyect as importe,
       amp.flag_estado,
       ot.centro_benef,
       amp.cencos,
       amp.cnta_prsp,
       pd.cantidad,
		 ot.fec_inicio,
       pd.costo_unit * pd.cantidad as importe_prsp
FROM orden_trabajo     ot,
     articulo_mov_proy amp,
     articulo          a,
     presupuesto_det   pd,
	  tt_ope_cencos	  tt,
	  centros_costo	  cc     
WHERE ot.nro_orden     = amp.nro_doc
  AND amp.tipo_doc     = (SELECT doc_ot FROM logparam WHERE reckey = '1')
  AND amp.cod_art      = a.cod_art    
  and ot.cencos_slc	  = cc.cencos 
  AND AMP.COD_ORIGEN   = PD.ORG_AMP_OT (+)
  AND amp.nro_mov      = pd.nro_amp_ot (+)
  AND amp.flag_estado  IN ('1', '2', '3')
  and trunc(amp.fec_proyect) between :ad_fecha1 and :ad_fecha2
  and ( (:as_tipo_cencos = '1' and ot.cencos_slc = tt.cencos) or
		(:as_tipo_cencos = '2' and ot.cencos_rsp = tt.cencos) )
order by ot.cencos_slc, ot.cencos_rsp, amp.fec_proyect" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_tipo_cencos", string)) )
