 retrieve="  SELECT PS.NRO_SOLICITUD,
         PS.FLAG_TIPO_SOL,   
		   PS.FEC_REGISTRO,
         PS.FLAG_ESTADO,   
			PS.DESCRIPCION,
			PS.USR_SOLICITANTE,
			PS.FLAG_REPLICACION,
			PS.USR_APROBACION,
			PS.FECHA_APROBACION
    FROM PRSP_SOLICITUD_VAR 	   PS
   WHERE substr(PS.NRO_SOLICITUD,1,2) = :as_origen
	  and ps.flag_estado = '2'
order by PS.FEC_REGISTRO
			    
