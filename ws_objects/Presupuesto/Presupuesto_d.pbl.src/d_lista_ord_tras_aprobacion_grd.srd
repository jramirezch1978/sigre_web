 retrieve="  SELECT DISTINCT OT.NRO_OTR,   
         OT.FLAG_ESTADO,   
         OT.FEC_REGISTRO,   
         OT.ALMACEN_ORG as almacen_org,   
         ALMACEN_A.DESC_ALMACEN,   
         OT.ALMACEN_DST as almacen_dst,   
         ALMACEN_B.DESC_ALMACEN,   
         OT.TIPO_DOC,   
         OT.NRO_DOC,   
         OT.USR_REGISTRA,   
         OT.USR_AUTORIZA,
			OT.FLAG_REPLICACION 
    FROM ORDEN_TRASLADO OT,   
         ALMACEN ALMACEN_A,   
         ALMACEN ALMACEN_B,
			ARTICULO_MOV_PROY AMP 
   WHERE OT.ALMACEN_ORG = ALMACEN_A.ALMACEN 
	  and OT.ALMACEN_DST = ALMACEN_B.ALMACEN 
	  AND AMP.NRO_DOC 	= OT.NRO_OTR
 	  AND AMP.TIPO_DOC	= (SELECT DOC_OTR FROM LOGPARAM WHERE RECKEY = '1')
	  AND NVL(AMP.CANT_FACTURADA,0) = 0
	  AND NVL(AMP.CANT_PROCESADA,0) = 0
	  and OT.FLAG_ESTADO 			= :as_flag_estado
     and substr(OT.NRO_OTR,1,2) = :as_origen
			    
" update="ORDEN_TRASLADO" updatewhere=0 updatekeyinplace=yes arguments=(("as_origen", string),("as_flag_estado", string)) )
