         ~"PRESUP_PRODUCCION_VAR~".~"COD_ART~",   
         ~"PRESUP_PRODUCCION_VAR~".~"MES~",   
         ~"PRESUP_PRODUCCION_VAR~".~"FECHA~",   
         ~"PRESUP_PRODUCCION_VAR~".~"CANTIDAD~",   
         ~"PRESUP_PRODUCCION_VAR~".~"TIPO_VARIACION~",   
         ~"PRESUP_PRODUCCION_VAR~".~"DESCRIPCION~",   
         ~"PRESUP_PRODUCCION_VAR~".~"COD_USR~",   
         ~"PRESUP_PRODUCCION_VAR~".~"FLAG_REPLICACION~",   
			~"PRESUP_PRODUCCION_VAR~".~"FLAG_AUTOMATICO~",   
         ~"ARTICULO~".~"UND~"  
    FROM ~"PRESUP_PRODUCCION_VAR~",   
         ~"CENTROS_COSTO~",   
         ~"ARTICULO~"  
   WHERE ( ~"PRESUP_PRODUCCION_VAR~".cencos = centros_costo.cencos (+)) and  
         ( ~"PRESUP_PRODUCCION_VAR~".cod_art = articulo.cod_art (+)) and  
         ( ( ~"PRESUP_PRODUCCION_VAR~".~"NRO_VARIACION~" = :as_nro ) )    
