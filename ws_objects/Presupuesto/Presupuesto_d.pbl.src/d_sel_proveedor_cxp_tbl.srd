table(column=(type=char(8) updatewhereclause=yes name=proveedor dbname="proveedor.proveedor" dbalias="p.proveedor" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="proveedor.nom_proveedor" dbalias="p.nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 retrieve="select /*+ PARALLEL(8) */
       distinct 
       p.proveedor,
       p.nom_proveedor,
       decode(p.ruc, null, p.nro_doc_ident, p.ruc) as ruc_dni
       
from vw_fin_pendiente_pagar cp,
     proveedor              p
where cp.cod_relacion = p.proveedor
  and CP.FLAG_ESTADO  in ('1', '2')
  --and cp.flag_control_reg = '0'
  and trunc(cp.vencimiento) <= trunc(:ad_fecha)
  and case when cp.cod_moneda = pkg_logistica.of_soles(null) then cp.saldo_sol else cp.saldo_dol end > 0
  and case when cp.cod_moneda = pkg_logistica.of_soles(null) then cp.saldo_sol else cp.saldo_dol end - 
    (select nvl(sum(pd.imp_proyectado),0) 
        from prsp_caja_det pd
      where pd.proveedor = cp.cod_relacion
        and pd.tipo_doc  = cp.tipo_doc
       and pd.nro_doc    = cp.nro_doc) > 0" arguments=(("ad_fecha", date))  sort="nom_proveedor A " )
