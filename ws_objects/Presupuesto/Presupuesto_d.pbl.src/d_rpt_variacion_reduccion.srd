 retrieve="SELECT ~"PRESUP_VARIACION~".~"CENCOS_ORIGEN~",   
         ~"PRESUP_VARIACION~".~"CNTA_PRSP_ORIGEN~",   
         ~"PRESUP_VARIACION~".~"FECHA~",   
         ~"PRESUP_VARIACION~".~"IMPORTE~",   
         ~"PRESUP_VARIACION~".~"DESCRIPCION~",   
         ~"CENTROS_COSTO~".~"DESC_CENCOS~",   
         ~"PRESUPUESTO_CUENTA~".~"DESCRIPCION~",   
         ~"PRESUP_VARIACION~".~"ANO~",   
         ~"PRESUP_VARIACION~".~"MES_ORIGEN~",   
         ~"PRESUP_VARIACION~".~"NRO_VARIACION~",   
         ~"TIPO_PRTDA_PRSP_DET~".~"DESC_TIPO_PRSP~",   
         ~"PRESUPUESTO_PARTIDA~".~"TIPO_PRTDA_PRSP~"  
    FROM ~"PRESUP_VARIACION~",   
         ~"CENTROS_COSTO~",   
         ~"PRESUPUESTO_CUENTA~",   
         ~"TT_PTO_CENCOS~",   
         ~"TT_PTO_CNTA_PRSP~",   
         ~"PRESUPUESTO_PARTIDA~",   
         ~"TIPO_PRTDA_PRSP_DET~"  
   WHERE ( ~"PRESUP_VARIACION~".~"CENCOS_ORIGEN~" = ~"CENTROS_COSTO~".~"CENCOS~" ) and  
         ( ~"PRESUP_VARIACION~".~"CNTA_PRSP_ORIGEN~" = ~"PRESUPUESTO_CUENTA~".~"CNTA_PRSP~" ) and  
         ( ~"PRESUP_VARIACION~".~"CENCOS_ORIGEN~" = ~"TT_PTO_CENCOS~".~"CENCOS~" ) and  
         ( ~"PRESUP_VARIACION~".~"CNTA_PRSP_ORIGEN~" = ~"TT_PTO_CNTA_PRSP~".~"CNTA_PRSP~" ) and  
         ( ~"PRESUP_VARIACION~".~"ANO~" = ~"PRESUPUESTO_PARTIDA~".~"ANO~" ) and  
         ( ~"PRESUP_VARIACION~".~"CENCOS_ORIGEN~" = ~"PRESUPUESTO_PARTIDA~".~"CENCOS~" ) and  
         ( ~"PRESUP_VARIACION~".~"CNTA_PRSP_ORIGEN~" = ~"PRESUPUESTO_PARTIDA~".~"CNTA_PRSP~" ) and  
         ( ~"PRESUPUESTO_PARTIDA~".~"TIPO_PRTDA_PRSP~" = ~"TIPO_PRTDA_PRSP_DET~".~"TIPO_PRTDA_PRSP~" ) and  
         ( ( ~"PRESUP_VARIACION~".~"TIPO_VARIACION~" = 'R' ) AND  
         ( TRUNC(~"PRESUP_VARIACION~".~"FECHA~") between trunc(:ad_fecha1) and trunc(:ad_fecha2) ) )   
ORDER BY ~"TIPO_PRTDA_PRSP_DET~".~"TIPO_PRTDA_PRSP~" ASC,   
         ~"PRESUP_VARIACION~".~"FECHA~" ASC   
" arguments=(("ad_fecha1", date),("ad_fecha2", date)) )
