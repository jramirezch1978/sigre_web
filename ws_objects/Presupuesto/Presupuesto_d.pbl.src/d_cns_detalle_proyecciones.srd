 retrieve="  SELECT PD.CENCOS,
			CC.DESC_CENCOS,   
			PD.CNTA_PRSP,
         PC.DESCRIPCION,   
         PD.FECHA,   
         PD.CANTIDAD,
			PD.COSTO_UNIT,   
         PD.COMENTARIO,
         PD.NRO_ASIGNACION,   
         PD.FLAG_PROCESO,   
         PD.MES_CORRESP AS MES,   
			PD.COD_ART,
			A.DESC_ART,
         PD.ANO   ,
			PD.SERVICIO,
			S.DESCRIPCION,
			PP.tipo_prtda_prsp,
			tpd.desc_tipo_prsp,
			PP.FLAG_CTRL,
			PD.CENTRO_BENEF
    FROM CENTROS_COSTO			CC,   
         PRESUPUESTO_CUENTA	PC,   
         PRESUPUESTO_DET		PD,
			ARTICULO					A ,
			SERVICIOS				S ,
			PRESUPUESTO_PARTIDA	PP,
			tipo_prtda_prsp_det	tpd
   WHERE CC.CENCOS 	 	= PD.CENCOS 
     and PC.CNTA_PRSP   = PD.CNTA_PRSP 
     and PD.COD_ART		= A.COD_ART   (+)
	  and PD.SERVICIO		= S.SERVICIO  (+)
	  and pp.tipo_prtda_prsp = tpd.tipo_prtda_prsp (+)
	  and PP.CENCOS 		= CC.CENCOS
	  AND PP.CNTA_PRSP	= PC.CNTA_PRSP
	  AND PP.ANO 			= :an_ano 
  	  AND PD.ANO 		   = :an_ano 
	  AND PD.MES_CORRESP = :an_mes 
     AND PD.CENCOS 		= :as_cencos 
	  AND PD.CNTA_PRSP 	= :as_cnta_prsp
" arguments=(("an_ano", number),("an_mes", number),("as_cencos", string),("as_cnta_prsp", string)) )
