 retrieve="Select distinct 
	 to_number(to_char(amp.fec_proyect, 'YYYY')) as year,
	 amp.tipo_doc,
	 amp.nro_doc,
	 ot.titulo,
	 amp.fec_proyect as fec_proyect,
    op.oper_sec,
    op.cod_labor,
    amp.cencos,
    amp.cnta_prsp,
    usf_cmp_prec_ult_compra(amp.cod_art, amp.almacen) as costo_unit,
    NVL(amp.cant_proyect,0) - NVL(amp.cant_procesada,0) as cantidad,
    a.cod_art,
    a.desc_art,
    NVL(a.flag_inventariable, '0') as flag_inventariable,
    NVL(a.flag_estado, '0') as flag_estado,
    'MATERIALES DE LA OT' as obs
  from articulo_mov_proy amp , 
       articulo          a,
		 orden_trabajo		 ot,
       operaciones       op,
		 tt_pto_selec_ots	 b
 where amp.cod_art = a.cod_art 
   and ot.nro_orden = amp.nro_doc
   and amp.TIPO_DOC = (SELECT DOC_OT FROM logparam WHERE RECKEY = '1' )
   and op.nro_orden = amp.nro_doc
   and amp.tipo_mov = (select l.oper_cons_interno from logparam l where reckey = '1')
   and NVL(amp.flag_estado, '0') in ('1', '3')
   and to_char(amp.fec_proyect, 'yyyy') = to_char(:ani_year)
   and ot.ot_adm like :asi_ot_adm
   and ot.nro_orden = b.nro_doc
   and (amp.cencos is null or amp.cnta_prsp is null
        or usf_cmp_prec_ult_compra(amp.cod_art, amp.almacen) = 0
        or NVL(amp.cant_proyect,0) < NVL(amp.cant_procesada,0)
        or NVL(a.flag_inventariable, '0') = '0'
        or NVL(a.flag_estado, '0') = '0'
		  or amp.fec_proyect is null )
union
Select distinct 
	 to_number(to_char(op.fec_inicio, 'YYYY')) as year,
	 (select doc_ot from logparam where reckey = '1') as tipo_doc,
	 ot.nro_orden as nro_doc,
	 ot.titulo,
	 op.fec_inicio as fec_proyect,
    op.oper_sec,
    op.cod_labor,
    ot.cencos_slc as cencos,
    l.cnta_prsp_3ro as cnta_prsp,
    le.costo_unitario as costo_unit,
    op.cant_proyect as cantidad,
    '' as cod_art,
    '' as desc_art,
    '' as flag_inventariable,
    '' as flag_estado,
    'SERVICIOS DE LA OT' as obs
 from operaciones    op, 
      labor_ejecutor le,
      labor          l,
      orden_trabajo	 ot,
		tt_pto_selec_ots b
 where l.cod_labor     = op.cod_labor
   and le.cod_labor    = op.cod_labor
   and le.cod_ejecutor = op.cod_ejecutor
   and op.nro_orden    = ot.nro_orden
   and NVL(op.flag_estado, '0') = '3'
   and to_char(op.fec_inicio, 'yyyy') = to_char(:ani_year)
   and ot.ot_adm like :asi_ot_adm
	and ot.nro_orden = b.nro_doc
   and le.cod_ejecutor = (select p.ejecutor_3ro from prod_param p where reckey = '1')
   and (ot.cencos_slc is null or l.cnta_prsp_3ro is null
       or NVL(le.costo_unitario,0) = 0 
		 or NVL(op.cant_proyect,0) = 0
		 or op.fec_inicio_est is null)" arguments=(("ani_year", number),("asi_ot_adm", string))  sort="obs A " )
