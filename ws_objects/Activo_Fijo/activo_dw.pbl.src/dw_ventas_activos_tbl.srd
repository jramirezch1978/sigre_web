        AFV.TIPO_DOC,   
        AFV.NRO_DOC, 
        CC.FECHA_DOCUMENTO,
        CC.COD_MONEDA,
        CC.IMPORTE_DOC,
        CC.OBSERVACION,
        CC.COD_USR,
        AFV.FLAG_REPLICACION
        
