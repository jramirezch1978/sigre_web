 retrieve="SELECT   AFM.COD_ORIGEN,
         AFM.COD_ACTIVO,   
         AFM.FLAG_ESTADO,   
         AFM.DESCRIPCION,   
         AFM.CENCOS,   
         AFM.IMAGEN,   
         AFM.COD_MARCA,   
         AFM.MODELO,   
         AFM.NRO_SERIE,   
         AFM.cod_sub_clase,   
         AFM.NRO_CONTRATO,   
         AFM.COD_MAQUINA,   
         AFM.FECHA_ADQ,   
         AFM.TIPO_DOC_ADQ,   
         AFM.NRO_DOC_ADQ,   
         AFM.PROVEEDOR,
         P.NOM_PROVEEDOR,   
         AFM.COD_MONEDA,
         MON.DESCRIPCION,   
         AFM.VALOR_ADQ_SOL,   
         AFM.VALOR_ADQ_DOL,   
         AFM.FLAG_DEPRECIACION,   
         AFM.FLAG_MOD_ADQ,   
         AFM.FECHA_INI_GARANTIA,   
         AFM.FECHA_FIN_GARANTIA,   
         AFM.FLAG_ADAPTACION,   
         AFM.FECHA_BAJA,   
         AFM.OBS,   
         AFM.COD_USR,
         AFM.USR_ASIGNADO,   
         U.NOMBRE AS NOM_USR_ASIGNADO,
         AFM.FLAG_REPLICACION,   
         CC.DESC_CENCOS,   
         M.NOM_MARCA,  
         AFC.COD_CLASE ,
         AFC.DESC_CLASE,   
         SC.DESC_SUB_CLASE,
         MQ.DESC_MAQ,   
         AFM.FECHA_INI_DEPRECIACION,   
         AFM.GASTOS_VINCULADOS_SOL,   
         AFM.UBICACION,   
         AFU.DESCRIPCION,
         AFM.ITEM_UBICACION,
         AFUD.DESCRIPCION,
         AFM.GASTOS_VINCULADOS_DOL,
         AFM.PROVEEDOR_ASIGNADO,
         AFM.PROV_TASADOR,
         AFM.FECHA_TASADOR,
         AFM.MONEDA_TASAC,
         AFM.PRECIO_TASACION,
         AFM.TASA_DEP,
         AFM.TASA_DEP_TRIB,
         AFM.MESES_DEP,
         AFM.FLAG_PROCEDENCIA,
         AFM.FLAG_OPERATIVIDAD,
         AFM.FLAG_CONDICION,
         AFM.ANO_FABRICACION,
         AFM.MATERIAL_FABRICAC,
         AFM.CAPACIDAD,
         AFM.DIMENSIONES,
      	AFM.MONTO_RESIDUAL_SOL,
         P1.NOM_PROVEEDOR AS NOM_PROV_ASIGNADO,
         P2.NOM_PROVEEDOR AS NOM_PROV_TASADOR
         
FROM     AF_MAESTRO            AFM,   
         CENTROS_COSTO         CC,   
         MARCA                 M,   
         AF_CLASE              AFC,
         AF_SUB_CLASE          SC,   
         MAQUINA               MQ,
         PROVEEDOR             P,
         PROVEEDOR             P1,
         PROVEEDOR             P2,
         MONEDA                MON,
         USUARIO               U,
         AF_UBICACION          AFU,
         AF_UBICACION_DET      AFUD
         
WHERE   AFM.cencos            = CC.cencos        (+) 
  AND   AFM.cod_marca         = M.cod_marca      (+)
  AND   AFM.Cod_Sub_Clase     = SC.cod_sub_clase (+)
  AND   SC.COD_CLASE          = AFC.COD_CLASE    (+)
  AND   AFM.cod_maquina       = MQ.cod_maquina   (+)
  AND   AFM.cod_moneda        = MON.cod_moneda   (+)
  AND   AFM.usr_asignado      = U.cod_usr        (+)
  AND   AFM.PROVEEDOR          = P.proveedor     (+)
  AND   AFM.UBICACION         = AFU.UBICACION    (+)
  AND   AFM.UBICACION         = AFUD.UBICACION   (+)
  AND   AFM.ITEM_UBICACION    = AFUD.ITEM        (+)
  AND   AFM.PROVEEDOR_ASIGNADO= P1.PROVEEDOR     (+)
  AND   AFM.PROV_TASADOR      = P2.PROVEEDOR     (+)
  AND   AFM.COD_ACTIVO      = :as_activo" update="AF_MAESTRO" updatewhere=0 updatekeyinplace=yes arguments=(("as_activo", string)) )
