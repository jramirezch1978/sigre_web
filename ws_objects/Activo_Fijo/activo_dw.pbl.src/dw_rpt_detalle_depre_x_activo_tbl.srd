 retrieve="SELECT AFC.COD_ACTIVO,  
       AFM.DESCRIPCION,
       AFM.TASA_DEP,
       AFM.VALOR_ADQ_SOL,
       AFC.ANO,
       SUM(NVL(AFC.VALOR_PRODUCCION,0)) AS DEPRE_PROD,
       SUM(NVL(AFC.VALOR_VEDA,0))       AS DEPRE_VEDA

FROM   AF_CALCULO_CNTBL  AFC,
       AF_MAESTRO        AFM

WHERE  AFC.COD_ACTIVO = AFM.COD_ACTIVO
  AND  AFC.COD_ACTIVO = :AS_COD_ACTIVO
  
GROUP BY AFC.COD_ACTIVO,  
         AFM.DESCRIPCION,
         AFM.TASA_DEP,
         AFM.VALOR_ADQ_SOL,
         AFC.ANO
" arguments=(("AS_COD_ACTIVO", string)) )
