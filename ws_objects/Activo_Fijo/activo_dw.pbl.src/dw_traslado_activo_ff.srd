 retrieve="SELECT  AFT.NRO_MOVIMIENTO,
        AFT.COD_ACTIVO ,
        AFM.DESCRIPCION,
        AFT.FECHA_REGISTRO ,
        AFT.CENCOS_ORIGEN,
        CCA.DESC_CENCOS,
        AFT.CENCOS_DESTINO ,
        CCB.DESC_CENCOS,
        AFT.USR_AUTORIZACION ,
        U.NOMBRE,
        AFT.USR_REGISTRA ,
        AFT.FECHA_MOVIMIENTO ,
        AFT.FECHA_AUTORIZACION ,
        AFT.FLAG_TIPO_DESTINO ,
        AFT.FLAG_TEMPORALIDAD ,
        AFT.NRO_DOC_FISICO ,
        AFT.OBS ,
        AFT.USR_ORIGEN,
        U2.NOMBRE,
        AFT.USR_DESTINO,
        U3.NOMBRE,
        AFT.FLAG_REPLICACION     
        
FROM    AF_TRASLADO          AFT,
        CENTROS_COSTO        CCA,
        CENTROS_COSTO        CCB,
        AF_MAESTRO           AFM,
        USUARIO              U,
        USUARIO              U2,
        USUARIO              U3

WHERE   AFT.CENCOS_ORIGEN    = CCA.CENCOS
   AND  AFT.CENCOS_DESTINO   = CCB.CENCOS
   AND  AFT.COD_ACTIVO       = AFM.COD_ACTIVO
   AND  AFT.USR_AUTORIZACION = U.COD_USR  (+)
   AND  AFT.USR_ORIGEN       = U2.COD_USR (+)
   AND  AFT.USR_DESTINO      = U3.COD_USR (+)" update="AF_TRASLADO" updatewhere=0 updatekeyinplace=yes )
