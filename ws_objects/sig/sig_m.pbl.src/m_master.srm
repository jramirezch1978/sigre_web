public function integer of_open (string as_window, string as_formato);Window 	lw_window
Integer	li_rc

IF as_formato = 'L' OR as_formato = 'l' THEN
	li_rc = OpenSheet(lw_window, as_window,ParentWindow,0,Layered!)
ELSE
	li_rc = OpenSheet(lw_window, as_window,ParentWindow,0,Original!)
END IF

IF li_rc <> 1 THEN MessageBox('Error', 'No se pudo abrir la ventana: ' + as_window)

RETURN li_rc
end function

public function integer of_open (string as_window);Window 	lw_window
Integer	li_rc

li_rc = OpenSheet(lw_window, as_window,ParentWindow,0,Original!)

IF li_rc <> 1 THEN MessageBox('Error', 'No se pudo abrir la ventana: ' + as_window)

RETURN li_rc
end function

