 retrieve="SELECT al.almacen, alm.desc_almacen, al.cod_art, aa.nom_articulo, aa.und, 
       ROUND(al.sldo_total,2) as stk_und, 
       ROUND(al.sldo_total*aa.peso_und/:an_factor,2) as stk_bolsa
  FROM articulo aa, articulo_almacen al, rel_articulo_grupo ra, almacen alm
 WHERE aa.cod_art = al.cod_art AND 
       aa.cod_art = ra.cod_art AND
       al.almacen = alm.almacen AND
       ra.grupo_art = :as_grupo AND
       al.sldo_total > 0 AND
       aa.peso_und > 0
order by al.almacen, al.cod_art " arguments=(("as_grupo", string),("an_factor", number)) )
