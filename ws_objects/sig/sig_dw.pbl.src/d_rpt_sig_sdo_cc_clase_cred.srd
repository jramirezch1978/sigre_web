 retrieve="select distinct cc.cod_relacion, p.nom_proveedor, cc.tipo_doc, cc.nro_doc, 
       cc.forma_pago, cc.fecha_vencimiento, cc.cod_moneda, cc.importe_doc, 
		 dp.saldo_dol
  from cntas_cobrar cc, cntas_cobrar_det ccd, forma_pago fp, articulo a, 
       doc_pendientes_cta_cte dp, proveedor p
 where (cc.tipo_doc = ccd.tipo_doc and
        cc.nro_doc = ccd.nro_doc) and
       (cc.cod_relacion = p.proveedor and
        dp.cod_relacion = p.proveedor ) and
       (cc.cod_relacion = dp.cod_relacion and
        cc.tipo_doc = dp.tipo_doc and
        cc.nro_doc = dp.nro_doc ) and
       (cc.tipo_doc = (select f.doc_fact_cobrar from finparam f where reckey='1') or
        cc.tipo_doc = (select f.doc_bol_cobrar from finparam f where reckey='1') ) and
       cc.flag_estado <> '0' and
      ( (cc.cod_moneda = (select cod_soles from logparam where reckey='1') and dp.sldo_sol  > 0) or
        (cc.cod_moneda = (select cod_dolares from logparam where reckey='1') and dp.saldo_dol > 0) ) and 
       cc.cod_relacion not in (select gpr.proveedor 
                                 from grupo_proveedor_rel gpr 
                                where gpr.grupo=:as_grupo_codrel) AND                 
       cc.forma_pago = fp.forma_pago and
       fp.dias_vencimiento>0 and
       ccd.cod_art = a.cod_art and
       a.cod_clase = :as_clase
order by cc.cod_relacion, cc.tipo_doc, cc.nro_doc
" arguments=(("as_grupo_codrel", string),("as_clase", string)) )
