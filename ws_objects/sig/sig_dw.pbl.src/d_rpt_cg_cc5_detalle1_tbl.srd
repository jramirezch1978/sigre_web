 column=(type=number updatewhereclause=yes name=cant_original dbname="cant_original" )
 retrieve="     SELECT o.cod_origen, o.NOMBRE,
				al.almacen,
				al.desc_almacen,
				a.cod_art,
			   a.desc_Art,
				a.und,
				a.und2,
				ag.und_reporte,
				DECODE(ag.und_reporte, a.und, NVL(sum(am.cant_procesada * amt.factor_sldo_total), 0), 
                    a.und2, NVL(sum(am.cant_proc_und2 * amt.factor_sldo_total), 0), 
             		usf_ap_conv_und(a.und, ag.und_reporte, 
							NVL(Sum(am.cant_procesada * amt.factor_sldo_total), 0))) as cant_reporte,
				NVL(sum(am.cant_procesada * amt.factor_sldo_total), 0) as cant_original       FROM vale_mov     		 vm,
            articulo_mov 		 am,
            articulo     		 a,
            almacen      		 al,
				origen		 		 o,
            articulo_mov_tipo  amt,
            rel_articulo_grupo rag,
				articulo_grupo		 ag
      WHERE vm.nro_vale = am.nro_vale
        AND a.cod_art   = am.cod_art
        AND rag.cod_art = am.cod_art
        AND amt.tipo_mov  = vm.tipo_mov
        AND al.almacen    = vm.almacen
		  and o.cod_origen  = al.cod_origen
		  and ag.grupo_art  = rag.grupo_art
        AND trunc(vm.fec_registro) <= trunc(:ad_fecha)
        AND vm.flag_estado <> '0'
        AND am.flag_estado <> '0'
        AND rag.grupo_art  = :as_grupo_art
group by o.cod_origen, o.NOMBRE,
				al.almacen,
				al.desc_almacen,
				a.cod_art,
			   a.desc_Art,
				a.und,
				a.und2,
				ag.und_reporte
having DECODE(ag.und_reporte, a.und, NVL(sum(am.cant_procesada * amt.factor_sldo_total), 0), 
                    a.und2, NVL(sum(am.cant_proc_und2 * amt.factor_sldo_total), 0), 
             		usf_ap_conv_und(a.und, ag.und_reporte, 
							NVL(Sum(am.cant_procesada * amt.factor_sldo_total), 0))) <> 0


