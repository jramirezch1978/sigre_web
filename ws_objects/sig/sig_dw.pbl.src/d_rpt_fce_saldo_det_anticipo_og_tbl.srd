 retrieve="select cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, cad.nro_docref1, 
       sg.usuario_aprob, cad.cnta_ctbl, sg.fecha_aprobacion,
       sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) as tot_dol
from cntbl_asiento ca, cntbl_asiento_det cad, solicitud_giro sg, proveedor p
where ( ca.origen=cad.origen and
        ca.ano=cad.ano and
        ca.mes=cad.mes and
        ca.nro_libro=cad.nro_libro and
        ca.nro_asiento=cad.nro_asiento ) and
      (cad.cod_relacion = p.proveedor ) and
      (sg.cod_relacion = p.proveedor ) and
      ( cad.cod_relacion = sg.cod_relacion and
        trim(cad.nro_docref1) = sg.nro_solicitud ) and
      ( ca.flag_estado<>'0' and
        ca.ano= :an_ano ) and
      ( substr(cad.cnta_ctbl,1,2)=:as_grupo_contable and
        cad.cod_relacion is not null and
        cad.tipo_docref1=:as_tipo_doc and
        cad.nro_docref1 is not null ) 
group by cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, cad.nro_docref1, 
       sg.usuario_aprob, cad.cnta_ctbl, sg.fecha_aprobacion
having sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) <>0
order by cad.cod_relacion, sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) desc
        " arguments=(("as_tipo_doc", string),("as_grupo_contable", string),("an_ano", number)) )
