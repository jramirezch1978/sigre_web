 retrieve="select dp.tipo_doc, dp.nro_doc, dp.cod_relacion, p.nom_proveedor, dp.fecha_vencimiento, dp.saldo_dol as tot_dolar
from cntas_pagar cp, doc_pendientes_cta_cte dp, proveedor p
where ( cp.cod_relacion=dp.cod_relacion and
        cp.tipo_doc=dp.tipo_doc and
        cp.nro_doc=dp.nro_doc) and
      ( cp.cod_relacion = p.proveedor and
        dp.cod_relacion = p.proveedor) and
      ( ( dp.cod_moneda = (SELECT cod_soles from logparam where reckey='1') AND dp.sldo_sol > 0) OR 
         ( dp.cod_moneda = (SELECT cod_dolares from logparam where reckey='1') AND dp.saldo_dol > 0) ) and        
        dp.tipo_doc = :as_tipo_doc and
      dp.cnta_ctbl is not null and
      cp.tipo_doc in (select tipo_doc from doc_grupo dg, doc_grupo_relacion dgr
where dg.grupo=dgr.grupo and dg.grupo= :as_grupo_doc)
order by dp.cod_relacion, dp.saldo_dol desc" arguments=(("as_tipo_doc", string),("as_grupo_doc", string)) )
