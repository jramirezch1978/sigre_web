table(column=(type=number updatewhereclause=yes name=tonel_azuc dbname="tonel_azuc" )
 column=(type=number updatewhereclause=yes name=tonel_proy dbname="tonel_proy" )
 column=(type=number updatewhereclause=yes name=ratio dbname="ratio" )
 retrieve="SELECT a.tonel_azuc, b.tonel_proy, 
	decode(:an_dias2,0,0,(b.tonel_proy*:an_dias1/:an_dias2)) as ratio
from 
  (Select sum( nvl(am.cant_procesada,0) * nvl(a.peso_und,0) ) / 1000 as tonel_azuc
    from vale_mov vm, articulo_mov am, articulo a
   where vm.cod_origen = am.cod_origen and 
         vm.nro_vale = am.nro_vale and
         vm.tipo_mov = (select l.ingreso_pptt from labparam l where reckey='1') and 
         vm.almacen = :as_almacen and 
         vm.flag_estado <> '0' and
         (vm.fec_registro between :ad_fec_ini and :ad_fec_fin) and
         am.cod_art  = a.cod_art and
         am.flag_estado <> '0' and
         a.cod_clase=(select clase_prod_term from sig_agricola where reckey='1')) a, 
   (select case 
            when to_number(to_char(sysdate,'mm'))=1 then ppu.proy_ene 
            when to_number(to_char(sysdate,'mm'))=2 then ppu.proy_feb
            when to_number(to_char(sysdate,'mm'))=3 then ppu.proy_mar
            when to_number(to_char(sysdate,'mm'))=4 then ppu.proy_abr
            when to_number(to_char(sysdate,'mm'))=5 then ppu.proy_may
            when to_number(to_char(sysdate,'mm'))=6 then ppu.proy_jun
            when to_number(to_char(sysdate,'mm'))=7 then ppu.proy_jul
            when to_number(to_char(sysdate,'mm'))=8 then ppu.proy_ago
            when to_number(to_char(sysdate,'mm'))=9 then ppu.proy_set
            when to_number(to_char(sysdate,'mm'))=10 then ppu.proy_oct
            when to_number(to_char(sysdate,'mm'))=11 then ppu.proy_nov
            when to_number(to_char(sysdate,'mm'))=12 then ppu.proy_dic
           else 0 
           end as tonel_proy
 from presup_produccion_und ppu
where ppu.ano=:an_ano and ppu.cod_art=:as_cod_art) b " arguments=(("ad_fec_ini", datetime),("ad_fec_fin", datetime),("as_almacen", string),("as_cod_art", string),("an_ano", number),("an_dias1", number),("an_dias2", number)) )
