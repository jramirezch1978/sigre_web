 retrieve="select c.cnta_ctbl, c.desc_cnta, sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) as tot_dolar
       from cntbl_asiento ca, cntbl_asiento_det cad, cntbl_cnta c
 where ( ca.origen=cad.origen and
         ca.ano=cad.ano and
         ca.mes=cad.mes and
         ca.nro_libro=cad.nro_libro and
         ca.nro_asiento=cad.nro_asiento ) and
       ( ca.flag_estado<>'0' and
         ca.ano= :an_ano ) and
       (cad.cnta_ctbl = c.cnta_ctbl ) and
       ( cad.cnta_ctbl in (select cnta_ctbl from rpt_subgrupo_det r
                           where r.reporte=:as_reporte and
                                 r.grupo=:as_grupo and
                                 r.subgrupo=:as_subgrupo) and
         cad.cod_relacion is not null and
         cad.tipo_docref1 is not null and
         cad.nro_docref1 is not null ) 
group by c.cnta_ctbl, c.desc_cnta
" arguments=(("an_ano", number),("as_reporte", string),("as_grupo", string),("as_subgrupo", string)) )
