 retrieve="SELECT C.COD_TRABAJADOR,
        P.NOM_PROVEEDOR,
         C.COD_MONEDA,
         SUM(C.MONT_ORIGINAL) as monto_original,
         SUM(C.SLDO_PRESTAMO) as saldo
    FROM CNTA_CRRTE C,   
         PROVEEDOR P,   
         MAESTRO M  
   WHERE ( C.COD_TRABAJADOR = P.PROVEEDOR ) and  
         ( C.COD_TRABAJADOR = M.COD_TRABAJADOR ) and  
         ( M.COD_TRABAJADOR = P.PROVEEDOR ) and  
         (m.flag_estado = '1' AND  
          m.flag_cal_plnlla = '1' ) AND  
          c.sldo_prestamo > 0   
GROUP BY C.COD_TRABAJADOR, P.NOM_PROVEEDOR, C.COD_MONEDA
ORDER BY SUM(C.SLDO_PRESTAMO) DESC" )
