 retrieve="SELECT d.tipo_doc, dt.desc_tipo_doc, sum( d.saldo_dol) as saldo_dolar
from doc_pendientes_cta_cte d, doc_referencias dr, doc_tipo dt
where ( d.cod_relacion=dr.cod_relacion(+) and
        d.tipo_doc = dr.tipo_doc(+) and
        d.nro_doc = dr.nro_doc(+) ) and
      (d.tipo_doc = dt.tipo_doc) and
      ( d.cnta_ctbl is not null ) and
      ( d.tipo_doc in (:as_doc_antic_oc, :as_doc_antic_os, :as_doc_antic_og)) and
		( d.sldo_sol > 0 and d.saldo_dol > 0 ) and
      (sysdate - d.fecha_doc > :an_dias )
group by d.tipo_doc, dt.desc_tipo_doc
" arguments=(("as_doc_antic_oc", string),("as_doc_antic_os", string),("as_doc_antic_og", string),("an_dias", number)) )
