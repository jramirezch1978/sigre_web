 retrieve="select c.cod_n2, cn2.descripcion, round(avg(sysdate-amp.fec_proyect),2) as dias_atras
from articulo_mov_proy amp, centros_costo c, cencos_niv2 cn2
where amp.cencos = c.cencos and
      amp.cant_proyect > amp.cant_procesada and
      (c.cod_n1 = cn2.cod_n1 and
       c.cod_n2 = cn2.cod_n2) and
      amp.flag_estado='1' and
      c.cod_n1 = :as_nivel1 and
      (sysdate - fec_proyect) > 1
group by c.cod_n2, cn2.descripcion    " arguments=(("as_nivel1", string)) )
