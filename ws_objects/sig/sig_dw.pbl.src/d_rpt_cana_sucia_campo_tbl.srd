 column=(type=number updatewhereclause=yes name=peso_neto dbname="peso_neto" )
 retrieve="   SELECT fcc.clase, fcc.descripcion, oc.corr_corte, 
          cc.cod_campo, c.desc_campo, c.frente_cosecha, 
          cc.has_netas, sum(cp.pesada - cp.tara)/1000 as peso_neto
   FROM campo_pesaje cp, campo_ciclo cc, orden_cosecha oc, 
        campo c, frente_cosecha fc, frente_cosecha_clase fcc
   WHERE cp.corr_corte = cc.corr_corte AND
         cp.corr_corte = oc.corr_corte AND
         cp.nro_orden = oc.nro_orden AND
         cp.flag_impurezas = '0' AND
			nvl(cp.tara,0) > 0 and
         cc.cod_campo = c.cod_campo AND
         c.frente_cosecha = fc.frente_cosecha and
         fc.clase = fcc.clase and
         (cp.hora_tara BETWEEN :adt_fec_ini AND :adt_fec_fin)
group by  fcc.clase, fcc.descripcion, oc.corr_corte, 
          cc.cod_campo, c.desc_campo, c.frente_cosecha, 
          cc.has_netas
order by fcc.clase, c.frente_cosecha, oc.corr_corte" arguments=(("adt_fec_ini", datetime),("adt_fec_fin", datetime)) )
