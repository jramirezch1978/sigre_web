 column=(type=number updatewhereclause=yes name=costo_dol dbname="costo_dol" )
 retrieve="select aa.almacen, a.desc_almacen, 
       ar.cod_clase, acl.desc_clase, 
       round(sum(aa.sldo_total*aa.costo_prom_dol),2) as costo_dol
from articulo_almacen aa, almacen a, articulo ar, articulo_clase acl
where aa.almacen = a.almacen and
      a.flag_tipo_almacen='M' and      
      aa.sldo_total >0 and
      aa.cod_art=ar.cod_art and
      ar.cod_clase = acl.cod_clase and
		ar.cod_clase = :as_clase
group by aa.almacen, a.desc_almacen, ar.cod_clase, acl.desc_clase
order by ar.cod_clase, aa.almacen
      " arguments=(("as_clase", string)) )
