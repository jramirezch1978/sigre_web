table(column=(type=char(12) updatewhereclause=yes name=cod_art dbname="articulo.cod_art" )
 column=(type=char(400) updatewhereclause=yes name=desc_art dbname="articulo.desc_art" )
 column=(type=char(3) updatewhereclause=yes name=und dbname="articulo_grupo.und_reporte" )
 column=(type=number updatewhereclause=yes name=cantidad dbname="cantidad" )
 retrieve="         SELECT a.cod_art,
                a.desc_art,
                ag.und_reporte,
                NVL(sum(DECODE( ag.und_reporte, a.und, am.cant_procesada, 
                                a.und2, am.cant_proc_und2, 
                                usf_ap_conv_und(a.und, ag.und_reporte, 
                                am.cant_procesada))),0) AS cantidad
           FROM vale_mov     vm,
                articulo_mov am,
                articulo     a,
                rel_articulo_grupo rag,
					 articulo_grupo ag
          WHERE vm.nro_vale = am.nro_vale
            AND a.cod_art   = am.cod_art
            AND a.cod_art   = rag.cod_art
            AND rag.grupo_art = ag.grupo_art
            AND vm.flag_estado <> '0'
            AND am.flag_estado <> '0'
            AND rag.grupo_art  = :as_grupo_art
            AND vm.tipo_mov    = (select oper_ing_prod from logparam where reckey = '1')
            AND to_char(vm.fec_registro, 'yyyy') = to_char(:ad_fecha, 'yyyy')
            AND vm.cod_origen  = :as_origen
GROUP BY a.cod_art, a.desc_art, ag.und_reporte 
having NVL(sum(DECODE( ag.und_reporte, a.und, am.cant_procesada, 
                                a.und2, am.cant_proc_und2, 
                                usf_ap_conv_und(a.und, ag.und_reporte, 
                                am.cant_procesada))),0) > 0" arguments=(("as_origen", string),("as_grupo_art", string),("ad_fecha", date)) )
