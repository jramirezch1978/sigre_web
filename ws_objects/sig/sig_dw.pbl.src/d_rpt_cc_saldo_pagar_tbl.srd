 retrieve="select distinct cp.tipo_doc, cp.nro_doc, cp.cod_relacion, dp.saldo_dol, 
       cp.cod_moneda, cp.importe_doc, p.nom_proveedor, cp.vencimiento
  from cntas_pagar cp, doc_pendientes_cta_cte dp, proveedor p
 where (cp.cod_relacion = dp.cod_relacion and
        cp.tipo_doc = dp.tipo_doc and
        cp.nro_doc = dp.nro_doc ) and
       (cp.cod_relacion = p.proveedor and
        dp.cod_relacion = p.proveedor ) and
       (cp.tipo_doc = (select f.doc_fact_pagar from finparam f where reckey='1') or
        cp.tipo_doc = (select doc_bol_pagar from finparam where reckey='1') or
        cp.tipo_doc = (select nota_deb_ncc from finparam where reckey='1') or
        cp.tipo_doc = (select f.doc_letra_pagar from finparam f where reckey='1') ) and
       cp.flag_estado <> '0' and
      ( (cp.cod_moneda = (select cod_soles from logparam where reckey='1') and dp.sldo_sol  > 0) or
        (cp.cod_moneda = (select cod_dolares from logparam where reckey='1') and dp.saldo_dol > 0) ) and 
       cp.cod_relacion not in (select gpr.proveedor 
                                 from grupo_proveedor_rel gpr 
                                where gpr.grupo=:as_grupo_codrel) 
order by cp.cod_relacion" arguments=(("as_grupo_codrel", string)) )
