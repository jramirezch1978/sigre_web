 column=(type=number updatewhereclause=yes name=consumo_valor dbname="consumo_valor" )
 retrieve="SELECT   ASM.ANO,   
         ASM.MES,
         ACL.COD_CLASE,
         ACL.DESC_CLASE,         
         SUM(ASM.VALOR_SALDO_TOTAL) as stock_valor, 
         SUM(ASM.VALOR_COMP_TOTAL) as compra_valor,
         SUM(ASM.VALOR_CONS_TOTAL) as consumo_valor
    FROM ARTICULO A,   
         ARTICULO_CLASE ACL,   
         ARTICULO_SALDO_MENSUAL ASM  
   WHERE ( ACL.COD_CLASE = A.COD_CLASE ) and  
         ( ASM.COD_ART = A.COD_ART ) and  
         ( ASM.ANO >= :an_ano_inicio ) AND  
         ( ASM.MES >= :an_mes_inicio ) AND  
         ( ASM.ANO <= :an_ano_fin ) AND  
         ( ASM.MES <= :an_mes_fin ) AND  
         ( A.COD_CLASE <> :as_categoria_pt) AND
         ( A.COD_CLASE <> :as_sub_prod)
GROUP BY ACL.COD_CLASE,
         ACL.DESC_CLASE,   
         ASM.ANO,   
         ASM.MES         
ORDER BY ASM.ANO ASC,   
         ASM.MES ASC,   
         ACL.COD_CLASE ASC " arguments=(("an_ano_inicio", number),("an_mes_inicio", number),("an_ano_fin", number),("an_mes_fin", number),("as_categoria_pt", string),("as_sub_prod", string)) )
