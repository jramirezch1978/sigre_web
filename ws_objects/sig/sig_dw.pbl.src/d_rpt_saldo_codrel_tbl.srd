 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="select d.cod_relacion, p.nom_proveedor, sum(d.saldo_dol*d.factor) as saldo_dol
from doc_pendientes_cta_cte d, grupo_proveedor_rel gpr, proveedor p
where (d.cod_relacion = p.proveedor) and
      (d.cod_relacion = gpr.proveedor) and
      (p.proveedor = gpr.proveedor) and
      (gpr.grupo = :as_grupo_rel) and
      d.cnta_ctbl is not null and
      ((d.cod_moneda = (SELECT cod_soles from logparam where reckey='1') AND d.sldo_sol > 0) OR 
       (d.cod_moneda = (SELECT cod_dolares from logparam where reckey='1') AND d.saldo_dol > 0) ) 
group by d.cod_relacion, p.nom_proveedor" arguments=(("as_grupo_rel", string)) )
