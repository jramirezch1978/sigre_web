 column=(type=number updatewhereclause=yes name=saldo_dol dbname="compute_0003" )
 retrieve="select d.cod_relacion, p.nom_proveedor, sum(decode(flag_debhab,'D', d.saldo_dol, d.saldo_dol*-1))
from doc_pendientes_cta_cte d, cntas_cobrar cc, proveedor p
where (d.cod_relacion = p.proveedor and
       cc.cod_relacion = p.proveedor) and
      d.cod_relacion in ( select distinct(c.proveedor) 
                          from sem_contrato s, campo_ciclo cc, campo c
                          where s.corr_corte=cc.corr_corte and
                                cc.cod_campo = c.cod_campo ) and
      ( d.cod_relacion = cc.cod_relacion and
        d.tipo_doc = cc.tipo_doc and
        d.nro_doc = cc.nro_doc ) AND
        d.cnta_ctbl is not null and
       ( (d.cod_moneda = (select l.cod_soles from logparam l where reckey='1') AND d.sldo_sol>0) OR
         (d.cod_moneda = (select l.cod_dolares from logparam l where reckey='1') and d.saldo_dol>0 ) ) AND
        (sysdate - d.fecha_doc) > :an_factor
group by d.cod_relacion, p.nom_proveedor
order by sum(decode(flag_debhab,'D', d.saldo_dol, d.saldo_dol*-1))
" arguments=(("an_factor", number)) )
