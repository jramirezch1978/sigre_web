 column=(type=datetime updatewhereclause=yes name=fecha_vencimiento dbname="doc_pendientes_cta_cte.fecha_vencimiento" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="select d.factor, d.cod_relacion, p.nom_proveedor, d.tipo_doc, d.nro_doc, d.fecha_doc, 
		 d.fecha_vencimiento, (d.saldo_dol*d.factor) as saldo_dol
from doc_pendientes_cta_cte d, proveedor p
where (d.cod_relacion = p.proveedor) and
      (d.cod_relacion = :as_cod_rel) and
      d.cnta_ctbl is not null and
      ((d.cod_moneda = (SELECT cod_soles from logparam where reckey='1') AND d.sldo_sol > 0) OR 
       (d.cod_moneda = (SELECT cod_dolares from logparam where reckey='1') AND d.saldo_dol > 0) ) 
order by d.factor, d.saldo_dol desc
" arguments=(("as_cod_rel", string)) )
