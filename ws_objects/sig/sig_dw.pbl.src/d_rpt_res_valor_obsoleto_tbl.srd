 column=(type=number updatewhereclause=yes name=tot_costo dbname="tot_costo" )
 retrieve="SELECT a.cod_clase, acl.desc_clase, 
		 sum(NVL(a.sldo_total,0) * NVL(a.costo_ult_compra,0)) as tot_costo
  FROM articulo a, articulo_clase acl
 WHERE a.cod_clase = acl.cod_clase and
       (a.fec_ult_salida is not null) and
       a.sldo_total >0 and
       a.costo_ult_compra > 0 and
       a.flag_inventariable='1' and
		 a.cod_clase <>(select clase_prod_term from sig_agricola where reckey='1') and
       usf_alm_dif_dias(a.fec_ult_salida, :ad_fecha_hoy ) >= :an_dias
GROUP BY a.cod_clase, acl.desc_clase
" arguments=(("an_dias", number),("ad_fecha_hoy", date)) )
