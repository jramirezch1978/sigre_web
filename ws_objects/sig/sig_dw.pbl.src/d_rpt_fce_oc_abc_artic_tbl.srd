table(column=(type=char(12) updatewhereclause=yes name=cod_art dbname="articulo_mov_proy.cod_art" )
 column=(type=char(400) updatewhereclause=yes name=articulo_desc_art dbname="articulo.desc_art" )
 column=(type=char(3) updatewhereclause=yes name=articulo_und dbname="articulo.und" )
 column=(type=number updatewhereclause=yes name=tot_dolar dbname="tot_dolar" )
 retrieve="select amp.cod_art, a.desc_art, a.und, sum(decode(oc.cod_moneda,
	  (select cod_dolares from logparam where reckey='1'),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento)/USF_CMP_TIPO_CAMBIO(oc.fec_registro))) as tot_dolar	  
from orden_compra oc, articulo_mov_proy amp, articulo a
where (oc.cod_origen=amp.cod_origen and
	     oc.nro_oc=amp.nro_doc and
	     oc.flag_estado <> '0' and
 	     amp.tipo_doc=(select l.doc_oc from logparam l where reckey='1') ) and
       (trunc(oc.fec_registro) between :ad_fec_ini and :ad_fec_fin) and
	     amp.cod_art = a.cod_art and
	     amp.flag_estado <> '0' 
group by amp.cod_art, a.desc_art, a.und, oc.cod_moneda
order by sum(decode(oc.cod_moneda,
	  (select cod_dolares from logparam where reckey='1'),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento)/USF_CMP_TIPO_CAMBIO(oc.fec_registro))) desc
" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("an_total", number)) )
