 column=(type=char(50) updatewhereclause=yes name=proveedor_nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=char(4) updatewhereclause=yes name=tipo_docref1 dbname="cntbl_asiento_det.tipo_docref1" )
 column=(type=char(15) updatewhereclause=yes name=nro_docref1 dbname="cntbl_asiento_det.nro_docref1" )
 column=(type=number updatewhereclause=yes name=tot_dolar dbname="tot_dolar" )
 retrieve="select cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, cad.nro_docref1, 
       sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) as tot_dolar
       from cntbl_asiento ca, cntbl_asiento_det cad, proveedor p
 where ( ca.origen=cad.origen and
         ca.ano=cad.ano and
         ca.mes=cad.mes and
         ca.nro_libro=cad.nro_libro and
         ca.nro_asiento=cad.nro_asiento ) and
       ( ca.flag_estado<>'0' and
         ca.ano= :an_ano ) and
       ( cad.cod_relacion = p.proveedor ) and
       ( cad.cnta_ctbl in (select cnta_ctbl from rpt_subgrupo_det r
                           where r.reporte=:as_reporte and
                                 r.grupo=:as_grupo and
                                 r.subgrupo=:as_subgrupo) and
         cad.cod_relacion is not null and
         cad.tipo_docref1 is not null and
         cad.nro_docref1 is not null ) and
         cad.cnta_ctbl=:as_cnta_ctbl
group by cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, cad.nro_docref1
having sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) <> 0
order by cod_relacion, sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) desc
" arguments=(("an_ano", number),("as_reporte", string),("as_grupo", string),("as_subgrupo", string),("as_cnta_ctbl", string)) )
