 column=(type=char(60) updatewhereclause=yes name=forma_pago_desc_forma_pago dbname="forma_pago.desc_forma_pago" )
 column=(type=number updatewhereclause=yes name=tot_dolar dbname="tot_dolar" )
 retrieve="select oc.forma_pago, fp.desc_forma_pago, sum(decode(oc.cod_moneda,
	  (select cod_dolares from logparam where reckey='1'),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento)/USF_CMP_TIPO_CAMBIO(oc.fec_registro))) as tot_dolar
from orden_compra oc, articulo_mov_proy amp, forma_pago fp
where oc.cod_origen=amp.cod_origen and
	  oc.nro_oc=amp.nro_doc and
	  oc.flag_estado <> '0' and
	  oc.forma_pago = fp.forma_pago and
	  amp.tipo_doc=(select l.doc_oc from logparam l where reckey='1') and
	  amp.flag_estado <> '0' and
	  (trunc(oc.fec_registro) between :ad_fec_ini and :ad_fec_fin)
group by oc.forma_pago, fp.desc_forma_pago, oc.cod_moneda
order by sum(decode(oc.cod_moneda,
	  (select cod_dolares from logparam where reckey='1'),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento),
	  amp.cant_proyect*(amp.precio_unit-amp.decuento)/USF_CMP_TIPO_CAMBIO(oc.fec_registro))) desc" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("an_total", number)) )
