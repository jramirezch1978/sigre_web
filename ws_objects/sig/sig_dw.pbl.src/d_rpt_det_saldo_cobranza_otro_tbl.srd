 column=(type=datetime updatewhereclause=yes name=fecha_vencimiento dbname="cntas_cobrar.fecha_vencimiento" )
 column=(type=char(3) updatewhereclause=yes name=cod_moneda dbname="cntas_cobrar.cod_moneda" )
 column=(type=decimal(2) updatewhereclause=yes name=importe_doc dbname="cntas_cobrar.importe_doc" )
 column=(type=number updatewhereclause=yes name=dias_vencidos dbname="dias_vencidos" )
 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="SELECT distinct(cc.tipo_doc), d.desc_tipo_doc, cc.nro_doc, cc.cod_relacion, 
		 p.nom_proveedor, cc.fecha_registro, cc.fecha_vencimiento,
	    cc.cod_moneda, cc.importe_doc, 
       round(sysdate-cc.fecha_vencimiento,2) as dias_vencidos,
		 NVL(dcc.factor,1)*NVL(dcc.saldo_dol,0) as saldo_dol
  FROM cntas_cobrar cc, cntas_cobrar_det ccd, doc_pendientes_cta_cte dcc, 
		 proveedor p, doc_tipo d
 WHERE ( cc.tipo_doc=ccd.tipo_doc AND
         cc.nro_doc = ccd.nro_doc ) AND
       ( cc.flag_estado <> '0' AND
         cc.fecha_vencimiento < sysdate ) AND
       (cc.tipo_doc = d.tipo_doc ) AND
       ( cc.cod_relacion = p.proveedor) AND
       ( cc.cod_relacion = dcc.cod_relacion AND
         cc.tipo_doc     = dcc.tipo_doc AND
         cc.nro_doc      = dcc.nro_doc ) AND
       ( cc.cod_relacion NOT IN 
                         (select gp.proveedor 
                          from grupo_proveedor_rel gp 
                          where gp.grupo=:ls_grupo_codrel)) AND
       ( ccd.rubro       = :ls_rubro ) AND
       ( ( dcc.cod_moneda = (SELECT cod_soles from logparam where reckey='1') AND dcc.sldo_sol > 0) OR 
         ( dcc.cod_moneda = (SELECT cod_dolares from logparam where reckey='1') AND dcc.saldo_dol > 0) ) 
ORDER BY cc.tipo_doc, cc.nro_doc, cc.fecha_vencimiento, cc.cod_relacion

" arguments=(("ls_grupo_codrel", string),("ls_rubro", string)) )
