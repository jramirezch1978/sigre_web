 column=(type=number updatewhereclause=yes name=valor_acum dbname="valor_acum" )
 column=(type=number updatewhereclause=yes name=ton dbname="ton" )
 column=(type=char(4) updatewhereclause=yes name=forma_embarque dbname="orden_venta.forma_embarque" )
 column=(type=char(30) updatewhereclause=yes name=descripcion dbname="forma_embarque.descripcion" )
 retrieve="select cc.cod_relacion,p.nom_proveedor, ccd.cod_art, a.desc_art, a.cod_clase, ac.desc_clase, cc.cod_moneda,
		   round((decode( cc.cod_moneda, 'S/.', sum(ccd.cantidad * ccd.precio_unitario), sum(ccd.cantidad * ccd.precio_unitario * nvl(cc.tasa_cambio,0)))),2) as valor_acum, 
	 	   round(sum(((ccd.cantidad * nvl(a.peso_und,1))/1000) ),2)  as Ton, ov.forma_embarque, fe.descripcion
  from cntas_cobrar cc, cntas_cobrar_det ccd, articulo a, proveedor p, articulo_clase ac, orden_venta ov, forma_embarque fe
 where cc.tipo_doc = ccd.tipo_doc
   and cc.nro_doc  = ccd.nro_doc
	and cc.tipo_doc <> 'NCC'
   and ccd.cod_art = a.cod_art
   and a.cod_clase = ac.cod_clase
   and a.cod_clase in ((select s.clase_prod_term from sigparam s where reckey='1'), (select s.clase_sub_prod from sigparam s where reckey='1')) 
   and cc.cod_relacion = p.proveedor
   and ccd.tipo_ref = 'OV'
   and ccd.nro_ref = ov.nro_ov
   and ov.forma_embarque = fe.forma_embarque
   and cc.flag_estado <> '0'
   and cc.fecha_registro >= :ad_fec_ini 
   and cc.fecha_registro <= :ad_fec_fin
group by cc.cod_relacion,p.nom_proveedor, ccd.cod_art, a.desc_art, a.cod_clase,ac.desc_clase, cc.cod_moneda , ov.forma_embarque, fe.descripcion
order by a.cod_clase, cc.cod_relacion
" arguments=(("ad_fec_ini", date),("ad_fec_fin", date)) )
