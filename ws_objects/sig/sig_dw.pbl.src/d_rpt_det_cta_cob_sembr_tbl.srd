 column=(type=number updatewhereclause=yes name=saldo_dol dbname="saldo_dol" )
 retrieve="select d.cod_relacion, p.nom_proveedor, d.tipo_doc, d.nro_doc, d.fecha_vencimiento, d.cnta_ctbl, dt.desc_tipo_doc, 
       sum(decode(flag_debhab,'D', d.saldo_dol, d.saldo_dol*-1)) as saldo_dol
from doc_pendientes_cta_cte d, cntas_cobrar cc, proveedor p, doc_tipo dt
where (d.cod_relacion = p.proveedor and
       cc.cod_relacion = p.proveedor) and
       d.cod_relacion = :as_cod_relacion and
       d.tipo_doc = dt.tipo_doc and
      ( d.cod_relacion = cc.cod_relacion and
        d.tipo_doc = cc.tipo_doc and
        d.nro_doc = cc.nro_doc ) AND
        d.cnta_ctbl is not null and
       ( (d.cod_moneda = (select l.cod_soles from logparam l where reckey='1') AND d.sldo_sol>0) OR
         (d.cod_moneda = (select l.cod_dolares from logparam l where reckey='1') and d.saldo_dol>0 ) ) AND
        (sysdate - d.fecha_doc) > :an_factor
group by d.cod_relacion, p.nom_proveedor, d.tipo_doc, d.nro_doc, d.fecha_vencimiento, d.cnta_ctbl, dt.desc_tipo_doc
order by sum(decode(flag_debhab,'D', d.saldo_dol, d.saldo_dol*-1))

" arguments=(("as_cod_relacion", string),("an_factor", number)) )
