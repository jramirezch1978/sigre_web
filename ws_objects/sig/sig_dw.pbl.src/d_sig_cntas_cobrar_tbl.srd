table(column=(type=char(1) updatewhereclause=yes name=rango dbname="rango" )
 column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="cod_relacion" )
 column=(type=char(200) updatewhereclause=yes name=nom_proveedor dbname="nom_proveedor" )
 column=(type=char(20) updatewhereclause=yes name=ruc_dni dbname="ruc_dni" )
 column=(type=number updatewhereclause=yes name=venta_dol dbname="venta_dol" )
 column=(type=number updatewhereclause=yes name=porcentaje dbname="porcentaje" )
 column=(type=number updatewhereclause=yes name=sum_porc_acum dbname="sum_porc_acum" )
 retrieve="select '1' as rango,
       vw1.cod_relacion,
       vw1.nom_proveedor,
       vw1.ruc_dni,
       vw1.venta_dol,
       vw1.porcentaje,
       vw1.sum_porc_acum
from(select vw.cod_relacion,
             vw.nom_proveedor,
             vw.ruc_dni,
             vw.venta_dol,
             vw.porcentaje,
             sum(porcentaje) over (order by porcentaje desc) sum_porc_acum
       from (select cc.cod_relacion,
                   p.nom_proveedor,
                   decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
                   sum(
                      case
                         when cc.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                           cc.importe_doc / cc.tasa_cambio
                         else
                           cc.importe_doc
                      end * decode(dt.flag_signo, '+', 1, -1)
                    ) venta_dol,
                    round(
                        sum(case
                               when cc.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                                 cc.importe_doc / cc.tasa_cambio
                               else
                                 cc.importe_doc
                            end * decode(dt.flag_signo, '+', 1, -1)
                          ) / (select sum(case
                                             when cc1.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                                               cc1.importe_doc / cc1.tasa_cambio
                                             else
                                               cc1.importe_doc
                                          end * decode(dt1.flag_signo, '+', 1, -1))
                                   from 	cntas_cobrar 	cc1,	
														doc_tipo			dt1
                                  where cc1.tipo_doc	= dt1.tipo_doc
 												and ((:as_filtro = '1' and cc1.ano = :ani_year and cc1.mes = :ani_mes) or
                            				  (:as_filtro = '2' and trunc(cc1.fecha_documento) between trunc(:adi_fecha1) and trunc(:adi_fecha2)))  
                                    and cc1.nro_libro = 04
                                    and cc1.flag_estado <> '0') * 100,4)  as porcentaje
            from cntas_cobrar 	 cc,
                 proveedor        p,
            	  doc_tipo         dt
            where cc.cod_relacion = p.proveedor
          and cc.tipo_doc    = dt.tipo_doc
              and cc.flag_estado <> '0'
              and cc.nro_libro = 04 -- registro de ventas
              and ((:as_filtro = '1' and cc.ano = :ani_year and cc.mes = :ani_mes) or
             (:as_filtro = '2' and trunc(cc.fecha_documento) between trunc(:adi_fecha1) and trunc(:adi_fecha2)))
            group by cc.cod_relacion,
                   p.nom_proveedor,
                   decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident)) vw
       ) vw1
where vw1.sum_porc_acum <= 90   
union
select '2' as rango,
       null as cod_relacion,
       'OTROS VARIOS CLIENTES ' as nom_proveedor,
       null as ruc_dni,
       sum(vw1.venta_dol) as venta_dol,
       sum(vw1.porcentaje) as porcentaje,
       100 as sum_porc_acum
from(select vw.cod_relacion,
             vw.nom_proveedor,
             vw.ruc_dni,
             vw.venta_dol,
             vw.porcentaje,
             sum(porcentaje) over (order by porcentaje desc) sum_porc_acum
       from (select cc.cod_relacion,
                   p.nom_proveedor,
                   decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident) as ruc_dni,
                   sum(
                      case
                         when cc.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                           cc.importe_doc / cc.tasa_cambio
                         else
                           cc.importe_doc
                      end * decode(dt.flag_signo, '+', 1, -1)
                    ) venta_dol,
                    round(
                        sum(case
                               when cc.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                                 cc.importe_doc / cc.tasa_cambio
                               else
                                 cc.importe_doc
                            end * decode(dt.flag_signo, '+', 1, -1)
                          ) / (select sum(case
                                             when cc1.cod_moneda = PKG_LOGISTICA.of_soles(null) then
                                               cc1.importe_doc / cc1.tasa_cambio
                                             else
                                               cc1.importe_doc
                                          end * decode(dt1.flag_signo, '+', 1, -1))
                                   from 	cntas_cobrar 	cc1,
														doc_tipo			dt1
                                  where cc1.tipo_doc		= dt1.tipo_doc
												and ((:as_filtro = '1' and cc1.ano = :ani_year and cc1.mes = :ani_mes) or
                            				  (:as_filtro = '2' and trunc(cc1.fecha_documento) between trunc(:adi_fecha1) and trunc(:adi_fecha2)))  
                                    and cc1.nro_libro = 04
                                    and cc1.flag_estado <> '0') * 100,4)  as porcentaje
            from cntas_cobrar   	cc,
                 proveedor       p,
            	  doc_tipo      	dt
            where cc.cod_relacion = p.proveedor
          and cc.tipo_doc    = dt.tipo_doc
              and cc.flag_estado <> '0'
              and cc.nro_libro = 04 -- registro de ventas
              and ((:as_filtro = '1' and cc.ano = :ani_year and cc.mes = :ani_mes) or
             (:as_filtro = '2' and trunc(cc.fecha_documento) between trunc(:adi_fecha1) and trunc(:adi_fecha2)))
            group by cc.cod_relacion,
                   p.nom_proveedor,
                   decode(p.tipo_doc_ident, '6', p.ruc, p.nro_doc_ident),
                   cc.cod_moneda ) vw
       ) vw1
where vw1.sum_porc_acum > 90   
    
order by rango asc, porcentaje desc            
