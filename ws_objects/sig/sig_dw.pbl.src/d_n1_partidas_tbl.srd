 column=(type=number updatewhereclause=yes name=ejecutado dbname="ejecutado" )
 retrieve="  SELECT CC.CENCOS,   
         CC.DESC_CENCOS,   
         PP.CNTA_PRSP,   
         PC.DESCRIPCION,   
         PP.FLAG_ESTADO,   
         PP.FLAG_CTRL,
			PP.TIPO_PRTDA_PRSP,
			PP.FLAG_INGR_EGR,
			PP.FLAG_CONTAB,   
         SUM(PP.PROY_ENE + PP.PROY_FEB + PP.PROY_MAR + PP.PROY_ABR + PP.PROY_MAY + PP.PROY_JUN + PP.PROY_JUL + PP.PROY_AGO + PP.PROY_SET + PP.PROY_OCT + PP.PROY_NOV + PP.PROY_DIC) as proyectado,  
         SUM(PP.VAR_ENE + PP.VAR_FEB + PP.VAR_MAR + PP.VAR_ABR + PP.VAR_MAY + PP.VAR_JUN + PP.VAR_JUL + PP.VAR_AGO + PP.VAR_SET + PP.VAR_OCT + PP.VAR_NOV + PP.VAR_DIC) as variacion,
         SUM(PP.EJEC_ENE + PP.EJEC_FEB + PP.EJEC_MAR + PP.EJEC_ABR + PP.EJEC_MAY + PP.EJEC_JUN + PP.EJEC_JUL + PP.EJEC_AGO + PP.EJEC_SET + PP.EJEC_OCT + PP.EJEC_NOV + PP.EJEC_DIC) as ejecutado 
    FROM CENTROS_COSTO 			CC,   
         PRESUPUESTO_PARTIDA 	PP,   
         PRESUPUESTO_CUENTA 	PC,
			TIPO_PRTDA_PRSP_DET	TPD  
   WHERE PP.CENCOS 			 = CC.CENCOS 
	  and PP.CNTA_PRSP 		 = PC.CNTA_PRSP 
	  AND PP.TIPO_PRTDA_PRSP = TPD.TIPO_PRTDA_PRSP
	  and PP.ANO 				 = to_number(:as_ano) 
  	  AND TPD.GRP_PRTDA_PRSP <> :as_no_tipo
	  AND PP.FLAG_ESTADO 	 <> '0'
	  AND PP.FLAG_INGR_EGR 	 = :as_tipo_partida
	  AND CC.COD_N1 			 = SUBSTR(:as_cencos,1,2)
GROUP BY CC.CENCOS,   
         CC.DESC_CENCOS,   
         PP.CNTA_PRSP,   
         PC.DESCRIPCION,   
         PP.FLAG_ESTADO,   
         PP.FLAG_CTRL,
			PP.TIPO_PRTDA_PRSP,
			PP.FLAG_INGR_EGR,
			PP.FLAG_CONTAB
" arguments=(("as_ano", string),("AS_CENCOS", string),("as_no_tipo", string),("as_tipo_partida", string)) )
