 column=(type=number updatewhereclause=yes name=valor_acum dbname="valor" )
 column=(type=number updatewhereclause=yes name=ton dbname="ton" )
 column=(type=char(6) updatewhereclause=yes name=motivo dbname="cntas_cobrar.motivo" )
 column=(type=char(50) updatewhereclause=yes name=descripcion dbname="motivo_nota.descripcion" )
 retrieve="select cc.tipo_doc, cc.nro_doc, cc.fecha_registro, ccd.tipo_ref, ccd.nro_ref, cc.cod_moneda, cc.cod_relacion, 
       p.nom_proveedor, ccd.cod_art, a.desc_art, a.cod_clase, ac.desc_clase, ccd.cantidad, ccd.precio_unitario, 
       round((decode( cc.cod_moneda, 'S/.', (ccd.cantidad * ccd.precio_unitario), (ccd.cantidad * ccd.precio_unitario * nvl(cc.tasa_cambio,0)))),2) as valor,
		(((ccd.cantidad * nvl(a.peso_und,1))/1000) ) as Ton, cc.motivo , mn.descripcion
  from cntas_cobrar cc, cntas_cobrar_det ccd, articulo a, proveedor p, articulo_clase ac, motivo_nota mn
 where cc.tipo_doc = ccd.tipo_doc
   and cc.nro_doc  = ccd.nro_doc
   and cc.tipo_doc = 'NCC' and cc.motivo in ('NCC001','NCC002' )
   and ccd.cod_art = a.cod_art
   and a.cod_clase = ac.cod_clase
   and cc.motivo = mn.motivo
   and ccd.cod_art = :as_cod_art  
   and a.cod_clase in ( (select s.clase_prod_term from sig_agricola s where reckey='1'), (select s.clase_sub_prod from sig_agricola s where reckey='1')) 
   and cc.cod_relacion = p.proveedor
   and cc.cod_relacion = :as_relacion
   and cc.flag_estado <> '0'
   and cc.fecha_registro >= :ad_fec_ini 
   and cc.fecha_registro <= :ad_fec_fin
" arguments=(("ad_fec_ini", date),("ad_fec_fin", date),("as_relacion", string),("as_cod_art", string)) )
