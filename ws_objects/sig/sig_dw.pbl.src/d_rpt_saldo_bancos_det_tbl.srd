 column=(type=number updatewhereclause=yes name=saldo_dolar dbname="saldo_dolar" )
 retrieve="select a.cod_banco, a.nom_banco, b.cod_ctabco,  
       b.cnta_ctbl, b.cod_origen, 
       b.cod_moneda, b.saldo_disponible, 
       round(decode(b.cod_moneda,
						 (SELECT l.cod_soles FROM logparam l WHERE l.reckey='1'),
						  saldo_disponible/:an_tipo_cambio,
						  saldo_disponible),2) as saldo_dolar 
from banco a, banco_cnta b
where a.cod_banco = b.cod_banco and
      NVL(b.saldo_disponible,0)<>0 and
		  b.flag_uso_interno='0'
order by a.cod_banco, b.cod_origen, b.cod_ctabco
" arguments=(("an_tipo_cambio", number)) )
