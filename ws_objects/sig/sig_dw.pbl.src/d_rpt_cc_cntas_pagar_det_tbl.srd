 column=(type=number updatewhereclause=yes name=tot_dolar dbname="tot_dolar" )
 retrieve="select distinct cp.tipo_doc, cp.nro_doc, cp.cod_relacion, p.nom_proveedor, 
       cp.cod_moneda, cp.importe_doc, cp.vencimiento, cp.fecha_emision,
       decode(cp.cod_moneda, (select cod_soles from logparam where reckey='1'), 
              cp.importe_doc / cp.tasa_cambio, cp.importe_doc ) as tot_dolar
  from cntas_pagar cp, forma_pago fp, proveedor p
 where (cp.cod_relacion = p.proveedor) and
       (cp.tipo_doc = (select f.doc_fact_pagar from finparam f where reckey='1') or
        cp.tipo_doc = (select doc_bol_pagar from finparam where reckey='1') ) and        
       (cp.fecha_emision between :ad_fecha_ini and :ad_fecha_fin) and 
       (cp.tipo_doc = (select f.doc_fact_pagar from finparam f where reckey='1') or
        cp.tipo_doc = (select f.doc_bol_pagar from finparam f where reckey='1') ) and
       cp.flag_estado <> '0' and
       cp.cod_relacion not in (select gpr.proveedor 
                                 from grupo_proveedor_rel gpr 
                                where gpr.grupo=:as_grupo_codrel) AND                 
       cp.forma_pago = fp.forma_pago and
       fp.dias_vencimiento > 0 
order by cp.cod_relacion" arguments=(("as_grupo_codrel", string),("ad_fecha_ini", date),("ad_fecha_fin", date)) )
