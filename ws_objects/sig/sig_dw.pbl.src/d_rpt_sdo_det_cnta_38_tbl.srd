 retrieve="select cad.cnta_ctbl, c.desc_cnta, cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, 
		 cad.nro_docref1, sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) as tot_dol
from cntbl_asiento ca, cntbl_asiento_det cad, cntbl_cnta c, proveedor p
where ( ca.origen=cad.origen and
        ca.ano=cad.ano and
        ca.mes=cad.mes and
        ca.nro_libro=cad.nro_libro and
        ca.nro_asiento=cad.nro_asiento ) and
      (cad.cnta_ctbl = c.cnta_ctbl ) and
      ( ca.flag_estado<>'0' and
        ca.ano= :an_ano ) and
      (cad.cnta_ctbl = :as_cnta_contab and
        cad.cod_relacion is not null and
        cad.tipo_docref1 is not null and
        cad.nro_docref1 is not null ) and
	   ( cad.cod_relacion = p.proveedor ) 
group by cad.cnta_ctbl, c.desc_cnta, cad.cod_relacion, p.nom_proveedor, cad.tipo_docref1, 
		 cad.nro_docref1
having sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) <> 0
order by cad.cod_relacion, sum(decode(cad.flag_debhab,'D',cad.imp_movdol,cad.imp_movdol*-1)) desc" arguments=(("an_ano", number),("as_cnta_contab", string)) )
