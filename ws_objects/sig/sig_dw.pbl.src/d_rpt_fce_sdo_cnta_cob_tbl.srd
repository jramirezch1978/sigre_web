table(column=(type=char(8) updatewhereclause=yes name=cod_relacion dbname="doc_pendientes_cta_cte.cod_relacion" )
 column=(type=char(50) updatewhereclause=yes name=proveedor_nom_proveedor dbname="proveedor.nom_proveedor" )
 column=(type=number updatewhereclause=yes name=saldo_dolares dbname="saldo_dolares" )
 column=(type=number updatewhereclause=yes name=vencido_dol dbname="vencido_dol" )
 column=(type=number updatewhereclause=yes name=saldo_soles dbname="saldo_soles" )
 column=(type=number updatewhereclause=yes name=vencido_sol dbname="vencido_sol" )
 retrieve="select dp.cod_relacion, p.nom_proveedor, 
       sum(nvl(dp.saldo_dol*dp.factor,0)) as saldo_dolares, 
       usf_sig_cnta_cob_pend_x_codrel(dp.cod_relacion, (select l.cod_dolares from logparam l where reckey='1')) as vencido_dol, 
       sum(nvl(dp.sldo_sol*dp.factor,0)) as saldo_soles,
       usf_sig_cnta_cob_pend_x_codrel(dp.cod_relacion, (select l.cod_soles from logparam l where reckey='1')) as vencido_sol
  from cntas_cobrar cc, doc_pendientes_cta_cte dp, proveedor p
 where (cc.cod_relacion = dp.cod_relacion and
        cc.tipo_doc = dp.tipo_doc and
        cc.nro_doc = dp.nro_doc) and
        (cc.cod_relacion = p.proveedor ) and
        (dp.cod_relacion = p.proveedor) and
       cc.flag_estado<>'0' and
       cc.nro_libro = (select f.libro_ventas from finparam f where reckey='1') and
       (dp.sldo_sol > 0 or dp.saldo_dol > 0) 
group by dp.cod_relacion, p.nom_proveedor
order by sum(nvl(dp.sldo_sol*dp.factor,0)) desc" )
