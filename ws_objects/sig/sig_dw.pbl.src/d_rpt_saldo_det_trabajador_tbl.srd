 retrieve="SELECT c.cod_trabajador, p.nom_proveedor, c.tipo_doc, c.nro_doc, c.fec_prestamo, 
       c.cod_moneda, c.mont_original, c.sldo_prestamo, c.cod_sit_prest
    FROM cnta_crrte c,   
         proveedor p,   
         maestro m  
   WHERE ( c.cod_trabajador = p.proveedor ) and  
         ( c.cod_trabajador = m.cod_trabajador ) and  
         ( m.cod_trabajador = p.proveedor ) and  
         ( c.cod_trabajador = :as_codrel) and
         ( m.flag_estado = '1' AND  
           m.flag_cal_plnlla = '1' ) AND  
           c.sldo_prestamo > 0   " arguments=(("as_codrel", string)) )
