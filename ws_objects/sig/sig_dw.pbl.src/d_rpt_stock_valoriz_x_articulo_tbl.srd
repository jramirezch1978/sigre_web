 retrieve="select aa.almacen, aa.cod_art, ar.nom_articulo, 
		 ar.und, aa.sldo_total, aa.costo_prom_sol, 
		 aa.costo_prom_dol
from articulo_almacen aa, almacen a, articulo ar, articulo_clase acl
where aa.almacen = a.almacen and
      a.flag_tipo_almacen='M' and      
      aa.sldo_total >0 and
      aa.cod_art=ar.cod_art and
      ar.cod_clase = acl.cod_clase and
		  ar.cod_clase = :as_cod_clase and
      aa.almacen=:as_almacen
order by sldo_total*costo_prom_dol desc" arguments=(("as_cod_clase", string),("as_almacen", string)) )
