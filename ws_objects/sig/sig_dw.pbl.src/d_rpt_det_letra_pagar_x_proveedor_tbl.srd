 column=(type=number updatewhereclause=yes name=saldo_dolar dbname="saldo_dolar" )
 retrieve="SELECT d.cod_relacion, p.nom_proveedor, d.tipo_doc, cp.vencimiento, d.nro_doc, d.fecha_doc, 
       NVL(d.saldo_dol,0) as saldo_dolar
from doc_pendientes_cta_cte d, cntas_pagar cp, proveedor p
where ( d.cod_relacion = cp.cod_relacion and
		    d.tipo_doc = cp.tipo_doc AND
		    d.nro_doc = cp.nro_doc ) AND
		( d.cod_relacion = p.proveedor and
      d.tipo_doc = :as_tipo_doc and 
      d.cod_relacion = :as_proveedor) and
		( cp.cod_relacion = p.proveedor ) and
     ( d.sldo_sol > 0 and d.saldo_dol > 0 ) 
ORDER BY saldo_dolar desc  " arguments=(("as_tipo_doc", string),("as_proveedor", string)) )
