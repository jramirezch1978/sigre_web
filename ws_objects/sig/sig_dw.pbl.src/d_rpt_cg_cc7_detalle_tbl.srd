 retrieve="      SELECT a.cod_clase,
				 ac.desc_clase,
				 a.cod_art,
				 a.desc_art,
				 a.und,
				 sum(am.cant_procesada * amt.factor_sldo_total) as cant_procesada,
             usf_fl_conv_mon(sum(NVL(am.cant_procesada,1) * am.precio_unit * amt.factor_sldo_total), 
         		am.cod_moneda, 
					(select cod_dolares from logparam where reckey = '1'), 
					trunc(:ad_fecha)) as importe
        FROM vale_mov     vm,
             articulo_mov am,
             articulo     a,
				 articulo_clase  ac,
             articulo_mov_tipo amt
       WHERE vm.nro_vale   = am.nro_vale
         AND a.cod_art     = am.cod_art
         AND amt.tipo_mov  = vm.tipo_mov
		   and a.cod_clase	= ac.cod_clase
         AND trunc(vm.fec_registro) <= trunc(:ad_fecha)
         AND vm.flag_estado <> '0'
         AND am.flag_estado <> '0'
         AND vm.cod_origen  = :as_origen
         AND am.cod_art     IN (SELECT DISTINCT amp_cons.cod_art
                                  FROM orden_trabajo ot2,
                                       articulo_mov_proy amp_ingr,
                                       articulo_mov_proy amp_cons,
                                       rel_articulo_grupo rag
                                 WHERE amp_ingr.tipo_doc = (select doc_ot from logparam where reckey = '1')
                                   AND amp_ingr.nro_doc  = ot2.nro_orden
                                   AND amp_cons.tipo_doc = amp_ingr.tipo_doc
                                   AND amp_cons.nro_doc  = amp_ingr.nro_doc
                                   AND amp_ingr.tipo_mov = (select oper_ing_prod from logparam where reckey = '1')
                                   AND amp_cons.tipo_mov = (select oper_cons_interno from logparam where reckey = '1')
                                   AND rag.cod_art       = amp_ingr.cod_art
                                   AND rag.grupo_art     = :as_grupo_art
                                   AND ot2.cod_origen    = :as_origen
                                   AND amp_cons.flag_estado IN ('1', '2')
                                   AND amp_cons.cant_proyect > 0)
group by a.cod_clase, ac.desc_clase, a.cod_art, a.desc_art, a.und, am.cod_moneda
having sum(am.cant_procesada * amt.factor_sldo_total) > 0
order by ac.desc_clase, a.cod_art
" arguments=(("as_origen", string),("as_grupo_art", string),("ad_fecha", date)) )
