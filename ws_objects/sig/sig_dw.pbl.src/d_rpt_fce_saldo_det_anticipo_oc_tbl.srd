 retrieve="SELECT d.cod_relacion, p.nom_proveedor, d.tipo_doc, d.fecha_vencimiento,  
       d.nro_doc, d.fecha_doc, dr.tipo_ref, 
       dr.nro_ref, d.saldo_dol
from doc_pendientes_cta_cte d, doc_referencias dr, proveedor p
where ( d.cod_relacion=dr.cod_relacion(+) and
        d.tipo_doc = dr.tipo_doc(+) and
        d.nro_doc = dr.nro_doc(+) ) and
      ( d.cod_relacion = p.proveedor) and        
      ( d.cnta_ctbl is not null ) and
      ( d.tipo_doc = :as_tipo_doc) and
      ( d.sldo_sol>0 and d.saldo_dol>0) and
      (sysdate - d.fecha_doc > :an_dias )
order by d.saldo_dol desc" arguments=(("as_tipo_doc", string),("an_dias", number)) )
