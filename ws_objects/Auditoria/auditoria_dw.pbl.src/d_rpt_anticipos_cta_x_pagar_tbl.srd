 retrieve="select trunc(c.fecha_emision) as fecha, 
       ( c.origen||'-'||to_char(c.ano)||'-'||to_char(c.mes)||'-'||to_char(c.nro_libro)||'-'||to_char(c.nro_asiento)) as voucher, 
       p.nom_proveedor, c.ano, c.mes, c.nro_libro, c.nro_asiento, 
       cp.cod_relacion, cp.tipo_doc, cp.nro_doc, cp.descripcion, cp.cod_moneda, 
       decode(cp.cod_moneda,:as_dolar,cp.importe_doc,cp.importe_doc/cp.tasa_cambio) as costo_dol, 
       decode(cp.cod_moneda,:as_dolar,cp.importe_doc*cp.tasa_cambio,cp.importe_doc) as costo_sol, 
       cp.saldo_dol, cp.saldo_sol, dr.tipo_ref, dr.nro_ref 
from cntas_pagar cp, proveedor p, caja_bancos_det cbd, caja_bancos c, doc_referencias dr 
where (cp.cod_relacion = p.proveedor) and 
		(cp.cod_relacion=cbd.cod_relacion and 
       cp.tipo_doc=cbd.tipo_doc and 
       cp.nro_doc=cbd.nro_doc and 
       cp.tipo_doc=:as_doc_anticipo and 
       cp.flag_estado<>'0') and 
      (cbd.origen=c.origen and 
       cbd.nro_registro=c.nro_registro and 
       c.flag_estado<>'0') and 
      (cp.cod_relacion=dr.cod_relacion and 
       cp.tipo_doc=dr.tipo_doc and 
       cp.nro_doc=dr.nro_doc) and 
      (trunc(c.fecha_emision) between :ad_fecha_ini and :ad_fecha_fin) 
order by cp.cod_relacion, cp.tipo_doc, cp.nro_doc       " arguments=(("as_doc_anticipo", string),("as_dolar", string),("ad_fecha_ini", date),("ad_fecha_fin", date)) )
