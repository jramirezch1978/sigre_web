 retrieve="SELECT CP.ORIGEN,
       CP.TIPO_DOC,
       CP.NRO_DOC,
       CP.FECHA_REGISTRO,
       CPD.ITEM,
		 CPD.DESCRIPCION,
		 CPD.IMPORTE,
       CP.COD_MONEDA,
       M.DESCRIPCION,
       CP.COD_RELACION,
       CP.COD_USR,
       P.NOM_PROVEEDOR,
       CPD.CENCOS,
       CC.DESC_CENCOS,
       CPD.CNTA_PRSP,
       CPD.CENTRO_BENEF,
       CB.DESC_CENTRO
       
  FROM CNTAS_PAGAR      CP,
       CNTAS_PAGAR_DET  CPD,
       CENTRO_BENEFICIO CB,
       CENTROS_COSTO    CC,
       PROVEEDOR        P,
		 DOC_REFERENCIAS  DR,
       MONEDA	         M
       
 WHERE CP.COD_RELACION  	= CPD.COD_RELACION
   AND CP.TIPO_DOC      	= CPD.TIPO_DOC
   AND CP.NRO_DOC       	= CPD.NRO_DOC
   AND CPD.CENTRO_BENEF 	= CB.CENTRO_BENEF(+)
   AND CPD.CENCOS       	= CC.CENCOS(+)
   AND P.PROVEEDOR      	= CP.COD_RELACION
   AND DR.COD_RELACION(+)  = CP.COD_RELACION
   AND DR.TIPO_DOC(+)      = CP.TIPO_DOC
   AND DR.NRO_DOC(+)       = CP.NRO_DOC
   AND M.COD_MONEDA	      = CP.COD_MONEDA
   AND CPD.ORG_AMP_REF 		IS NULL
   AND CPD.NRO_AMP_REF 		IS NULL
   AND CP.TIPO_DOC IN(SELECT NVL(F.DOC_FACT_PAGAR,' ')FROM FINPARAM F)
   AND trunc(CP.FECHA_REGISTRO) BETWEEN (:ad_fecha1) AND (:ad_fecha2)
   AND CP.ORIGEN LIKE :as_cod_origen
   AND CP.FLAG_ESTADO <> '0'
   AND CP.FLAG_PROVISIONADO <> 'D'
   AND DR.NRO_REF 	IS NULL
   AND DR.ORIGEN_REF IS NULL
   AND DR.TIPO_REF 	IS NULL" arguments=(("ad_fecha1", date),("ad_fecha2", date),("as_cod_origen", string)) )
